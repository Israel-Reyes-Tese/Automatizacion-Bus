        ptIOHWMTP2LinkRxOverflows       Integer32,
        ptIOHWMTP2LinkNumberOfMSUToSend Integer32,
        ptIOHWMTP2LinkSizeOfMSUToTx     Integer32,
        ptIOHWMTP2LinkN1                Integer32,
        ptIOHWMTP2LinkN2                Integer32,
        ptIOHWMTP2LinkLinkType          INTEGER,
        ptIOHWMTP2LinkL3LinkNumber      Integer32,
        ptIOHWMTP2LinkIOManagerIndex    Integer32,
        ptIOHWMTP2LinkLSLChannelsIndex  Integer32,
        ptIOHWMTP2LinkActions           INTEGER,
        ptIOHWMTP2LinkParentOID         OBJECT IDENTIFIER,
        ptIOHWMTP2LinkRowStatus         RowStatus,
        ptIOHWMTP2LinkIndex             Integer32
    }

