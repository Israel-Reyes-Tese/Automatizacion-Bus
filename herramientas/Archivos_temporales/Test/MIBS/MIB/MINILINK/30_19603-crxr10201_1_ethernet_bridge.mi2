             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.1
        xfStpMstiPortEnable OBJECT-TYPE
            SYNTAX EnabledStatus
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "To set the Administrative Bridge Port state for the specified Port to
                Disabled or Enabled.
                
                Default is disabled(2).
                "
            ::= { xfStpMstiPortEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.2
        xfStpMstiPortForwardTransitions OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The number of times this port has transitioned from the Learning state
                to the Forwarding state."
            ::= { xfStpMstiPortEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.3
        xfStpMstiPortDesignatedBridge OBJECT-TYPE
            SYNTAX BridgeId
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The Bridge Identifier of the bridge that this port considers
                to be the Designated Bridge for this port's segment."
            ::= { xfStpMstiPortEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.4
        xfStpMstiPortDesignatedCost OBJECT-TYPE
            SYNTAX Integer32 (0..200000000)
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The path cost of the Designated Port of the segment
                connected to this port. This value is compared to
                the Root Path Cost field in received bridge PDUs."
            ::= { xfStpMstiPortEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.5
        xfStpMstiPortDesignatedPort OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE (2))
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The Port ID of the port on the Designated Bridge for this
                port's network segment."
            ::= { xfStpMstiPortEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.6
        xfStpMstiPortDesignatedRoot OBJECT-TYPE
            SYNTAX BridgeId
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The unique Bridge Identifier of the Bridge recorded as
                the Root in the Configuration BPDUs transmitted by the
                Designated Bridge for the segment to which the port is
                attached."
            ::= { xfStpMstiPortEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.7
        xfStpMstiPortDisputed OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The current value of the disputed variable."
            ::= { xfStpMstiPortEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.8
        xfStpMstiPortPathCost OBJECT-TYPE
            SYNTAX Integer32 (0..200000000)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The contribution of this port to the path cost of paths towards
                the spanning tree instance root which include this port."
            ::= { xfStpMstiPortEntry 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.9
        xfStpMstiPortPriority OBJECT-TYPE
            SYNTAX INTEGER (0..255)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The priority field for the Port Identifier.
                
                Default value is 128."
            ::= { xfStpMstiPortEntry 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.10
        xfStpMstiPortPortRole OBJECT-TYPE
            SYNTAX INTEGER
                {
                disabled(1),
                alternate(2),
                backup(3),
                designated(4),
                root(5),
                master(6),
                notApplicable(7)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The current Port Role for the Port."
            ::= { xfStpMstiPortEntry 10 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.11
        xfStpMstiPortState OBJECT-TYPE
            SYNTAX INTEGER
                {
                disabled(1),
                blocking(2),
                listening(3),
                learning(4),
                forwarding(5),
                broken(6)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The current state of the Port (i.e., Disabled, Listening,
                Learning, Forwarding, or Blocking).
                
                Reference 8.4,13.35 of 802.1Q-2005."
            ::= { xfStpMstiPortEntry 11 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.2.3.1.12
        xfStpMstiPortPortUpTime OBJECT-TYPE
            SYNTAX TimeTicks
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Count in seconds of the time elapsed since the Port was
                last reset or initialized. Same as ifLastChange."
            ::= { xfStpMstiPortEntry 12 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3
        xfBridgeStpMsti OBJECT IDENTIFIER ::= { xfBridgeStp 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1
        xfStpMstiTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfStpMstiEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table is used to obtain information regarding the Bridge's Bridge
                Protocol Entity for the specified Spanning Tree instance.
                
                It is also used to modify parameters in the Bridge's Bridge Protocol
                Entity for the specified Spanning Tree instance, in order to force a
                configuration of the spanning tree and/or tune the reconfiguration
                time to suit a specific topology."
            ::= { xfBridgeStpMsti 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1
        xfStpMstiEntry OBJECT-TYPE
            SYNTAX XfStpMstiEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of a MSTI instance."
            INDEX { xfStpMstiId }
            ::= { xfStpMstiTable 1 }

        XfStpMstiEntry ::=
            SEQUENCE {
                xfStpMstiId
                    Integer32,
                xfStpMstiBridgeIdentifier
                    BridgeId,
                xfStpMstiBridgePriority
                    INTEGER,
                xfStpMstiDesignatedRoot
                    BridgeId,
                xfStpMstiRootPathCost
                    Integer32,
                xfStpMstiRootPort
                    Integer32,
                xfStpMstiTimeSinceTopologyChange
                    TimeTicks,
                xfStpMstiTopologyChange
                    TruthValue,
                xfStpMstiTopologyChangeCount
                    Counter32,
                xfStpMstiVids
                    OCTET STRING,
                xfStpMstiRowStatus
                    RowStatus
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.1
        xfStpMstiId OBJECT-TYPE
            SYNTAX Integer32 (1..4094)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Identifier of the MST Instance."
            ::= { xfStpMstiEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.2
        xfStpMstiBridgeIdentifier OBJECT-TYPE
            SYNTAX BridgeId
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The Bridge Identifier for the spanning tree instance identified by the
                xfStpMstId."
            ::= { xfStpMstiEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.3
        xfStpMstiBridgePriority OBJECT-TYPE
            SYNTAX INTEGER (0..65535)
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "The priority part of the Bridge Identifier for the Spanning Tree
                instance.
                
                The default priority is 32768."
            ::= { xfStpMstiEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.4
        xfStpMstiDesignatedRoot OBJECT-TYPE
            SYNTAX BridgeId
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The Bridge Identifier of the Root Bridge for the spanning tree
                instance identified by the xfStpMstId."
            ::= { xfStpMstiEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.5
        xfStpMstiRootPathCost OBJECT-TYPE
            SYNTAX Integer32 (0..200000000)
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The path cost from the transmitting Bridge to the Root Bridge for
                the spanning tree instance identified by the xfStpMstId."
            ::= { xfStpMstiEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.6
        xfStpMstiRootPort OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The Root Port for the spanning tree instance identified by
                the xfStpMstId."
            ::= { xfStpMstiEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.7
        xfStpMstiTimeSinceTopologyChange OBJECT-TYPE
            SYNTAX TimeTicks
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Count in seconds of the time elapsed since tcWhile
                (see 13.21 of 802.1Q-2005) was last non-zero for any Port for the
                given MSTI."
            ::= { xfStpMstiEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.8
        xfStpMstiTopologyChange OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "True if tcWhile is non-zero for any Port for the given MST."
            ::= { xfStpMstiEntry 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.9
        xfStpMstiTopologyChangeCount OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Count of the times tcWhile (see 13.21 of 802.1Q-2005) has been
                non-zero for any Port for the given MSTI since the Bridge was
                powered on or initialized."
            ::= { xfStpMstiEntry 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.10
        xfStpMstiVids OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE (512))
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Set VLAN members of the MSTI. Bitmap with 4094 positions
                for each VLAN.
                "
            ::= { xfStpMstiEntry 10 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.6.3.1.1.11
        xfStpMstiRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Used to create/delete an MSTI instance."
            ::= { xfStpMstiEntry 11 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7
        xfBridgeLag OBJECT IDENTIFIER ::= { xfEthernetBridgeObjects 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1
        xfLagTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfLagEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table used for link aggregation. A link aggregation group has
                one master port which is used for configuration.Only the master
                port is available for configuration;the other members inherit their
                configuration from the master port. An exception is RSTP
                configurations which are not inherited by the other members.
                
                LAG membership criteria:
                   - port not connected LAN-DCN
                   - port not used as mirror-to-port
                   - port not member of VLAN(s) other than default vlan (1).
                   - port not in layer1 role
                "
            ::= { xfBridgeLag 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1
        xfLagEntry OBJECT-TYPE
            SYNTAX XfLagEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of a link aggregation group. The group is indexed by the master port
                Therefore, it is not possible to remove the master port without killing the whole
                group."
            INDEX { xfLagMasterPort }
            ::= { xfLagTable 1 }

        XfLagEntry ::=
            SEQUENCE {
                xfLagMasterPort
                    Integer32,
                xfLagMembers
                    PortList,
                xfLagName
                    SnmpAdminString,
                xfLagAlarmStatus
                    BITS,
                xfLagAlarmEnable
                    BITS,
                xfLagDesignatedPort
                    Integer32,
                xfLagRowStatus
                    RowStatus,
                xfLagHashing
                    XfHashMode,
                xfLagLACPcontrolmode
                    INTEGER,
                xfLagOperStatus
                    INTEGER,
                xfLagAdminStatus
                    INTEGER,
                xfLagLACPExtendedBuffer
                    INTEGER,
                xfLagSwitchoverCount
                    Counter32,
                xfLagProtectionMode
                    INTEGER
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.1
        xfLagMasterPort OBJECT-TYPE
            SYNTAX Integer32 (1..2147483647)
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "The bridge port number used as the master port
                for the group."
            ::= { xfLagEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.2
        xfLagMembers OBJECT-TYPE
            SYNTAX PortList
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "A portlist that shows all members of the group. The bit position of the
                master port is always set and can not be modified."
            ::= { xfLagEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.3
        xfLagName OBJECT-TYPE
            SYNTAX SnmpAdminString (SIZE (0..32))
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Adminstrative assigned text string used as a name
                for the group."
            ::= { xfLagEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.4
        xfLagAlarmStatus OBJECT-TYPE
            SYNTAX BITS
                {
                lagDegradedService0(0),
                lagDegradedService1(1),
                lagDegradedService2(2),
                lagNoTraffic0(3),
                lagNoTraffic1(4),
                lagNoTraffic2(5),
                lagUnableToProtect0(6),
                lagUnableToProtect1(7),
                lagUnableToProtect2(8)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "This object indicates the status of the LAG group
                
                    Severity is indicated by three bits per alarm:
                    bits 012
                         000   clear
                         001   undetermined
                         010   warning
                         011   minor
                         100   major
                         101   critical"
            ::= { xfLagEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.5
        xfLagAlarmEnable OBJECT-TYPE
            SYNTAX BITS
                {
                lagDegradedService(0),
                lagNoTraffic(1),
                lagUnableToProtect(2)
                }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Used to enable/disable alarm from this group.
                
                lagDegradedService(0) :  Setting this bit enables degradedService(some
                                         LAG members are down) alarm.
                
                lagNoTraffic(1)       :  Setting this bit enables noTraffic (all members are
                                         down) alarm.
                
                lagUnableToProtect(2) :  Setting this bit enables unableToProtect (one port
                                         is down in LACP 1+1 mode) alarm.
                "
            ::= { xfLagEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.6
        xfLagDesignatedPort OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Shows the lag member port that is used to send
                BPDU (Bridge PDU e.g. RSTP)"
            ::= { xfLagEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.7
        xfLagRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Used to create or destroy a group. "
            ::= { xfLagEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.8
        xfLagHashing OBJECT-TYPE
            SYNTAX XfHashMode
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Used to set hashing algorithm. "
            DEFVAL { srcDstMac }
            ::= { xfLagEntry 8 }

         -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.9
         xfLagLACPcontrolmode OBJECT-TYPE
            SYNTAX INTEGER
            {
              active(1),
              static(2),
              onePlusOne(3)
            }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "LACP control mode.
                
                active(1)  : LACP is enabled. Traffic is distributed between LAG members.
                
                static(2)  : LACP is disabled. Traffic is distributed between LAG members.
                
                onePlusOne(3) : LAG operates in 1+1 protective mode with LACP enabled.
                             By default traffic is sent on the LAG master port.
                             In case master port is down the standby member takes over.
                "
            DEFVAL { static }
            ::= { xfLagEntry 9 }

         -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.10
         xfLagOperStatus OBJECT-TYPE
            SYNTAX INTEGER
            {
              up(1),
              down(2)
            }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "only used if xfLagTable.xfLagLACPcontrolmode is active. "
            ::= { xfLagEntry 10 }

         -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.11
         xfLagAdminStatus OBJECT-TYPE
            SYNTAX INTEGER
            {
              up(1),
              down(2)
            }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "only used if xfLagTable.xfLagLACPcontrolmode is active. "
            ::= { xfLagEntry 11 }

         -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.12
         xfLagLACPExtendedBuffer OBJECT-TYPE
            SYNTAX INTEGER
            {
              disabled(0),
              enabled(1)
            }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Enabling extended buffering in LAG-LACP mode"
            ::= { xfLagEntry 12 }

         -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.13
         xfLagSwitchoverCount OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The number of protective and revertive switches since the last restart.
                Applicable in LACP 1+1 mode.
                "
            ::= { xfLagEntry 13 }

         -- 1.3.6.1.4.1.193.81.4.1.2.1.7.1.1.14
         xfLagProtectionMode OBJECT-TYPE
            SYNTAX INTEGER {
              nonRevertive(1),
              revertive(2)
            }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "This object is used to enable/disable revertive switching in LACP 1+1 mode"
            DEFVAL { nonRevertive }
            ::= { xfLagEntry 14 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8
        xfEthernetService OBJECT IDENTIFIER ::= { xfEthernetBridgeObjects 8 }

