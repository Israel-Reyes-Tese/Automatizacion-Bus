        ptGTTGTTConfigurationUnavailablePeerIDs       DisplayString,
        ptGTTGTTConfigurationAdvancedSearch   INTEGER,
        ptGTTGTTConfigurationDefaultDBDefName     DisplayString,
        ptGTTGTTConfigurationCrossNAUDTS INTEGER,
        ptGTTGTTConfigurationActions    INTEGER,
        ptGTTGTTConfigurationParentOID  OBJECT IDENTIFIER,
        ptGTTGTTConfigurationRowStatus  RowStatus,
        ptGTTGTTConfigurationIndex      INTEGER
    }

ptGTTGTTConfigurationName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Obsolete as of 9.0."
    ::= { ptGTTGTTConfigurationEntry 1 }

