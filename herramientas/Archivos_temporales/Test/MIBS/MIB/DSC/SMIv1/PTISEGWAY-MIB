    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IP service port (IP-SS7 converter only). \nSignaling Gateway connection for User Application Protocol (UAP) management"
    ::= { ptL4CvtrL4CvtrConfigurationEntry 15 }

ptL4CvtrL4CvtrConfigurationCPAMID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "CPA Message identifier (IP-SS7 converter only). \nUsed to envelop an unsupported message sent to the IP application for Custom Pass Along (CPA) back to the SS7 network. Valid range is 0 to 255."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 16 }

ptL4CvtrL4CvtrConfigurationCPAPID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "CPA parameter identifier (IP-SS7 converter only). \nUsed to envelop an unsupported parameter sent to the IP application for Custom Pass Along (CPA) back to the SS7 network. Valid range is 0 to 255."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 17 }

ptL4CvtrL4CvtrConfigurationSignalingIndicator OBJECT-TYPE
    SYNTAX          INTEGER { isup(5), sccp(3), isupAndSccp(255), 
                        unknown(254) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Licensed Signaling Indicator. \nWill be set to ISUP, SCCP, or ISUP and SCCP based on the license. \nWill be set to UNKNOWN if unlicensed. \nObsolete as of 14.1"
    ::= { ptL4CvtrL4CvtrConfigurationEntry 18 }

ptL4CvtrL4CvtrConfigurationIncomingAppIDPrecedence OBJECT-TYPE
    SYNTAX          INTEGER { opc(0), dpc(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Incoming AppID Precedence (OPC/DPC). \nPrecedence only applies if both associated Route Mappings for an MSU conversion contain an Incoming AppID. The chosen Incoming AppID will be taken from the Routing Mapping corresponding with either the OPC or DPC as per this attribute."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 19 }

