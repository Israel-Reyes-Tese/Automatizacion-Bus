PtIOHWHSLATMChannelEntry ::=
    SEQUENCE {
        ptIOHWHSLATMChannelChannelNumber INTEGER,
        ptIOHWHSLATMChannelCLLI         DisplayString,
        ptIOHWHSLATMChannelPort         INTEGER,
        ptIOHWHSLATMChannelChannelState INTEGER,
        ptIOHWHSLATMChannelVPI          INTEGER,
        ptIOHWHSLATMChannelVCI          INTEGER,
        ptIOHWHSLATMChannelScrambling   INTEGER,
        ptIOHWHSLATMChannelHECPolynomial INTEGER,
        ptIOHWHSLATMChannelHECVerification  INTEGER,
        ptIOHWHSLATMChannelQualityOfService   INTEGER,
        ptIOHWHSLATMChannelT1           INTEGER,
        ptIOHWHSLATMChannelT2           INTEGER,
        ptIOHWHSLATMChannelT3           INTEGER,
        ptIOHWHSLATMChannelAlignment    INTEGER,
        ptIOHWHSLATMChannelN1           INTEGER,
        ptIOHWHSLATMChannelCongestionLevel1Onset        INTEGER,
        ptIOHWHSLATMChannelCongestionLevel2Onset        INTEGER,
        ptIOHWHSLATMChannelCongestionLevel3Onset        INTEGER,
        ptIOHWHSLATMChannelCongestionLevel1Abate        INTEGER,
        ptIOHWHSLATMChannelCongestionLevel2Abate        INTEGER,
        ptIOHWHSLATMChannelCongestionLevel3Abate        INTEGER,
        ptIOHWHSLATMChannelL3Use        INTEGER,
        ptIOHWHSLATMChannelDebug        DisplayString,
        ptIOHWHSLATMChannelManagement   INTEGER,
        ptIOHWHSLATMChannelSSCFMessages INTEGER,
        ptIOHWHSLATMChannelSSCOPMessages INTEGER,
        ptIOHWHSLATMChannelPDUs         INTEGER,
        ptIOHWHSLATMChannelIOManagerIndex INTEGER,
        ptIOHWHSLATMChannelHSLATMChannelsIndex      INTEGER,
        ptIOHWHSLATMChannelActions      INTEGER,
        ptIOHWHSLATMChannelParentOID    OBJECT IDENTIFIER,
        ptIOHWHSLATMChannelRowStatus    RowStatus,
        ptIOHWHSLATMChannelIndex        INTEGER
    }

