        ptDSCDSCNodesCurrentWeekSystemTPSHighWaterMark              INTEGER,
        ptDSCDSCNodesCurrentMonthSystemTPSHighWaterMark               INTEGER,
        ptDSCDSCNodesCurrentYearSystemTPSHighWaterMark              INTEGER,
        ptDSCDSCNodesPreviousDaySystemTPSHighWaterMark              INTEGER,
        ptDSCDSCNodesPreviousWeekSystemTPSHighWaterMark               INTEGER,
        ptDSCDSCNodesPreviousMonthSystemTPSHighWaterMark                INTEGER,
        ptDSCDSCNodesPreviousYearSystemTPSHighWaterMark               INTEGER,
        ptDSCDSCNodesRealmStatsName     INTEGER,
        ptDSCDSCNodesLocalOverloadParameters    DisplayString,
        ptDSCDSCNodesLDLDebugLevel      INTEGER,
        ptDSCDSCNodesQueueSize          INTEGER,
        ptDSCDSCNodesQueueUsageThreshold1 INTEGER,
        ptDSCDSCNodesQueueUsageThreshold2 INTEGER,
        ptDSCDSCNodesQueueUsageThreshold3 INTEGER,
        ptDSCDSCNodesDefaultRoutingReturnPrimaryError             INTEGER,
        ptDSCDSCNodesDSCIndex           INTEGER,
