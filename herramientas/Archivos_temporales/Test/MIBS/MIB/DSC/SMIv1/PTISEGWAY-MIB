ptConfiguration OBJECT IDENTIFIER
    ::= { ptConnGroup 306 }

ptConfigurationTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtConfigurationEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptConfiguration 1 }

ptConfigurationEntry OBJECT-TYPE
    SYNTAX          PtConfigurationEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of Configuration attributes."
    INDEX           { ptConnGroupIndex, ptConfigurationIndex }
    ::= { ptConfigurationTable 1 }

PtConfigurationEntry ::=
    SEQUENCE {
        ptConfigurationCLIPort          INTEGER,
        ptConfigurationAppletPort       INTEGER,
        ptConfigurationGUIPort          INTEGER,
        ptConfigurationTL1Port          INTEGER,
        ptConfigurationConfigurationFile DisplayString,
        ptConfigurationTrapCommunity    DisplayString,
        ptConfigurationCurrentSequenceNumber    INTEGER,
        ptConfigurationSystemName       DisplayString,
        ptConfigurationSingleIntervalStatsFiles       INTEGER,
        ptConfigurationSoftwareVersionInstalled       DisplayString,
        ptConfigurationSingleTrapStream INTEGER,
        ptConfigurationDSCRESTAPI       INTEGER,
        ptConfigurationActiveTraps      INTEGER,
        ptConfigurationParentOID        OBJECT IDENTIFIER,
        ptConfigurationRowStatus        RowStatus,
        ptConfigurationIndex            INTEGER
    }

ptConfigurationCLIPort OBJECT-TYPE
    SYNTAX          INTEGER (-32768..32767)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This attribute displays the port used for CLI communication."
    ::= { ptConfigurationEntry 1 }

ptConfigurationAppletPort OBJECT-TYPE
    SYNTAX          INTEGER (-32768..32767)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Obsolete."
    ::= { ptConfigurationEntry 2 }

ptConfigurationGUIPort OBJECT-TYPE
    SYNTAX          INTEGER (-32768..32767)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This attribute displays the port used for the text GUI communication."
    ::= { ptConfigurationEntry 3 }

ptConfigurationTL1Port OBJECT-TYPE
    SYNTAX          INTEGER (-32768..32767)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This attribute displays the port used for TL1 communication."
    ::= { ptConfigurationEntry 4 }

ptConfigurationConfigurationFile OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This attribute displays the path to the configuration File."
    ::= { ptConfigurationEntry 5 }

ptConfigurationTrapCommunity OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This attribute displays the community name that will be sent within SNMP Traps. Maximum length is 64 characters. Only alphanumeric characters are allowed."
    ::= { ptConfigurationEntry 6 }

ptConfigurationCurrentSequenceNumber OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Each Trap leaves the system with an associated Trap sequence number. This sequence number can be used by the operators to see if the last Traps received are the last Traps sent.\n\n The Trap sequence number increases by one every time a Trap is generated. On system restart, the Trap sequence number is 0 until a trap gets sent."
    ::= { ptConfigurationEntry 7 }

ptConfigurationSystemName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field displays the mean to name the system. The system name is in the top left corner of any application user interface. The default value is Unset."
    ::= { ptConfigurationEntry 8 }

ptConfigurationSingleIntervalStatsFiles OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This attribute, when ENABLED, results in generation of single interval statistics files. \nThe standard DSC system statistics files contain the object based statistics for the specified interval (5 minutes, 30 minutes, or 1 day) on a daily basis all into the same file. \nThe feature is disabled by default."
    ::= { ptConfigurationEntry 9 }

ptConfigurationSoftwareVersionInstalled OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The Software version number installed on the system to conform to Ribbon format: VMM.mm.dd[Build Type]xxx where MM is major version, mm is minor version, dd is dot version, xxx is build number. [Build Type] is Engineering (A), Release (R), Special (S)."
    ::= { ptConfigurationEntry 10 }

ptConfigurationSingleTrapStream OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This attribute, when set to ENABLED, results in sending only one stream of traps with the floating IP address as the trap source IP. The setting of the source IP is accomplished by firewall. Two streams of traps, with the source IP set to the IP of their associated Management CPU, are sent when this attribute is set to DISABLED. \nThe feature is disabled by default."
    ::= { ptConfigurationEntry 11 }

ptConfigurationDSCRESTAPI OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This attribute allows or disallows access to DSC REST API. The feature is DISABLED by default. The REST API is accessed on port 8890."
    ::= { ptConfigurationEntry 12 }

ptConfigurationActiveTraps OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This attribute, when set to ENABLED, displays the list of current active traps on the DSC system in the associated Active Traps List object under the SNMP application. The Active Traps List object is empty when this attribute is set to DISABLED. \nThe feature is disabled by default."
    ::= { ptConfigurationEntry 13 }

ptConfigurationParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptConfigurationEntry 253 }

