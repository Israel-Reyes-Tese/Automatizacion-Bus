		xfCNPacketTestTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfCNPacketTestEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"MOs for testing packet. It is a indexed table (by entPhysicalIndex)."
			::= { xfCNPacketTestObjects 1 }
		
		-- 1.3.6.1.4.1.193.81.7.1.1.7.1.1
		xfCNPacketTestEntry OBJECT-TYPE
			SYNTAX XfCNPacketTestEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Packet test entry.
				The table is indexed with entPhysicalIndex of the modem (CN500)"
			INDEX { entPhysicalIndex }
			::=	{ xfCNPacketTestTable 1}			
		
		XfCNPacketTestEntry ::=
			SEQUENCE {
				xfCNTestNearEnd  	INTEGER,
				xfCNNearEndTestResult SnmpAdminString
				}
				
		-- 1.3.6.1.4.1.193.81.7.1.1.7.1.1.1
		xfCNTestNearEnd OBJECT-TYPE
			SYNTAX INTEGER {
					other (1),
					start  (2),
					pending  (3),
					finished (4)
          
      }
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Start,
				pending (read-only),
				finished (read-only).
				Start actuates the test, pending is during the test, finished then the result can be read."
			DEFVAL { other }	
			::= { xfCNPacketTestEntry 1 }
		
		-- 1.3.6.1.4.1.193.81.7.1.1.7.1.1.2			 
		xfCNNearEndTestResult OBJECT-TYPE
			SYNTAX SnmpAdminString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"TestOk
				ErrorTBD1,
				ErrorTBD2.
				Result can be read 30 seconds after the actuation and will be presented in the GUI as is. 
				That means that more test results can be added without updating the Craft."	
			::= { xfCNPacketTestEntry 2 }
		
		
