sonusSs7ServicesMIBNotifications OBJECT IDENTIFIER
    ::= { sonusSs7ServicesMIB 2 }

sonusSs7ServicesMIBNotificationsPrefix OBJECT IDENTIFIER
    ::= { sonusSs7ServicesMIBNotifications 0 }

sonusSs7ServicesMIBNotificationsObjects OBJECT IDENTIFIER
    ::= { sonusSs7ServicesMIBNotifications 1 }

sonusSs7NodeServiceType OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..6))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The type of service supported by this SS7 node connection (ISUP or TCAP)."
    ::= { sonusSs7ServicesMIBNotificationsObjects 1 }

sonusSs7NodeStateChangeReason OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..101))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The reason why this SS7 node connection has  changed state."
    ::= { sonusSs7ServicesMIBNotificationsObjects 2 }

sonusSs7NodeCurrentState OBJECT-TYPE
    SYNTAX          INTEGER { offline(1), active(2), initializing(3), 
                        failed(4), disabled(5), congested(6) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The current state of this SS7 node connection."
    ::= { sonusSs7ServicesMIBNotificationsObjects 3 }

sonusSs7NodeAffectedPointCode OBJECT-TYPE
    SYNTAX          PointCode
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The SS7 point code affected by this MTP state change."
    ::= { sonusSs7ServicesMIBNotificationsObjects 4 }

sonusSs7NodeACCObject OBJECT-TYPE
    SYNTAX          INTEGER { sonusSs7NodeTimerTAcc(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This object is a bit mask that indicates what ACC
             object in the SS7 Node table has changed."
    ::= { sonusSs7ServicesMIBNotificationsObjects 5 }

sonusSs7NodeServerProtocolChangeReason OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..101))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The reason why SS7 Node server protocol is changed to SUPERRAW "
    ::= { sonusSs7ServicesMIBNotificationsObjects 6 }

sonusSs7NodeConnectionEstablishedNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, sonusSs7NodeGatewayAssignment, 
                      sonusSs7NodeServiceType, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that a connection has been established to the
             specified SS7 gateway. Information provided by this trap includes
             the name of the connected SS7 node, the name of the SS7 gateway 
             on which the node resides, and the type of SS7 service that has been
             established (ISUP or TCAP)."
    ::= 1

sonusSs7NodeConnectionFailedNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, sonusSs7NodeGatewayAssignment, 
                      sonusSs7NodeServiceType, 
                      sonusSs7NodeStateChangeReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that an established connection to an SS7 gateway
             has failed. Information provided by this trap includes the name of the
             failed SS7 node, the name of the SS7 gateway on which the node resides,
             the type of SS7 service affected by this failure, and the reason why
             the connection has failed."
    ::= 2

sonusSs7NodeConnectionStateChangeNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, sonusSs7NodeGatewayAssignment, 
                      sonusSs7NodeServiceType, sonusSs7NodeCurrentState, 
                      sonusSs7NodeStateChangeReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that an established connection to an SS7 gateway
             has changed state. Information provided by this trap includes the name 
             of the affected SS7 node, the name of the SS7 gateway on which the node 
             resides, the type of SS7 service affected by this failure, the new state
             of the node, and the reason for the state change."
    ::= 3

sonusSs7NodeMtpStateChangeNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, sonusSs7NodeGatewayAssignment, 
                      sonusSs7NodeServiceType, sonusSs7NodeMtpStatus, 
                      sonusSs7NodeAffectedPointCode, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that a MTP state change event has been received from the
             SS7 gateway, affecting the specified destination (or remote) point 
             code (DPC). Information provided by this trap includes the name 
             of the affected SS7 node, the name of the SS7 gateway on which the node 
             resides, the updated MTP state, and the DPC affected by this event."
    ::= 4

sonusSs7NodeACCConfNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, sonusSs7NodeACCObject, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that a ACC Control 
             configuration object in  
             the SS7 node table has changed.  The  
             sonusSs7nodeACCObject parameter indicates what  
             object or objects have changed."
    ::= 5

sonusSs7NodeServerProtocolChangedToSuperRaw TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, 
                      sonusSs7NodeServerProtocolChangeReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that SS7 Node's server protocol
             is forced to SUPERRAW, since PROP/ RAW modes are not
             supported in this GSX Release."
    ::= 6

sonusSs7NodeFailedCriticalNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, sonusSs7NodeGatewayAssignment, 
                      sonusSs7NodeServiceType, 
                      sonusSs7NodeStateChangeReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This critical trap indicates that a established connection to an SS7 gateway
             has failed. Information provided by this trap includes the name of the
             failed SS7 node, the name of the SS7 gateway on which the node resides,
             the type of SS7 service affected by this failure, and the reason why
             the connection has failed."
    ::= 7

sonusSs7NodeConnectivityChangeNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, sonusSs7NodeServiceType, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates the connectivity of the named SS7 node 
             to the SS7 network has changed.  The new state can be either 
             CONNECTED or DISCONNECTED."
    ::= 8

sonusSs7NodeRpcStateChangeNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, sonusSs7NodeServiceType, 
                      sonusSs7NodeMtpStatus, 
                      sonusSs7NodeAffectedPointCode, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that the state of the named remote point 
             code in relation to the named node has changed.  The new state 
             can be one of PAUSE, RESUME, CONGESTED, or UPU"
    ::= 9

sonusSs7NodeRoutingKeyOverriddenNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusSs7NodeName, sonusSs7NodeAffectedPointCode, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that the active status of an M3UA 
             routing key between the named node and the remote point 
             code of this chassis has been overridden."
    ::= 10

sonusSs7StandbyNullPointerNotification TRAP-TYPE
    ENTERPRISE      sonusSs7ServicesMIBNotifications
    VARIABLES       { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that either the pNode or pConnMgmt pointer in the
             standby SS7FE is NULL.  This trap will be generated
             either when the Standby Node is going to Active state or
             when the Standby Node receives Redundancy messages from
             Active Node."
    ::= 11

END -- end of module SONUS-SS7-SERVICES-MIB.
