XF-ADM-ALARM-STATUS-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        MODULE-IDENTITY,
        OBJECT-TYPE
            FROM SNMPv2-SMI

        TEXTUAL-CONVENTION
            FROM SNMPv2-TC

        ifIndex
            FROM IF-MIB

        xfSdhAdm
            FROM XF-TOP-MIB;


       xfAdmAlarmStatusMib MODULE-IDENTITY
            LAST-UPDATED "201110271200Z"    -- Oct. 27, 2011 at 12:00 GMT
            ORGANIZATION
                "EDD/FBM/XBB"
            CONTACT-INFO
                "Joachim Hasler "
             DESCRIPTION
                "Alarm/Fault manangement information for SDH add-drop multiplexer (ADM).

                ALARM ABBREVIATIONS (in alphabetical order):
                  - ais:     Alarm indication signal
                  - deg:     Degraded signal
                  - exc:     Excessive errors
                  - lom:     Loss of multiframe
                  - lop:     Loss of pointer
                  - los:     Loss of signal
                  - plm:     Payload mismatch
                  - rdi:     Remote defect indication
                  - tim:     Trace identifier mismatch
                  - uastfe:  far end unavailable state
                  - uastne:  near end Unavailable state
                  - ueq:     Unequipped
                  - k2mism:  K2 mismatch                  
                "
           REVISION     "201110271200Z"    -- Oct. 27, 2011 at 12:00 GMT
           DESCRIPTION
                "REV PA3:
                  Added new alarm status bits for k2mism to xfMsTtpAlarmStatus
                  Added xfMsTtpAlarmSeverityK2mism"
           REVISION     "200802281200Z"    -- Feb. 28, 2008 at 12:00 GMT
           DESCRIPTION
                "REV PA2: Use official MIB module OID (Jens Engel)."
           REVISION     "200802051000Z"    -- Feb. 05, 2008 at 10:00 GMT
           DESCRIPTION
                " UASt Alarm (Unavailable State) added.
                  If Performance Monitoring is activated at an interface and the unavailable state is entered
                  at this interface, the alarm shall be set (begin unavailble time).
                  If the unavailble condition ceases, the alarm ceases as well (end unavailable time).
                  Unavailable State alarms may appear as near end UASt alarms (uastne) or as far end UASt alarms (uastfe)
                "
           REVISION     "200801211000Z"    -- Jan. 21, 2008 at 10:00 GMT
           DESCRIPTION
                " Alarm severity codes slightly modified.  Harmonized with XF-VCAT-MIB.
                "
           REVISION     "200712051000Z"     -- Dec. 05, 2007 at 10:00 GMT
           DESCRIPTION
                "CLEANUP (Jens Engel):
                  - Replace local ...IfIndex with ifIndex  (OID-change)
                  - Use AUGMENTS statement for ...AlarmSeverityTables
                  - Harmonize AlarmStatus bit ordering to be mostly the same
                    for MsTtp, Au4Ctp, Vc4Ttp, Tu3Ctp, Vc3Ttp, Tu12Ctp, Vc12Ttp:
                        ais, rdi, exc, deg, tim, ueq, lop, plm, lom.
                  - AlarmSeverity columns: Change ordering accordingly (OID-change)
                "
            REVISION     "200711061000Z"    -- Nov. 06, 2007 at 10:00 GMT
            DESCRIPTION
                "CLEANUP: Remove unused-imports (Jens Engel):
                Integer32, NOTIFICATION-TYPE, MODULE-COMPLIANCE, OBJECT-GROUP."
            REVISION     "200706120800Z"    -- June 12, 2007 at 08:00 GMT
            DESCRIPTION
               "Initial Version"
            ::= { xfSdhAdm 4 }


--
-- Textual conventions
--

 XfAlarmSeverity ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The various alarm severities."
    SYNTAX  INTEGER  {
                clear (0),
                undetermined (1),
                warning (2),
                minor (3),
                major (4),
                critical (5)
                }



-- MIB Object Definitions
xfAdmAlarmStatusMibObjects  OBJECT IDENTIFIER ::= { xfAdmAlarmStatusMib 1 }

xfAdmAlarmStatus            OBJECT IDENTIFIER ::= { xfAdmAlarmStatusMibObjects 1 }
xfAdmAlarmSeverity          OBJECT IDENTIFIER ::= { xfAdmAlarmStatusMibObjects 2 }



-- xfSdhNe alarm status of interface - SpiOptical
xfSpiOpticalAlarmStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF XfSpiOpticalAlarmStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table lists all current alarm status for SpiOptical."
    ::= { xfAdmAlarmStatus 1 }



xfSpiOpticalAlarmStatusEntry OBJECT-TYPE
    SYNTAX      XfSpiOpticalAlarmStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry will be automatically created only when the particular
        equipment, related to this interface has been equipped.
        An entry will be automatically deleted when the particular
        equipment, related to this interface has been de-equipped."
    INDEX  { ifIndex }
    ::= { xfSpiOpticalAlarmStatusTable 1 }


XfSpiOpticalAlarmStatusEntry ::=
    SEQUENCE  {
        xfSpiOpticalAlarmStatus         BITS
    }

xfSpiOpticalAlarmStatus OBJECT-TYPE
    SYNTAX BITS
    {
         los0(0),
         los1(1),
         los2(2)
    }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Status of the Loss of Signal failure.
        This objects provides information on the alarm status.
        The severity is indicated by three bits per alarm:
           bits 012
                000   clear
                001   undetermined
                010   warning
                011   minor
                100   major
                101   critical
       "
    ::= { xfSpiOpticalAlarmStatusEntry 1 }




