    SEQUENCE {
        sunHwMonNumericVoltageSensorIndex Integer32,
        sunHwMonNumericVoltageSensorType SunFruOrSensorTypeTC,
        sunHwMonNumericVoltageSensorName DisplayString,
        sunHwMonNumericVoltageSensorParentFruIndex          Integer32,
        sunHwMonNumericVoltageSensorParentFruName         DisplayString,
        sunHwMonNumericVoltageSensorAlarmStatus       SunAlarmStatusTC,
        sunHwMonNumericVoltageSensorStateDescr      DisplayString,
        sunHwMonNumericVoltageSensorCurrentValue        Integer32,
        sunHwMonNumericVoltageSensorBaseUnit    SunBaseUnitsTC,
        sunHwMonNumericVoltageSensorExponent    Integer32,
        sunHwMonNumericVoltageSensorUpperNonRecoverableThreshold                        Integer32,
        sunHwMonNumericVoltageSensorUpperCriticalThreshold                  Integer32,
        sunHwMonNumericVoltageSensorUpperNonCriticalThreshold                     Integer32,
        sunHwMonNumericVoltageSensorLowerNonRecoverableThreshold                        Integer32,
        sunHwMonNumericVoltageSensorLowerCriticalThreshold                  Integer32,
        sunHwMonNumericVoltageSensorLowerNonCriticalThreshold                     Integer32,
        sunHwMonNumericVoltageSensorEnabledThresholds             SunNumericSensorEnabledThresholdsTC
    }

