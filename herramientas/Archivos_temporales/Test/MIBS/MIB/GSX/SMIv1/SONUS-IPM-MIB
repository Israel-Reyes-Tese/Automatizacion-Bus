    SYNTAX          SEQUENCE OF SonusWhitePolicerDiscardRingEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The table of statistics for the white list
             policer discard ring."
    ::= { sonusIpmMIBObjects 6 }

sonusWhitePolicerDiscardRingEntry OBJECT-TYPE
    SYNTAX          SonusWhitePolicerDiscardRingEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The white list discard ring entries."
    INDEX           { sonusWhitePolicerDiscardRingShelfIndex, 
                      sonusWhitePolicerDiscardRingSlotIndex, 
                      sonusWhitePolicerDiscardRingPositionIndex }
    ::= { sonusWhitePolicerDiscardRingTable 1 }

SonusWhitePolicerDiscardRingEntry ::=
    SEQUENCE {
        sonusWhitePolicerDiscardRingShelfIndex      SonusShelfIndex,
        sonusWhitePolicerDiscardRingSlotIndex     SonusSlotIndex,
        sonusWhitePolicerDiscardRingPositionIndex         Gauge,
        sonusWhitePolicerDiscardRingNIFSIFName      SonusNameReference,
        sonusWhitePolicerDiscardRingIpAddress     IpAddress,
        sonusWhitePolicerDiscardRingIpProtocol      Gauge,
        sonusWhitePolicerDiscardRingDiscardCount        Counter,
        sonusWhitePolicerDiscardRingIp6Address      Ipv6Address,
        sonusWhitePolicerDiscardRingIpVer INTEGER
    }

sonusWhitePolicerDiscardRingShelfIndex OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The shelf index into this table."
    ::= { sonusWhitePolicerDiscardRingEntry 1 }

sonusWhitePolicerDiscardRingSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The slot index into this table."
    ::= { sonusWhitePolicerDiscardRingEntry 2 }

sonusWhitePolicerDiscardRingPositionIndex OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The position index into this table ordered by
             worst offender with 1 being the worst."
    ::= { sonusWhitePolicerDiscardRingEntry 3 }

sonusWhitePolicerDiscardRingNIFSIFName OBJECT-TYPE
    SYNTAX          SonusNameReference
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The name of the NIF or sub-interface of 
             table entry."
    ::= { sonusWhitePolicerDiscardRingEntry 4 }

