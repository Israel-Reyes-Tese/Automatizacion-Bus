    ::= { sonusSipArsEndpointRecoveryConfigObjects 6 }

sonusSipEtsProfile OBJECT IDENTIFIER
    ::= { sonusSipSignallingMIBObjects 24 }

sonusSipEtsProfileNextIndex OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The next valid index to use when creating a new sonusSipEtsProfileEntry"
    ::= { sonusSipEtsProfile 1 }

sonusSipEtsProfileTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipEtsProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains information about each Ets
             Profile configured on the node."
    ::= { sonusSipEtsProfile 2 }

sonusSipEtsProfileEntry OBJECT-TYPE
    SYNTAX          SonusSipEtsProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table describes the Ets Profile."
    INDEX           { sonusSipEtsProfIndex }
    ::= { sonusSipEtsProfileTable 1 }

SonusSipEtsProfileEntry ::=
    SEQUENCE {
        sonusSipEtsProfIndex            Gauge,
        sonusSipEtsProfName             SonusName,
        sonusSipEtsProfRowStatus        RowStatus,
        sonusSipEtsProfRphEnabled       SonusAdminState,
        sonusSipEtsProfRphEtsDefault    Gauge,
        sonusSipEtsProfRphWpsPrecedence SonusAdminState,
        sonusSipEtsProfRphIncludeRequire SonusAdminState,
        sonusSipEtsProfRph417ResponseAction   INTEGER,
        sonusSipEtsProfRphInclude417Accept  SonusAdminState,
        sonusSipEtsProfRphUseIncomingEts SonusAdminState,
        sonusSipEtsProfRphRejectNonEtsDn SonusAdminState,
        sonusSipEtsProfRph400With417Stats SonusAdminState,
        sonusSipEtsProfRph417Stats      SonusAdminState,
        sonusSipEtsProfRph420Stats      SonusAdminState,
        sonusSipEtsProfEtsCpcPrecedence INTEGER,
        sonusSipEtsProfWpsMlppPrecedence INTEGER,
        sonusSipEtsProfSipCpcPriority   INTEGER,
        sonusSipEtsProfRphSendEtsInResponse   SonusAdminState
    }

