    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A condensed textual representation of the SS7 linkset object."
    ::= { ptMTPLinksetEntry 1 }

