    SYNTAX          SonusName
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The name of this SIP Ets Profile."
    ::= { sonusSipEtsProfileEntry 2 }

sonusSipEtsProfRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            ""
    ::= { sonusSipEtsProfileEntry 3 }

sonusSipEtsProfRphEnabled OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This parameter controls whether Ets is used or generated."
    DEFVAL          { disabled }
    ::= { sonusSipEtsProfileEntry 4 }

sonusSipEtsProfRphEtsDefault OBJECT-TYPE
    SYNTAX          Gauge (0..4)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Default value used for the ETS namespace."
    DEFVAL          { 0 }
    ::= { sonusSipEtsProfileEntry 5 }

sonusSipEtsProfRphWpsPrecedence OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This governs whether the ETS value will be taken from WPS precedence
             or from the configured default."
    DEFVAL          { disabled }
    ::= { sonusSipEtsProfileEntry 6 }

sonusSipEtsProfRphIncludeRequire OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This controls whether a Require: Resource-Priority will be be
             included in an egress INVITE."
    DEFVAL          { disabled }
    ::= { sonusSipEtsProfileEntry 7 }

sonusSipEtsProfRph417ResponseAction OBJECT-TYPE
    SYNTAX          INTEGER { reject(1), resendnoets(2), 
                        resendetsdefault(3) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This parameter controls the GSX behavior when it receives a 417 response."
    DEFVAL          { resendnoets }
    ::= { sonusSipEtsProfileEntry 8 }

sonusSipEtsProfRphInclude417Accept OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This determines whether GSX includes the Accept-Resource-Priority header
             when sending a 417 response."
    DEFVAL          { disabled }
    ::= { sonusSipEtsProfileEntry 9 }

sonusSipEtsProfRphUseIncomingEts OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This parameter controls whether the GSX accept the receievd ETS value or
             always override with the default."
    DEFVAL          { disabled }
    ::= { sonusSipEtsProfileEntry 10 }

sonusSipEtsProfRphRejectNonEtsDn OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This governs whether a call with Ets but non-ETS-DN is accepted."
    DEFVAL          { disabled }
    ::= { sonusSipEtsProfileEntry 11 }

sonusSipEtsProfRph400With417Stats OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This controls if to count incoming and outgoing 400 with 417 towards SIP statistics."
    DEFVAL          { enabled }
    ::= { sonusSipEtsProfileEntry 12 }

sonusSipEtsProfRph417Stats OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This controls if to count incoming and outgoing 417 towards SIP statistics."
    DEFVAL          { enabled }
    ::= { sonusSipEtsProfileEntry 13 }

sonusSipEtsProfRph420Stats OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This controls if to count incoming and outgoing 420 towards SIP statistics."
    DEFVAL          { enabled }
    ::= { sonusSipEtsProfileEntry 14 }

sonusSipEtsProfEtsCpcPrecedence OBJECT-TYPE
    SYNTAX          INTEGER { ets(1), cpc(2), hpc(3), sip-cpc(4) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This parameter controls the GSX behavior when it receives conflicting ETS(RPH), and CPC(ISUP-MIME) values.
             Starting in Release 7.3, SIP-CPC is no longer supported."
    DEFVAL          { hpc }
    ::= { sonusSipEtsProfileEntry 15 }

sonusSipEtsProfWpsMlppPrecedence OBJECT-TYPE
    SYNTAX          INTEGER { wps(1), mlpp(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This parameter controls the GSX behavior when it receives conflicting WPS(RPH)) and MLPP(ISUP-MIME) values."
    DEFVAL          { mlpp }
    ::= { sonusSipEtsProfileEntry 16 }

sonusSipEtsProfSipCpcPriority OBJECT-TYPE
    SYNTAX          INTEGER { none(1), priority(2) }
    ACCESS          read-write
    STATUS          obsolete
    DESCRIPTION     
            "This field has been obsolete since release 7.3."
    DEFVAL          { none }
    ::= { sonusSipEtsProfileEntry 17 }

sonusSipEtsProfRphSendEtsInResponse OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This parameter controls whether to send received ets value in response."
    DEFVAL          { disabled }
    ::= { sonusSipEtsProfileEntry 18 }

sonusSipSigPortTlsStatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipSigPortTlsStatEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Table of SIP Signalling Port TLS statistics data."
    ::= { sonusSipSignallingMIBObjects 25 }

sonusSipSigPortTlsStatEntry OBJECT-TYPE
    SYNTAX          SonusSipSigPortTlsStatEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "SIP Signalling Port TLS Statistics."
    INDEX           { sonusSipSigPortTlsStatIndex }
    ::= { sonusSipSigPortTlsStatTable 1 }

SonusSipSigPortTlsStatEntry ::=
    SEQUENCE {
        sonusSipSigPortTlsStatIndex     Gauge,
        sonusSipSigPortTlsStatActiveClientSessions          Gauge,
        sonusSipSigPortTlsStatActiveServerSessions          Gauge,
        sonusSipSigPortTlsStatSuspendedClientSessions             Gauge,
        sonusSipSigPortTlsStatSuspendedServerSessions             Gauge,
        sonusSipSigPortTlsStatIncClientHandshakes         Gauge,
        sonusSipSigPortTlsStatIncServerHandshakes         Gauge,
        sonusSipSigPortTlsStatTotalClientSessions         Counter,
        sonusSipSigPortTlsStatTotalServerSessions         Counter,
        sonusSipSigPortTlsStatTotalServerResumed        Counter,
        sonusSipSigPortTlsStatNoCipherSuite   Counter,
        sonusSipSigPortTlsStatHandshakeTimeout      Counter,
        sonusSipSigPortTlsStatHigherAuthTimeout       Counter,
        sonusSipSigPortTlsStatClientAuthFail    Counter,
        sonusSipSigPortTlsStatServerAuthFail    Counter,
        sonusSipSigPortTlsStatFatalAlerts Counter,
        sonusSipSigPortTlsStatWarningAlerts   Counter,
        sonusSipSigPortTlsStatHandshakeFails    Counter,
        sonusSipSigPortTlsStatRcvFails  Counter,
        sonusSipSigPortTlsStatSendFails Counter,
        sonusSipSigPortTlsStatNoAuthDrops Counter,
        sonusSipSigPortTlsStatNoAuth488 Counter,
        sonusSipSigPortTlsStatMidConnHello  Counter,
        sonusSipSigPortTlsStatNoClientCert  Counter,
        sonusSipSigPortTlsStatCountReset INTEGER
    }

sonusSipSigPortTlsStatIndex OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the SIP Signaling Port entry."
    ::= { sonusSipSigPortTlsStatEntry 1 }

sonusSipSigPortTlsStatActiveClientSessions OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of current up sessions in which the GSX is the client"
    ::= { sonusSipSigPortTlsStatEntry 2 }

sonusSipSigPortTlsStatActiveServerSessions OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of current up sessions in which the GSX is the server"
    ::= { sonusSipSigPortTlsStatEntry 3 }

sonusSipSigPortTlsStatSuspendedClientSessions OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of current suspended sessions in which the GSX is the client"
    ::= { sonusSipSigPortTlsStatEntry 4 }

sonusSipSigPortTlsStatSuspendedServerSessions OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of current suspended sessions in which the GSX is the server"
    ::= { sonusSipSigPortTlsStatEntry 5 }

sonusSipSigPortTlsStatIncClientHandshakes OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of current incomplete handshakes in which the GSX is the client"
    ::= { sonusSipSigPortTlsStatEntry 6 }

sonusSipSigPortTlsStatIncServerHandshakes OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of current incomplete handshakes in which the GSX is the server"
    ::= { sonusSipSigPortTlsStatEntry 7 }

sonusSipSigPortTlsStatTotalClientSessions OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Cumulative total number of sessions in which the GSX is the client"
    ::= { sonusSipSigPortTlsStatEntry 8 }

sonusSipSigPortTlsStatTotalServerSessions OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Cumulative total number of sessions in which the GSX is the server"
    ::= { sonusSipSigPortTlsStatEntry 9 }

sonusSipSigPortTlsStatTotalServerResumed OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of sessions that have been resumed in which the GSX is the server"
    ::= { sonusSipSigPortTlsStatEntry 10 }

sonusSipSigPortTlsStatNoCipherSuite OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of handshake failures due to inability to converge on a cipher suite"
    ::= { sonusSipSigPortTlsStatEntry 11 }

sonusSipSigPortTlsStatHandshakeTimeout OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of handshakes that timed out before completing"
    ::= { sonusSipSigPortTlsStatEntry 12 }

sonusSipSigPortTlsStatHigherAuthTimeout OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of handshakes in which the GSX was the server
             and the client did not authenticate itself through TLS
             and failed to authenticate itself through any higher level
             protocol (e.g. SIP) before timing out"
    ::= { sonusSipSigPortTlsStatEntry 13 }

sonusSipSigPortTlsStatClientAuthFail OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of times peer failed TLS authentication when
             the GSX was the client"
    ::= { sonusSipSigPortTlsStatEntry 14 }

sonusSipSigPortTlsStatServerAuthFail OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of times peer failed TLS authentication when
             the GSX was the server"
    ::= { sonusSipSigPortTlsStatEntry 15 }

sonusSipSigPortTlsStatFatalAlerts OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of FATAL Alerts received"
    ::= { sonusSipSigPortTlsStatEntry 16 }

sonusSipSigPortTlsStatWarningAlerts OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of WARNING Alerts received"
    ::= { sonusSipSigPortTlsStatEntry 17 }

sonusSipSigPortTlsStatHandshakeFails OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Cumulative total of all handshake failures"
    ::= { sonusSipSigPortTlsStatEntry 18 }

sonusSipSigPortTlsStatRcvFails OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of receive failures (failure to decrypt)"
    ::= { sonusSipSigPortTlsStatEntry 19 }

sonusSipSigPortTlsStatSendFails OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of send failures (failure to encrypt)"
    ::= { sonusSipSigPortTlsStatEntry 20 }

sonusSipSigPortTlsStatNoAuthDrops OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of SIP messages dropped due to lack
             of peer authentication at the SIP level"
    ::= { sonusSipSigPortTlsStatEntry 21 }

sonusSipSigPortTlsStatNoAuth488 OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of SIP messages rejected with 488 due to lack
             of peer authentication at the SIP level"
    ::= { sonusSipSigPortTlsStatEntry 22 }

sonusSipSigPortTlsStatMidConnHello OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number times a peer sent a TLS Hello on a connection
             that was already up (i.e. the peer tried to renegotiate.)"
    ::= { sonusSipSigPortTlsStatEntry 23 }

sonusSipSigPortTlsStatNoClientCert OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number times we were the client and we failed to
             derive a certificate to send the server (in the mutual
             authentication case). This is probably because there was
             no overlap between our configured trust anchors and the list
             of trust anchors the server sent in its Certificate Request"
    ::= { sonusSipSigPortTlsStatEntry 24 }

sonusSipSigPortTlsStatCountReset OBJECT-TYPE
    SYNTAX          INTEGER { unknown(1), reset(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "set opertion on this object resets all the TLS statistics counts to zero"
    ::= { sonusSipSigPortTlsStatEntry 25 }

sonusSipSigPortTlsStatusTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipSigPortTlsStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Table of SIP Signalling Port TLS status data."
    ::= { sonusSipSignallingMIBObjects 26 }

sonusSipSigPortTlsStatusEntry OBJECT-TYPE
    SYNTAX          SonusSipSigPortTlsStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "SIP Signalling Port TLS Status information."
    INDEX           { sonusSipSigPortTlsStatusSSPIndex, 
                      sonusSipSigPortTlsStatusPeerIpAddress, 
                      sonusSipSigPortTlsStatusPeerIp6Address, 
                      sonusSipSigPortTlsStatusVersion, 
                      sonusSipSigPortTlsStatusSession }
    ::= { sonusSipSigPortTlsStatusTable 1 }

SonusSipSigPortTlsStatusEntry ::=
    SEQUENCE {
        sonusSipSigPortTlsStatusSSPIndex Gauge,
        sonusSipSigPortTlsStatusSession Gauge,
        sonusSipSigPortTlsStatusPeerIpAddress     IpAddress,
        sonusSipSigPortTlsStatusPeerPortNum   INTEGER,
        sonusSipSigPortTlsStatusSocket  INTEGER,
        sonusSipSigPortTlsStatusSessionID OCTET STRING,
        sonusSipSigPortTlsStatusConnId  INTEGER,
        sonusSipSigPortTlsStatusSessState INTEGER,
        sonusSipSigPortTlsStatusRole    INTEGER,
        sonusSipSigPortTlsStatusResumptions   Counter,
        sonusSipSigPortTlsStatusBytesSent Counter,
        sonusSipSigPortTlsStatusRecordsSent   Counter,
        sonusSipSigPortTlsStatusBytesRecOK  Counter,
        sonusSipSigPortTlsStatusRecordsRecOK    Counter,
        sonusSipSigPortTlsStatusRowStatus RowStatus,
        sonusSipSigPortTlsStatusPeerIp6Address      Ipv6Address,
        sonusSipSigPortTlsStatusVersion INTEGER
    }

sonusSipSigPortTlsStatusSSPIndex OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the SIP Signaling Port entry."
    ::= { sonusSipSigPortTlsStatusEntry 1 }

sonusSipSigPortTlsStatusSession OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the TLS session. This is an internal session tracking
             number and should not be confused with the session ID that
             is actually used on the wire between the TLS peers."
    ::= { sonusSipSigPortTlsStatusEntry 2 }

sonusSipSigPortTlsStatusPeerIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IP address of the peer of this session"
    ::= { sonusSipSigPortTlsStatusEntry 3 }

sonusSipSigPortTlsStatusPeerPortNum OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "TCP port number used by the peer of this session"
    ::= { sonusSipSigPortTlsStatusEntry 4 }

sonusSipSigPortTlsStatusSocket OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "socket number used internally for the TCP component of this session"
    ::= { sonusSipSigPortTlsStatusEntry 5 }

sonusSipSigPortTlsStatusSessionID OBJECT-TYPE
    SYNTAX          OCTET STRING (SIZE(0..32))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The TLS Session ID of this session"
    ::= { sonusSipSigPortTlsStatusEntry 6 }

sonusSipSigPortTlsStatusConnId OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Current internal connection ID of this session
             (0 means none, i.e. suspended session)"
    ::= { sonusSipSigPortTlsStatusEntry 7 }

sonusSipSigPortTlsStatusSessState OBJECT-TYPE
    SYNTAX          INTEGER { none(1), suspended(2), tcpConnecting(3), 
                        negotiating(4), highLevelAuth(5), up(6) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Connection state of this session"
    ::= { sonusSipSigPortTlsStatusEntry 8 }

sonusSipSigPortTlsStatusRole OBJECT-TYPE
    SYNTAX          INTEGER { client(1), server(2) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The role taken by the GSX in this session"
    ::= { sonusSipSigPortTlsStatusEntry 9 }

sonusSipSigPortTlsStatusResumptions OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of times this session has been resumed"
    ::= { sonusSipSigPortTlsStatusEntry 10 }

sonusSipSigPortTlsStatusBytesSent OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of payload bytes sent on the current connection associated
             with this session"
    ::= { sonusSipSigPortTlsStatusEntry 11 }

sonusSipSigPortTlsStatusRecordsSent OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of TLS records sent on the current connection associated
             with this session"
    ::= { sonusSipSigPortTlsStatusEntry 12 }

sonusSipSigPortTlsStatusBytesRecOK OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of payload bytes received on the current connection
             without integrity check failure"
    ::= { sonusSipSigPortTlsStatusEntry 13 }

sonusSipSigPortTlsStatusRecordsRecOK OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of TLS records received on the current connection
             without integrity check failure"
    ::= { sonusSipSigPortTlsStatusEntry 14 }

sonusSipSigPortTlsStatusRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is for SNMP use."
    ::= { sonusSipSigPortTlsStatusEntry 15 }

sonusSipSigPortTlsStatusPeerIp6Address OBJECT-TYPE
    SYNTAX          Ipv6Address
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IP address of the peer of this session"
    ::= { sonusSipSigPortTlsStatusEntry 16 }

sonusSipSigPortTlsStatusVersion OBJECT-TYPE
    SYNTAX          INTEGER { afInet(2), afInet6(24) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The role taken by the GSX in this session"
    ::= { sonusSipSigPortTlsStatusEntry 17 }

sonusFilterProfile OBJECT IDENTIFIER
    ::= { sonusSipSignallingMIBObjects 27 }

sonusFilterProfileNextIndex OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The next valid index to use when creating a new sonusFilterProfileEntry"
    ::= { sonusFilterProfile 1 }

sonusFilterProfileTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusFilterProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains information about each Filter
             Profile configured on the node."
    ::= { sonusFilterProfile 2 }

sonusFilterProfileEntry OBJECT-TYPE
    SYNTAX          SonusFilterProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table describes the Filter Profile."
    INDEX           { sonusFilterProfIndex }
    ::= { sonusFilterProfileTable 1 }

SonusFilterProfileEntry ::=
    SEQUENCE {
        sonusFilterProfIndex            Gauge,
        sonusFilterProfName             SonusName,
        sonusFilterProfRowStatus        RowStatus,
        sonusFilterProfAccepted         SonusFilterProfileState,
        sonusFilterProfAcceptResourcePriority     SonusFilterProfileState,
        sonusFilterProfAlertInfo        SonusFilterProfileState,
        sonusFilterProfAllowed          SonusFilterProfileState,
        sonusFilterProfAlso             SonusFilterProfileState,
        sonusFilterProfAnonymity        SonusFilterProfileState,
        sonusFilterProfAuthorization    SonusFilterProfileState,
        sonusFilterProfCallConnectId    SonusFilterProfileState,
        sonusFilterProfContentDisp      SonusFilterProfileState,
        sonusFilterProfDiversion        SonusFilterProfileState,
        sonusFilterProfError            SonusFilterProfileState,
        sonusFilterProfErrorInfo        SonusFilterProfileState,
        sonusFilterProfEvent            SonusFilterProfileState,
        sonusFilterProfExpire           SonusFilterProfileState,
        sonusFilterProfMaxForwards      SonusFilterProfileState,
        sonusFilterProfMinExpires       SonusFilterProfileState,
        sonusFilterProfMinSe            SonusFilterProfileState,
        sonusFilterProfPAssertedId      SonusFilterProfileState,
        sonusFilterProfPCarrierInfo     SonusFilterProfileStateWithUnknown,
        sonusFilterProfPChargingVector  SonusFilterProfileState,
        sonusFilterProfPKAdn            SonusFilterProfileStateWithUnknown,
        sonusFilterProfPPreferredId     SonusFilterProfileState,
        sonusFilterProfPSigInfo         SonusFilterProfileState,
        sonusFilterProfPSvcInfo         SonusFilterProfileState,
        sonusFilterProfPTermCa          SonusFilterProfileStateWithUnknown,
        sonusFilterProfPath             SonusFilterProfileState,
        sonusFilterProfPCdrInfo         SonusFilterProfileState,
        sonusFilterProfPChargeInfo      SonusFilterProfileState,
        sonusFilterProfPKCfl            SonusFilterProfileState,
        sonusFilterProfPKCfo            SonusFilterProfileState,
        sonusFilterProfPrivacy          SonusFilterProfileState,
        sonusFilterProfProxyAuthorization SonusFilterProfileState,
        sonusFilterProfProxyReq         SonusFilterProfileState,
        sonusFilterProfReason           SonusFilterProfileState,
        sonusFilterProfRemoteParty      SonusFilterProfileState,
        sonusFilterProfReplaces         SonusFilterProfileState,
        sonusFilterProfReplyTo          SonusFilterProfileState,
        sonusFilterProfRequestBy        SonusFilterProfileState,
        sonusFilterProfRequired         SonusFilterProfileState,
        sonusFilterProfResourcePriority SonusFilterProfileState,
        sonusFilterProfRetryAfter       SonusFilterProfileState,
        sonusFilterProfRoute            SonusFilterProfileState,
        sonusFilterProfServiceRoute     SonusFilterProfileState,
        sonusFilterProfSessExpires      SonusFilterProfileState,
        sonusFilterProfSubscriptionState SonusFilterProfileState,
        sonusFilterProfSupported        SonusFilterProfileState,
        sonusFilterProfTimeStamp        SonusFilterProfileStateWithUnknown,
        sonusFilterProfUnknown          SonusFilterProfileState,
        sonusFilterProfUnsupported      SonusFilterProfileState,
        sonusFilterProfWWWAuthenticate  SonusFilterProfileState,
        sonusFilterProfAcceptContact    SonusFilterProfileStateWithUnknown,
        sonusFilterProfHistoryInfo      SonusFilterProfileStateWithUnknown,
        sonusFilterProfPChgMsgInfo      SonusFilterProfileStateWithUnknown,
        sonusFilterProfPOrigCa          SonusFilterProfileStateWithUnknown,
        sonusFilterProfTargetDialog     SonusFilterProfileStateWithUnknown,
        sonusFilterProfUserAgent        SonusFilterProfileStateWithUnknown,
        sonusFilterProfServer           SonusFilterProfileStateWithUnknown,
        sonusFilterProfWarning          SonusFilterProfileStateWithUnknown,
        sonusFilterProfAcceptLanguage   SonusFilterProfileStateWithUnknown,
        sonusFilterProfCallInfo         SonusFilterProfileStateWithUnknown,
        sonusFilterProfSipEtag          SonusFilterProfileStateWithUnknown,
        sonusFilterProfSipIfMatch       SonusFilterProfileStateWithUnknown,
        sonusFilterProfSuppressIfMatch  SonusFilterProfileStateWithUnknown,
        sonusFilterProfReferSub         SonusFilterProfileStateWithUnknown,
        sonusFilterProfProxyAuthenticate SonusFilterProfileStateWithUnknown,
        sonusFilterProfPEarlyMedia      SonusFilterProfileStateWithUnknown,
        sonusFilterProfPAssociatedUri   SonusFilterProfileStateWithUnknown,
        sonusFilterProfXSrvcIndicator   SonusFilterProfileStateWithUnknown,
        sonusFilterProfPDcsBillingInfo  SonusFilterProfileStateWithUnknown,
        sonusFilterProfRecordRoute      SonusFilterProfileState,
        sonusFilterProfReferTo          SonusFilterProfileState,
        sonusFilterProfReferredBy       SonusFilterProfileState
    }

