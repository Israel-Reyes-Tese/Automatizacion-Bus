PtDSCDSCRoutingSearchUtilityEntry ::=
    SEQUENCE {
        ptDSCDSCRoutingSearchUtilityDSCNodeName       DisplayString,
        ptDSCDSCRoutingSearchUtilityRequestIncomingADNName                  DisplayString,
        ptDSCDSCRoutingSearchUtilityAnswerIncomingADNName                 DisplayString,
        ptDSCDSCRoutingSearchUtilityInitialRoutingTableName                   DisplayString,
        ptDSCDSCRoutingSearchUtilityResult  DisplayString,
        ptDSCDSCRoutingSearchUtilityDiagnosticMessage             DisplayString,
        ptDSCDSCRoutingSearchUtilityDSCIndex    Integer32,
        ptDSCDSCRoutingSearchUtilityDSCRoutingTablesIndex                 Integer32,
        ptDSCDSCRoutingSearchUtilityActions   INTEGER,
        ptDSCDSCRoutingSearchUtilityParentOID     OBJECT IDENTIFIER,
        ptDSCDSCRoutingSearchUtilityRowStatus     RowStatus,
        ptDSCDSCRoutingSearchUtilityIndex Integer32
    }

ptDSCDSCRoutingSearchUtilityDSCNodeName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Name used to logically identify the DSC Node. \nThe attribute is mandatory and is used to identify the DSC Node where the routing search is performed. \nThe Node Name value must be defined on the DSC instance of the Search Utility."
    ::= { ptDSCDSCRoutingSearchUtilityEntry 1 }

ptDSCDSCRoutingSearchUtilityRequestIncomingADNName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Incoming Adjacent Diameter Node Name for Request Message. \nThe attribute is mandatory and is used to identify the Request Message DSC Incoming ADN where the TEST message is received. \nFor Answer Test Message, this represents the incoming ADN of the original Request Message. \nThe ADN DID value must be defined on the input DSC Node and must have at least one connection."
    ::= { ptDSCDSCRoutingSearchUtilityEntry 2 }

ptDSCDSCRoutingSearchUtilityAnswerIncomingADNName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Incoming Adjacent Diameter Node Name for Answer Message. \nThe attribute is mandatory for Answer Message but is not required for REQUEST Message. It is used to identify the Answer Message Incoming ADN where the TEST message is received. \nThe ADN DID value must be defined on the input DSC Node diameter ID and must have at least one connection."
    ::= { ptDSCDSCRoutingSearchUtilityEntry 3 }

ptDSCDSCRoutingSearchUtilityInitialRoutingTableName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Initial Routing Table Name to Test. \nThe Test Message will be routed starting from this routing table. The Initial Routing Table Name must be defined on the search interface instance routing table. \nIf not supplied, it will use the Initial Routing Table Name of the input node for Request message. For Answer message, this will return INVALID INPUT result"
    ::= { ptDSCDSCRoutingSearchUtilityEntry 4 }

ptDSCDSCRoutingSearchUtilityResult OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Routing Test Result of the message (MESSAGE TESTED SUCCESSFULLY, RETURN ERROR, DISCARD MESSAGE, INVALID INPUT, MESSAGE TESTED NOT COMPLETED)."
    ::= { ptDSCDSCRoutingSearchUtilityEntry 20 }

ptDSCDSCRoutingSearchUtilityDiagnosticMessage OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The Diagnostic Message will display the header of the test message for user to confirm the test message content. It will also capture the message path of the test message. \nif an error is detected in the search criteria, this will alert the user on what is specifically wrong with the data."
    ::= { ptDSCDSCRoutingSearchUtilityEntry 21 }

ptDSCDSCRoutingSearchUtilityDSCIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
