        ::= { xfModemSdcTable 1 }

    XfModemSdcEntry ::=
        SEQUENCE
        {
            xfMMUSdcCapability           INTEGER,
            xfMMUSdcAdminStatus          INTEGER,
            xfMMUSdcOperStatus           INTEGER,
