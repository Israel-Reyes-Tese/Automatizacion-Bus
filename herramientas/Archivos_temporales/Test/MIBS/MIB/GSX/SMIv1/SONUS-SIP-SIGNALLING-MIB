sonusSipSigPortConnectionStatusTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipSigPortConnectionStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Table of SIP Signalling Port connection status data."
    ::= { sonusSipSignallingMIBObjects 28 }

sonusSipSigPortConnectionStatusEntry OBJECT-TYPE
    SYNTAX          SonusSipSigPortConnectionStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "SIP Signalling Port Connection Status information."
    INDEX           { sonusSipSigPortConnectionStatusSSPIndex, 
                      sonusSipSigPortConnectionStatusPeerIpAddress, 
                      sonusSipSigPortConnectionStatusPeerIp6Address, 
                      sonusSipSigPortConnectionStatusPeerIpVersion, 
                      sonusSipSigPortConnectionStatusConnId }
    ::= { sonusSipSigPortConnectionStatusTable 1 }

SonusSipSigPortConnectionStatusEntry ::=
    SEQUENCE {
        sonusSipSigPortConnectionStatusSSPIndex       Gauge,
        sonusSipSigPortConnectionStatusPeerIpAddress            IpAddress,
        sonusSipSigPortConnectionStatusConnId     Gauge,
        sonusSipSigPortConnectionStatusPeerPortNum          INTEGER,
        sonusSipSigPortConnectionStatusSocket     INTEGER,
        sonusSipSigPortConnectionStatusTransport        INTEGER,
        sonusSipSigPortConnectionStatusState    INTEGER,
        sonusSipSigPortConnectionStatusRole   INTEGER,
        sonusSipSigPortConnectionStatusAging    INTEGER,
        sonusSipSigPortConnectionStatusIdleTime       Gauge,
        sonusSipSigPortConnectionStatusBytesSent        Counter,
        sonusSipSigPortConnectionStatusBytesRcvd        Counter,
        sonusSipSigPortConnectionStatusPduSendQueued            Counter,
        sonusSipSigPortConnectionStatusPduRecvQueued            Counter,
        sonusSipSigPortConnectionStatusRowStatus        RowStatus,
        sonusSipSigPortConnectionStatusPeerIp6Address             Ipv6Address,
        sonusSipSigPortConnectionStatusPeerIpVersion            INTEGER
    }

sonusSipSigPortConnectionStatusSSPIndex OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the SIP Signaling Port entry."
    ::= { sonusSipSigPortConnectionStatusEntry 1 }

sonusSipSigPortConnectionStatusPeerIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IP address of the peer of this connection"
    ::= { sonusSipSigPortConnectionStatusEntry 2 }

sonusSipSigPortConnectionStatusConnId OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifier used internally for the connection"
    ::= { sonusSipSigPortConnectionStatusEntry 3 }

sonusSipSigPortConnectionStatusPeerPortNum OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "port number used by the peer of this connection"
    ::= { sonusSipSigPortConnectionStatusEntry 4 }

sonusSipSigPortConnectionStatusSocket OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "socket number used internally for the connection"
    ::= { sonusSipSigPortConnectionStatusEntry 5 }

sonusSipSigPortConnectionStatusTransport OBJECT-TYPE
    SYNTAX          INTEGER { udp(1), tcp(2), tls-tcp(3), sctp(4) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The transport type used for the connection"
    ::= { sonusSipSigPortConnectionStatusEntry 6 }

sonusSipSigPortConnectionStatusState OBJECT-TYPE
    SYNTAX          INTEGER { none(0), connecting(1), connected(2), 
                        blocked(3), deleted(4) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "state of this connection"
    ::= { sonusSipSigPortConnectionStatusEntry 7 }

sonusSipSigPortConnectionStatusRole OBJECT-TYPE
    SYNTAX          INTEGER { client(0), server(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The role taken by the GSX in this connection"
    ::= { sonusSipSigPortConnectionStatusEntry 8 }

sonusSipSigPortConnectionStatusAging OBJECT-TYPE
    SYNTAX          INTEGER { no(0), yes(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Whether or not an idle connection can be taken down"
    ::= { sonusSipSigPortConnectionStatusEntry 9 }

