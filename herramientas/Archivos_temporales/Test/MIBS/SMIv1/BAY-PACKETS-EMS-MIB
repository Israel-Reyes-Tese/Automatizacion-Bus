    ::= { rsiCommonMib 38 }

rsiAlarmDestEntry OBJECT-TYPE
    SYNTAX          RsiAlarmDestEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of a Alarm Destination"
    INDEX           { rsiAlarmDestId }
    ::= { rsiAlarmDestTable 1 }

RsiAlarmDestEntry ::=
    SEQUENCE {
        rsiAlarmDestId                  INTEGER,
        rsiAlarmDestGroupId             INTEGER,
        rsiAlarmDestTODStart            DisplayString,
        rsiAlarmDestTODEnd              DisplayString,
        rsiAlarmDestDOFWeek             DisplayString,
        rsiAlarmDestEntryType           INTEGER,
        rsiAlarmDestType                INTEGER,
        rsiAlarmDest                    DisplayString,
        rsiAlarmDestRowStatus           RowStatus
    }

