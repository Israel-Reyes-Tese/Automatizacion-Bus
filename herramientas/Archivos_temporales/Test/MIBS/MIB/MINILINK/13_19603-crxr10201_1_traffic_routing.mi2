--
-- Node definitions
--
	
		-- 1.3.6.1.4.1.193.81.2.6.1
		xfCrossConnectObjects OBJECT IDENTIFIER ::= { xfTrafficRoutingMIB 1 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.1
		xfCrossConnectNextIndex OBJECT-TYPE
			SYNTAX Integer32 (1..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The manager can get the next cross-connection index 
				for creating new row. 
				This object has not to be used when cross-connections are created
				with index 1. In that case the agent will autoatically assing a new
				index."
			::= { xfCrossConnectObjects 1 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.2
		xfCrossConnectTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfCrossConnectEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Contains all bidirectional cross-connections"
			::= { xfCrossConnectObjects 2 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.2.1
		xfCrossConnectEntry OBJECT-TYPE
			SYNTAX XfCrossConnectEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"There are to ways to add an entry to this table:
				-ask for a new index through the xfCrossConnectNextIndex and
				 use that index to crarete a new row
				-OR create a row with index 1 and then at activation time a new index
				 is choosen and a row is created.
				
				An entry, describing a bidirectional cross-connection, can
				be created if:
				-xfCrossConnectIf1 and xfCrossConnectIf2, point to 
				 interfaces of compatible ifTypes and ifSpecific.
				-both interfaces have the same ifSpeed and ifHighSpeed.
				-both interfaces are not already involved in a cross-
				 connection.
				-both interfaces are not already involved in a SNCP.
				-both interfaces can be cross-connected in the XF 
				 backplane, i.e. higherLayer in the ifStackTable is 
				 0.
				The possiblity to cross-connect based on the last 
				three issues is also expressed in the 
				xfCrossConnectIfTable.
				"
			INDEX { xfCrossConnectId }
			::= { xfCrossConnectTable 1 }

		
		XfCrossConnectEntry ::=
			SEQUENCE { 
				xfCrossConnectId
					Integer32,
				xfCrossConnectName
					DisplayString,
				xfCrossConnectIf1
					InterfaceIndex,
				xfCrossConnectIf2
					InterfaceIndex,
				xfCrossConnectStatus
					RowStatus
			 }

		-- 1.3.6.1.4.1.193.81.2.6.1.2.1.1
		xfCrossConnectId OBJECT-TYPE
			SYNTAX Integer32 (1..2147483647)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Unique identifier for each bidirectional cross-connection. "
			::= { xfCrossConnectEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.2.1.2
		xfCrossConnectName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..128))
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"A name given to the cross-connection by the manager. 
				This object may optionaly be set."
			DEFVAL { "" }
			::= { xfCrossConnectEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.2.1.3
		xfCrossConnectIf1 OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"One of the interfaces involved in the cross-connection.
				As long as this objects isn't set the xfCrossConnectStatus
				will be 'notReady'."
			DEFVAL { 1 }
			::= { xfCrossConnectEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.2.1.4
		xfCrossConnectIf2 OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The other interface involved in the cross-conection.
				As long as this objects isn't set the xfCrossConnectStatus
				will be 'notReady'."
			DEFVAL { 1 }
			::= { xfCrossConnectEntry 4 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.2.1.5
		xfCrossConnectStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Standard row status used for setting-up and releasing 
				cross-connections.
				active(1)      activates a cross-connect entry created 
				               with 'createAndWait' and indicates normal 
				               operation. All objects in the row must
				               have a vallid value before it can be 
				               activated. Only the xfCrossConnectName 
				               can be changed whilst 'active'.
				               When using row 1 to create a cross-connection activate
				               will lead to a new row with a new index and the row
				               with index 1 being deleted.
				
				notInService(2)after a succesfull createAndWait the row will have 
				               this status. A row cannot be set into 'notInService' 
				               (inconsistent value) it has to be destroyed
				               and created again. 
				
				notReady(3)    A row will get this status after 'createAndWait' 
				               and an unsuccesfull 'active' and not all 
				               objects are properly set.
				
				createAndGo(4) used to set-up a cross-connection and 
				               activate it. Used in multiple variable 
				               bindings since all objects in the row
				               must have a valid value before the row 
				               is activated.
				
				createAndWait(5) the row for the cross-connection is 
				                 created but not actived. Used when 
				                 multiple variable binding are not 
				                 available.A row will be created with 
				                 default values.
				
				destroy(6)       removes a cross-connection"
			::= { xfCrossConnectEntry 5 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.3
		xfCrossConnectIfTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfCrossConnectIfEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table holds entries for all interfaces that can
				be cross-connected and indicates the cross-connection
				they are involved in.
				This table can also be used to check which Service
				Connection Points are available for services as e.g.
				Ethernet bridge. PDH interfaces that can be used will be
				in this list with a xfCrossConnectId equal 0."
			::= { xfCrossConnectObjects 3 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.3.1
		xfCrossConnectIfEntry OBJECT-TYPE
			SYNTAX XfCrossConnectIfEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table."
			INDEX { ifIndex }
			::= { xfCrossConnectIfTable 1 }

		
		XfCrossConnectIfEntry ::=
			SEQUENCE { 
				xfCrossConnectIfId
					Integer32
			 }

		-- 1.3.6.1.4.1.193.81.2.6.1.3.1.2
		xfCrossConnectIfId OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates whether an interface is involved in 
				a cross-connection or not. Values:
				
				0      not involved in a cross-connection
				
				other  points to the cross-connection in which this 
				       interface is involved, i.e. xfCrossConnectId."
			::= { xfCrossConnectIfEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.4
		xfCrossConnectLastChange OBJECT-TYPE
			SYNTAX TimeTicks
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The value of sysUpTime at the time of the last creation or
				deletion of an entry in the xfCrossConnectTable.  If the 
				number of entries has been unchanged since the last 
				re-initialization of the local network management 
				subsystem, then this object contains a zero value"
			::= { xfCrossConnectObjects 4 }

		
		-- 1.3.6.1.4.1.193.81.2.6.1.5
		xfCrossConnectVariant OBJECT-TYPE
			SYNTAX Integer32 (1..2)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Specifies the PDH cross-connection objects capability changes.
				This value is incremented whenever the cross-connection objects capability is modified.
				
				    Variant
					1      Starting value. Indicates the node supports 30 characters length name for PDH cross-connection
					2      Indicates the node supports 128 characters length name for PDH cross-connection (circuit names)
				
				See also xfCrossConnectName."
			::= { xfCrossConnectObjects 5 }

		
-- The term SNCP in this MIB corresponds to 1+1 E1 SNCP in 
-- other documentation like customer doc.'Unreliable' 
-- corresponds to 'unprotected' and 'reliable' with 
-- 'protected'.
		-- 1.3.6.1.4.1.193.81.2.6.2
		xfSncpObjects OBJECT IDENTIFIER ::= { xfTrafficRoutingMIB 2 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2
		xfSncpTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfSncpEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table of all SNCPs realised by the TDM bus in the node. 
				Creating a row in this table will also create a new row 
				in the ifTable and two entries in the ifStackTable. In the 
				ifSTackTable there will be two entries: one for reliable/unreliable1
				and one for reliable/unreliable2. The first entry will be
				the active one as default and will have rowStatus 'active' in 
				the stack table whilst the second entry will have rowStatus 'notInService'.
				'Unreliable' /'Reliable' corresponds with the term 
				'unprotected' /'protected' in other documentation.
				The ifIndex of the new row will have to comply with the rules
				for static ifIndexes as described in 2/155 19-CRXR102 01/1 XF-BN: 
				SNMP Objects. This means that the ifIndex has the value of 
				xfSncpUnreliableInterface1 with bit13 set to '1'.
				
				
				An new way to create an entry in this table is to use row 1 to create 
				(andWait or andGo). Then a t activatin time a all data is copied to a new
				row with a valid index, calculated by the agent. Once the new row is 
				created the row 1 is deleted."
			::= { xfSncpObjects 2 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2.1
		xfSncpEntry OBJECT-TYPE
			SYNTAX XfSncpEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table."
			INDEX { ifIndex }
			::= { xfSncpTable 1 }

		
		XfSncpEntry ::=
			SEQUENCE { 
				xfSncpUnreliableInterface1
					InterfaceIndex,
				xfSncpUnreliableInterface2
					InterfaceIndex,
				xfSncpMode
					INTEGER,
				xfSncpHoldOffTime
					Integer32,
				xfSncpSwitchStatus
					Integer32,
				xfSncpSwitchCount
					Counter32,
				xfSncpProtectionStatus
					BITS,
				xfSncpOperStatus
					INTEGER,
				xfSncpRowStatus
					RowStatus
			 }

		-- 1.3.6.1.4.1.193.81.2.6.2.2.1.2
		xfSncpUnreliableInterface1 OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object points to one of the 2 unreliable 
				interfaces the 1+1 SNCP consists. 'Unreliable'
				corresponds with the term 'unprotected' in 
				other documentation."
			DEFVAL { 1 }
			::= { xfSncpEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2.1.3
		xfSncpUnreliableInterface2 OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object points to the other unreliable interface 
				the 1+1 SNCP consists.'Unreliable'
				corresponds with the term 'unprotected' in 
				other documentation."
			DEFVAL { 1 }
			::= { xfSncpEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2.1.4
		xfSncpMode OBJECT-TYPE
			SYNTAX INTEGER
				{
				automatic(1),
				manual(2)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The SNCP group can be set into two modi:
				automatic: a switch will be performed upon a Signal 
				           Failure(SF) if the other interface is OK.
				manual:    no switch due to SF will be performed."
			DEFVAL { automatic }
			::= { xfSncpEntry 4 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2.1.5
		xfSncpHoldOffTime OBJECT-TYPE
			SYNTAX Integer32 (0..100)
			UNITS "100 milliseconds"
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Time between detection of a Signal Failure (SF) that 
				cause a switch and the actual switching. This time can 
				be used to delay a switch in order to give protection 
				at lower layers time to switch first.
				
				Value 0 represents 'as fast as possible' which is 
				20 msec in the XF case."
			DEFVAL { 0 }
			::= { xfSncpEntry 5 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2.1.7
		xfSncpSwitchStatus OBJECT-TYPE
			SYNTAX Integer32 (1..2)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object  inidicates which unreliable interface is 
				active(protected), i.e. used for traffic reception.
				
				1 means the interface xfSncpUnreliableInterface1 is 
				  the active interface and xfSncpUnreliableInterface2 
				  is the passive (protecting).
				 
				2 means the interface xfSncpUnreliableInterface2 is the 
				  active interface and xfSncpUnreliableInterface1 is 
				  the passive.
				
				This object is also used to switch from external management.
				The entry for the active unreliable interface in the 
				ifStackTable will have rowStatus 'active' whilst the entry for 
				the passive interface will have rowStatus 'notInService'.
				Default is 1 but in case a SNCP is setup for an already 
				cross-connected interface then the interface that is 
				cross-connected will be the value for this object.
				"
			DEFVAL { 1 }
			::= { xfSncpEntry 7 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2.1.8
		xfSncpSwitchCount OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A counter object that counts the number of protection 
				switches for this reliable interface. Any switch is included,
				i.e. automatic switches, manual switches, switches of not connected
				SNCPs etc.
				It will be reset at a warm/cold restart."
			::= { xfSncpEntry 8 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2.1.9
		xfSncpProtectionStatus OBJECT-TYPE
			SYNTAX BITS
				{
				xfSncpUnprotected0(0),
				xfSncpUnprotected1(1),
				xfSncpUnprotected2(2),
				xfSncpAisOnProtection0(3),
				xfSncpAisOnProtection1(4),
				xfSncpAisOnProtection2(5)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This variable indicates the protection status and severity.
				 	Severity is indicated by three bits per alarm:
				 	      bits 012 
				 	           000   clear
				 	           001   undetermined
				 	           010   warning
				 	           011   minor
				 	           100   major
				 	           101   critical
				In case one of the ureliable interfaces fails it will be minor.
				In case both fail severity will be critical."
			::= { xfSncpEntry 9 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2.1.10
		xfSncpOperStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				fullyProtected(1),
				unProtected(2),
				completeFail(3),
				lockedToFail(4),
				interfaceMismatch(5)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Indicates status of protection group:
				fullyProtected(1)    both unreliable interfaces are OK 
				
				unProtected(2)       the protected interface is OK, 
				                     but the protecting interface fails.
				
				completeFail(3)      both unreliable interfaces fail.
				
				lockToFail(4)        protected interface fails 
				                     but protecting is OK and switch is 
				                     locked to the active.
				
				interfaceMismatch(5) Interfaces on both sides of the 
				                     network connection are incompatible.
				                     Only supported if this can be detected."
			::= { xfSncpEntry 10 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.2.1.11
		xfSncpRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Standard row status used to create and destroy a
				row in this table.
				active(1)      Indicates that the SNCP is created and 
				               activated. Also the ifOperStatus will become
				               'up'. And the various objects in the if-row
				               will inherit the values from the unreliable
				               interfaces. Whilst in 'active' status all
				               read-create objects in the row can be 
				               changed except for the xfScnpUnreliable-
				               Interface1 and 2.
				               If the ifIndex for row creation doesn't comply
				               with the rules for static ifIndexes the row 
				               cannot be set to 'active'.
				
				notInService(2) After a succesfull createAndWait the row will
				                be notInService. A manager cannot set this
				                value.
				
				notReady(3)     After a 'createAndWait' and an
				                unsuccessful 'active' the row will be 
				                'notReady'.
				
				createAndGo(4) Used to create a row and activate it. Both
				               unreliable interfaces must be set before the
				               status will be 'active'. Other objects that are
				               not specified will have default value. For
				               impact on ifTable see createAndWait and active.
				
				createAndWait(5) All objects in the row are created but with 
				                 default values. Both unreliable interfaces 
				                 must be set before the status can be set to
				                 'active'. Other objects that are
				                 not specified will have default value. 
				                 Also the entry in the ifTable for the reliable 
				                 interface is created. The ifOperStatus will then be 
				                 'notPresent' and the values of the various objects 
				                 in the ifTable-row will be not meaningfull.
				
				destroy(6)     Removes an SNCP. If the reliable interface
				               was cross-connected the unreliable interface
				               that was active will inherit the cross-
				               connection. Also the entry in the ifTable will
				               be removed.
				
				"
			::= { xfSncpEntry 11 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.3
		xfSncpIfTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfSncpIfEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table indicates whether an interface can be used in 
				an SNCP or in case it is already involved in an SNCP 
				it indicates the reliable interface."
			::= { xfSncpObjects 3 }

		
		-- 1.3.6.1.4.1.193.81.2.6.2.3.1
		xfSncpIfEntry OBJECT-TYPE
			SYNTAX XfSncpIfEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table."
			INDEX { ifIndex }
			::= { xfSncpIfTable 1 }

		
		XfSncpIfEntry ::=
			SEQUENCE { 
				xfSncpIfReliable
					Integer32
