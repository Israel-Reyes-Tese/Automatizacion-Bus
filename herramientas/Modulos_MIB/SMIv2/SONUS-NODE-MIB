sonusParam OBJECT IDENTIFIER
    ::= { sonusNodeMIBObjects 5 }

sonusParamStatusObjects OBJECT IDENTIFIER
    ::= { sonusParam 1 }

sonusParamSaveSeqNumber OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The parameter save sequence number assigned to this parameter file.
             This value is unique within the current sequence epoch."
    ::= { sonusParamStatusObjects 1 }

sonusParamSaveTimeStart OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The system uptime, measured in milliseconds, when the last
             parameter save cycle started."
    ::= { sonusParamStatusObjects 2 }

sonusParamSaveTimeStop OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The system uptime, measured in milliseconds, when the last
             parameter save cycle ended."
    ::= { sonusParamStatusObjects 3 }

sonusParamSaveDuration OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The measured time in milliseconds to complete the last parameter
             save cycle."
    ::= { sonusParamStatusObjects 4 }

sonusParamSaveTotalBytes OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of bytes contained in the last binary parameter
             file saved."
    ::= { sonusParamStatusObjects 5 }

sonusParamSaveTotalRecords OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of individual parameter records contained in the last
             binary parameter file saved."
    ::= { sonusParamStatusObjects 6 }

sonusParamSaveFilename OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..31))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the base filename of the parameter file being written."
    ::= { sonusParamStatusObjects 7 }

sonusParamSaveState OBJECT-TYPE
    SYNTAX          INTEGER { idle(1), synchronize(2), lock(3), 
                        holdoff(4), fopen(5), collect(6), fclose(7), 
                        done(8), retry(9), check(10) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current state of the active Management Server's parameter
             saving process."
    ::= { sonusParamStatusObjects 8 }

sonusParamLoadServer OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The IP Address of the NFS server from which parameters were
             loaded from."
    ::= { sonusParamStatusObjects 9 }

sonusParamLoadFileType OBJECT-TYPE
    SYNTAX          INTEGER { primary(1), backup(2), temporary(3), 
                        none(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type of binary parameter which was loaded.  Under normal
             circumstances, the primary(1) parameter file will always be
             loaded.  When default parameters are used, no parameter file
             is loaded, and the value none(4) is used for the file type."
    ::= { sonusParamStatusObjects 10 }

sonusParamLoadStatus OBJECT-TYPE
    SYNTAX          INTEGER { defaults(1), success(2), paramError(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Indicates the status of the last binary parameter file load.
             The value defaults(1) indicates that parameters were not loaded
             and that the node began with default parameters.  The value
             success(2) indicates that a binary parameter file was 
             successfully loaded when the node booted.  The value paramError(3)
             indicates that the node attempted to load a binary parameter file
             and that there was an error in the processing of the file.  The
             node may be running with a partial parameter file when this error
             is indicated."
    ::= { sonusParamStatusObjects 11 }

sonusParamLoadSeqNumber OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The sequence number of the parameter file that was loaded.
             This value is unique for the sequence epoch in which it was saved."
    ::= { sonusParamStatusObjects 12 }

sonusParamLoadTotalRecords OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            ""
    ::= { sonusParamStatusObjects 13 }

sonusParamLoadTotalBytes OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            ""
    ::= { sonusParamStatusObjects 14 }

sonusParamLoadDuration OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            ""
    ::= { sonusParamStatusObjects 15 }

sonusParamLoadSerialNum OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..31))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the serial number of the node which created the
             parameter file loaded by this node."
    ::= { sonusParamStatusObjects 16 }

sonusParamSavePrimarySrvrState OBJECT-TYPE
    SYNTAX          INTEGER { unknown(1), failed(2), failing(3), 
                        behind(4), current(5), ahead(6), inactive(7) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current state of parameter saving to this NFS server."
    ::= { sonusParamStatusObjects 17 }

sonusParamSavePrimarySrvrReason OBJECT-TYPE
    SYNTAX          INTEGER { success(1), create(2), fopen(3), header(4), 
                        timer(5), nfs(6), flush(7), write(8), close(9), 
                        move(10), memory(11), none(12) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
