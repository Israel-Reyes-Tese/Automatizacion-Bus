PtIOHWLSLH110ChannelEntry ::=
    SEQUENCE {
        ptIOHWLSLH110ChannelChannelNumber Integer32,
        ptIOHWLSLH110ChannelBaudRate    INTEGER,
        ptIOHWLSLH110ChannelCTBusTimeSlotRx   INTEGER,
        ptIOHWLSLH110ChannelCTBusTimeSlotTx   INTEGER,
        ptIOHWLSLH110ChannelLoopbackMode INTEGER,
        ptIOHWLSLH110ChannelCLLI        DisplayString,
        ptIOHWLSLH110ChannelSupportDebug DisplayString,
        ptIOHWLSLH110ChannelL3Use       INTEGER,
        ptIOHWLSLH110ChannelMTP3API     INTEGER,
        ptIOHWLSLH110ChannelLSSU        INTEGER,
        ptIOHWLSLH110ChannelFISUAndMSU  INTEGER,
        ptIOHWLSLH110ChannelIOManagerIndex  Integer32,
        ptIOHWLSLH110ChannelLSLChannelsIndex    Integer32,
        ptIOHWLSLH110ChannelParentOID   OBJECT IDENTIFIER,
        ptIOHWLSLH110ChannelRowStatus   RowStatus,
        ptIOHWLSLH110ChannelIndex       Integer32
    }

ptIOHWLSLH110ChannelChannelNumber OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of the Signaling Channel being mapped"
    ::= { ptIOHWLSLH110ChannelEntry 1 }

ptIOHWLSLH110ChannelBaudRate OBJECT-TYPE
    SYNTAX          INTEGER { ds064k(0), ds0a56k(1), baud48k(2), 
                        baud48kRev(3), baudTtc48k(4) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Baud rate for the timeslot. 64K uses all 8 bits. 56K uses 7 of 8 bits and discards the LSB. 48K uses 6 of 8 bits and discards the 2 LSBs. 48K_REV uses 6 of 8 bits and discards the two MSBs. TTC_48K uses 6 of 8 bits and discards the LSB and MSB."
    ::= { ptIOHWLSLH110ChannelEntry 6 }

ptIOHWLSLH110ChannelCTBusTimeSlotRx OBJECT-TYPE
    SYNTAX          INTEGER { notConfigured(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Receive Timeslot on the CTBus (1-4096)"
    ::= { ptIOHWLSLH110ChannelEntry 8 }

ptIOHWLSLH110ChannelCTBusTimeSlotTx OBJECT-TYPE
    SYNTAX          INTEGER { notConfigured(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Transmit Timeslot on the CTBus (1-4096)"
    ::= { ptIOHWLSLH110ChannelEntry 9 }

ptIOHWLSLH110ChannelLoopbackMode OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Redirects incoming data on Tx timeslot back to Rx timeslot (Remote end L2 alignment may be achieved). ENABLED or DISABLED"
    ::= { ptIOHWLSLH110ChannelEntry 10 }

