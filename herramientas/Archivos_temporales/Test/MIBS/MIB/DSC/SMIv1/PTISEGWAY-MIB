    SYNTAX          SEQUENCE OF PtdscdbDSCDBConfigurationEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptdscdbDSCDBConfiguration 1 }

ptdscdbDSCDBConfigurationEntry OBJECT-TYPE
    SYNTAX          PtdscdbDSCDBConfigurationEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of dscdbDSCDBConfiguration attributes."
    INDEX           { ptConnGroupIndex, ptdscdbDSCDBConfigurationIndex }
    ::= { ptdscdbDSCDBConfigurationTable 1 }

PtdscdbDSCDBConfigurationEntry ::=
    SEQUENCE {
        ptdscdbDSCDBConfigurationTimeToLive   INTEGER,
        ptdscdbDSCDBConfigurationGracePeriod    INTEGER,
        ptdscdbDSCDBConfigurationLogSyncInterval        INTEGER,
        ptdscdbDSCDBConfigurationReplicationFactor          INTEGER,
        ptdscdbDSCDBConfigurationErrors INTEGER,
        ptdscdbDSCDBConfigurationSetupManagement        INTEGER,
        ptdscdbDSCDBConfigurationExecutionTrace       INTEGER,
        ptdscdbDSCDBConfigurationDistributionManagement               INTEGER,
        ptdscdbDSCDBConfigurationParentOID  OBJECT IDENTIFIER,
        ptdscdbDSCDBConfigurationRowStatus  RowStatus,
        ptdscdbDSCDBConfigurationIndex  INTEGER
    }

ptdscdbDSCDBConfigurationTimeToLive OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Time-To-Live value given to newly inserted records into the database."
    ::= { ptdscdbDSCDBConfigurationEntry 1 }

ptdscdbDSCDBConfigurationGracePeriod OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Grace time a record is kept in DB after it is marked for deletion."
    ::= { ptdscdbDSCDBConfigurationEntry 2 }

ptdscdbDSCDBConfigurationLogSyncInterval OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Inverval time for how often records are synced to disk."
    ::= { ptdscdbDSCDBConfigurationEntry 3 }

ptdscdbDSCDBConfigurationReplicationFactor OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Number of Nodes among which data is replicated."
    ::= { ptdscdbDSCDBConfigurationEntry 4 }

ptdscdbDSCDBConfigurationErrors OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "If set to LOGGED, the system logs to debug all DB Agent errors. \nShould be set to LOGGED for live systems."
    ::= { ptdscdbDSCDBConfigurationEntry 5 }

ptdscdbDSCDBConfigurationSetupManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "If set to LOGGED, the system logs to debug the DB Agent setup information. \nShould be set to LOGGED for live systems."
    ::= { ptdscdbDSCDBConfigurationEntry 6 }

ptdscdbDSCDBConfigurationExecutionTrace OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "If set to LOGGED, the system logs to debug the execution trace during the processing of incoming DB Agent traffic. \nMust be set to NOT LOGGED for live systems."
    ::= { ptdscdbDSCDBConfigurationEntry 7 }

ptdscdbDSCDBConfigurationDistributionManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "If set to LOGGED, the system logs to debug the DB Agent distribution information. \nMust be set to NOT LOGGED for live systems."
    ::= { ptdscdbDSCDBConfigurationEntry 8 }

ptdscdbDSCDBConfigurationParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptdscdbDSCDBConfigurationEntry 253 }

