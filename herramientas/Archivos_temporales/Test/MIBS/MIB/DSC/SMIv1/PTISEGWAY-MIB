ptMTPM2UALink OBJECT IDENTIFIER
    ::= { ptMTPLinkset 70 }

ptMTPM2UALinkTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtMTPM2UALinkEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of Link attributes."
    ::= { ptMTPM2UALink 1 }

ptMTPM2UALinkEntry OBJECT-TYPE
    SYNTAX          PtMTPM2UALinkEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of MTPM2UALink attributes."
    INDEX           { ptConnGroupIndex, ptMTPM2UALinkMTP3NAManagerIndex, 
                      ptMTPM2UALinkMTP3NAIndex, 
                      ptMTPM2UALinkLinksetsIndex, 
                      ptMTPM2UALinkLinksetIndex, ptMTPM2UALinkIndex }
    ::= { ptMTPM2UALinkTable 1 }

PtMTPM2UALinkEntry ::=
    SEQUENCE {
        ptMTPM2UALinkContext            DisplayString,
        ptMTPM2UALinkAPC                INTEGER,
        ptMTPM2UALinkSLC                INTEGER,
        ptMTPM2UALinkDREID              INTEGER,
        ptMTPM2UALinkCage               INTEGER,
        ptMTPM2UALinkIID                INTEGER,
        ptMTPM2UALinkL2State            INTEGER,
        ptMTPM2UALinkL3State            INTEGER,
        ptMTPM2UALinkCongestionLevel    INTEGER,
        ptMTPM2UALinkDiagnosticMessage  DisplayString,
        ptMTPM2UALinkL2DebugLevel       INTEGER,
        ptMTPM2UALinkUserActivated      INTEGER,
        ptMTPM2UALinkLinkName           DisplayString,
        ptMTPM2UALinkAlarms             INTEGER,
        ptMTPM2UALinkLinkOscillationFilter  INTEGER,
        ptMTPM2UALinkDataLinkBandwidth  INTEGER,
        ptMTPM2UALinkRxMSUCount         INTEGER,
        ptMTPM2UALinkTxMSUCount         INTEGER,
        ptMTPM2UALinkMTP3NAManagerIndex INTEGER,
        ptMTPM2UALinkMTP3NAIndex        INTEGER,
        ptMTPM2UALinkLinksetsIndex      INTEGER,
        ptMTPM2UALinkLinksetIndex       INTEGER,
        ptMTPM2UALinkActions            INTEGER,
        ptMTPM2UALinkParentOID          OBJECT IDENTIFIER,
        ptMTPM2UALinkRowStatus          RowStatus,
        ptMTPM2UALinkIndex              INTEGER
    }

ptMTPM2UALinkContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A condensed textual representation of the link object."
    ::= { ptMTPM2UALinkEntry 1 }

ptMTPM2UALinkAPC OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Adjacent SS7 point code of this link"
    ::= { ptMTPM2UALinkEntry 2 }

ptMTPM2UALinkSLC OBJECT-TYPE
    SYNTAX          INTEGER (0..15)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Signaling Link Code for this link. Value of 0-15"
    ::= { ptMTPM2UALinkEntry 3 }

ptMTPM2UALinkDREID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "DRE to own this link."
    ::= { ptMTPM2UALinkEntry 4 }

ptMTPM2UALinkCage OBJECT-TYPE
    SYNTAX          INTEGER (-1..127)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Cage used by this link"
    ::= { ptMTPM2UALinkEntry 5 }

ptMTPM2UALinkIID OBJECT-TYPE
    SYNTAX          INTEGER (-1 | 1..65534)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Channel used by this link"
    ::= { ptMTPM2UALinkEntry 7 }

ptMTPM2UALinkL2State OBJECT-TYPE
    SYNTAX          INTEGER { deactivated(0), proving(1), inService(2), 
                        outOfService(3), failed(4), localProcOutage(5), 
                        remProcOutage(6), boProcOutage(7), 
                        localLoopback(8), notEnabled(9), 
                        notInitialized(10), userShutdown(11) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Status of MTP Level 2 for this link. Deactivated, Proving, In-Service, Out of Service, Failed, Local Processor Outage, Remote Processor Outage, Both Processor Outage, Local Loopback mode, Not Enabled, Not Initialized"
    ::= { ptMTPM2UALinkEntry 8 }

ptMTPM2UALinkL3State OBJECT-TYPE
    SYNTAX          INTEGER { deactivated(0), proving(1), inService(2), 
                        outOfService(3), locallyInhibited(4), 
                        remInhibited(5), bothInhibited(6), blocked(7), 
                        blockedInhibited(8), localProcOutage(9) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Status of MTP Level 3 for this link. Deactivated, Proving, In-Service, Out of Service, Locally Inhibited, Remotely Inhibited, Both Inhibited, Blocked, Blocked and Uninhibited, Local Processor Outage"
    ::= { ptMTPM2UALinkEntry 9 }

ptMTPM2UALinkCongestionLevel OBJECT-TYPE
    SYNTAX          INTEGER { false(0), level1(1), level2(2), level3(3), 
                        true(4) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Congestion level of this link. For ANSI, from 1 to 3, or FALSE if not congested. For ITU, TRUE or FALSE."
    ::= { ptMTPM2UALinkEntry 10 }

