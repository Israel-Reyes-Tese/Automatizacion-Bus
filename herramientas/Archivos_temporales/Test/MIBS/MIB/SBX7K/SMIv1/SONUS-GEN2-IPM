sonusSystemIpPolicingSystemIntStatsTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSystemIpPolicingSystemIntStatsEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "
             The Sonus System Policer Interval table.
             "
    ::= { sonusGen2IpmMIB 11 }

sonusSystemIpPolicingSystemIntStatsEntry OBJECT-TYPE
    SYNTAX          SonusSystemIpPolicingSystemIntStatsEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            ""
    INDEX           { sonusSystemIpPolicingSystemIntStatsNumber, 
                      sonusSystemIpPolicingSystemIntStatsSystemName }
    ::= { sonusSystemIpPolicingSystemIntStatsTable 1 }

SonusSystemIpPolicingSystemIntStatsEntry ::=
    SEQUENCE {
        sonusSystemIpPolicingSystemIntStatsNumber         Gauge,
        sonusSystemIpPolicingSystemIntStatsSystemName             String,
        sonusSystemIpPolicingSystemIntStatsIntervalValid                INTEGER,
        sonusSystemIpPolicingSystemIntStatsTime       Gauge,
--         sonusSystemIpPolicingSystemIntStatsBadEtherIpHdrDiscardPacketCount                                  Counter64,
        sonusSystemIpPolicingSystemIntStatsBadEtherIpHdrPeakDiscardRate                               Gauge,
        sonusSystemIpPolicingSystemIntStatsBadEtherIpHdrMinorAlarmCount                               Gauge,
        sonusSystemIpPolicingSystemIntStatsBadEtherIpHdrMinorAlarmDuration                                  Gauge,
        sonusSystemIpPolicingSystemIntStatsBadEtherIpHdrMajorAlarmCount                               Gauge,
        sonusSystemIpPolicingSystemIntStatsBadEtherIpHdrMajorAlarmDuration                                  Gauge,
--         sonusSystemIpPolicingSystemIntStatsArpDiscardPacketCount                        Counter64,
--         sonusSystemIpPolicingSystemIntStatsArpAcceptPacketCount                       Counter64,
        sonusSystemIpPolicingSystemIntStatsArpPeakDiscardRate                     Gauge,
        sonusSystemIpPolicingSystemIntStatsArpMinorAlarmCount                     Gauge,
        sonusSystemIpPolicingSystemIntStatsArpMinorAlarmDuration                        Gauge,
        sonusSystemIpPolicingSystemIntStatsArpMajorAlarmCount                     Gauge,
        sonusSystemIpPolicingSystemIntStatsArpMajorAlarmDuration                        Gauge,
--         sonusSystemIpPolicingSystemIntStatsUFlowDiscardPacketCount                          Counter64,
--         sonusSystemIpPolicingSystemIntStatsUFlowAcceptPacketCount                         Counter64,
        sonusSystemIpPolicingSystemIntStatsUFlowPeakDiscardRate                       Gauge,
        sonusSystemIpPolicingSystemIntStatsUFlowMinorAlarmCount                       Gauge,
        sonusSystemIpPolicingSystemIntStatsUFlowMinorAlarmDuration                          Gauge,
        sonusSystemIpPolicingSystemIntStatsUFlowMajorAlarmCount                       Gauge,
        sonusSystemIpPolicingSystemIntStatsUFlowMajorAlarmDuration                          Gauge,
--         sonusSystemIpPolicingSystemIntStatsAclDiscardPacketCount                        Counter64,
--         sonusSystemIpPolicingSystemIntStatsAclAcceptPacketCount                       Counter64,
        sonusSystemIpPolicingSystemIntStatsAclPeakDiscardRate                     Gauge,
        sonusSystemIpPolicingSystemIntStatsAclMinorAlarmCount                     Gauge,
        sonusSystemIpPolicingSystemIntStatsAclMinorAlarmDuration                        Gauge,
        sonusSystemIpPolicingSystemIntStatsAclMajorAlarmCount                     Gauge,
        sonusSystemIpPolicingSystemIntStatsAclMajorAlarmDuration                        Gauge,
--         sonusSystemIpPolicingSystemIntStatsAggregateDiscardPacketCount                              Counter64,
--         sonusSystemIpPolicingSystemIntStatsAggregateAcceptPacketCount                             Counter64,
        sonusSystemIpPolicingSystemIntStatsAggregatePeakDiscardRate                           Gauge,
        sonusSystemIpPolicingSystemIntStatsAggregateMinorAlarmCount                           Gauge,
        sonusSystemIpPolicingSystemIntStatsAggregateMinorAlarmDuration                              Gauge,
        sonusSystemIpPolicingSystemIntStatsAggregateMajorAlarmCount                           Gauge,
        sonusSystemIpPolicingSystemIntStatsAggregateMajorAlarmDuration                              Gauge,
--         sonusSystemIpPolicingSystemIntStatsIpSecDecryptDiscardPacketCount                                 Counter64,
--         sonusSystemIpPolicingSystemIntStatsIpSecDecryptAcceptPacketCount                                Counter64,
        sonusSystemIpPolicingSystemIntStatsIpSecDecryptPeakDiscardRate                              Gauge,
        sonusSystemIpPolicingSystemIntStatsIpSecDecryptMinorAlarmCount                              Gauge,
        sonusSystemIpPolicingSystemIntStatsIpSecDecryptMinorAlarmDuration                                 Gauge,
        sonusSystemIpPolicingSystemIntStatsIpSecDecryptMajorAlarmCount                              Gauge,
        sonusSystemIpPolicingSystemIntStatsIpSecDecryptMajorAlarmDuration                                 Gauge,
--         sonusSystemIpPolicingSystemIntStatsMediaDiscardPacketCount                          Counter64,
--         sonusSystemIpPolicingSystemIntStatsMediaAcceptPacketCount                         Counter64,
        sonusSystemIpPolicingSystemIntStatsMediaPeakDiscardRate                       Gauge,
        sonusSystemIpPolicingSystemIntStatsMediaMinorAlarmCount                       Gauge,
        sonusSystemIpPolicingSystemIntStatsMediaMinorAlarmDuration                          Gauge,
        sonusSystemIpPolicingSystemIntStatsMediaMajorAlarmCount                       Gauge,
        sonusSystemIpPolicingSystemIntStatsMediaMajorAlarmDuration                          Gauge,
--         sonusSystemIpPolicingSystemIntStatsRogueMediaDiscardPacketCount                               Counter64,
        sonusSystemIpPolicingSystemIntStatsRogueMediaPeakDiscardRate                            Gauge,
        sonusSystemIpPolicingSystemIntStatsRogueMediaMinorAlarmCount                            Gauge,
        sonusSystemIpPolicingSystemIntStatsRogueMediaMinorAlarmDuration                               Gauge,
        sonusSystemIpPolicingSystemIntStatsRogueMediaMajorAlarmCount                            Gauge,
        sonusSystemIpPolicingSystemIntStatsRogueMediaMajorAlarmDuration                               Gauge,
--         sonusSystemIpPolicingSystemIntStatsDiscardRuleDiscardPacketCount                                Counter64,
        sonusSystemIpPolicingSystemIntStatsDiscardRulePeakDiscardRate                             Gauge,
        sonusSystemIpPolicingSystemIntStatsDiscardRuleMinorAlarmCount                             Gauge,
        sonusSystemIpPolicingSystemIntStatsDiscardRuleMinorAlarmDuration                                Gauge,
        sonusSystemIpPolicingSystemIntStatsDiscardRuleMajorAlarmCount                             Gauge,
        sonusSystemIpPolicingSystemIntStatsDiscardRuleMajorAlarmDuration                                Gauge,
--         sonusSystemIpPolicingSystemIntStatsSrtpDecryptDiscardPacketCount                                Counter64,
        sonusSystemIpPolicingSystemIntStatsSrtpDecryptPeakDiscardRate                             Gauge,
        sonusSystemIpPolicingSystemIntStatsSrtpDecryptMinorAlarmCount                             Gauge,
        sonusSystemIpPolicingSystemIntStatsSrtpDecryptMinorAlarmDuration                                Gauge,
        sonusSystemIpPolicingSystemIntStatsSrtpDecryptMajorAlarmCount                             Gauge,
        sonusSystemIpPolicingSystemIntStatsSrtpDecryptMajorAlarmDuration                                Gauge
    }

sonusSystemIpPolicingSystemIntStatsNumber OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "
             A sequence number which identifies the
             interval for which the set of statistics is required.
             The interval N identifies the recently completed interval,
             Interval N is identifed by the last sequence value and N-( number of intervals)
             is the earlest completed interval.
             "
    ::= { sonusSystemIpPolicingSystemIntStatsEntry 1 }

sonusSystemIpPolicingSystemIntStatsSystemName OBJECT-TYPE
    SYNTAX          String (SIZE(0..255))
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "
             Name of the system.
             "
    ::= { sonusSystemIpPolicingSystemIntStatsEntry 2 }

sonusSystemIpPolicingSystemIntStatsIntervalValid OBJECT-TYPE
    SYNTAX          INTEGER { true(1), false(2) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "
             The member indicating the validity of the interval.
             "
    ::= { sonusSystemIpPolicingSystemIntStatsEntry 3 }

sonusSystemIpPolicingSystemIntStatsTime OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "
             The system up time when the interval statisitic is collected.
             "
    ::= { sonusSystemIpPolicingSystemIntStatsEntry 4 }

