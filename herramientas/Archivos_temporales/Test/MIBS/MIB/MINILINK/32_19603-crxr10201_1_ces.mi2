    --
    -- pwXTable --
    --
		
		-- 1.3.6.1.4.1.193.81.4.4.1.5.1
    pwXTable        OBJECT-TYPE
        SYNTAX      SEQUENCE OF PwXEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "This table extends the pwTable"
        ::= { xfCesStandardExtensions 1 }

		-- 1.3.6.1.4.1.193.81.4.4.1.5.1.1
    pwXEntry        OBJECT-TYPE
        SYNTAX      PwXEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "An entry containing additional TDM configuration
                applicable to a particular PW."
        AUGMENTS    { pwEntry }
        ::= { pwXTable 1 }

    pwTimingInput OBJECT-TYPE
        SYNTAX INTEGER
            {
            looped(1),
            external(2),
            recovered(3)
            }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "This object is used to set one of the following timing:
            looped(1)    Emulated service is used for timing
            external(2)  Node sync is used for timing (Network Synchronization)
            recovered(3) Ethernet line is used to recover the clock"
        DEFVAL { 1 }
        ::= { pwXEntry 1 }

		PwXEntry ::=
			SEQUENCE { 
        pwTimingInput 
            INTEGER
			 }

    --
    -- pwTDMXTable --
    --

		-- 1.3.6.1.4.1.193.81.4.4.1.5.1.2
    pwTDMXTable        OBJECT-TYPE
        SYNTAX      SEQUENCE OF PwTDMXEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "This table extends the pwTDMTable"
        ::= { xfCesStandardExtensions 2 }

		-- 1.3.6.1.4.1.193.81.4.4.1.5.1.2.1
    pwTDMXEntry        OBJECT-TYPE
        SYNTAX      PwTDMXEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "An entry containing additional management information
                applicable to a particular PW."
        AUGMENTS    { pwTDMEntry }
        ::= { pwTDMXTable 1 }
	
		PwTDMXEntry ::=
			SEQUENCE { 
				pwTDMAlarmStatus
					BITS
			 }

		-- 1.3.6.1.4.1.193.81.4.4.1.5.1.2.1.1
		pwTDMAlarmStatus OBJECT-TYPE
			SYNTAX BITS
				{
				pwTDMLossOfFrames0(0),
				pwTDMLossOfFrames1(1),
				pwTDMLossOfFrames2(2),
				pwTDMLateFrames0(3),
				pwTDMLateFrames1(4),
				pwTDMLateFrames2(5),
				pwTDMJtrBufferOverrun0(6),
				pwTDMJtrBufferOverrun1(7),
				pwTDMJtrBufferOverrun2(8),
				pwTDMMalformedFrames0(9),
				pwTDMMalformedFrames1(10),
				pwTDMMalformedFrames2(11),
				pwTDMRemoteLossOfFrames0(12),
				pwTDMRemoteLossOfFrames1(13),
				pwTDMRemoteLossOfFrames2(14),
				pwTDMJtrBufferUndererrun0(15),
				pwTDMJtrBufferUndererrun1(16),
				pwTDMJtrBufferUndererrun2(17)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates the alarm status of a CES Pseudowire.
				
				Severity is indicated by 3 bits per status:
				   bits 012
					    000   clear
					    001   undetermined
					    010   warning
					    011   minor
					    100   major
					    101   critical
				
				E.g. If
				   pwTDMMalformedFrames0=0
				   pwTDMMalformedFrames1=1
				   pwTDMMalformedFrames2=1
				then this MO reports a Malformed Frames alarm whose severity is minor.
				"
			::= { pwTDMXEntry 1 }


