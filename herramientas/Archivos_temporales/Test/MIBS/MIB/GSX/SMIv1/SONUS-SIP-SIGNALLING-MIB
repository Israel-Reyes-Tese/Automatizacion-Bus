SonusTransmitProfileEntry ::=
    SEQUENCE {
        sonusTransmitProfIndex          Gauge,
        sonusTransmitProfName           SonusName,
        sonusTransmitProfRowStatus      RowStatus,
        sonusTransmitProfAccepted       SonusAdminState,
        sonusTransmitProfAcceptResourcePriority       SonusAdminState,
        sonusTransmitProfAlertInfo      SonusAdminState,
        sonusTransmitProfAllowed        SonusAdminState,
        sonusTransmitProfAlso           SonusAdminState,
        sonusTransmitProfAnonymity      SonusAdminState,
        sonusTransmitProfAuthorization  SonusAdminState,
        sonusTransmitProfCallConnectId  SonusAdminState,
        sonusTransmitProfContentDisp    SonusAdminState,
        sonusTransmitProfDiversion      SonusAdminState,
        sonusTransmitProfError          SonusAdminState,
        sonusTransmitProfErrorInfo      SonusAdminState,
        sonusTransmitProfEvent          SonusAdminState,
        sonusTransmitProfExpire         SonusAdminState,
        sonusTransmitProfMaxForwards    SonusAdminState,
        sonusTransmitProfMinExpires     SonusAdminState,
        sonusTransmitProfMinSe          SonusAdminState,
        sonusTransmitProfPAssertedId    SonusAdminState,
        sonusTransmitProfPCarrierInfo   SonusAdminState,
        sonusTransmitProfPChargingVector SonusAdminState,
        sonusTransmitProfPKAdn          SonusAdminState,
        sonusTransmitProfPPreferredId   SonusAdminState,
        sonusTransmitProfPSigInfo       SonusAdminState,
        sonusTransmitProfPSvcInfo       SonusAdminState,
        sonusTransmitProfPTermCa        SonusAdminState,
        sonusTransmitProfPath           SonusAdminState,
        sonusTransmitProfPCdrInfo       SonusAdminState,
        sonusTransmitProfPChargeInfo    SonusAdminState,
        sonusTransmitProfPKCfl          SonusAdminState,
        sonusTransmitProfPKCfo          SonusAdminState,
        sonusTransmitProfPrivacy        SonusAdminState,
        sonusTransmitProfProxyAuthorization   SonusAdminState,
        sonusTransmitProfProxyReq       SonusAdminState,
        sonusTransmitProfReason         SonusAdminState,
        sonusTransmitProfRemoteParty    SonusAdminState,
        sonusTransmitProfReplaces       SonusAdminState,
        sonusTransmitProfReplyTo        SonusAdminState,
        sonusTransmitProfRequestBy      SonusAdminState,
        sonusTransmitProfRequired       SonusAdminState,
        sonusTransmitProfResourcePriority SonusAdminState,
        sonusTransmitProfRetryAfter     SonusAdminState,
        sonusTransmitProfRoute          SonusAdminState,
        sonusTransmitProfServiceRoute   SonusAdminState,
        sonusTransmitProfSessExpires    SonusAdminState,
        sonusTransmitProfSubscriptionState  SonusAdminState,
        sonusTransmitProfSupported      SonusAdminState,
        sonusTransmitProfTimeStamp      SonusAdminState,
        sonusTransmitProfUnknown        SonusAdminState,
        sonusTransmitProfUnsupported    SonusAdminState,
        sonusTransmitProfWWWAuthenticate SonusAdminState,
        sonusTransmitProfAcceptContact  SonusAdminState
    }

