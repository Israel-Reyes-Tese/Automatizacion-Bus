        xfMacLoopbackIndex OBJECT-TYPE
            SYNTAX Integer32 (1..255)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Identifies the MAC loopback instance.
                Note: ML66 only supports a single instance with index value 1."
            ::= { xfMacLoopbackEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.19.1.1.2
        xfMacLoopbackInterface OBJECT-TYPE
            SYNTAX InterfaceIndexOrZero
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies an individual LAN/WAN interface or a LAG/L1B master interface,
                on which the MAC loopback is to be performed.
                Value 0 indicates that an interface has not been assigned yet."
            DEFVAL { 0 }
            ::= { xfMacLoopbackEntry 2 }

