sonusDsx3FarEndIntervalDayTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDsx3FarEndIntervalDayEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The DS3 Interval Table contains various
             far end statistics collected by each DS3 interface
             over the previous 24 hours of operation."
    ::= { sonusDsx3MIBObjects 10 }

sonusDsx3FarEndIntervalDayEntry OBJECT-TYPE
    SYNTAX          SonusDsx3FarEndIntervalDayEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the DS3 Far End Interval table."
    INDEX           { sonusDsx3FarEndIntervalDayShelfIndex, 
                      sonusDsx3FarEndIntervalDaySlotIndex, 
                      sonusDsx3FarEndIntervalDayPortIndex, 
                      sonusDsx3FarEndIntervalDayDs3Index }
    ::= { sonusDsx3FarEndIntervalDayTable 1 }

SonusDsx3FarEndIntervalDayEntry ::=
    SEQUENCE {
        sonusDsx3FarEndIntervalDayShelfIndex    SonusShelfIndex,
        sonusDsx3FarEndIntervalDaySlotIndex   SonusSlotIndex,
        sonusDsx3FarEndIntervalDayPortIndex   Integer32,
        sonusDsx3FarEndIntervalDayDs3Index  Integer32,
        sonusDsx3FarEndIntervalDayCVCPs PerfCurrentCount,
        sonusDsx3FarEndIntervalDayESCPs PerfCurrentCount,
        sonusDsx3FarEndIntervalDaySESCPs PerfCurrentCount,
        sonusDsx3FarEndIntervalDaySASCPs PerfCurrentCount,
        sonusDsx3FarEndIntervalDayUASCPs PerfCurrentCount,
        sonusDsx3FarEndIntervalDayPathIDF TruthValue,
        sonusDsx3FarEndIntervalDayFCCP  Integer32
    }

sonusDsx3FarEndIntervalDayShelfIndex OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Shelf index into this table"
    ::= { sonusDsx3FarEndIntervalDayEntry 1 }

sonusDsx3FarEndIntervalDaySlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Slot index into this table"
    ::= { sonusDsx3FarEndIntervalDayEntry 2 }

sonusDsx3FarEndIntervalDayPortIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Port Index into this table"
    ::= { sonusDsx3FarEndIntervalDayEntry 3 }

sonusDsx3FarEndIntervalDayDs3Index OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Ds3 Index into this table"
    ::= { sonusDsx3FarEndIntervalDayEntry 4 }

sonusDsx3FarEndIntervalDayCVCPs OBJECT-TYPE
