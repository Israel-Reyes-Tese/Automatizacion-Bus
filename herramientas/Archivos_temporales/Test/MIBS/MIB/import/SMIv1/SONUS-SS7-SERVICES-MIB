        sonusSs7TimerT15                INTEGER,
        sonusSs7TimerT16                INTEGER,
        sonusSs7TimerT17                INTEGER,
        sonusSs7TimerT18                INTEGER,
        sonusSs7TimerT19                INTEGER,
        sonusSs7TimerT20                INTEGER,
        sonusSs7TimerT21                INTEGER,
