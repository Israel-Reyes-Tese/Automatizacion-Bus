ptSGM3UA OBJECT IDENTIFIER
    ::= { ptConnGroup 175 }

ptSGM3UATable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtSGM3UAEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptSGM3UA 1 }

ptSGM3UAEntry OBJECT-TYPE
    SYNTAX          PtSGM3UAEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of SGM3UA attributes."
    INDEX           { ptConnGroupIndex, ptSGM3UAIndex }
    ::= { ptSGM3UATable 1 }

PtSGM3UAEntry ::=
    SEQUENCE {
        ptSGM3UAPendingTimer            INTEGER,
        ptSGM3UAMonitoring              INTEGER,
        ptSGM3UAAssociationCapacity     Integer32,
        ptSGM3UAHighUsageOnset          INTEGER,
        ptSGM3UAHighUsageAbatement      INTEGER,
        ptSGM3UAUsageIntervalSeconds    Integer32,
        ptSGM3UAIMFSupport              INTEGER,
        ptSGM3UASCTPProtocolIDValidation INTEGER,
        ptSGM3UAASMasterTimer           Integer32,
        ptSGM3UAASMasterPendingTimer    INTEGER,
        ptSGM3UAActions                 INTEGER,
        ptSGM3UAParentOID               OBJECT IDENTIFIER,
        ptSGM3UARowStatus               RowStatus,
        ptSGM3UAIndex                   Integer32
    }

ptSGM3UAPendingTimer OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Duration of PENDING state in msecs for an M3UA AS. Pending timer is started when an AS transitions out of ACTIVE state. \nIf an ASP sends an ASPAC before the timer expires, the AS will immediately transition back to the ACTIVE status. Otherwise, when the timer expires the AS will either transition to INACTIVE or DOWN state. This timer is used by all M3UA AS of type other than AS Master. \nMessages will be queued while in PENDING state. \nValid range of this timer is 1 to 2000. \nTo disable the timer, set to 0. \nDefault value is 2000 msecs."
    ::= { ptSGM3UAEntry 2 }

ptSGM3UAMonitoring OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Monitoring feature (ENABLED/DISABLED)."
    ::= { ptSGM3UAEntry 3 }

ptSGM3UAAssociationCapacity OBJECT-TYPE
    SYNTAX          Integer32 (0..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Bandwidth of SCTP association. Number of bytes per second support by association."
    ::= { ptSGM3UAEntry 4 }

ptSGM3UAHighUsageOnset OBJECT-TYPE
    SYNTAX          INTEGER { default(-1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Traffic level at which high usage alarms are triggered. (% of association capacity).\n This setting is used for both the transmit and receive high usage alarms for SCTP associations used by M3UA ASPs.\n The alarm is automatically cleared when traffic levels fall below the High Usage Abatement setting."
    ::= { ptSGM3UAEntry 5 }

ptSGM3UAHighUsageAbatement OBJECT-TYPE
    SYNTAX          INTEGER { default(-1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Traffic level at which high usage alarms are cleared. (% of association capacity).\n This setting is used for both the transmit and receive high usage alarms for SCTP associations used by M3UA ASPs."
    ::= { ptSGM3UAEntry 6 }

ptSGM3UAUsageIntervalSeconds OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Interval in seconds used to calculate average traffic load."
    ::= { ptSGM3UAEntry 8 }

ptSGM3UAIMFSupport OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Integrated Monitoring Feed feature support for M3UA. \nIMF must be licensed to enable this attribute."
    ::= { ptSGM3UAEntry 9 }

ptSGM3UASCTPProtocolIDValidation OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Validation of protocol ID of received data. \nWhen enabled, the SG will discard all M3UA messages that do not have an SCTP Protocol ID of either 0 or 3 as described in RFC 4666. \nWhen disabled, the SG will not inspect the SCTP protocol ID and accept all M3UA messages."
    ::= { ptSGM3UAEntry 10 }

ptSGM3UAASMasterTimer OBJECT-TYPE
    SYNTAX          Integer32 (10..300)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Amount of time (in seconds) all AS of type AS Master must be in ACTIVE state before the PT_C20 Application ID is registered with MTP3. This feature ensures a more stable system start-up procedure for SP2000 deployments as MTP3 links, based on link type, will only initiate alignment or clear local processor outage on the PT_C20 Application ID registration. Following AS Master recovery due to maintenance or connection loss beyond the AS Master Pending Timer, the MTP3 links will remain unavailable (out of service or processor outage) until this timer expires and PT_C20 registration is triggered. Valid values: 10 to 300 seconds. Default value: 60 seconds."
    ::= { ptSGM3UAEntry 11 }

ptSGM3UAASMasterPendingTimer OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Duration of PENDING state in msecs for an M3UA AS of type AS Master. Pending timer is started when an AS of transitions out of ACTIVE state. \nIf an ASP sends an ASPAC before the timer expires, the AS will immediately transition back to the ACTIVE status. Otherwise, when the timer expires the AS will either transition to INACTIVE or DOWN state. \nMessages bound for an AS, of type AS Master, during PENDING state will not be queued. \nTo disable the timer, set to 0. \nValid values: 0 to 120000 milliseconds. \nDefault value: 30000 msecs."
    ::= { ptSGM3UAEntry 12 }

ptSGM3UAActions OBJECT-TYPE
    SYNTAX          INTEGER { startMonitoring(1), stopMonitoring(2) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of all possible actions for this instance"
    ::= { ptSGM3UAEntry 252 }

ptSGM3UAParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptSGM3UAEntry 253 }

