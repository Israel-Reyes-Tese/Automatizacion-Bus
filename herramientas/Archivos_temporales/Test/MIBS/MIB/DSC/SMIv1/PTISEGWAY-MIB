    SYNTAX          INTEGER { undefined(255), ansi(1), itu(2), china(3), 
                        ntt(4), ttc(5) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The SCCP SS7 Variant for the given NA."
    ::= { ptSGSCCPNAEntry 2 }

