        sonusServerPolicerAlarmMinorPolicerDiscardRateProfileName                         SonusNameReference,
        sonusServerPolicerAlarmMajorPolicerDiscardRateProfileName                         SonusNameReference,
        sonusServerPolicerAlarmRowStatus RowStatus
    }

