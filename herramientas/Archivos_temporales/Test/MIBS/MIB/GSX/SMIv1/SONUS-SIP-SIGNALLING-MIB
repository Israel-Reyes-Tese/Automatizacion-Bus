sonusSipPeerOvldObject OBJECT IDENTIFIER
    ::= { sonusSipSignallingMIBObjects 44 }

sonusSipPeerOvldSamplingPeriod OBJECT-TYPE
    SYNTAX          Gauge (1..10)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The sampling period (in seconds) for the peer-overload backoff algorithm."
    DEFVAL          { 2 }
    ::= { sonusSipPeerOvldObject 1 }

sonusSipPeerOvldDiscardPeriod OBJECT-TYPE
    SYNTAX          Gauge (1..10)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The discard period (in seconds) for the peer-overload backoff algorithm."
    DEFVAL          { 2 }
    ::= { sonusSipPeerOvldObject 2 }

sonusSipPeerOvldStepDownGain OBJECT-TYPE
    SYNTAX          Gauge (1..200)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The step down gain for the peer overload backoff algorithm."
    DEFVAL          { 100 }
    ::= { sonusSipPeerOvldObject 3 }

sonusSipPeerOvldRejectThreshold OBJECT-TYPE
    SYNTAX          Gauge (0..50)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The reject threshold (in rejects/sec) for the peer overload backoff algorithm."
    DEFVAL          { 1 }
    ::= { sonusSipPeerOvldObject 4 }

sonusSipSigPeerOvldStatusTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipSigPeerOvldStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Table of SIP Signaling Peer Overload Status."
    ::= { sonusSipSignallingMIBObjects 45 }

sonusSipSigPeerOvldStatusEntry OBJECT-TYPE
    SYNTAX          SonusSipSigPeerOvldStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "SIP Signaling Peer Overload Status."
    INDEX           { sonusSipSigPeerOvldStatusSlotIndex, 
                      sonusSipSigPeerOvldStatusIpAddress, 
                      sonusSipSigPeerOvldStatusSigPortId, 
                      sonusSipSigPeerOvldStatusIp6Address, 
                      sonusSipSigPeerOvldStatusIpVersion }
    ::= { sonusSipSigPeerOvldStatusTable 1 }

SonusSipSigPeerOvldStatusEntry ::=
    SEQUENCE {
        sonusSipSigPeerOvldStatusSlotIndex  SonusSlotIndex,
        sonusSipSigPeerOvldStatusIpAddress  IpAddress,
        sonusSipSigPeerOvldStatusSigPortId  Gauge,
        sonusSipSigPeerOvldStatusZoneId Gauge,
        sonusSipSigPeerOvldStatusZoneName SonusNameReference,
        sonusSipSigPeerOvldStatusCallsSent  Gauge,
        sonusSipSigPeerOvldStatusCallsRejected      Gauge,
        sonusSipSigPeerOvldStatus503sReceived     Gauge,
        sonusSipSigPeerOvldStatusAllowedRate    Gauge,
        sonusSipSigPeerOvldStatusIp6Address   Ipv6Address,
        sonusSipSigPeerOvldStatusIpVersion  INTEGER
    }

sonusSipSigPeerOvldStatusSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The card slot for the Sip Signaling Overload Peering"
    ::= { sonusSipSigPeerOvldStatusEntry 1 }

sonusSipSigPeerOvldStatusIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IP Address of the Sip Signaling Overload Peer"
    ::= { sonusSipSigPeerOvldStatusEntry 2 }

sonusSipSigPeerOvldStatusSigPortId OBJECT-TYPE
    SYNTAX          Gauge (0..65535)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The signaling port Id for this signaling peer."
    ::= { sonusSipSigPeerOvldStatusEntry 3 }

sonusSipSigPeerOvldStatusZoneId OBJECT-TYPE
    SYNTAX          Gauge (0..65535)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the Zone Id for this signaling peer."
    ::= { sonusSipSigPeerOvldStatusEntry 4 }

sonusSipSigPeerOvldStatusZoneName OBJECT-TYPE
    SYNTAX          SonusNameReference
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the Zone for this signaling peer."
    ::= { sonusSipSigPeerOvldStatusEntry 5 }

sonusSipSigPeerOvldStatusCallsSent OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of calls sent in the last sampling period for peer."
    ::= { sonusSipSigPeerOvldStatusEntry 6 }

sonusSipSigPeerOvldStatusCallsRejected OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of calls rejected in the last sampling period for peer."
    ::= { sonusSipSigPeerOvldStatusEntry 7 }

sonusSipSigPeerOvldStatus503sReceived OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of 503s received in the last sampling period."
