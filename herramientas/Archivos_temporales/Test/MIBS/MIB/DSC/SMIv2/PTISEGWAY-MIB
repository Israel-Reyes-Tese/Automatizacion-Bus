                        localLoopback(8), notEnabled(9), 
                        notInitialized(10), userShutdown(11) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of M2PA for this link. Deactivated, Proving, In-Service, Out-of-service, Failed, Local Processor Outage, Remote Processor Outage, Both Processor Outage, Local Loopback mode, Not Enabled, Not Initialized, User Shutdown"
    ::= { ptMTPM2PALinkEntry 11 }

ptMTPM2PALinkL3State OBJECT-TYPE
    SYNTAX          INTEGER { deactivated(0), proving(1), inService(2), 
                        outOfService(3), locallyInhibited(4), 
                        remInhibited(5), bothInhibited(6), blocked(7), 
                        blockedInhibited(8), localProcOutage(9) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of MTP Level 3 for this link. Deactivated, Proving, In-Service, Out-of-Service, Locally Inhibited, Remotely Inhibited, Both Inhibited, Blocked, Blocked and Uninhibited, Local Processor Outage."
    ::= { ptMTPM2PALinkEntry 12 }

ptMTPM2PALinkCongestionLevel OBJECT-TYPE
    SYNTAX          INTEGER { false(0), level1(1), level2(2), level3(3), 
                        true(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Congestion level of this link. From 1 to 3, or FALSE if not congested."
    ::= { ptMTPM2PALinkEntry 13 }

ptMTPM2PALinkDiagnosticMessage OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Link management diagnostic message."
    ::= { ptMTPM2PALinkEntry 14 }

ptMTPM2PALinkDataDebugLevel OBJECT-TYPE
    SYNTAX          Integer32 (0..255)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Data Debug Level \n\nAmount of information displayed concerning MSU transfers. Higher is more information, 0 is DISABLED. \nThe recommended value for this attribute is 1."
    ::= { ptMTPM2PALinkEntry 15 }

ptMTPM2PALinkProtocolDebugLevel OBJECT-TYPE
    SYNTAX          Integer32 (0..255)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Protocol Debug Level \n\nAmount of information displayed concerning the M2PA protocol. This includes interaction with the M2PA Peer as well as with MTP3 and the SCTP. Higher values cause more logging, a value of 0 disables logging of M2PA Protocol related messages. \nThe recommended value for this attribute is 10."
    ::= { ptMTPM2PALinkEntry 16 }

ptMTPM2PALinkSoftwareDebugLevel OBJECT-TYPE
    SYNTAX          Integer32 (0..255)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Software Debug Level \n\nIndicates how much information to display concerning the internals of the software. A value of 0 indicates no software related information is to be displayed, 1 shows only errors, more than 1 shows more detail. \nThe recommended value for this attribute is 1."
    ::= { ptMTPM2PALinkEntry 17 }

ptMTPM2PALinkSCTPDebugLevel OBJECT-TYPE
    SYNTAX          Integer32 (0..255)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Debug: SCTP debug level for association debugging. \nThe recommended value for this attribute is 1."
    ::= { ptMTPM2PALinkEntry 18 }

ptMTPM2PALinkUserActivated OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Link activation flag. Used on stack restart for alignment attempt. Only affected by link activation/deactivation."
    ::= { ptMTPM2PALinkEntry 19 }

