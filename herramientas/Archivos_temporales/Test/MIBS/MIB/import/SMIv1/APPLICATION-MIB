        applPastTransStreamInvRspTimes  Gauge,
--         applPastTransStreamPerforms     Unsigned64TC,
        applPastTransStreamPerformsLow  Gauge,
        applPastTransStreamPrfCumTimes  Gauge,
        applPastTransStreamPrfRspTimes  Gauge
    }

