PtGTTGTTNAEntry ::=
    SEQUENCE {
        ptGTTGTTNANetworkAppearance     INTEGER,
        ptGTTGTTNAPCRegistrations       INTEGER,
        ptGTTGTTNANonConnectedPeerIDs   DisplayString,
        ptGTTGTTNADBDefName             DisplayString,
        ptGTTGTTNAAppIDRegistrations    INTEGER,
        ptGTTGTTNANATraffic             INTEGER,
        ptGTTGTTNAUserDeactivated       INTEGER,
        ptGTTGTTNALPCRegistration       INTEGER,
        ptGTTGTTNAConnectionType        INTEGER,
        ptGTTGTTNAGTTNAManagerIndex     INTEGER,
        ptGTTGTTNAActions               INTEGER,
        ptGTTGTTNAParentOID             OBJECT IDENTIFIER,
        ptGTTGTTNARowStatus             RowStatus,
        ptGTTGTTNAIndex                 INTEGER
    }

ptGTTGTTNANetworkAppearance OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Network Appearance identifier. \nUsed to determine the default SCCP GTT port."
    ::= { ptGTTGTTNAEntry 1 }

ptGTTGTTNAPCRegistrations OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This attribute provides the number of PC Registrations managed by this Network Appearance. The GTT process may register to the STP with a Virtual Point Code to support another Point Code on the network. Please see the VNode feature at the MTP level for more information. \nThis attribute shows the number of successful registrations across all CPUs. \nBefore a successful registration can take place the MTP level 3 process must have an active VNode provisioned."
    ::= { ptGTTGTTNAEntry 3 }

ptGTTGTTNANonConnectedPeerIDs OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "List of peers that are currently not connected to SCCP as per the active GTT. \nIf the traffic is active but the SCCP is not connected, make sure the peers are available."
    ::= { ptGTTGTTNAEntry 4 }

ptGTTGTTNADBDefName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "GTT Database Definition Name. \nThis attribute specifies which translation database to use for incoming messages from the NA."
    ::= { ptGTTGTTNAEntry 6 }

ptGTTGTTNAAppIDRegistrations OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This attribute provides the number of AppID Registrations managed by this Network Appearance. \nThis attribute shows the number of successful registrations across all CPUs."
    ::= { ptGTTGTTNAEntry 7 }

ptGTTGTTNANATraffic OBJECT-TYPE
    SYNTAX          INTEGER { inactive(0), active(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "NA traffic (INACTIVE/ACTIVE)."
    ::= { ptGTTGTTNAEntry 8 }

ptGTTGTTNAUserDeactivated OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Activation flag for NA vs system-wide."
    ::= { ptGTTGTTNAEntry 9 }

ptGTTGTTNALPCRegistration OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Determines whether or not the LPC will be automatically registered with SCCP."
    ::= { ptGTTGTTNAEntry 10 }

