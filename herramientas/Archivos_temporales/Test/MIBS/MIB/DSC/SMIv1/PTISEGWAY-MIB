    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The row number for the SP2KEvent table"
    ::= { ptSP2KEventEntry 255 }

ptActiveTrapList OBJECT IDENTIFIER
    ::= { ptConfiguration 323 }

ptActiveTrapListTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtActiveTrapListEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptActiveTrapList 1 }

ptActiveTrapListEntry OBJECT-TYPE
    SYNTAX          PtActiveTrapListEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of ActiveTrapList attributes."
    INDEX           { ptConnGroupIndex, 
                      ptActiveTrapListConfigurationIndex, 
                      ptActiveTrapListIndex }
    ::= { ptActiveTrapListTable 1 }

PtActiveTrapListEntry ::=
    SEQUENCE {
        ptActiveTrapListQuantity        INTEGER,
        ptActiveTrapListConfigurationIndex  INTEGER,
        ptActiveTrapListParentOID       OBJECT IDENTIFIER,
        ptActiveTrapListRowStatus       RowStatus,
        ptActiveTrapListIndex           INTEGER
    }

ptActiveTrapListQuantity OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This attribute displays the current number of outstanding events in the system."
    ::= { ptActiveTrapListEntry 1 }

ptActiveTrapListConfigurationIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is Configuration."
    ::= { ptActiveTrapListEntry 242 }

