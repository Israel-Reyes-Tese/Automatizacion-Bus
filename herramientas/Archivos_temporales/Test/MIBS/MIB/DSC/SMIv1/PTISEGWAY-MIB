PthwmonShelfEntry ::=
    SEQUENCE {
        pthwmonShelfShelfID             INTEGER,
        pthwmonShelfBoardManufactureDate DisplayString,
        pthwmonShelfBoardPartNumber     DisplayString,
        pthwmonShelfProductManufacturer DisplayString,
        pthwmonShelfProductName         DisplayString,
        pthwmonShelfProductModelNumber  DisplayString,
        pthwmonShelfProductVersion      DisplayString,
        pthwmonShelfProductSerialNumber DisplayString,
        pthwmonShelfHardwareMonitorIndex INTEGER,
        pthwmonShelfSlotIndex           INTEGER,
        pthwmonShelfParentOID           OBJECT IDENTIFIER,
        pthwmonShelfRowStatus           RowStatus,
        pthwmonShelfIndex               INTEGER
    }

pthwmonShelfShelfID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Shelf ID."
    ::= { pthwmonShelfEntry 1 }

pthwmonShelfBoardManufactureDate OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Manufacturing Date of the product."
    ::= { pthwmonShelfEntry 2 }

pthwmonShelfBoardPartNumber OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Part number of the shelf backplane."
    ::= { pthwmonShelfEntry 3 }

pthwmonShelfProductManufacturer OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Manufacturer of the product."
    ::= { pthwmonShelfEntry 4 }

pthwmonShelfProductName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Name of the product."
    ::= { pthwmonShelfEntry 5 }

pthwmonShelfProductModelNumber OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Model number of the product."
    ::= { pthwmonShelfEntry 6 }

pthwmonShelfProductVersion OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Version of the product."
    ::= { pthwmonShelfEntry 7 }

pthwmonShelfProductSerialNumber OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Serial number of the product."
    ::= { pthwmonShelfEntry 8 }

pthwmonShelfHardwareMonitorIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is HardwareMonitor."
    ::= { pthwmonShelfEntry 242 }

pthwmonShelfSlotIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is Slot."
    ::= { pthwmonShelfEntry 243 }

