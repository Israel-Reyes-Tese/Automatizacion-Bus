sonusStChannelBindingEntry OBJECT-TYPE
    SYNTAX          SonusStChannelBindingEntry
    ACCESS          not-accessible
    STATUS          deprecated
    DESCRIPTION     
            " This table has been replaced by sonusStRefClkBindingEntry... "
    INDEX           { sonusStChannelBindingShelfIndex, 
                      sonusStChannelBindingSlotIndex, 
                      sonusStChannelBindingChannel, 
                      sonusStChannelBindingRefClk }
    ::= { sonusStChannelBindingTable 1 }

SonusStChannelBindingEntry ::=
    SEQUENCE {
        sonusStChannelBindingShelfIndex SonusShelfIndex,
        sonusStChannelBindingSlotIndex  SonusSlotIndex,
        sonusStChannelBindingChannel    INTEGER,
        sonusStChannelBindingRefClk     INTEGER,
        sonusStChannelBindingRowStatus  RowStatus
    }

sonusStChannelBindingShelfIndex OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          deprecated
    DESCRIPTION     
            "  "
    ::= { sonusStChannelBindingEntry 1 }

sonusStChannelBindingSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          deprecated
    DESCRIPTION     
            " Server slot number to provide a recovered clock source. The
             same server can be configured to provide both possible recovered 
             clocks as long as different channels(spans) are selected, and 
             the REFCLK each is bound to differs "
    ::= { sonusStChannelBindingEntry 2 }

sonusStChannelBindingChannel OBJECT-TYPE
    SYNTAX          INTEGER { pimUpper(85), pimLower(86) }
    ACCESS          read-only
    STATUS          deprecated
    DESCRIPTION     
            " T1/E1 span or OC12 port number to provide the recovered clock source.
             T1 span range is 1-12(CNS10), 1-28(CNS30), E1 span range is 1-8(CNS20), 
             OC12 port number(PNS20) is 1 only."
    ::= { sonusStChannelBindingEntry 3 }

sonusStChannelBindingRefClk OBJECT-TYPE
    SYNTAX          INTEGER { refClkA(1), refClkB(2) }
    ACCESS          read-only
    STATUS          deprecated
    DESCRIPTION     
            "  Backplane signal the recovered clock source should be 
             bound to. Each REFCLK must only be bound to a single 
             Server slot/span. "
    ::= { sonusStChannelBindingEntry 4 }

sonusStChannelBindingRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          deprecated
    DESCRIPTION     
            "This object indicates the row status for this table."
    ::= { sonusStChannelBindingEntry 5 }

sonusStRefClkBindingTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusStRefClkBindingEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            " This table describes how derived timing 
             sources should be mapped to the MTA hardware"
    ::= { sonusSystemTimingMIBObjects 8 }

sonusStRefClkBindingEntry OBJECT-TYPE
    SYNTAX          SonusStRefClkBindingEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "  "
    INDEX           { sonusStRefClkBindingIndex }
    ::= { sonusStRefClkBindingTable 1 }

SonusStRefClkBindingEntry ::=
    SEQUENCE {
        sonusStRefClkBindingIndex       INTEGER,
        sonusStRefClkBindingShelfIndex  SonusShelfIndex,
        sonusStRefClkBindingSlotIndex   SonusSlotIndex,
        sonusStRefClkBindingPortIndex   INTEGER,
        sonusStRefClkBindingOcxIndex    INTEGER,
        sonusStRefClkBindingDs3Index    INTEGER,
        sonusStRefClkBindingDs1Index    INTEGER,
        sonusStRefClkBindingRefClk      INTEGER,
        sonusStRefClkBindingRowStatus   RowStatus
    }

sonusStRefClkBindingIndex OBJECT-TYPE
    SYNTAX          INTEGER (1..128)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The index into the System Timing Reference Clock Bindings"
    ::= { sonusStRefClkBindingEntry 1 }

sonusStRefClkBindingShelfIndex OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            " The shelf in which the reference clock is located. "
    ::= { sonusStRefClkBindingEntry 2 }

sonusStRefClkBindingSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            " The slot in which the reference clock is located. "
    ::= { sonusStRefClkBindingEntry 3 }

sonusStRefClkBindingPortIndex OBJECT-TYPE
    SYNTAX          INTEGER (0..36)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The physical port of the reference clock."
    ::= { sonusStRefClkBindingEntry 4 }

sonusStRefClkBindingOcxIndex OBJECT-TYPE
    SYNTAX          INTEGER (0..3)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The oc index, if applicable, of the reference clock. "
    ::= { sonusStRefClkBindingEntry 5 }

sonusStRefClkBindingDs3Index OBJECT-TYPE
    SYNTAX          INTEGER (0..10)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The DS3 index, if applicable, of the reference clock."
    ::= { sonusStRefClkBindingEntry 6 }

sonusStRefClkBindingDs1Index OBJECT-TYPE
    SYNTAX          INTEGER (0..99)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The DS1 index, if applicable, of the reference clock."
    ::= { sonusStRefClkBindingEntry 7 }

sonusStRefClkBindingRefClk OBJECT-TYPE
    SYNTAX          INTEGER { refClkA(1), refClkB(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The reference clock binding A/B of the reference clock."
    ::= { sonusStRefClkBindingEntry 8 }

sonusStRefClkBindingRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            ""
    ::= { sonusStRefClkBindingEntry 9 }

