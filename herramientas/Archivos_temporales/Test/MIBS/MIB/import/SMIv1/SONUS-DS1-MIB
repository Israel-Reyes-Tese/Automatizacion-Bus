SonusDsx1FarEndCurrentDayEntry ::=
    SEQUENCE {
        sonusDsx1FarEndCurrentDayShelfIndex   SonusShelfIndex,
        sonusDsx1FarEndCurrentDaySlotIndex  SonusSlotIndex,
        sonusDsx1FarEndCurrentDayPortIndex  INTEGER,
        sonusDsx1FarEndCurrentDayDs1Index INTEGER,
        sonusDsx1FarEndCurrentDayElapsedTime    INTEGER,
        sonusDsx1FarEndCurrentDayLESs   PerfCurrentCount,
        sonusDsx1FarEndCurrentDayPCVs   PerfCurrentCount,
        sonusDsx1FarEndCurrentDayPESs   PerfCurrentCount,
        sonusDsx1FarEndCurrentDayPSESs  PerfCurrentCount,
        sonusDsx1FarEndCurrentDaySEFSs  PerfCurrentCount,
        sonusDsx1FarEndCurrentDayCSSs   PerfCurrentCount,
        sonusDsx1FarEndCurrentDayUASs   PerfCurrentCount,
        sonusDsx1FarEndCurrentDayFC     INTEGER,
        sonusDsx1FarEndCurrentDayTcaLES TruthValue,
        sonusDsx1FarEndCurrentDayTcaPCV TruthValue,
        sonusDsx1FarEndCurrentDayTcaPES TruthValue,
        sonusDsx1FarEndCurrentDayTcaPSES TruthValue,
        sonusDsx1FarEndCurrentDayTcaSEFS TruthValue,
        sonusDsx1FarEndCurrentDayTcaCSS TruthValue,
        sonusDsx1FarEndCurrentDayTcaUAS TruthValue,
        sonusDsx1FarEndCurrentDayMinorTcaLES    TruthValue,
        sonusDsx1FarEndCurrentDayMinorTcaPCV    TruthValue,
        sonusDsx1FarEndCurrentDayMinorTcaPES    TruthValue,
        sonusDsx1FarEndCurrentDayMinorTcaPSES     TruthValue,
        sonusDsx1FarEndCurrentDayMinorTcaSEFS     TruthValue,
        sonusDsx1FarEndCurrentDayMinorTcaCSS    TruthValue,
        sonusDsx1FarEndCurrentDayMinorTcaUAS    TruthValue,
        sonusDsx1FarEndCurrentDayMajorTcaLES    TruthValue,
        sonusDsx1FarEndCurrentDayMajorTcaPCV    TruthValue,
        sonusDsx1FarEndCurrentDayMajorTcaPES    TruthValue,
        sonusDsx1FarEndCurrentDayMajorTcaPSES     TruthValue,
        sonusDsx1FarEndCurrentDayMajorTcaSEFS     TruthValue,
        sonusDsx1FarEndCurrentDayMajorTcaCSS    TruthValue,
        sonusDsx1FarEndCurrentDayMajorTcaUAS    TruthValue,
        sonusDsx1FarEndCurrentDayCriticalTcaLES       TruthValue,
        sonusDsx1FarEndCurrentDayCriticalTcaPCV       TruthValue,
        sonusDsx1FarEndCurrentDayCriticalTcaPES       TruthValue,
        sonusDsx1FarEndCurrentDayCriticalTcaPSES        TruthValue,
        sonusDsx1FarEndCurrentDayCriticalTcaSEFS        TruthValue,
        sonusDsx1FarEndCurrentDayCriticalTcaCSS       TruthValue,
        sonusDsx1FarEndCurrentDayCriticalTcaUAS       TruthValue
    }

