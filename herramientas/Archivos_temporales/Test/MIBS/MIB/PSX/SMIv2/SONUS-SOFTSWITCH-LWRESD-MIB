    OBJECTS         { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "DNS Serevr is Recovered"
    ::= { sonusSoftSwitchLwresdMIBNotificationsPrefix 2 }

sonusSoftswitchLWRESDEDNSResponseFailure NOTIFICATION-TYPE
    OBJECTS         { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Failure Response for EDNS query with NOT-IMPLE, FORMERR or SERVFAIL"
    ::= { sonusSoftSwitchLwresdMIBNotificationsPrefix 3 }

sonusSoftswitchLWRESDEDNSResponseSuccess NOTIFICATION-TYPE
    OBJECTS         { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Successful Response for EDNS query which failed earlier for that domain"
    ::= { sonusSoftSwitchLwresdMIBNotificationsPrefix 4 }

sonusSoftswitchLWRESDTTLExpiredRRFallback NOTIFICATION-TYPE
    OBJECTS         { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Fallback to TTL expired RR due to DNS response failure"
    ::= { sonusSoftSwitchLwresdMIBNotificationsPrefix 5 }

