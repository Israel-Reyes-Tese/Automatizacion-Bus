    ENTERPRISE      envTrapGroup
    VARIABLES       { sysHwPowerSupply }
--     STATUS          mandatory
    DESCRIPTION     
            "A power supply on the sending device has recovered 
             after failure. The sysHwPowerSupply object identifies 
             the recovered supply."
    ::= 2

