    ::= { ptDSCAdjacentDiameterNodeEntry 11 }

ptDSCAdjacentDiameterNodeRemoteIPValidation OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Validate remote IP addresses of incoming connections (DISABLED/ENABLED). \nIf enabled, the resolved values of the Remote Host of the ADN Connection must \nmatch the source address(es) of received TCP or SCTP packets. If that fails \nthen an answer message is returned with error DIAMETER_UNKNOWN_PEER. \nDefault value: ENABLED."
    ::= { ptDSCAdjacentDiameterNodeEntry 12 }

