PtSGM3UAASPAssociationEntry ::=
    SEQUENCE {
        ptSGM3UAASPAssociationAssociationID   INTEGER,
        ptSGM3UAASPAssociationSlot      INTEGER,
        ptSGM3UAASPAssociationRemoteIPAddress1      DisplayString,
        ptSGM3UAASPAssociationRemoteIPAddress2      DisplayString,
        ptSGM3UAASPAssociationRemotePort INTEGER,
        ptSGM3UAASPAssociationLocalIPAddress1     DisplayString,
        ptSGM3UAASPAssociationLocalIPAddress2     DisplayString,
        ptSGM3UAASPAssociationLocalPort INTEGER,
        ptSGM3UAASPAssociationUserActivated   INTEGER,
        ptSGM3UAASPAssociationSCTPState INTEGER,
        ptSGM3UAASPAssociationM3UAState INTEGER,
        ptSGM3UAASPAssociationRxMSGCount INTEGER,
        ptSGM3UAASPAssociationTxMSGCount INTEGER,
        ptSGM3UAASPAssociationM3UAIndex INTEGER,
        ptSGM3UAASPAssociationM3UAConfiguredASPsIndex             INTEGER,
        ptSGM3UAASPAssociationM3UAMultiAssociationASPIndex                  INTEGER,
        ptSGM3UAASPAssociationActions   INTEGER,
        ptSGM3UAASPAssociationParentOID OBJECT IDENTIFIER,
        ptSGM3UAASPAssociationRowStatus RowStatus,
        ptSGM3UAASPAssociationIndex     INTEGER
    }

