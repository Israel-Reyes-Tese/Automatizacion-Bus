sonusSoftSwitchLwresdZoneHpcCounters OBJECT IDENTIFIER
    ::= { sonusSoftSwitchLwresdStatistics 4 }

sonusSoftSwitchLwresdHpcCountersZoneTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSoftSwitchLwresdHpcCountersZoneEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Zone table to hold ENUM/DNS Zone Hpc counters."
    ::= { sonusSoftSwitchLwresdZoneHpcCounters 1 }

sonusSoftSwitchLwresdHpcCountersZoneEntry OBJECT-TYPE
    SYNTAX          SonusSoftSwitchLwresdHpcCountersZoneEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "An entry in the ENUM/DNS Zone table."
    INDEX           { sonusSoftSwitchLwresdHpcCountersZoneNameLength, 
                      sonusSoftSwitchLwresdHpcCountersZoneName, 
                      sonusSoftSwitchLwresdHpcCountersZoneTransportType }
    ::= { sonusSoftSwitchLwresdHpcCountersZoneTable 1 }

SonusSoftSwitchLwresdHpcCountersZoneEntry ::=
    SEQUENCE {
        sonusSoftSwitchLwresdHpcCountersZoneNameLength              SonusSoftSwitchDomainNameLength,
        sonusSoftSwitchLwresdHpcCountersZoneName        SonusSoftSwitchDomainName,
        sonusSoftSwitchLwresdHpcCountersZoneTransportType                 INTEGER,
        sonusSoftSwitchLwresdHpcCountersZoneHpcReq          Counter,
        sonusSoftSwitchLwresdHpcCountersZoneHpcRsp          Counter,
        sonusSoftSwitchLwresdHpcCountersZoneHpcNoAnsRsp               Counter,
        sonusSoftSwitchLwresdHpcCountersZoneHpcTimeout              Counter,
        sonusSoftSwitchLwresdHpcCountersZoneHpcDestUnreachable                      Counter,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcReq             Counter,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcRsp             Counter,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcNoAnsRsp                  Counter,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcTimeout                 Counter,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcDestUnreachable                         Counter
    }

sonusSoftSwitchLwresdHpcCountersZoneNameLength OBJECT-TYPE
    SYNTAX          SonusSoftSwitchDomainNameLength
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Length of the Zone Name."
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 1 }

sonusSoftSwitchLwresdHpcCountersZoneName OBJECT-TYPE
    SYNTAX          SonusSoftSwitchDomainName
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Zone Name for which counters are applicable."
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 2 }

sonusSoftSwitchLwresdHpcCountersZoneTransportType OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The transport type used towards the Zone.1-tcp, 2-udp, 3-tcp fallback"
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 3 }

sonusSoftSwitchLwresdHpcCountersZoneHpcReq OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of HPC DNS requests sent for the Zone."
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 4 }

sonusSoftSwitchLwresdHpcCountersZoneHpcRsp OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of HPC DNS responses received for the Zone."
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 5 }

sonusSoftSwitchLwresdHpcCountersZoneHpcNoAnsRsp OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of HPC DNS no answer responses received for the Zone."
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 6 }

sonusSoftSwitchLwresdHpcCountersZoneHpcTimeout OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of HPC DNS requests timed out for the Zone."
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 7 }

