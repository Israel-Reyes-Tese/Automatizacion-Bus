        ptIOHWSCTPServerRTOMinimum      INTEGER,
        ptIOHWSCTPServerRTOMaximum      INTEGER,
        ptIOHWSCTPServerMaximumPathRetrans  INTEGER,
        ptIOHWSCTPServerMaximumAssocRetrans   INTEGER,
        ptIOHWSCTPServerBundling        INTEGER,
        ptIOHWSCTPServerBundlingTO      INTEGER,
        ptIOHWSCTPServerDelayedAckTO    INTEGER,
        ptIOHWSCTPServerFastRetransThreshold    INTEGER,
        ptIOHWSCTPServerChecksum        INTEGER,
        ptIOHWSCTPServerProtocol        INTEGER,
        ptIOHWSCTPServerIOManagerIndex  INTEGER,
        ptIOHWSCTPServerParentOID       OBJECT IDENTIFIER,
        ptIOHWSCTPServerRowStatus       RowStatus,
        ptIOHWSCTPServerIndex           INTEGER
    }

