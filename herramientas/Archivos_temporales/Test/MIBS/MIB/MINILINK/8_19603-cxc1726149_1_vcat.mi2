    xfVcatVc4ConfigTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF XfVcatVc4ConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "This table contains the VCAT configuration entries for all VCAT
            interfaces."
        ::= { xfVcatConfig 4 }

    xfVcatVc4ConfigEntry OBJECT-TYPE
        SYNTAX     XfVcatVc4ConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "The entry contains VCAT configuration parameters for one VCAT
            interface."
        INDEX { ifIndex }
        ::= { xfVcatVc4ConfigTable 1 }

    XfVcatVc4ConfigEntry ::= SEQUENCE
    {
      xfVcatVc4TtpConfigVcgIfIndexEgress   InterfaceIndexOrZero,
      xfVcatVc4TtpConfigVcgIfIndexIngress  InterfaceIndexOrZero,
      xfVcatVc4TtpConfigWithinCapEgress    TruthValue,
      xfVcatVc4TtpConfigTxSeqNumber        Integer32,
      xfVcatVc4TtpConfigWithinCapIngress   TruthValue,
      xfVcatVc4TtpConfigRxSeqNumber        Integer32,
      xfVcatVc4TtpConfigBusyEgress         TruthValue,
      xfVcatVc4TtpConfigBusyIngress        TruthValue,
      xfVcatVc4TtpConfigLcasEnable         TruthValue
    }

    xfVcatVc4TtpConfigVcgIfIndexEgress OBJECT-TYPE
        SYNTAX     InterfaceIndexOrZero
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
            "Assigns the source of the given TTP to the VCG using the
            ifIndex of the VCG interface.
            Note that assigning the TTP must update objects containing the
            operative egress capacity of the VCG.
             A value of 0 defines the TTP as unassigned."
        DEFVAL { 0 }
        ::= { xfVcatVc4ConfigEntry 1 }

    xfVcatVc4TtpConfigVcgIfIndexIngress OBJECT-TYPE
