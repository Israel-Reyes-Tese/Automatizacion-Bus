    SYNTAX          Integer32 (0..16777215)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The maximum file size (expressed in records) for any file being sent. The
             default value is 16,777,215."
    ::= { amaLimit 4 }

amaLimitFileBlockOut OBJECT-TYPE
    SYNTAX          Integer32 (0..32760)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The maximum block size (expressed in bytes) for any block being sent to a
             RAO from the DPMS. The default value is 32,760. This value must be less than
             the value of amaLimitFileBytesOut."
    ::= { amaLimit 5 }

amaStorage OBJECT IDENTIFIER
    ::= { ama 4 }

amaStorageTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF AmaStorageEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The (conceptual) table of long-term storage devices contained by the host for
             AMA data storage. A (conceptual) entry exists only if a correspondent value of
             hrDeviceType (from the Host Resources MIB module) object is
             `hrDeviceDiskStorage'."
    ::= { amaStorage 1 }

amaStorageEntry OBJECT-TYPE
    SYNTAX          AmaStorageEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A (conceptual) entry for one long-term storage device contained by the host.
             The hrDeviceIndex (from the Host Resources MIB module) in the index
             represents the entry in the hrDeviceTable (from the Host Resources MIB
             module) that corresponds to the amaStorageEntry."
    INDEX           { hrDeviceIndex }
    ::= { amaStorageTable 1 }

AmaStorageEntry ::=
    SEQUENCE {
        amaStorageTotalOccuLevel        Gauge32,
        amaStoragePriOccuLevel          Gauge32,
        amaStoragePriOccuThres          Integer32,
        amaStorageAmaFiles              Gauge32,
        amaStorageAmaRecords            Gauge32,
        amaStorageErrorFiles            Gauge32,
        amaStorageErrorRecords          Gauge32,
        amaStorageDbRecords             Gauge32,
        amaStorageDbOccuLevel           Gauge32,
        amaStorageEntryStatus           INTEGER
    }

amaStorageTotalOccuLevel OBJECT-TYPE
    SYNTAX          Gauge32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Percentage of hrDeviceIndex (from the Host Resources MIB module) mass
             storage occupied (e.g., 45 = 45%)."
    ::= { amaStorageEntry 1 }

amaStoragePriOccuLevel OBJECT-TYPE
    SYNTAX          Gauge32 (0..100)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Percentage of hrDeviceIndex (from the Host Resources MIB module) mass
             storage occupied by primary AMA data (e.g., 45 = 45%)."
    ::= { amaStorageEntry 2 }

amaStoragePriOccuThres OBJECT-TYPE
    SYNTAX          Integer32 (0..100)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The primary AMA data occupancy level threshold (e.g., 90 = 90%). The default
             value is 90."
    ::= { amaStorageEntry 3 }

amaStorageAmaFiles OBJECT-TYPE
    SYNTAX          Gauge32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Number of primary AMA files currently on the hrDeviceIndex (from the Host
             Resources MIB module) storage device."
    ::= { amaStorageEntry 4 }

amaStorageAmaRecords OBJECT-TYPE
    SYNTAX          Gauge32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Number of primary AMA records, contained in AMA files, currently on the
             hrDeviceIndex (from the Host Resources MIB module) storage device."
    ::= { amaStorageEntry 5 }

amaStorageErrorFiles OBJECT-TYPE
    SYNTAX          Gauge32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Number of primary error files currently on the hrDeviceIndex (from the Host
             Resources MIB module) storage device."
    ::= { amaStorageEntry 6 }

amaStorageErrorRecords OBJECT-TYPE
    SYNTAX          Gauge32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Number of primary error records, contained in Error files, currently on the
