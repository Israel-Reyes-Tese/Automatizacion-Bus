        ptIOHWMTP2LinkCongestionLevel2Onset   Integer32,
        ptIOHWMTP2LinkCongestionLevel3Onset   Integer32,
        ptIOHWMTP2LinkCongestionLevel1Abate   Integer32,
        ptIOHWMTP2LinkCongestionLevel2Abate   Integer32,
        ptIOHWMTP2LinkCongestionLevel3Abate   Integer32,
        ptIOHWMTP2LinkSUERMThreshold    Integer32,
        ptIOHWMTP2LinkNormalAERMThreshold Integer32,
        ptIOHWMTP2LinkEmergencyAERMThreshold    Integer32,
        ptIOHWMTP2LinkT1                Integer32,
        ptIOHWMTP2LinkT2                Integer32,
        ptIOHWMTP2LinkT3                Integer32,
        ptIOHWMTP2LinkT4n               Integer32,
        ptIOHWMTP2LinkT4e               Integer32,
        ptIOHWMTP2LinkT5                Integer32,
        ptIOHWMTP2LinkT6                Integer32,
        ptIOHWMTP2LinkT7                Integer32,
        ptIOHWMTP2LinkStatisticsReporting INTEGER,
        ptIOHWMTP2LinkReportingInterval Integer32,
        ptIOHWMTP2LinkTxErlang          DisplayString,
        ptIOHWMTP2LinkRxErlang          DisplayString,
        ptIOHWMTP2LinkMSUsTransmitted   Integer32,
        ptIOHWMTP2LinkMSUsReceived      Integer32,
        ptIOHWMTP2LinkMSUsRetransmitted Integer32,
        ptIOHWMTP2LinkFIBInversions     Integer32,
        ptIOHWMTP2LinkBIBInversions     Integer32,
