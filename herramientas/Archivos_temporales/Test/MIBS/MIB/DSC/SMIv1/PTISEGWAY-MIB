ptMTPATMLink OBJECT IDENTIFIER
    ::= { ptMTPLinkset 72 }

ptMTPATMLinkTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtMTPATMLinkEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of Link attributes."
    ::= { ptMTPATMLink 1 }

ptMTPATMLinkEntry OBJECT-TYPE
    SYNTAX          PtMTPATMLinkEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of MTPATMLink attributes."
    INDEX           { ptConnGroupIndex, ptMTPATMLinkMTP3NAManagerIndex, 
                      ptMTPATMLinkMTP3NAIndex, ptMTPATMLinkLinksetsIndex, 
                      ptMTPATMLinkLinksetIndex, ptMTPATMLinkIndex }
    ::= { ptMTPATMLinkTable 1 }

PtMTPATMLinkEntry ::=
    SEQUENCE {
        ptMTPATMLinkContext             DisplayString,
        ptMTPATMLinkAPC                 INTEGER,
        ptMTPATMLinkSLC                 INTEGER,
        ptMTPATMLinkDREID               INTEGER,
        ptMTPATMLinkCage                INTEGER,
        ptMTPATMLinkChannel             INTEGER,
        ptMTPATMLinkL2State             INTEGER,
        ptMTPATMLinkL3State             INTEGER,
        ptMTPATMLinkCongestionLevel     INTEGER,
        ptMTPATMLinkDiagnosticMessage   DisplayString,
        ptMTPATMLinkL2DebugLevel        INTEGER,
        ptMTPATMLinkUserActivated       INTEGER,
        ptMTPATMLinkLinkName            DisplayString,
        ptMTPATMLinkAlarms              INTEGER,
        ptMTPATMLinkRxMSUCount          INTEGER,
        ptMTPATMLinkTxMSUCount          INTEGER,
        ptMTPATMLinkMTP3NAManagerIndex  INTEGER,
        ptMTPATMLinkMTP3NAIndex         INTEGER,
        ptMTPATMLinkLinksetsIndex       INTEGER,
        ptMTPATMLinkLinksetIndex        INTEGER,
        ptMTPATMLinkActions             INTEGER,
        ptMTPATMLinkParentOID           OBJECT IDENTIFIER,
        ptMTPATMLinkRowStatus           RowStatus,
        ptMTPATMLinkIndex               INTEGER
    }

ptMTPATMLinkContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A condensed textual representation of the link object."
    ::= { ptMTPATMLinkEntry 1 }

