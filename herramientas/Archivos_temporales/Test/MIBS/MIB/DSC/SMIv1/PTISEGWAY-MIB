    SYNTAX          PtLBConnectionsEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of LBConnections attributes."
    INDEX           { ptConnGroupIndex, ptLBConnectionsLoadBalancerIndex, 
                      ptLBConnectionsIndex }
    ::= { ptLBConnectionsTable 1 }

PtLBConnectionsEntry ::=
    SEQUENCE {
        ptLBConnectionsLoadBalancerIndex INTEGER,
        ptLBConnectionsParentOID        OBJECT IDENTIFIER,
        ptLBConnectionsRowStatus        RowStatus,
        ptLBConnectionsIndex            INTEGER
    }

