        ptSGM3UASCTPServersMaximumAssocRetrans      Integer32,
        ptSGM3UASCTPServersMaximumPathRetrans     Integer32,
        ptSGM3UASCTPServersMaximumInitRetrans     Integer32,
        ptSGM3UASCTPServersBundling     INTEGER,
        ptSGM3UASCTPServersBundlingTimeout  Integer32,
        ptSGM3UASCTPServersDelayedAckTimeout    Integer32,
        ptSGM3UASCTPServersRTOInitial   Integer32,
        ptSGM3UASCTPServersRTOMinimum   Integer32,
        ptSGM3UASCTPServersRTOMaximum   Integer32,
        ptSGM3UASCTPServersChecksum     INTEGER,
        ptSGM3UASCTPServersInStreams    Integer32,
        ptSGM3UASCTPServersOutStreams   Integer32,
        ptSGM3UASCTPServersDefaultNetworkAppearance           Integer32,
        ptSGM3UASCTPServersM3UAIndex    Integer32,
        ptSGM3UASCTPServersActions      INTEGER,
        ptSGM3UASCTPServersParentOID    OBJECT IDENTIFIER,
        ptSGM3UASCTPServersRowStatus    RowStatus,
        ptSGM3UASCTPServersIndex        Integer32
    }

