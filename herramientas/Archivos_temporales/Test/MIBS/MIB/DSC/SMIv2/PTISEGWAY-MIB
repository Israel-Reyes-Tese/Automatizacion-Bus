ptL4CvtrMessageRuleMessageID OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Message Rule message identifier as per SS7 specifications."
    ::= { ptL4CvtrMessageRuleEntry 1 }

ptL4CvtrMessageRuleAction OBJECT-TYPE
    SYNTAX          INTEGER { discard(0), passThrough(1), process(2), 
                        cpam(3) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Message Rule Action (DISCARD/PASS THROUGH/PROCESS/CPAM). \nMessage rules necessary to the conversion must be set on initial setup via the SS7 Variant object Load Default Rules action once both SS7 Variants are set. The message rule action applies to all MSUs with the matching SI and ID received by the associated SS7 Variant. \nIf no message rule is found for an incoming MSU, the default action is PASS THROUGH. \nThe default rules apply may be enhanced as required by the network deployment or the national variant. \nSet the action to DISCARD to discard the matching MSUs. \nSet the action to PASS THROUGH to forward the matching MSUs with no additional conversions beyond the routing label and no parameter rules are applied. \nSet the action to PROCESS to trigger pre-defined conversion and apply parameter rules to matching MSUs. \nThe CPAM action is reserved for SS7-IP deployments to encapsulate the matching MSUs to the IP network."
    ::= { ptL4CvtrMessageRuleEntry 2 }

ptL4CvtrMessageRuleDescription OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Message Rule description."
    ::= { ptL4CvtrMessageRuleEntry 3 }

ptL4CvtrMessageRuleSI OBJECT-TYPE
    SYNTAX          INTEGER { isup(5), sccp(3) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Message Rule signaling indicator (ISUP/SCCP)."
    ::= { ptL4CvtrMessageRuleEntry 4 }

ptL4CvtrMessageRuleL4CvtrSS7VariantIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is L4CvtrSS7Variant."
    ::= { ptL4CvtrMessageRuleEntry 242 }

ptL4CvtrMessageRuleMessageRulesIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is MessageRules."
    ::= { ptL4CvtrMessageRuleEntry 243 }

