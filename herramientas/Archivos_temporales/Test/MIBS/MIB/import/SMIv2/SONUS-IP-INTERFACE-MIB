SonusNrsMgmtIfEntry ::=
    SEQUENCE {
        sonusNrsMgmtIfIndex             Integer32,
        sonusNrsMgmtIfShelf             SonusShelfIndex,
        sonusNrsMgmtIfSlot              SonusSlotIndex,
        sonusNrsMgmtIfPort              Integer32,
        sonusNrsMgmtIfCircuit           Integer32,
        sonusNrsMgmtIfType              IANAifType,
        sonusNrsMgmtIfIpAddress         IpAddress,
        sonusNrsMgmtIfIpMask            IpAddress,
        sonusNrsMgmtIfMtu               Integer32,
        sonusNrsMgmtIfGatewayAddress    IpAddress,
        sonusNrsMgmtIfMode              INTEGER,
        sonusNrsMgmtIfAdminStatus       INTEGER,
        sonusNrsMgmtIfOperStatus        INTEGER,
        sonusNrsMgmtIfLogicMgmtIpAddress IpAddress,
        sonusNrsMgmtIfName              SonusName,
        sonusNrsMgmtIfDGPBucketSize     Integer32,
        sonusNrsMgmtIfDGPFillRate       Integer32,
        sonusNrsMgmtIfDGPAdminState     INTEGER,
        sonusNrsMgmtIfRowStatus         RowStatus,
        sonusNrsMgmtIfIpv6Address       Ipv6Address,
        sonusNrsMgmtIfGatewayIpv6Address Ipv6Address,
        sonusNrsMgmtIfIpv6Prefixlen     Integer32,
        sonusNrsMgmtIfLogicMgmtIpv6Address  Ipv6Address,
        sonusNrsMgmtIfLlIpv6Address     Ipv6Address
    }

sonusNrsMgmtIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this management NIF when it was created.
             This will not change until the NIF is deleted."
    ::= { sonusNrsMgmtIfEntry 1 }

sonusNrsMgmtIfShelf OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the shelf of the Management
             Interface"
    ::= { sonusNrsMgmtIfEntry 2 }

sonusNrsMgmtIfSlot OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the slot of the Management
             Interface"
    ::= { sonusNrsMgmtIfEntry 3 }

sonusNrsMgmtIfPort OBJECT-TYPE
    SYNTAX          Integer32 (1..4)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the port of the Management
             Interface.  For MNS10 or MNS11 hardware this must be 1 or 2.
             For	MNS20 or MNS25 hardware this can be up to 4."
    ::= { sonusNrsMgmtIfEntry 4 }

sonusNrsMgmtIfCircuit OBJECT-TYPE
    SYNTAX          Integer32 (1)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the circuit of the Management
             Interface.  For current hardware this must be 1."
    DEFVAL          { 1 }
    ::= { sonusNrsMgmtIfEntry 5 }

sonusNrsMgmtIfType OBJECT-TYPE
    SYNTAX          IANAifType
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type of interface, distinguished according to the
             physical/link protocol(s) immediately 'below' the network
             layer in the protocol stack.
             
             Values are from RFC1213 for the ifType object.
             
             Current hardware supports only the ethernet-csmacd (6)
             type."
    ::= { sonusNrsMgmtIfEntry 6 }

sonusNrsMgmtIfIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The IP Address of the Management Interface"
    ::= { sonusNrsMgmtIfEntry 7 }

sonusNrsMgmtIfIpMask OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The IP subnet mask of this Management Interface."
    ::= { sonusNrsMgmtIfEntry 8 }

sonusNrsMgmtIfMtu OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The size of the largest datagram that can be sent/received
             on the interface, specified in octets."
    ::= { sonusNrsMgmtIfEntry 9 }

sonusNrsMgmtIfGatewayAddress OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The IP address of the default gateway for this Management
             Interface."
    ::= { sonusNrsMgmtIfEntry 10 }

sonusNrsMgmtIfMode OBJECT-TYPE
    SYNTAX          INTEGER { standby(1), active(2) }
    MAX-ACCESS      read-write
    STATUS          obsolete
    DESCRIPTION     
            "The requested mode of the interface."
    ::= { sonusNrsMgmtIfEntry 11 }

sonusNrsMgmtIfAdminStatus OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The requested status of the interface."
    ::= { sonusNrsMgmtIfEntry 12 }

sonusNrsMgmtIfOperStatus OBJECT-TYPE
    SYNTAX          INTEGER { outOfService(1), inService(2), 
                        outOfServiceDisabled(3), outOfServiceLinkDown(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The actual status of the management interface.  A link will
             be InService if the sonusNrsMgmtIfAdminStatus is enabled and
             the physical link is up.  It will be OutOfService if the admin
             status is enabled but the physical link is down.  It will be
             OutOfServiceDisabled if the physical link is up but the admin
             status is disabled.  It will be OutOfServiceLinkDown if the
             physical link is down and the admin status is disabled."
    ::= { sonusNrsMgmtIfEntry 13 }

sonusNrsMgmtIfLogicMgmtIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The logical Mgmt IP address assigned to the mgmt interface if
             this mgmt interface is active and in service."
    ::= { sonusNrsMgmtIfEntry 14 }

sonusNrsMgmtIfName OBJECT-TYPE
    SYNTAX          SonusName
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The assigned name for this MGMT Interface."
    ::= { sonusNrsMgmtIfEntry 15 }

sonusNrsMgmtIfDGPBucketSize OBJECT-TYPE
    SYNTAX          Integer32 (4096..65520)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The per MGMT IF dark gray policer bucket size, in bytes,
             used to policing all traffic that does not match a
             white list policer. The range is 4096 bytes to 65520
             bytes, in 16 byte increments. 0 to 128 pseudo-packets
             in 1/32 packet increments."
    ::= { sonusNrsMgmtIfEntry 16 }

sonusNrsMgmtIfDGPFillRate OBJECT-TYPE
    SYNTAX          Integer32 (2048..16776960)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The per MGMT IF dark gray policer fill rate, in bytes
             per second, used to policing all traffic that does 
             not match a white list policer. The range is 2048 
             bytes/sec to 16M bytes/second in increments of 
             256 bytes/sec, 0.5 to 32K pseudo-pps in increments 
             of 0.5 pps."
    ::= { sonusNrsMgmtIfEntry 17 }

sonusNrsMgmtIfDGPAdminState OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Administrative state of dark gray policer."
    ::= { sonusNrsMgmtIfEntry 18 }

sonusNrsMgmtIfRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            ""
    ::= { sonusNrsMgmtIfEntry 19 }

