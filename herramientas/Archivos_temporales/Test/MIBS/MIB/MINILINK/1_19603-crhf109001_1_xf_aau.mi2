-- 
		-- 1.3.6.1.4.1.193.81.4.2.1.1.2
		xfImaLayerHandling OBJECT IDENTIFIER ::= { xfAtmAggregationUnitMibObjects 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.1
		xfImaGroupIndexNext OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object contains the next available value
				to be used for imaGroupIndex when creating a new
				entry (IMA Group) in the imaGroupTable.
				
				The value 0 indicates that no unassigned entries
				are available, i.e. the maximum number of either
				ATM G.804 interfaces or ATM IMA interfaces has
				been reached. At each IMA group creation, the
				application increases the value to the next
				unassigned index; when the maximum number of interfaces
				is reached, the MO value is set to '0'.
				An assigned value will be unavailable until the
				IMA group is present in the system. After an IMA
				group removal, the corresponding imaGroupIndex value
				will not be reused."
			::= { xfImaLayerHandling 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.2
		xfImaGroupXTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfImaGroupXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains the information about which
				IMA device is currently allocating one IMA group.
				This table is an extension of the imaGroupTable,
				and it exploits the same indexing (imaGroupIndex)."
			::= { xfImaLayerHandling 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.2.1
		xfImaGroupXEntry OBJECT-TYPE
			SYNTAX XfImaGroupXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"List of entries in the table.
				The number of entries is equal to the number
				of entries in the imaGroupTable, i.e. to the
				number of IMA groups currently set up."
			AUGMENTS { imaGroupEntry }
			::= { xfImaGroupXTable 1 }

		
		XfImaGroupXEntry ::=
			SEQUENCE { 
				xfImaGroupDeviceIndex
					INTEGER,
				xfImaGroupRestartCommand
					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.2.1.1
		xfImaGroupDeviceIndex OBJECT-TYPE
			SYNTAX INTEGER
