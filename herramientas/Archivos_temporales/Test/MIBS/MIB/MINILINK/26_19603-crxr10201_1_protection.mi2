--
-- Node definitions
--

		-- 1.3.6.1.4.1.193.81.2.11.1
		xfProtectionObjects OBJECT IDENTIFIER ::= { xfProtectionMib 1 }


		-- 1.3.6.1.4.1.193.81.2.11.1.1
		xfEquipmentProtectionTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfEquipmentProtectionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains entries for line and equipment protection.
				The table is indexed on the entPhysicalIndex of the line module
				(APU) in the lowest slot.
				"
			::= { xfProtectionObjects 1 }


		-- 1.3.6.1.4.1.193.81.2.11.1.1.1
		xfEquipmentProtectionEntry OBJECT-TYPE
			SYNTAX XfEquipmentProtectionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table. An entry is created by a 1+1 setup which is
				specific to the specific protection scheme like Radio 1+1 or MSP.
				What initiated a 1+1/1+0 setup is outside the scope of this MIB."
			INDEX { entPhysicalIndex }
			::= { xfEquipmentProtectionTable 1 }


		XfEquipmentProtectionEntry ::=
			SEQUENCE {
				xfEquipmentProtectionUnit2
					PhysicalIndex,
				xfEquipmentProtectionName
					DisplayString,
				xfEquipmentProtectionActiveUnit
					INTEGER,
				xfEquipmentProtectionMode
					INTEGER,
				xfEquipmentProtectionLine
					Integer32,
				xfEquipmentProtectionTrapEnable
					INTEGER,
				xfEquipmentProtectionManualSwitchCommand
					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.2.11.1.1.1.3
		xfEquipmentProtectionUnit2 OBJECT-TYPE
			SYNTAX PhysicalIndex
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object hold the PhysicalIndex of line module (APU) in the
				highest slot."
			::= { xfEquipmentProtectionEntry 3 }


		-- 1.3.6.1.4.1.193.81.2.11.1.1.1.4
		xfEquipmentProtectionName OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the name of the protection as used in
				notifications. The following naming convention applies:
				- in case of 2 board protection the name is: '1/x+y' where x
				  is the lowest slot position and y the highest
				- in case of 3 board protection the name is: '1/z+' where z is
				  slot position of the protection module (SMU)"
			::= { xfEquipmentProtectionEntry 4 }


		-- 1.3.6.1.4.1.193.81.2.11.1.1.1.5
		xfEquipmentProtectionActiveUnit OBJECT-TYPE
			SYNTAX INTEGER
				{
				unit1(1),
				unit2(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates the active unit in respect to the TDM
				switch to the backplane. It can be used to perform an
				equipment protection switch. At creation of the protection
				group is object indicates which unit is active at
				creation.
				At a node cold restart unit 1 will always be the active one."
			DEFVAL { unit1 }
			::= { xfEquipmentProtectionEntry 5 }


		-- 1.3.6.1.4.1.193.81.2.11.1.1.1.6
		xfEquipmentProtectionMode OBJECT-TYPE
			SYNTAX INTEGER
				{
				automatic(1),
				manual(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object applies for all switches involved in this protection,
				i.e. equipment protection switches, line protection switches and
				specific switches like the Tx Radio switch.
				The protection switches can be set into two modi:
				automatic: a switch will be performed upon a Signal
				           Failure(SF)/Signal Degrade(SD) or equipment failure if the other
				           line interface/unit is OK.
				manual:    no switch due to SF/SD or equipment failure will be performed. The switches
				           are locked.
				Default shall be automatic.
				"
			::= { xfEquipmentProtectionEntry 6 }


		-- 1.3.6.1.4.1.193.81.2.11.1.1.1.7
		xfEquipmentProtectionLine OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The object is the corresponding index in the xfLineProtectionTable."
			::= { xfEquipmentProtectionEntry 7 }


		-- 1.3.6.1.4.1.193.81.2.11.1.1.1.8
		xfEquipmentProtectionTrapEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates whether any protection related notifications are
				sent."
			DEFVAL { enable }
			::= { xfEquipmentProtectionEntry 8 }

		-- 1.3.6.1.4.1.193.81.2.11.1.1.1.9
		xfEquipmentProtectionManualSwitchCommand OBJECT-TYPE
			SYNTAX INTEGER
				{
				eqpManualSwitch(1),
				eqpManualNoSwitch(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used to request a EQuipment Protection switch.
				Applicable for EQP configurations and when EQP protection SwitchMode is set to Manual.

				Value:
				  - eqpManualSwitch
				  - eqpManualNoSwitch."
			DEFVAL { eqpManualNoSwitch }
			::= { xfEquipmentProtectionEntry 9 }

		-- 1.3.6.1.4.1.193.81.2.11.1.2
		xfLineProtectionTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfLineProtectionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in this table is created as a result of a successful
				1+1 set-up. The table is indexed by the ifIndex of the
				protected/reliable interface."
			::= { xfProtectionObjects 2 }


		-- 1.3.6.1.4.1.193.81.2.11.1.2.1
		xfLineProtectionEntry OBJECT-TYPE
			SYNTAX XfLineProtectionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Description."
			INDEX { ifIndex }
			::= { xfLineProtectionTable 1 }


		XfLineProtectionEntry ::=
			SEQUENCE {
				xfLineProtectionRx1
					InterfaceIndex,
				xfLineProtectionRx2
					InterfaceIndex,
				xfLineProtectionWorkingRx
					INTEGER,
				xfLineProtectionStatus
					BITS,
				xfLineProtectionRxSwitchCounter
					Integer32,
				xfLineProtectionRevertiveRx
					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.2.11.1.2.1.1
		xfLineProtectionRx1 OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the ifIndex of Rx Protection/Unreliable Line 1."
			::= { xfLineProtectionEntry 1 }


		-- 1.3.6.1.4.1.193.81.2.11.1.2.1.2
		xfLineProtectionRx2 OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the ifIndex of Rx Protection/Unreliable Line 2."
			::= { xfLineProtectionEntry 2 }


		-- 1.3.6.1.4.1.193.81.2.11.1.2.1.3
		xfLineProtectionWorkingRx OBJECT-TYPE
			SYNTAX INTEGER
				{
				rx1(1),
				rx2(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicated which of the two lines is the working Rx line.
				Upon creation of a protection group and cold restart it will be
				the line on the active unit.
				The object can also be used to perform a manual switch."
			::= { xfLineProtectionEntry 3 }


		-- 1.3.6.1.4.1.193.81.2.11.1.2.1.6
		xfLineProtectionStatus OBJECT-TYPE
			SYNTAX BITS
				{
				unableToProtect0(0),
				unableToProtect1(1),
				unableToProtect2(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the status of the line protection and equipemnt protection:
				Severity is indicated by three bits per alarm:
				 	bits 012
				 	     000   clear
				 	     001   undetermined
				 	     010   warning
				 	     011   minor
				 	     100   major
				 	     101   critical
				unableToProtect - minor if one of the lines/units fail
				                - critical if both lines/units fail
				In case of line failure a communication alarm will be sent. In case of
				an unit failure a equipment alarm will be sent."
			::= { xfLineProtectionEntry 6 }


		-- 1.3.6.1.4.1.193.81.2.11.1.2.1.7
		xfLineProtectionRxSwitchCounter OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the number of line switches since
				last node warm/cold restart or protection group creation.
				Manual switches are included."
			::= { xfLineProtectionEntry 7 }


		-- 1.3.6.1.4.1.193.81.2.11.1.2.1.8
		xfLineProtectionRevertiveRx OBJECT-TYPE
			SYNTAX INTEGER
				{
				rx1(1),
				rx2(2),
				off(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"In case revertive switching is supported this object indicates
				the preferred/primary Rx Line.
				Default shall be off(3)."
			::= { xfLineProtectionEntry 8 }


		-- 1.3.6.1.4.1.193.81.2.11.1.3
		xfPortProtectionTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfPortProtectionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains entries for port protection.
				The table is indexed on the entPhysicalIndex of the port
				having the lowest index."
			::= { xfProtectionObjects 3 }


		-- 1.3.6.1.4.1.193.81.2.11.1.3.1
		xfPortProtectionEntry OBJECT-TYPE
			SYNTAX XfPortProtectionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Description."
			INDEX { entPhysicalIndex }
			::= { xfPortProtectionTable 1 }


		XfPortProtectionEntry ::=
			SEQUENCE {
				xfPortProtectionActivePort
					INTEGER,
				xfPortProtectionMode
					INTEGER,
				xfPortProtectionName
					DisplayString,
				xfPortProtectionPort2
					PhysicalIndex,
				xfPortProtectionRowStatus
					RowStatus,
				xfPortProtectionStatus
					BITS,
				xfPortProtectionSwitchCounter
					Integer32,
				xfPortProtectionResetSwitchCounter
					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.2.11.1.3.1.1
		xfPortProtectionActivePort OBJECT-TYPE
			SYNTAX INTEGER
				{
				port1(1),
				port2(2)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"When read, this parameter indicates which port is active.
				When written, this parameter is used by the operator to initiate a switch."
			DEFVAL { port1 }
			::= { xfPortProtectionEntry 1 }


		-- 1.3.6.1.4.1.193.81.2.11.1.3.1.2
		xfPortProtectionMode OBJECT-TYPE
			SYNTAX INTEGER
				{
				automatic(1),
				manual(2)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This parameters indicates whether switch is automatic or manual
				automatic(1): as soon as a failure occurs, switching is enabled automatically.
				manual(2):    failures do not enable manual switch, which can only be initiated
				              by the operator."
			DEFVAL { automatic }
			::= { xfPortProtectionEntry 2 }


		-- 1.3.6.1.4.1.193.81.2.11.1.3.1.3
		xfPortProtectionName OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This parameter indicates the name of the protection group
				used in notifications.
				The name is Rack/Slot, where Slot is <port1>+<port2> as
				indicated by the index of the xfPortProtectionTable and
				xfPortProtectionPort2."
			::= { xfPortProtectionEntry 3 }


		-- 1.3.6.1.4.1.193.81.2.11.1.3.1.4
		xfPortProtectionPort2 OBJECT-TYPE
			SYNTAX PhysicalIndex
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This parameter indicates the index of the port having
				the highest index."
			::= { xfPortProtectionEntry 4 }


		-- 1.3.6.1.4.1.193.81.2.11.1.3.1.5
		xfPortProtectionRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This is used to create/destroy an entry in this table."
			::= { xfPortProtectionEntry 5 }


		-- 1.3.6.1.4.1.193.81.2.11.1.3.1.6
		xfPortProtectionStatus OBJECT-TYPE
			SYNTAX BITS
				{
				unableToProtect0(0),
				unableToProtect1(1),
				unableToProtect2(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the status of the port protection.
				The severity is indicated by three bits per alarm:
				bits  012
				      000   clear
				      001   undetermined
				      010   warning
				      011   minor
				      100   major
				      101   critical

				unableToProtect - critical if both ports fail"
			::= { xfPortProtectionEntry 6 }


		-- 1.3.6.1.4.1.193.81.2.11.1.3.1.7
		xfPortProtectionSwitchCounter OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates the number of port switches since last node restart,
				protection group creation or switch counter reset.
				Manual switches are included."
			::= { xfPortProtectionEntry 7 }


		-- 1.3.6.1.4.1.193.81.2.11.1.3.1.8
		xfPortProtectionResetSwitchCounter OBJECT-TYPE
			SYNTAX INTEGER { reset(1) }
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Used to force a reset of the port switch counter."
			::= { xfPortProtectionEntry 8 }

		-- 1.3.6.1.4.1.193.81.2.11.1.4
		xfPortProtectionCapabilityTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfPortProtectionCapabilityEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table indicating port protection capabilities per port."
			::= { xfProtectionObjects 4 }


		-- 1.3.6.1.4.1.193.81.2.11.1.4.1
		xfPortProtectionCapabilityEntry OBJECT-TYPE
			SYNTAX XfPortProtectionCapabilityEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table. The table is indexed by entPhysicalIndex."
			INDEX { entPhysicalIndex }
				::= { xfPortProtectionCapabilityTable 1 }

		XfPortProtectionCapabilityEntry ::=
			SEQUENCE {
				xfPortProtectionCapability
					TruthValue
			}

		-- 1.3.6.1.4.1.193.81.2.11.1.4.1.1
		xfPortProtectionCapability OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Value true(1) indicates that port protection can be enabled on the port.
				Value false(2) indicates that port protection can not be enabled on the port.
				"
			::= { xfPortProtectionCapabilityEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.11.1.5
		xfPortProtectionVariant OBJECT-TYPE
			SYNTAX Integer32 (1..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Specifies the current Port Protection capability of the node.

				Variant
				1: Ethernet Port Protection supported for LAN interfaces according to each port's
				   xfPortProtectionCapabilities bit for Ethernet Port Protection
				"
			::= { xfProtectionObjects 5 }

		-- 1.3.6.1.4.1.193.81.2.11.1.6
		xfProtectionScalarObjects OBJECT IDENTIFIER ::= { xfProtectionObjects 6 }

		-- 1.3.6.1.4.1.193.81.2.11.1.6.1
		xfNodeProtectionRole OBJECT-TYPE
			SYNTAX INTEGER
				{
				standalone(1),
				master(2),
				slave(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used by CNx10 R2 to change role of this node. Setting role to
				slave is not allowed and will result in a set failed response."
			::= { xfProtectionScalarObjects 1 }

		-- 1.3.6.1.4.1.193.81.2.11.1.6.2
		xfNodeProtectionStatus OBJECT-TYPE
			SYNTAX BITS
				{
				roleMismatch0(0),
				roleMismatch1(1),
				roleMismatch2(2),
				rpsCableDisconnected0(3),
				rpsCableDisconnected1(4),
				rpsCableDisconnected2(5)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the status of the node protection:
				Severity is indicated by three bits per alarm:
					bits 012
						 000   clear
						 001   undetermined
						 010   warning
						 011   minor
						 100   major
						 101   critical
				roleMismatch - critical if units have roles which are not compatible
				rpsCableDisconnected - critical if no RPS/HSL cable is detected"
			::= { xfProtectionScalarObjects 2 }

		-- 1.3.6.1.4.1.193.81.2.11.1.6.3
		xfNodeProtectionTrapEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates whether any protection related notifications are
				sent."
			DEFVAL { enable }
			::= { xfProtectionScalarObjects 3 }

		-- 1.3.6.1.4.1.193.81.2.11.1.7
		xfSlotPairsTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfSlotPairsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
			"The table contains the slot pairs."
				::= { xfProtectionObjects 7 }


		-- 1.3.6.1.4.1.193.81.2.11.1.7.1
		xfSlotPairsEntry OBJECT-TYPE
			SYNTAX XfSlotPairsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
			"The table contains the slot pairs for protection."
			INDEX { xfSlotIndex }
				::= { xfSlotPairsTable 1 }

		XfSlotPairsEntry ::=
			SEQUENCE {
			xfSlotIndex
				Integer32,
			xfSlotPairIndex
				Integer32,
			xfSlotFunction
				INTEGER
			}

		-- 1.3.6.1.4.1.193.81.2.11.1.7.1.1
		xfSlotIndex OBJECT-TYPE
			SYNTAX Integer32(1..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Index of the given slot"
				::= { xfSlotPairsEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.11.1.7.1.2
		xfSlotPairIndex OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Other slot index that is paired with the given slot."
				::= { xfSlotPairsEntry 2 }

		-- 1.3.6.1.4.1.193.81.2.11.1.7.1.3
		xfSlotFunction OBJECT-TYPE
			SYNTAX INTEGER
			{
				none(0),
				primary(1),
				secondary(2)
			}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Function of the given slot."
				::= { xfSlotPairsEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8
		xfNpuProtection OBJECT IDENTIFIER ::= { xfProtectionObjects 8 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.1
		xfNpuProtectionEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				disable(0),
				enable(1)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to enable or disable the NPU protection. Default is disabled."
			DEFVAL { disable }
			::= { xfNpuProtection 1 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.2
		xfNpuProtectionStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				disabled(0),
				unprotected(1),
				protected(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the status of the NPU protection:
				disabled: NPU protection is disabled
				unprotected: NPU protection is enabled, but the Master NPU cannot be protected
				protected: NPU protection is enabled and the Master NPU is protected"
			::= { xfNpuProtection 2 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.3
		xfNpuProtectionAutoRestoreDelay OBJECT-TYPE
			SYNTAX Integer32 (0..918000)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates the delay time of automatic protection mode
				restoration in seconds.
				0   : Automatic protection mode restoration is disabled.
				1.. : Number of seconds to elapse without any NPU protection blocking
				      issues before automatic protection mode is (re-)enabled."
			::= { xfNpuProtection 3 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.4
		xfNpuProtectionFlawlessDuration OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the number of seconds elapsed without any
				NPU blocking issues."
			::= { xfNpuProtection 4 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.5
		xfNpuProtectionBlockingReason OBJECT-TYPE
			SYNTAX BITS
				{
				internalError(0),
				missingOtherNpu(1),
				missingLicense(2),
				configApplyLag(3)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the current NPU protection blocking reasons.
				internalError   : Unexpected error(s).
				missingOtherNpu : No other NPU is present to protect.
				missingLicense  : Corresponding license is not available.
				configApplyLag  : Configuration change is being applied (temporary)."
			::= { xfNpuProtection 5 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.6
		xfNpuProtectionFailoverCounter OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates the number of NPU failovers.
				Manual switches are not included."
			::= { xfNpuProtection 6 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.7
		xfNpuProtectionResetFailoverCounter OBJECT-TYPE
			SYNTAX INTEGER { reset(1) }
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used to force a reset of the NPU failover counter to 0."
			::= { xfNpuProtection 7 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.8
		xfPortExtensionTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfPortExtensionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains the settings of the extendable NPU ports. "
			::= { xfNpuProtection 8 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.8.1
		xfPortExtensionEntry OBJECT-TYPE
			SYNTAX XfPortExtensionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"One entry represents an extendable port on the slave NPU."
			INDEX { xfPortExtensionInterface }
			::= { xfPortExtensionTable 1 }


		XfPortExtensionEntry ::=
			SEQUENCE {
				xfPortExtensionInterface
					INTEGER,
				xfPortExtensionEnabled
					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.2.11.1.8.8.1.1
		xfPortExtensionInterface OBJECT-TYPE
			SYNTAX INTEGER (0..1023)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object holds the NPU port number of the extendable interface."
			::= { xfPortExtensionEntry 1 }


		-- 1.3.6.1.4.1.193.81.2.11.1.8.8.1.2
		xfPortExtensionEnabled OBJECT-TYPE
			SYNTAX INTEGER { enabled(1), disabled(2) }
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to enable or disable extension of the given
                 port on the slave NPU. Default is disabled(2)."
			::= { xfPortExtensionEntry 2 }

		-- 1.3.6.1.4.1.193.81.2.11.2
		xfProtectionConformance OBJECT IDENTIFIER ::= { xfProtectionMib 2 }


		-- 1.3.6.1.4.1.193.81.2.11.2.1
		xfProtectionCompliances OBJECT IDENTIFIER ::= { xfProtectionConformance 1 }


		-- 1.3.6.1.4.1.193.81.2.11.2.1.1
		xfProtectionFullCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION
				"Full compliance."
			MODULE -- this module
				MANDATORY-GROUPS { xfProtectionGroup }
