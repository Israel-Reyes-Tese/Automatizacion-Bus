ptMTPRoutesetContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A condensed textual representation of the routeset object."
    ::= { ptMTPRoutesetEntry 1 }

ptMTPRoutesetDPC OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Destination SS7 point code of this routeset This value should be set a creation time of the routeset and cannot be modified thereafter. The point code of the routeset gives the destination that one is seeking. The routes within the routeset will indicate the adjacent point codes that can be used to the get to the destination."
    ::= { ptMTPRoutesetEntry 2 }

ptMTPRoutesetDestinationType OBJECT-TYPE
    SYNTAX          INTEGER { default(1), network(2), cluster(3), 
                        member(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Describes the destination type of the routeset (network, cluster, member, or default)."
    ::= { ptMTPRoutesetEntry 3 }

ptMTPRoutesetCLLI OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Common Language Location Identifier (CLLI) of this routeset."
    ::= { ptMTPRoutesetEntry 4 }

ptMTPRoutesetRoutesetState OBJECT-TYPE
    SYNTAX          INTEGER { active(0), inactive(1), xlist(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of this routeset (active, inactive, xlist) xlist means that the routeset was dynamically created by the MTP L3 and has exceptions, such as some routes may not be available."
    ::= { ptMTPRoutesetEntry 5 }

ptMTPRoutesetCongestionLevel OBJECT-TYPE
    SYNTAX          INTEGER { false(0), level1(1), level2(2), level3(3), 
                        true(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Congestion level of the routeset. \nIn ANSI, TTC and NTT networks the values range from 1 to 3 or FALSE if not congested. \nIn ITU networks the values are TRUE or FALSE."
    ::= { ptMTPRoutesetEntry 6 }

ptMTPRoutesetDestinationState OBJECT-TYPE
    SYNTAX          INTEGER { inaccessible(0), accessible(1), 
                        accessibleAlt(2), manuallyBlocked(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of the routeset destination. The possible states are, inaccessible, accessible, manually blocked, accessible via alternate route. When a routeset is deactivated the routeset destination state is set to manually blocked and the routeset state is set to inactive whether the destination state is accessible or inaccessible."
    ::= { ptMTPRoutesetEntry 7 }

