ptDSCMobileCodesDefinitions OBJECT IDENTIFIER
    ::= { ptDSCDSCDefinitions 1861 }

ptDSCMobileCodesDefinitionsTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtDSCMobileCodesDefinitionsEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptDSCMobileCodesDefinitions 1 }

ptDSCMobileCodesDefinitionsEntry OBJECT-TYPE
    SYNTAX          PtDSCMobileCodesDefinitionsEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of DSCMobileCodesDefinitions attributes."
    INDEX           { ptConnGroupIndex, 
                      ptDSCMobileCodesDefinitionsDSCIndex, 
                      ptDSCMobileCodesDefinitionsDSCDefinitionsIndex, 
                      ptDSCMobileCodesDefinitionsIndex }
    ::= { ptDSCMobileCodesDefinitionsTable 1 }

PtDSCMobileCodesDefinitionsEntry ::=
    SEQUENCE {
        ptDSCMobileCodesDefinitionsSearchType     INTEGER,
        ptDSCMobileCodesDefinitionsDSCIndex   INTEGER,
        ptDSCMobileCodesDefinitionsDSCDefinitionsIndex              INTEGER,
        ptDSCMobileCodesDefinitionsParentOID    OBJECT IDENTIFIER,
        ptDSCMobileCodesDefinitionsRowStatus    RowStatus,
        ptDSCMobileCodesDefinitionsIndex INTEGER
    }

ptDSCMobileCodesDefinitionsSearchType OBJECT-TYPE
    SYNTAX          INTEGER { exactMatch(1), longestFromRight(2), 
                        longestFromLeft(3) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Search Type (LONGEST FROM LEFT) \nMobile Codes represent the IMSI Mobile Code (MCC) and Mobile Network Code (MNC) values. \nThis table defines the MCC MNC values whose MNC values are 3 digits long. It is used to lookup the length of the MNC value ( 2 or 3 digits ) based on the MCC value. The Diameter Routing Modification command replace-dest-realm-from-imsi perfoms a lookup of the AVP IMSI in this table. \nWhen looking up MCC MNC values, the search type is used to figure out the MCC and MNC digits from the IMSI value. \nIf the search matches an entry in the table, the MNC is 3 digits. If no match is found, the MNC is 2 digits. \nPopulate the table with MCC whose MNC values are all 3 digits. If an MCC value has both 2 digits and 3 digits MNC values then include only the 3 digits MNC values in the table using the format MCCMNC (6 digits). \nDefault value: LONGEST FROM LEFT."
    ::= { ptDSCMobileCodesDefinitionsEntry 1 }

ptDSCMobileCodesDefinitionsDSCIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSC."
    ::= { ptDSCMobileCodesDefinitionsEntry 242 }

ptDSCMobileCodesDefinitionsDSCDefinitionsIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSCDefinitions."
    ::= { ptDSCMobileCodesDefinitionsEntry 243 }

ptDSCMobileCodesDefinitionsParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptDSCMobileCodesDefinitionsEntry 253 }

ptDSCMobileCodesDefinitionsRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The row status for the DSCMobileCodesDefinitions table"
    ::= { ptDSCMobileCodesDefinitionsEntry 254 }

