sonusSipSigPeerStatusTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipSigPeerStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Table of SIP Signalling Peer status."
    ::= { sonusSipSignallingMIBObjects 42 }

sonusSipSigPeerStatusEntry OBJECT-TYPE
    SYNTAX          SonusSipSigPeerStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "SIP Signalling Peer Status."
    INDEX           { sonusSipSigPeerStatusSlotIndex, 
                      sonusSipSigPeerStatusIpAddress, 
                      sonusSipSigPeerStatusZoneId, 
                      sonusSipSigPeerStatusPortNum, 
                      sonusSipSigPeerStatusIp6Address, 
                      sonusSipSigPeerStatusIpVersion }
    ::= { sonusSipSigPeerStatusTable 1 }

SonusSipSigPeerStatusEntry ::=
    SEQUENCE {
        sonusSipSigPeerStatusSlotIndex  SonusSlotIndex,
        sonusSipSigPeerStatusIpAddress  IpAddress,
        sonusSipSigPeerStatusZoneId     Gauge,
        sonusSipSigPeerStatusPortNum    Gauge,
        sonusSipSigPeerStatusType       INTEGER,
        sonusSipSigPeerStatusCreatedAt  DateAndTime,
        sonusSipSigPeerStatusIngressCallsAllowed        Gauge,
        sonusSipSigPeerStatusIngressCallsActive       Gauge,
        sonusSipSigPeerStatusIngressCallRejectsLimit            Gauge,
        sonusSipSigPeerStatusIngressCallRejectsRate           Gauge,
        sonusSipSigPeerStatusEgressCallsAllowed       Gauge,
        sonusSipSigPeerStatusEgressCallsActive      Gauge,
        sonusSipSigPeerStatusEgressCallRejectsLimit           Gauge,
        sonusSipSigPeerStatusEgressCallRejectsRate          Gauge,
        sonusSipSigPeerStatusState      INTEGER,
        sonusSipSigPeerStatusIngressSubscribeAllowed            Gauge,
        sonusSipSigPeerStatusIngressSubscribeRejectsRate                Gauge,
        sonusSipSigPeerStatusEgressSubscribeAllowed           Gauge,
        sonusSipSigPeerStatusEgressSubscribeRejectsRate               Gauge,
        sonusSipSigPeerStatusIngressReferAllowed        Gauge,
        sonusSipSigPeerStatusIngressReferRejectsRate            Gauge,
        sonusSipSigPeerStatusEgressReferAllowed       Gauge,
        sonusSipSigPeerStatusEgressReferRejectsRate           Gauge,
        sonusSipSigPeerStatusIngressNotifyAllowed         Gauge,
        sonusSipSigPeerStatusIngressNotifyRejectsRate             Gauge,
        sonusSipSigPeerStatusEgressNotifyAllowed        Gauge,
        sonusSipSigPeerStatusEgressNotifyRejectsRate            Gauge,
        sonusSipSigPeerStatusIngressOptionsAllowed          Gauge,
        sonusSipSigPeerStatusIngressOptionsRejectsRate              Gauge,
        sonusSipSigPeerStatusEgressOptionsAllowed         Gauge,
        sonusSipSigPeerStatusEgressOptionsRejectsRate             Gauge,
        sonusSipSigPeerStatusIngressMessageAllowed          Gauge,
        sonusSipSigPeerStatusIngressMessageRejectsRate              Gauge,
        sonusSipSigPeerStatusEgressMessageAllowed         Gauge,
        sonusSipSigPeerStatusEgressMessageRejectsRate             Gauge,
        sonusSipSigPeerStatusIngressOtherReqAllowed           Gauge,
        sonusSipSigPeerStatusIngressOtherReqRejectsRate               Gauge,
        sonusSipSigPeerStatusEgressOtherReqAllowed          Gauge,
        sonusSipSigPeerStatusEgressOtherReqRejectsRate              Gauge,
        sonusSipSigPeerStatusIngressResponseAllowed           Gauge,
        sonusSipSigPeerStatusIngressResponseRejectsRate               Gauge,
        sonusSipSigPeerStatusEgressResponseAllowed          Gauge,
        sonusSipSigPeerStatusEgressResponseRejectsRate              Gauge,
        sonusSipSigPeerStatusIngressAggrMsgAllowed          Gauge,
        sonusSipSigPeerStatusIngressAggrMsgRejectsRate              Gauge,
        sonusSipSigPeerStatusEgressAggrMsgAllowed         Gauge,
        sonusSipSigPeerStatusEgressAggrMsgRejectsRate             Gauge,
        sonusSipSigPeerStatusIngressAggrRejAllowed          Gauge,
        sonusSipSigPeerStatusIngressAggrRejRejectsRate              Gauge,
        sonusSipSigPeerStatusIp6Address Ipv6Address,
        sonusSipSigPeerStatusIpVersion  INTEGER
    }

sonusSipSigPeerStatusSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The card slot for the Sip Signaling Peering"
    ::= { sonusSipSigPeerStatusEntry 1 }

sonusSipSigPeerStatusIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IP Address of the Sip Signaling Peer"
    ::= { sonusSipSigPeerStatusEntry 2 }

sonusSipSigPeerStatusZoneId OBJECT-TYPE
    SYNTAX          Gauge (0..65535)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the Zone ID for this signaling peer."
    ::= { sonusSipSigPeerStatusEntry 3 }

sonusSipSigPeerStatusPortNum OBJECT-TYPE
    SYNTAX          Gauge (0..65535)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The port number for this signaling peer. Always zero for static and non-NATed registrant peer."
    ::= { sonusSipSigPeerStatusEntry 4 }

sonusSipSigPeerStatusType OBJECT-TYPE
    SYNTAX          INTEGER { static(0), registrant(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the TYPE of signaling peer."
    ::= { sonusSipSigPeerStatusEntry 5 }

sonusSipSigPeerStatusCreatedAt OBJECT-TYPE
