ptSWMSwitch OBJECT IDENTIFIER
    ::= { ptSWMSwitches 1183 }

ptSWMSwitchTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtSWMSwitchEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptSWMSwitch 1 }

ptSWMSwitchEntry OBJECT-TYPE
    SYNTAX          PtSWMSwitchEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of SWMSwitch attributes."
    INDEX           { ptConnGroupIndex, ptSWMSwitchSwitchesIndex, 
                      ptSWMSwitchIndex }
    ::= { ptSWMSwitchTable 1 }

PtSWMSwitchEntry ::=
    SEQUENCE {
        ptSWMSwitchSlotNumber           Integer32,
        ptSWMSwitchType                 INTEGER,
        ptSWMSwitchIPAddress            DisplayString,
        ptSWMSwitchSlotStatus           INTEGER,
        ptSWMSwitchSwitchSyncStatus     INTEGER,
        ptSWMSwitchSwitchIntegrityStatus INTEGER,
        ptSWMSwitchSwitchesIndex        Integer32,
        ptSWMSwitchParentOID            OBJECT IDENTIFIER,
        ptSWMSwitchRowStatus            RowStatus,
        ptSWMSwitchIndex                Integer32
    }

ptSWMSwitchSlotNumber OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Physical slot location of the switch board."
    ::= { ptSWMSwitchEntry 1 }

ptSWMSwitchType OBJECT-TYPE
    SYNTAX          INTEGER { unknown(0), cpc4416(1), cpc6600(2), 
                        cpc6620(3), amc671(4), mez10gbe(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Name of the switch board detected. The switch type could be a CPC4416, CPC6600, CPC6620, AMC671, MEZ10GBE or UNKNOWN. readonly field"
    ::= { ptSWMSwitchEntry 2 }

ptSWMSwitchIPAddress OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "IP Address of the switch board detected."
    ::= { ptSWMSwitchEntry 3 }

ptSWMSwitchSlotStatus OBJECT-TYPE
    SYNTAX          INTEGER { querying(0), extracted(1), available(2), 
                        pendingExtractionRequest(3), 
                        readyToBeExtracted(4), lostConnection(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Current status for this slot. Can be one of the following states; AVAILABLE: slot is communicating normally. UNKNOWN_STATE: slot is not present. LOST_CONNECTION: Switch Monitor has lost connection to this slot."
    ::= { ptSWMSwitchEntry 4 }

ptSWMSwitchSwitchSyncStatus OBJECT-TYPE
    SYNTAX          INTEGER { down(0), up(1), unknown(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Current status of the Switch Sync Interface for this slot. Can be one of the following states; UP: interface is communicating normally. DOWN: interface is down for this slot. UNKNOWN: interface is unknown or not applicable for this slot."
    ::= { ptSWMSwitchEntry 5 }

ptSWMSwitchSwitchIntegrityStatus OBJECT-TYPE
    SYNTAX          INTEGER { passed(0), failed(1), unknown(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Current status of the Switch Integrity for this slot. Can be one of the following states; PASSED: switch integrity tests Passed. FAILED: switch integrity tests failed for this slot. UNKNOWN: integrity status is unknown for this slot."
    ::= { ptSWMSwitchEntry 6 }

ptSWMSwitchSwitchesIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is Switches."
    ::= { ptSWMSwitchEntry 242 }

ptSWMSwitchParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptSWMSwitchEntry 253 }

ptSWMSwitchRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The row status for the SWMSwitch table"
    ::= { ptSWMSwitchEntry 254 }

ptSWMSwitchIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The row number for the SWMSwitch table"
    ::= { ptSWMSwitchEntry 255 }

