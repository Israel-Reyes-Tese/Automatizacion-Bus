        ptIOHWIOManagerProvisioning     INTEGER,
        ptIOHWIOManagerMaxLSL           INTEGER,
        ptIOHWIOManagerMaxHSLAnnexA     INTEGER,
        ptIOHWIOManagerMaxHSLATM        INTEGER,
        ptIOHWIOManagerLSLSupport       INTEGER,
        ptIOHWIOManagerHSLAnnexASupport INTEGER,
        ptIOHWIOManagerHSLATMSupport    INTEGER,
        ptIOHWIOManagerSelectConfiguration  INTEGER,
        ptIOHWIOManagerRestartRequired  INTEGER,
        ptIOHWIOManagerProtocolVariant  INTEGER,
        ptIOHWIOManagerActions          INTEGER,
        ptIOHWIOManagerParentOID        OBJECT IDENTIFIER,
        ptIOHWIOManagerRowStatus        RowStatus,
        ptIOHWIOManagerIndex            INTEGER
    }

