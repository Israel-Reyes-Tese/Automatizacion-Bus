sonusDsx1CurrentTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDsx1CurrentEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The DS1 current table contains various statistics
             being collected for the near end current 15 minute
             interval."
    ::= { sonusDsx1MIBObjects 5 }

sonusDsx1CurrentEntry OBJECT-TYPE
    SYNTAX          SonusDsx1CurrentEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "An entry in the DS1 Current table."
    INDEX           { sonusDsx1CurrentShelfIndex, 
                      sonusDsx1CurrentSlotIndex, 
                      sonusDsx1CurrentPortIndex, 
                      sonusDsx1CurrentDs1Index }
    ::= { sonusDsx1CurrentTable 1 }

SonusDsx1CurrentEntry ::=
    SEQUENCE {
        sonusDsx1CurrentShelfIndex      SonusShelfIndex,
        sonusDsx1CurrentSlotIndex       SonusSlotIndex,
        sonusDsx1CurrentPortIndex       INTEGER,
        sonusDsx1CurrentDs1Index        INTEGER,
        sonusDsx1CurrentElapsedTime     INTEGER,
        sonusDsx1CurrentLCVs            PerfCurrentCount,
        sonusDsx1CurrentLESs            PerfCurrentCount,
        sonusDsx1CurrentLSESs           PerfCurrentCount,
        sonusDsx1CurrentPCVs            PerfCurrentCount,
        sonusDsx1CurrentPESs            PerfCurrentCount,
        sonusDsx1CurrentPSESs           PerfCurrentCount,
        sonusDsx1CurrentSASs            PerfCurrentCount,
        sonusDsx1CurrentCSSs            PerfCurrentCount,
        sonusDsx1CurrentUASs            PerfCurrentCount,
        sonusDsx1CurrentFC              INTEGER,
        sonusDsx1CurrentTcaLCV          TruthValue,
        sonusDsx1CurrentTcaLES          TruthValue,
        sonusDsx1CurrentTcaLSES         TruthValue,
        sonusDsx1CurrentTcaPCV          TruthValue,
        sonusDsx1CurrentTcaPES          TruthValue,
        sonusDsx1CurrentTcaPSES         TruthValue,
        sonusDsx1CurrentTcaSAS          TruthValue,
        sonusDsx1CurrentTcaCSS          TruthValue,
        sonusDsx1CurrentTcaUAS          TruthValue,
        sonusDsx1CurrentMinorTcaLCV     TruthValue,
        sonusDsx1CurrentMinorTcaLES     TruthValue,
