sonusTcp OBJECT IDENTIFIER
    ::= { sonusMib-2 6 }

sonusTcpGeneralGroupTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusTcpGeneralGroupEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The collection of TCP General Group information for the
             specified instance."
    ::= { sonusTcp 1 }

sonusTcpGeneralGroupEntry OBJECT-TYPE
    SYNTAX          SonusTcpGeneralGroupEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The collection of TCP General Group information for the
             specified instance."
    INDEX           { sonusTcpGeneralGroupShelf, 
                      sonusTcpGeneralGroupSlot, 
                      sonusTcpGeneralGroupInstance }
    ::= { sonusTcpGeneralGroupTable 1 }

SonusTcpGeneralGroupEntry ::=
    SEQUENCE {
        sonusTcpRtoAlgorithm            INTEGER,
        sonusTcpRtoMin                  INTEGER,
        sonusTcpRtoMax                  INTEGER,
        sonusTcpMaxConn                 INTEGER,
        sonusTcpActiveOpens             Counter,
        sonusTcpPassiveOpens            Counter,
        sonusTcpAttemptFails            Counter,
        sonusTcpEstabResets             Counter,
        sonusTcpCurrEstab               Gauge,
        sonusTcpInSegs                  Counter,
        sonusTcpOutSegs                 Counter,
        sonusTcpRetransSegs             Counter,
        sonusTcpInErrs                  Counter,
        sonusTcpOutRsts                 Counter,
        sonusTcpGeneralGroupShelf       SonusShelfIndex,
        sonusTcpGeneralGroupSlot        SonusSlotIndex,
        sonusTcpGeneralGroupInstance    INTEGER,
        sonusTcpMaxSegSize              Counter
    }

sonusTcpRtoAlgorithm OBJECT-TYPE
    SYNTAX          INTEGER { other(1), constant(2), rsre(3), vanj(4) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The algorithm used to determine the timeout value used for
             retransmitting unacknowledged octets."
    ::= { sonusTcpGeneralGroupEntry 1 }

sonusTcpRtoMin OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The minimum value permitted by a TCP implementation for the
             retransmission timeout, measured in milliseconds.  More
             refined semantics for objects of this type depend upon the
             algorithm used to determine the retransmission timeout.  In
             particular, when the timeout algorithm is rsre(3), an object
             of this type has the semantics of the LBOUND quantity
             described in RFC 793."
    ::= { sonusTcpGeneralGroupEntry 2 }

sonusTcpRtoMax OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The maximum value permitted by a TCP implementation for the
             retransmission timeout, measured in milliseconds.  More
             refined semantics for objects of this type depend upon the
             algorithm used to determine the retransmission timeout.  In
             particular, when the timeout algorithm is rsre(3), an object
             of this type has the semantics of the UBOUND quantity
             described in RFC 793."
    ::= { sonusTcpGeneralGroupEntry 3 }

sonusTcpMaxConn OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The limit on the total number of TCP connections the entity
             can support.  In entities where the maximum number of
             connections is dynamic, this object should contain the value
             -1."
    ::= { sonusTcpGeneralGroupEntry 4 }

sonusTcpActiveOpens OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The number of times TCP connections have made a direct
             transition to the SYN-SENT state from the CLOSED state."
    ::= { sonusTcpGeneralGroupEntry 5 }

sonusTcpPassiveOpens OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The number of times TCP connections have made a direct
             transition to the SYN-RCVD state from the LISTEN state."
    ::= { sonusTcpGeneralGroupEntry 6 }

sonusTcpAttemptFails OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The number of times TCP connections have made a direct
             transition to the CLOSED state from either the SYN-SENT
             state or the SYN-RCVD state, plus the number of times TCP
             connections have made a direct transition to the LISTEN
             state from the SYN-RCVD state."
    ::= { sonusTcpGeneralGroupEntry 7 }

sonusTcpEstabResets OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The number of times TCP connections have made a direct
             transition to the CLOSED state from either the ESTABLISHED
             state or the CLOSE-WAIT state."
    ::= { sonusTcpGeneralGroupEntry 8 }

sonusTcpCurrEstab OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The number of TCP connections for which the current state
             is either ESTABLISHED or CLOSE- WAIT."
    ::= { sonusTcpGeneralGroupEntry 9 }

sonusTcpInSegs OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The total number of segments received, including those
             received in error.  This count includes segments received on
             currently established connections."
    ::= { sonusTcpGeneralGroupEntry 10 }

sonusTcpOutSegs OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The total number of segments sent, including those on
             current connections but excluding those containing only
             retransmitted octets."
    ::= { sonusTcpGeneralGroupEntry 11 }

sonusTcpRetransSegs OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The total number of segments retransmitted - that is, the
             number of TCP segments transmitted containing one or more
             previously transmitted octets."
    ::= { sonusTcpGeneralGroupEntry 12 }

sonusTcpInErrs OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The total number of segments received in error (e.g., bad
             TCP checksums)."
    ::= { sonusTcpGeneralGroupEntry 14 }

sonusTcpOutRsts OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The number of TCP segments sent containing the RST flag."
    ::= { sonusTcpGeneralGroupEntry 15 }

sonusTcpGeneralGroupShelf OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the shelf
             that the TCP General Group table applies to"
    ::= { sonusTcpGeneralGroupEntry 16 }

sonusTcpGeneralGroupSlot OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the slot
             that the TCP General Group table applies to"
    ::= { sonusTcpGeneralGroupEntry 17 }

sonusTcpGeneralGroupInstance OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the instance
             that the TCP General Group table applies to"
    ::= { sonusTcpGeneralGroupEntry 18 }

sonusTcpMaxSegSize OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Current maximum segement size."
    ::= { sonusTcpGeneralGroupEntry 19 }

sonusTcpConnTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusTcpConnEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A table containing TCP connection-specific information."
    ::= { sonusTcp 13 }

sonusTcpConnEntry OBJECT-TYPE
    SYNTAX          SonusTcpConnEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A conceptual row of the tcpConnTable containing information
             about a particular current TCP connection.  Each row of this
             table is transient, in that it ceases to exist when (or soon
             after) the connection makes the transition to the CLOSED
             state."
    INDEX           { sonusTcpConnIpVersion, sonusTcpConnShelf, 
                      sonusTcpConnSlot, sonusTcpConnInstance, 
                      sonusTcpConnLocalAddress, sonusTcpConnLocalPort, 
                      sonusTcpConnRemAddress, sonusTcpConnRemPort, 
                      sonusTcpConnLocal6Address, sonusTcpConnRem6Address }
    ::= { sonusTcpConnTable 1 }

SonusTcpConnEntry ::=
    SEQUENCE {
        sonusTcpConnState               INTEGER,
        sonusTcpConnLocalAddress        IpAddress,
        sonusTcpConnLocalPort           INTEGER,
        sonusTcpConnRemAddress          IpAddress,
        sonusTcpConnRemPort             INTEGER,
        sonusTcpConnShelf               SonusShelfIndex,
        sonusTcpConnSlot                SonusSlotIndex,
        sonusTcpConnInstance            INTEGER,
        sonusTcpConnLocal6Address       Ipv6Address,
        sonusTcpConnRem6Address         Ipv6Address,
        sonusTcpConnIpVersion           INTEGER
    }

sonusTcpConnState OBJECT-TYPE
    SYNTAX          INTEGER { closed(1), listen(2), synSent(3), 
                        synReceived(4), established(5), finWait1(6), 
                        finWait2(7), closeWait(8), lastAck(9), 
                        closing(10), timeWait(11), deleteTCB(12) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The state of this TCP connection.
             
             The only value which may be set by a management station is
             deleteTCB(12).  Accordingly, it is appropriate for an agent
             to return a `badValue' response if a management station
             attempts to set this object to any other value.
             
             If a management station sets this object to the value
             deleteTCB(12), then this has the effect of deleting the TCB
             (as defined in RFC 793) of the corresponding connection on
             the managed node, resulting in immediate termination of the
             connection.
             
             As an implementation-specific option, a RST segment may be
             sent from the managed node to the other TCP endpoint (note
             however that RST segments are not sent reliably)."
    ::= { sonusTcpConnEntry 1 }

sonusTcpConnLocalAddress OBJECT-TYPE
    SYNTAX          IpAddress
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The local IP address for this TCP connection.  In the case
             of a connection in the listen state which is willing to
             accept connections for any IP interface associated with the
             node, the value 0.0.0.0 is used."
    ::= { sonusTcpConnEntry 2 }

sonusTcpConnLocalPort OBJECT-TYPE
    SYNTAX          INTEGER (0..65535)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The local port number for this TCP connection."
    ::= { sonusTcpConnEntry 3 }

sonusTcpConnRemAddress OBJECT-TYPE
    SYNTAX          IpAddress
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The remote IP address for this TCP connection."
    ::= { sonusTcpConnEntry 4 }

sonusTcpConnRemPort OBJECT-TYPE
    SYNTAX          INTEGER (0..65535)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The remote port number for this TCP connection."
    ::= { sonusTcpConnEntry 5 }

sonusTcpConnShelf OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the shelf
             that the TCP Connection Group table applies to"
    ::= { sonusTcpConnEntry 6 }

sonusTcpConnSlot OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the slot
             that the TCP Connection Group table applies to"
    ::= { sonusTcpConnEntry 7 }

sonusTcpConnInstance OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the instance
             that the TCP Connection Group table applies to"
    ::= { sonusTcpConnEntry 8 }

sonusTcpConnLocal6Address OBJECT-TYPE
    SYNTAX          Ipv6Address
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The local IPv6 address for this TCP connection.  In the case
             of a connection in the listen state which is willing to
             accept connections for any IPv6 interface associated with the
             node, the value :: is used."
    ::= { sonusTcpConnEntry 9 }

sonusTcpConnRem6Address OBJECT-TYPE
    SYNTAX          Ipv6Address
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The remote IPv6 address for this TCP connection."
    ::= { sonusTcpConnEntry 10 }

sonusTcpConnIpVersion OBJECT-TYPE
    SYNTAX          INTEGER { afInet(2), afInet6(24) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The remote IP version for this TCP connection."
    ::= { sonusTcpConnEntry 11 }

sonusUdp OBJECT IDENTIFIER
    ::= { sonusMib-2 7 }

sonusUdpGeneralGroupTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusUdpGeneralGroupEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The collection of UDP General Group information for the
             specified instance."
    ::= { sonusUdp 1 }

sonusUdpGeneralGroupEntry OBJECT-TYPE
    SYNTAX          SonusUdpGeneralGroupEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The collection of UDP General Group information for the
             specified instance."
    INDEX           { sonusUdpGeneralGroupShelf, 
                      sonusUdpGeneralGroupSlot, 
                      sonusUdpGeneralGroupInstance }
    ::= { sonusUdpGeneralGroupTable 1 }

SonusUdpGeneralGroupEntry ::=
    SEQUENCE {
        sonusUdpInDatagrams             Counter,
        sonusUdpNoPorts                 Counter,
        sonusUdpInErrors                Counter,
        sonusUdpOutDatagrams            Counter,
        sonusUdpGeneralGroupShelf       SonusShelfIndex,
        sonusUdpGeneralGroupSlot        SonusSlotIndex,
        sonusUdpGeneralGroupInstance    INTEGER
    }

sonusUdpInDatagrams OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The total number of UDP datagrams delivered to UDP users."
    ::= { sonusUdpGeneralGroupEntry 1 }

sonusUdpNoPorts OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The total number of received UDP datagrams for which there
             was no application at the destination port."
    ::= { sonusUdpGeneralGroupEntry 2 }

sonusUdpInErrors OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The number of received UDP datagrams that could not be
             delivered for reasons other than the lack of an application
             at the destination port."
    ::= { sonusUdpGeneralGroupEntry 3 }

sonusUdpOutDatagrams OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The total number of UDP datagrams sent from this entity."
    ::= { sonusUdpGeneralGroupEntry 4 }

sonusUdpGeneralGroupShelf OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the shelf
             that the UDP General Group table applies to"
    ::= { sonusUdpGeneralGroupEntry 5 }

sonusUdpGeneralGroupSlot OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the slot
             that the UDP General Group table applies to"
    ::= { sonusUdpGeneralGroupEntry 6 }

sonusUdpGeneralGroupInstance OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the instance
             that the UDP General Group table applies to"
    ::= { sonusUdpGeneralGroupEntry 7 }

sonusUdpTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusUdpEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A table containing UDP listener information."
    ::= { sonusUdp 5 }

sonusUdpEntry OBJECT-TYPE
    SYNTAX          SonusUdpEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Information about a particular current UDP listener."
    INDEX           { sonusUdpIpVersion, sonusUdpShelf, sonusUdpSlot, 
                      sonusUdpInstance, sonusUdpLocalAddress, 
                      sonusUdpLocalPort, sonusUdpLocal6Address }
    ::= { sonusUdpTable 1 }

SonusUdpEntry ::=
    SEQUENCE {
        sonusUdpLocalAddress            IpAddress,
        sonusUdpLocalPort               INTEGER,
        sonusUdpShelf                   SonusShelfIndex,
        sonusUdpSlot                    SonusSlotIndex,
        sonusUdpInstance                INTEGER,
        sonusUdpLocal6Address           Ipv6Address,
        sonusUdpIpVersion               INTEGER
    }

sonusUdpLocalAddress OBJECT-TYPE
    SYNTAX          IpAddress
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The local IP address for this UDP listener.  In the case of
             a UDP listener which is willing to accept datagrams for any
             IP interface associated with the node, the value 0.0.0.0 is
             used."
    ::= { sonusUdpEntry 1 }

sonusUdpLocalPort OBJECT-TYPE
    SYNTAX          INTEGER (0..65535)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The local port number for this UDP listener."
    ::= { sonusUdpEntry 2 }

sonusUdpShelf OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the shelf
             that the UDP Listener table applies to"
    ::= { sonusUdpEntry 3 }

sonusUdpSlot OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the slot
             that the UDP Listener table applies to"
    ::= { sonusUdpEntry 4 }

sonusUdpInstance OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the instance
             that the UDP Listener table applies to"
    ::= { sonusUdpEntry 5 }

sonusUdpLocal6Address OBJECT-TYPE
    SYNTAX          Ipv6Address
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The local IPv6 address for this UDP listener.  In the case of
             a UDP listener which is willing to accept datagrams for any
             IP interface associated with the node, the value :: is
             used."
    ::= { sonusUdpEntry 6 }

sonusUdpIpVersion OBJECT-TYPE
    SYNTAX          INTEGER { afInet(2), afInet6(24) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The local IP version for this UDP connection."
    ::= { sonusUdpEntry 7 }

sonusSctp OBJECT IDENTIFIER
    ::= { sonusMib-2 8 }

sonusSctpGeneralGroupTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSctpGeneralGroupEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The collection of SCTP General Group information for the
             specified instance."
    ::= { sonusSctp 1 }

sonusSctpGeneralGroupEntry OBJECT-TYPE
    SYNTAX          SonusSctpGeneralGroupEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The collection of SCTP General Group information for the
             specified instance."
    INDEX           { sonusSctpGeneralGroupShelf, 
                      sonusSctpGeneralGroupSlot, 
                      sonusSctpGeneralGroupInstance }
    ::= { sonusSctpGeneralGroupTable 1 }

SonusSctpGeneralGroupEntry ::=
    SEQUENCE {
        sonusSctpActiveAssociations     Counter,
        sonusSctpActiveStreams          Counter,
        sonusSctpAttemptFails           Counter,
        sonusSctpCurrEstab              Counter,
        sonusSctpInPkts                 Counter,
        sonusSctpInBadchecksum          Counter,
        sonusSctpInErrs                 Counter,
        sonusSctpInChunks               Counter,
        sonusSctpInInitChunks           Counter,
        sonusSctpInDataChunks           Counter,
        sonusSctpInSackChunks           Counter,
        sonusSctpInHeartBeatChunks      Counter,
        sonusSctpInAbortChunks          Counter,
        sonusSctpInErrChunks            Counter,
        sonusSctpInOotbChunks           Counter,
        sonusSctpOutPkts                Counter,
        sonusSctpOutChunks              Counter,
        sonusSctpOutInitChunks          Counter,
        sonusSctpOutDataChunks          Counter,
        sonusSctpOutSackChunks          Counter,
        sonusSctpOutHeartBeatChunks     Counter,
        sonusSctpOutAbortChunks         Counter,
        sonusSctpOutErrChunks           Counter,
        sonusSctpGeneralGroupShelf      SonusShelfIndex,
        sonusSctpGeneralGroupSlot       SonusSlotIndex,
        sonusSctpGeneralGroupInstance   INTEGER
    }

sonusSctpActiveAssociations OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of active SCTP associations that are in 
             > COOKIE_WAIT state"
    ::= { sonusSctpGeneralGroupEntry 1 }

sonusSctpActiveStreams OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Total number of streams in all the established
             associations combined. At minimum there will be as
             many streams as the number of associations"
    ::= { sonusSctpGeneralGroupEntry 2 }

sonusSctpAttemptFails OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of failed attempts by SCTP to initiate an
             association with peer."
    ::= { sonusSctpGeneralGroupEntry 3 }

sonusSctpCurrEstab OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Total number of associations in ESTABLISHED state"
    ::= { sonusSctpGeneralGroupEntry 4 }

sonusSctpInPkts OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Total number of SCTP packets received from all the peers
             combined."
    ::= { sonusSctpGeneralGroupEntry 5 }

sonusSctpInBadchecksum OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of received packets with bad checksum"
    ::= { sonusSctpGeneralGroupEntry 6 }

sonusSctpInErrs OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of packets received in error"
    ::= { sonusSctpGeneralGroupEntry 7 }

