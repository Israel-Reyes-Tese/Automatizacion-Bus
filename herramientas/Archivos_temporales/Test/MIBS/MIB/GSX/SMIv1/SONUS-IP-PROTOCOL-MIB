        sonusIpSecSadIkeStatusLocalIkeIdentityIpAddr            IpAddress,
        sonusIpSecSadIkeStatusLocalIkeIdentityFqdn          DisplayString,
        sonusIpSecSadIkeStatusPeerIkeIdentityType         INTEGER,
        sonusIpSecSadIkeStatusPeerIkeIdentityIpAddr           IpAddress,
        sonusIpSecSadIkeStatusPeerIkeIdentityFqdn         DisplayString,
        sonusIpSecSadIkeStatusAuthenticationType        INTEGER,
        sonusIpSecSadIkeStatusIntegrityCipher     INTEGER,
        sonusIpSecSadIkeStatusEncryptionCipher      INTEGER,
        sonusIpSecSadIkeStatusLifetimeRemaining       INTEGER,
        sonusIpSecSadIkeStatusProtectionProfile       DisplayString,
        sonusIpSecSadIkeStatusLocalIp6Addr  Ipv6Address,
        sonusIpSecSadIkeStatusPeerIp6Addr Ipv6Address,
        sonusIpSecSadIkeStatusLocalIkeIdentityIp6Addr             Ipv6Address,
        sonusIpSecSadIkeStatusPeerIkeIdentityIp6Addr            Ipv6Address,
        sonusIpSecSadIkeStatusIpVersion INTEGER,
        sonusIpSecSadIkeStatusIkeIdentityIpVersion          INTEGER,
        sonusIpSecSadIkeStatusIkeVersion INTEGER
    }

