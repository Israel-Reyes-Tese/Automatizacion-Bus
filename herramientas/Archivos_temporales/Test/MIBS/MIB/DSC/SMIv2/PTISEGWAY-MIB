    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptDSCDiameterProtocolParameterSet 1 }

ptDSCDiameterProtocolParameterSetEntry OBJECT-TYPE
    SYNTAX          PtDSCDiameterProtocolParameterSetEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of DSCDiameterProtocolParameterSet attributes."
    INDEX           { ptConnGroupIndex, 
                      ptDSCDiameterProtocolParameterSetDSCIndex, 
