ptIOHWPort OBJECT IDENTIFIER
    ::= { ptIOHWIOManager 1089 }

ptIOHWPortTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtIOHWPortEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptIOHWPort 1 }

ptIOHWPortEntry OBJECT-TYPE
    SYNTAX          PtIOHWPortEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of IOHWPort attributes."
    INDEX           { ptConnGroupIndex, ptIOHWPortIOManagerIndex, 
                      ptIOHWPortIndex }
    ::= { ptIOHWPortTable 1 }

PtIOHWPortEntry ::=
    SEQUENCE {
        ptIOHWPortPortNumber            INTEGER,
        ptIOHWPortInterfaceType         INTEGER,
        ptIOHWPortEncoding              INTEGER,
        ptIOHWPortLineLength            INTEGER,
        ptIOHWPortFacilityDataLink      INTEGER,
        ptIOHWPortAlarmSquelch          INTEGER,
        ptIOHWPortAlarmEgress           INTEGER,
        ptIOHWPortLoopbackMode          INTEGER,
        ptIOHWPortFramingStatus         INTEGER,
        ptIOHWPortNoAlarm               INTEGER,
        ptIOHWPortRemoteRxLOF           INTEGER,
        ptIOHWPortRemoteTxLOF           INTEGER,
        ptIOHWPortRxAIS                 INTEGER,
        ptIOHWPortTxAIS                 INTEGER,
        ptIOHWPortLossOfFrame           INTEGER,
        ptIOHWPortLossOfSynchronization INTEGER,
        ptIOHWPortLoopbackState         INTEGER,
        ptIOHWPortT16AIS                INTEGER,
        ptIOHWPortRemoteRxLOMF          INTEGER,
        ptIOHWPortRemoteTxLOMF          INTEGER,
        ptIOHWPortRxTestCode            INTEGER,
        ptIOHWPortOtherFailure          INTEGER,
        ptIOHWPortUnavailableSignalState INTEGER,
        ptIOHWPortNetEquipmentOOS       INTEGER,
        ptIOHWPortRxPayloadAIS          INTEGER,
        ptIOHWPortPerformanceThreshold  INTEGER,
        ptIOHWPortNoLoopback            INTEGER,
        ptIOHWPortRemotePayloadLoopback INTEGER,
        ptIOHWPortRemoteLineLoopback    INTEGER,
        ptIOHWPortLocalLineLoopback     INTEGER,
        ptIOHWPortFarEndPayloadLoopback INTEGER,
        ptIOHWPortFarEndLineLoopback    INTEGER,
        ptIOHWPortCurrentGain           INTEGER,
        ptIOHWPortCurrentAttenuation    INTEGER,
        ptIOHWPortStatistics            INTEGER,
        ptIOHWPortClockSource           INTEGER,
        ptIOHWPortStatusAlarms          INTEGER,
        ptIOHWPortFarEndLoopbackType    INTEGER,
        ptIOHWPortFarEndLoopbackActivationTime      INTEGER,
        ptIOHWPortFarEndLoopbackDeactivationTime        INTEGER,
        ptIOHWPortBERTPattern           INTEGER,
        ptIOHWPortPRBSDetectionTimer    INTEGER,
        ptIOHWPortBERTDuration          INTEGER,
        ptIOHWPortBERTStatus            DisplayString,
        ptIOHWPortBERTResult            DisplayString,
        ptIOHWPortPRBSSyncSeconds       INTEGER,
        ptIOHWPortPRBSLostSeconds       INTEGER,
        ptIOHWPortPRBSBitErrors         INTEGER,
        ptIOHWPortPRBSErrorInstances    INTEGER,
        ptIOHWPortIOManagerIndex        INTEGER,
        ptIOHWPortActions               INTEGER,
        ptIOHWPortParentOID             OBJECT IDENTIFIER,
        ptIOHWPortRowStatus             RowStatus,
        ptIOHWPortIndex                 INTEGER
    }

ptIOHWPortPortNumber OBJECT-TYPE
    SYNTAX          INTEGER { port0(0), port1(1), port2(2), port3(3), 
                        port4(4), port5(5), port6(6), port7(7), 
                        monitorPort(8), notConfigured(9), port8(10) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Port number as indicated on physical device."
    ::= { ptIOHWPortEntry 1 }

