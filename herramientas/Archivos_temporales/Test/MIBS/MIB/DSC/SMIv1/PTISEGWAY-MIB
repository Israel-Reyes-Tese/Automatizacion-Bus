    SYNTAX          SEQUENCE OF PtimfIMFConnectionEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptimfIMFConnection 1 }

ptimfIMFConnectionEntry OBJECT-TYPE
    SYNTAX          PtimfIMFConnectionEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of imfIMFConnection attributes."
    INDEX           { ptConnGroupIndex, 
                      ptimfIMFConnectionIMFConnectionsIndex, 
                      ptimfIMFConnectionIndex }
    ::= { ptimfIMFConnectionTable 1 }

PtimfIMFConnectionEntry ::=
    SEQUENCE {
        ptimfIMFConnectionName          DisplayString,
        ptimfIMFConnectionSlot          INTEGER,
        ptimfIMFConnectionInterface     INTEGER,
        ptimfIMFConnectionIPProtocol    INTEGER,
        ptimfIMFConnectionTrafficDirection  INTEGER,
        ptimfIMFConnectionLocalIPAddress1 DisplayString,
        ptimfIMFConnectionLocalIPAddress2 DisplayString,
        ptimfIMFConnectionLocalPort     INTEGER,
        ptimfIMFConnectionRemoteIPAddress1  DisplayString,
        ptimfIMFConnectionRemoteIPAddress2  DisplayString,
        ptimfIMFConnectionRemotePort    INTEGER,
        ptimfIMFConnectionStatus        INTEGER,
        ptimfIMFConnectionConnectionID  INTEGER,
        ptimfIMFConnectionIMFConnectionsIndex     INTEGER,
        ptimfIMFConnectionActions       INTEGER,
        ptimfIMFConnectionParentOID     OBJECT IDENTIFIER,
        ptimfIMFConnectionRowStatus     RowStatus,
        ptimfIMFConnectionIndex         INTEGER
    }

ptimfIMFConnectionName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IMF Connection unique name. The maximum number of characters the attribute may contain is 15."
    ::= { ptimfIMFConnectionEntry 1 }

ptimfIMFConnectionSlot OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IMF Connection slot number."
    ::= { ptimfIMFConnectionEntry 2 }

ptimfIMFConnectionInterface OBJECT-TYPE
    SYNTAX          INTEGER { default(0) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IMF Connection interface (DEFAULT)."
    ::= { ptimfIMFConnectionEntry 3 }

ptimfIMFConnectionIPProtocol OBJECT-TYPE
    SYNTAX          INTEGER { tcp(0) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IMF Connection IP protocol (TCP only)."
    ::= { ptimfIMFConnectionEntry 4 }

ptimfIMFConnectionTrafficDirection OBJECT-TYPE
    SYNTAX          INTEGER { all(0), incoming(1), outgoing(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Interface supported traffic direction. \nValid values: ALL, INCOMING, and OUTGOING. \nDefault value: ALL."
    ::= { ptimfIMFConnectionEntry 5 }

ptimfIMFConnectionLocalIPAddress1 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Primary local IP address of this IMF Connection. \nThis may be a hostname provisioned in /etc/hosts or an IP Address. \nThis information is only used if the connection IP Protocol attribute is set to SCTP. TCP/IP does not require knowledge of the local address."
    ::= { ptimfIMFConnectionEntry 6 }

ptimfIMFConnectionLocalIPAddress2 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Secondary local IP address of this IMF Connection. \nThis may be a hostname provisioned in /etc/hosts or an IP Address. \nThis information is only used if the connection IP Protocol attribute is set to SCTP. TCP/IP does not require knowledge of the local address."
    ::= { ptimfIMFConnectionEntry 7 }

ptimfIMFConnectionLocalPort OBJECT-TYPE
    SYNTAX          INTEGER (0..65535)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Local port of this IMF Connection. \nThis information is only used if the connection IP Protocol attribute is set to SCTP. TCP/IP does not require knowledge of the local port, will be assigned internally."
    ::= { ptimfIMFConnectionEntry 8 }

ptimfIMFConnectionRemoteIPAddress1 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Primary remote IP address of this IMF Connection. \nThis may be a hostname provisioned in /etc/hosts or an IP Address. \nThis is the address of the IMF 3rd party probe."
    ::= { ptimfIMFConnectionEntry 9 }

ptimfIMFConnectionRemoteIPAddress2 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Secondary remote IP address of this IMF Connection. \nThis may be a hostname provisioned in /etc/hosts or an IP Address. \nThis information is only used if the connection IP Protocol attribute is set to SCTP. TCP/IP does not require knowledge of the local address."
    ::= { ptimfIMFConnectionEntry 10 }

ptimfIMFConnectionRemotePort OBJECT-TYPE
    SYNTAX          INTEGER (0..65535)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Remote port of this IMF Connection. \nThis is the port of the IMF 3rd party probe."
    ::= { ptimfIMFConnectionEntry 11 }

ptimfIMFConnectionStatus OBJECT-TYPE
    SYNTAX          INTEGER { notApplicable(0), inactive(1), 
                        notConnected(2), connected(3) }
