        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.4
        xfServiceOamMaCfgTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfServiceOamMaCfgEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table is an extension of the ieee8021CfmMaCompTable and rows
                are automatically added or deleted from this table based upon row
                creation and destruction of the ieee8021CfmMaCompTable.
                This table represents the Maintenance Entity Group (Y.1731) or
                Maintenance Association (802.1ag). An MEG/MA is a set of MEPs,
                each configured to the same service inside a common OAM domain.
                This is the part of the complete MEG/MA table that is variable
                across the Bridges in a Maintenance Domain, or across the
                components of a single Bridge. Each row in the table represents an
                MEG/MA.
                The writable objects in this table need to be persistent
                upon reboot or restart of a device.
                "
            ::= { xfServiceOam 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.4.1
        xfServiceOamMaCfgEntry OBJECT-TYPE
            SYNTAX XfServiceOamMaCfgEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "The conceptual row of xfServiceOamMaCfgTable."
            AUGMENTS {
                ieee8021CfmMaCompEntry
            }
            ::= { xfServiceOamMaCfgTable 1 }

        XfServiceOamMaCfgEntry ::=
            SEQUENCE {
                xfServiceOamMaCfgVlanType   INTEGER,
                xfServiceOamMaCfgCfmAlarm   INTEGER,
                xfServiceOamMaCfgPmAlarm    TruthValue,
                xfServiceOamMaCfgRowStatus  RowStatus,
                xfServiceOamMaCfgHighestSeverityAlarm PerceivedSeverity
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.3.1.1
        xfServiceOamMaCfgVlanType OBJECT-TYPE
            SYNTAX INTEGER
                {
                noVlan(0),
                cvlan(1),
                svlan(2)
                }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "MA VLAN type.
                
                noVlan(0) : associate MA with no VLAN.
                cvlan(1)  : associate MA with C-VLAN according Q-Standard (IEEE 802.1Q-2005)
                svlan(2)  : associate MA with S-VLAN according D-Standard (802.1D-2004)
                "
        ::= { xfServiceOamMaCfgEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.3.1.2
        xfServiceOamMaCfgCfmAlarm OBJECT-TYPE
            SYNTAX INTEGER
                {
                enable8021CfmAlarm(0),
                enableY1731CfmAlarm(1),
                disableCfmAlarm(2)
                }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "This object controls CFM Alarm Notification:
                    enable8021CfmAlarm(0): enable IEEE 802.1ag CFM alarm
                    enableY1731CfmAlarm(1): enable ITU-T Y.1731 CFM alarm
                    disableCfmAlarm(2): disable the Cfm alarm that is currently enabled
                 Note: we can only enable either 8021.ag alarm or Y.1731 alarm but not both.
                "
            DEFVAL { enable8021CfmAlarm }
            ::= { xfServiceOamMaCfgEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.3.1.3
        xfServiceOamMaCfgPmAlarm OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "A Boolean flag set to true to enable PM alarm notifications.
                 PM notifications include Delay degrade and Delay Variation degrade.
                "
            DEFVAL { false }
            ::= { xfServiceOamMaCfgEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.3.1.4
        xfServiceOamMaCfgRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Used to create or destroy a group."
            ::= { xfServiceOamMaCfgEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.3.1.5
        xfServiceOamMaCfgHighestSeverityAlarm OBJECT-TYPE
            SYNTAX PerceivedSeverity
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "This objects indicates the highest alarm severity of the MA."
            ::= { xfServiceOamMaCfgEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.5
        xfServiceOamMepTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfServiceOamMepEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table is an extension of the dot1agCfmMepTable and rows
                are automatically added or deleted from this table based upon row
                creation and destruction of the dot1agCfmMepTable.
                This table represents the status of a MEG End Point or Maintenance End
                Point (MEP), which is a provisioned OAM reference point capable of
                initiating and terminating proactive SOAM PDU frames. A MEP is also capable
                of initiating and reacting to diagnostic SOAM PDU frames.
                Terminology is MEG End Point (Y.1731) or MA End Point (802.1ag).
            "
            ::= { xfServiceOam 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.5.1
        xfServiceOamMepEntry OBJECT-TYPE
            SYNTAX XfServiceOamMepEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "The conceptual row of xfServiceOamMepTable."
            AUGMENTS {
                dot1agCfmMepEntry
            }
            ::= { xfServiceOamMepTable 1 }

        XfServiceOamMepEntry ::=
            SEQUENCE {
                xfServiceOamMepTransmitLbmInterval       Unsigned32,
                xfServiceOamMepTransmitLbmDataTlvLength  Unsigned32,
                xfServiceOamMepTransmitLbmTimeout        Unsigned32,
                xfServiceOamMepRowStatus                 RowStatus,
                xfServiceOamMepCcmStatusUpdateEnable     TruthValue
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.5.1.1
        xfServiceOamMepTransmitLbmInterval OBJECT-TYPE
            SYNTAX Unsigned32 (1000..60000)
            UNITS  "ms"
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "This attribute specifies the period between LBM transmissions in
                 an LB Session. For an LB Session, the period for LBM transmission
                 is configurable in the range 1 and sixty seconds (60s).
                 Granularity of 100ms is required.

                 The transmission of the next LBM is not dependent upon the reception
                 the first LBR. The next LBM is sent out based upon the interval count.
                "
            DEFVAL { 1000 }
            ::= { xfServiceOamMepEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.5.1.2
        xfServiceOamMepTransmitLbmDataTlvLength OBJECT-TYPE
            SYNTAX Unsigned32 (0..9186)
            UNITS  "bytes"
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "This attribute specifies the content length of the Data TLV test. The
                 value can be set between 0 and 9186.
                 Note that there is no guarantee that the Max value(9186) will pass through
                 since the frame could be dropped due to configured Frame Size and VLAN type along the loop path.
                 
                 The default value is 0 byte.

                 The first 32 bytes of the Data TLV is configurable by dot1agCfmMepTransmitLbmDataTlv.
                 These 32 bytes (or less) shall be repeated to fill the rest of the Data TLV.
                "
            DEFVAL { 0 }
            ::= { xfServiceOamMepEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.5.1.3
        xfServiceOamMepTransmitLbmTimeout OBJECT-TYPE
            SYNTAX Unsigned32 (1..10000)
            UNITS  "ms"
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "This attribute specifies how long the loopback initiating MEP will wait for LBR until 
                 it is considered as lost. 
                 The default value is 5000 ms.
                "
            DEFVAL { 5000 }
            ::= { xfServiceOamMepEntry 3 }

         -- 1.3.6.1.4.1.193.81.4.1.2.1.12.5.1.4
        xfServiceOamMepRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Used to create or destroy a group. "
            ::= { xfServiceOamMepEntry 4 }

         -- 1.3.6.1.4.1.193.81.4.1.2.1.12.5.1.5
        xfServiceOamMepCcmStatusUpdateEnable OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "If set to true, the MEP will trigger oper status changes
                 on the MEP's interface when an RMEP states change"
            DEFVAL { false }
            ::= { xfServiceOamMepEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.12.6
