PtPCEPCENAEntry ::=
    SEQUENCE {
        ptPCEPCENANA                    Integer32,
        ptPCEPCENAType                  INTEGER,
        ptPCEPCENAMTPPort               Integer32,
        ptPCEPCENASCCPGTTPort           Integer32,
        ptPCEPCENAPCEGTTPort            Integer32,
        ptPCEPCENANetworkIndicator      INTEGER,
        ptPCEPCENAPCENAManagerIndex     Integer32,
        ptPCEPCENAParentOID             OBJECT IDENTIFIER,
        ptPCEPCENARowStatus             RowStatus,
        ptPCEPCENAIndex                 Integer32
    }

ptPCEPCENANA OBJECT-TYPE
    SYNTAX          Integer32 (1..99)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "At the PCE level Network Appearances (NAs) are usually known as Public, Private-1, and Private-2. However the NA's are known at the MTP3, SCCP, and GTT by their numbers. \nCan only be modified when PCE traffic is deactivated. \nBy default, NA 1 is public while NA 2 and 3 are private."
    ::= { ptPCEPCENAEntry 1 }

ptPCEPCENAType OBJECT-TYPE
    SYNTAX          INTEGER { public(1), private1(2), private2(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Network Appearance Type (PUBLIC/PRIVATE 1/PRIVATE 2)."
    ::= { ptPCEPCENAEntry 2 }

ptPCEPCENAMTPPort OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "User Port of the MTP3 for the given NA. \nThis is a read-only attribute for informational purposes."
    ::= { ptPCEPCENAEntry 4 }

ptPCEPCENASCCPGTTPort OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "GTT Port of the SCCP for the given NA. \nThis is a read-only attribute for informational purposes."
    ::= { ptPCEPCENAEntry 5 }

ptPCEPCENAPCEGTTPort OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Service Port of the GTT for the given NA. \nPCE connects to SCCP GTT port to intercept traffic. The GTT for the given NA is connected to this port. \nThis is a read-only attribute for informational purposes."
    ::= { ptPCEPCENAEntry 6 }

ptPCEPCENANetworkIndicator OBJECT-TYPE
    SYNTAX          INTEGER { undefined(255), ni00(0), ni01(1), ni10(2), 
                        ni11(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "MTP3 NA associated Network Indicator. \nThis is a read-only attribute for informational purposes."
    ::= { ptPCEPCENAEntry 7 }

ptPCEPCENAPCENAManagerIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is PCENAManager."
    ::= { ptPCEPCENAEntry 242 }

