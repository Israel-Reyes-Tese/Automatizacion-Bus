ptMTPGenericLink OBJECT IDENTIFIER
    ::= { ptMTPLinkset 80 }

ptMTPGenericLinkTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtMTPGenericLinkEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of Link attributes."
    ::= { ptMTPGenericLink 1 }

ptMTPGenericLinkEntry OBJECT-TYPE
    SYNTAX          PtMTPGenericLinkEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of MTPGenericLink attributes."
    INDEX           { ptConnGroupIndex, 
                      ptMTPGenericLinkMTP3NAManagerIndex, 
                      ptMTPGenericLinkMTP3NAIndex, 
                      ptMTPGenericLinkLinksetsIndex, 
                      ptMTPGenericLinkLinksetIndex, 
                      ptMTPGenericLinkIndex }
    ::= { ptMTPGenericLinkTable 1 }

PtMTPGenericLinkEntry ::=
    SEQUENCE {
        ptMTPGenericLinkContext         DisplayString,
        ptMTPGenericLinkSLC             Integer32,
        ptMTPGenericLinkDREID           Integer32,
        ptMTPGenericLinkLocalIPAddress1 DisplayString,
        ptMTPGenericLinkLocalIPAddress2 DisplayString,
        ptMTPGenericLinkLocalPort       Integer32,
        ptMTPGenericLinkRemoteIPAddress1 DisplayString,
        ptMTPGenericLinkRemoteIPAddress2 DisplayString,
        ptMTPGenericLinkRemotePort      Integer32,
        ptMTPGenericLinkL2State         INTEGER,
        ptMTPGenericLinkL3State         INTEGER,
        ptMTPGenericLinkDiagnosticMessage DisplayString,
        ptMTPGenericLinkDataDebugLevel  Integer32,
        ptMTPGenericLinkSoftwareDebugLevel  Integer32,
        ptMTPGenericLinkSCTPDebugLevel  Integer32,
        ptMTPGenericLinkUserActivated   INTEGER,
        ptMTPGenericLinkLinksetSCTPValues INTEGER,
        ptMTPGenericLinkHeartbeat       INTEGER,
        ptMTPGenericLinkHeartbeatInterval Integer32,
        ptMTPGenericLinkRTOInitial      Integer32,
        ptMTPGenericLinkRTOMinimum      Integer32,
        ptMTPGenericLinkRTOMaximum      Integer32,
        ptMTPGenericLinkMaximumPathRetrans  Integer32,
        ptMTPGenericLinkMaximumAssocRetrans   Integer32,
        ptMTPGenericLinkBundling        INTEGER,
        ptMTPGenericLinkBundlingTO      Integer32,
        ptMTPGenericLinkDelayedACKTO    Integer32,
        ptMTPGenericLinkFastRetransThreshold    Integer32,
        ptMTPGenericLinkChecksum        INTEGER,
        ptMTPGenericLinkDataLinkBandwidth Integer32,
        ptMTPGenericLinkAlarms          INTEGER,
        ptMTPGenericLinkNetworkAppearance INTEGER,
        ptMTPGenericLinkRoutingContext  INTEGER,
        ptMTPGenericLinkInStreams       Integer32,
        ptMTPGenericLinkOutStreams      Integer32,
        ptMTPGenericLinkLinkType        INTEGER,
        ptMTPGenericLinkRxMSUCount      Integer32,
        ptMTPGenericLinkTxMSUCount      Integer32,
        ptMTPGenericLinkMTP3NAManagerIndex  Integer32,
        ptMTPGenericLinkMTP3NAIndex     Integer32,
        ptMTPGenericLinkLinksetsIndex   Integer32,
        ptMTPGenericLinkLinksetIndex    Integer32,
        ptMTPGenericLinkActions         INTEGER,
        ptMTPGenericLinkParentOID       OBJECT IDENTIFIER,
        ptMTPGenericLinkRowStatus       RowStatus,
        ptMTPGenericLinkIndex           Integer32
    }

ptMTPGenericLinkContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A condensed textual representation of the link object."
    ::= { ptMTPGenericLinkEntry 1 }

ptMTPGenericLinkSLC OBJECT-TYPE
    SYNTAX          Integer32 (0..15)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Signaling Link Code for this link. Value of 0-15."
    ::= { ptMTPGenericLinkEntry 3 }

ptMTPGenericLinkDREID OBJECT-TYPE
    SYNTAX          Integer32 (1..64)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "DRE ID of the DRE that owns this link. Since the corresponding DRE process runs on a specific slot, this ID implies the slot where the link terminates locally, and hence the choice of valid local IP addresses for this link."
    ::= { ptMTPGenericLinkEntry 4 }

ptMTPGenericLinkLocalIPAddress1 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Primary local address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned in /etc/hosts and should resolve to a subset of the local IP Addresses configured for the slot where the link terminates. The slot where the link terminates is indirectly implied by the DRE ID."
    ::= { ptMTPGenericLinkEntry 5 }

ptMTPGenericLinkLocalIPAddress2 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Secondary local address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned in /etc/hosts and should resolve to a subset of the local IP Addresses configured for the slot where the link terminates. The slot where the link terminates is indirectly implied by the DRE ID."
    ::= { ptMTPGenericLinkEntry 6 }

ptMTPGenericLinkLocalPort OBJECT-TYPE
    SYNTAX          Integer32 (0..65535)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Local Port."
    ::= { ptMTPGenericLinkEntry 7 }

ptMTPGenericLinkRemoteIPAddress1 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Primary remote address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned in /etc/hosts. This value is used as the SCTP Primary path to the remote SCTP endpoint."
    ::= { ptMTPGenericLinkEntry 8 }

ptMTPGenericLinkRemoteIPAddress2 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Secondary remote address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned in /etc/hosts."
    ::= { ptMTPGenericLinkEntry 9 }

