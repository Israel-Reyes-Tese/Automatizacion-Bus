				"Indicates third DNS server for this XF NE."
			::= { xfDcnIPv6DNSServer 3 }

		-- 1.3.6.1.4.1.193.81.2.8.14
		xfDcnIPv6Ppp OBJECT IDENTIFIER ::= { xfDcnMIB 14 }

		-- 1.3.6.1.4.1.193.81.2.8.14.1
		xfDcnIPv6PppConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfDcnIPv6PppConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table has an entry for each PPP link and
				is indexed by the ifindex. It can be used to
				configure an IPv6 address for the links
				(only numbered interfaces on IPv6), and
				it show the IPv6 address of the far-end.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6Ppp 1 }

		-- 1.3.6.1.4.1.193.81.2.8.14.1.1
		xfDcnIPv6PppConfigEntry OBJECT-TYPE
			SYNTAX XfDcnIPv6PppConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table.
				
				ML66: This object is not supported."
			INDEX { ifIndex }
			::= { xfDcnIPv6PppConfigTable 1 }

		XfDcnIPv6PppConfigEntry ::=
			SEQUENCE {
				xfDcnIPv6PppConfigNumbered
					INTEGER,
				xfDcnIPv6PppConfigIpAddress
					InetAddressIPv6,
				xfDcnIPv6PppConfigFarEnd
					InetAddressIPv6,
				xfDcnIPv6PppLinkUpDownTrapEnable
					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.2.8.14.1.1.1
		xfDcnIPv6PppConfigNumbered OBJECT-TYPE
			SYNTAX INTEGER
				{
				unNumbered(1),
				numbered(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object is used to indicate the PPP link
				to be either unNumbered(1) or numbered(2).
				An IPv6 PPP link is always numbered(2).
				
				ML66: This object is not supported."
			::= { xfDcnIPv6PppConfigEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.8.14.1.1.2
		xfDcnIPv6PppConfigIpAddress OBJECT-TYPE
			SYNTAX InetAddressIPv6
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object specifies the IPv6
				address of the near-end side of the link.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6PppConfigEntry 2 }

		-- 1.3.6.1.4.1.193.81.2.8.14.1.1.3
		xfDcnIPv6PppConfigFarEnd OBJECT-TYPE
			SYNTAX InetAddressIPv6
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the IPv6 address of the
				far-end side of the PPP link.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6PppConfigEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.8.14.1.1.4
		xfDcnIPv6PppLinkUpDownTrapEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				enabled(1),
				disabled(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Indicates whether linkUp/linkDown traps should be generated
				for this interface in case of IPv6 protocol.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6PppConfigEntry 4 }

			-- 1.3.6.1.4.1.193.81.2.8.15
		xfDcnIPv6DHCPRelay OBJECT IDENTIFIER ::= { xfDcnMIB 15 }

		-- 1.3.6.1.4.1.193.81.2.8.15.1
		xfDcnIPv6DHCPRelayAddress OBJECT-TYPE
			SYNTAX XFIpAddressOrName
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"IPv6 Address to the DHCP server this node is a relay for."
			::= { xfDcnIPv6DHCPRelay 1 }

    -- 1.3.6.1.4.1.193.81.2.8.16
    xfDcnIPv6mib-2 OBJECT IDENTIFIER ::= { xfDcnMIB 16 }

        xfDcnIPv6at    OBJECT IDENTIFIER ::= { xfDcnIPv6mib-2 3 }

          xfDcnIPv6atTable OBJECT-TYPE
              SYNTAX SEQUENCE OF XfDcnIPv6AtEntry
              MAX-ACCESS not-accessible
              STATUS current
              DESCRIPTION
                      "The Address Translation tables contain the
                      NetworkAddress to 'physical' address equivalences.
                      Some interfaces do not use translation tables for
                      determining address equivalences (e.g., DDN-X.25
                      has an algorithmic method); if all interfaces are
                      of this type, then the Address Translation table
                      is empty, i.e., has zero entries."
              ::= { xfDcnIPv6at 1 }

          xfDcnIPv6atEntry OBJECT-TYPE
              SYNTAX XfDcnIPv6AtEntry
              MAX-ACCESS not-accessible
              STATUS current
              DESCRIPTION
                      "Each entry contains one NetworkAddress to
                      'physical' address equivalence."
              INDEX   { xfDcnIPv6atIfIndex,
                        xfDcnIPv6atNetAddress }
              ::= { xfDcnIPv6atTable 1 }

          XfDcnIPv6AtEntry ::=
              SEQUENCE {
                  xfDcnIPv6atIfIndex
                      InterfaceIndex,
                  xfDcnIPv6atPhysAddress
                      PhysAddress,
                  xfDcnIPv6atNetAddress
                      InetAddressIPv6
              }

          xfDcnIPv6atIfIndex OBJECT-TYPE
              SYNTAX InterfaceIndex
              MAX-ACCESS read-only
              STATUS current
              DESCRIPTION
                      "The interface on which this entry's equivalence
                      is effective.  The interface identified by a
                      particular value of this index is the same
                      interface as identified by the same value of
                      ifIndex."
              ::= { xfDcnIPv6atEntry 1 }

          xfDcnIPv6atPhysAddress OBJECT-TYPE
              SYNTAX PhysAddress
              MAX-ACCESS read-only
              STATUS current
              DESCRIPTION
                      "The media-dependent 'physical' address.

                      Setting this object to a null string (one of zero
                      length) has the effect of invaliding the
                      corresponding entry in the atTable object.  That
                      is, it effectively dissasociates the interface
                      identified with said entry from the mapping
                      identified with said entry.  It is an
                      implementation-specific matter as to whether the
                      agent removes an invalidated entry from the table.
                      Accordingly, management stations must be prepared
                      to receive tabular information from agents that
                      corresponds to entries not currently in use.
                      Proper interpretation of such entries requires
                      examination of the relevant atPhysAddress object."
              ::= { xfDcnIPv6atEntry 2 }

          xfDcnIPv6atNetAddress OBJECT-TYPE
              SYNTAX InetAddressIPv6
              MAX-ACCESS read-only
              STATUS current
              DESCRIPTION
                      "The InetAddressIPv6 corresponding to the
                      media-dependent 'physical' address."
              ::= { xfDcnIPv6atEntry 3 }

        xfDcnIPv6ip    OBJECT IDENTIFIER ::= { xfDcnIPv6mib-2 4 }

          xfDcnIPv6ipAddressTable OBJECT-TYPE
              SYNTAX SEQUENCE OF XfDcnIPv6IpAddressEntry
              MAX-ACCESS not-accessible
              STATUS current
              DESCRIPTION
                    "This table contains addressing information relevant to the
                     entity's interfaces.
                     
                     This table is similar to ipAddressTable defined by RFC4293,
                     the differences are:
                       - It has xfDcnIPv6ipAddressIfIndex as MIB index, so same IP addresses
                         can be added to the table assuming the xfDcnIPv6ipAddressIfIndex is
                         different (case of Multi-Link PPPs).
                       - It includes xfDcnIPv6ipAddressPrefixPrefix and xfDcnIPv6ipAddressPrefixLength
                         instead of RowPointer type xfDcnIPv6ipAddressPrefix (pointing to
                         ipAddressPrefixTable row). This makes implementation of
                         ipAddressPrefixTable unnecessary.

                     This table does not contain multicast address information.
                     Tables for such information should be contained in multicast
                     specific MIBs, such as RFC 3019.

                     Note well: When including IPv6 link-local addresses in this
                     table, the entry must use an InetAddressType of 'ipv6z' in
                     order to differentiate between the possible interfaces."
              ::= { xfDcnIPv6ip 1 }

          xfDcnIPv6ipAddressEntry OBJECT-TYPE
              SYNTAX XfDcnIPv6IpAddressEntry
              MAX-ACCESS not-accessible
              STATUS current
              DESCRIPTION
                    "An address mapping for a particular interface."
              INDEX   { xfDcnIPv6ipAddressAddrType,
                        xfDcnIPv6ipAddressAddr,
                        xfDcnIPv6ipAddressIfIndex }
              ::= { xfDcnIPv6ipAddressTable 1 }

          XfDcnIPv6IpAddressEntry ::= SEQUENCE {
                  xfDcnIPv6ipAddressAddrType     InetAddressType,
                  xfDcnIPv6ipAddressAddr         InetAddress,
                  xfDcnIPv6ipAddressIfIndex      InterfaceIndex,
                  xfDcnIPv6ipAddressType         INTEGER,
                  xfDcnIPv6ipAddressPrefixPrefix InetAddress,
                  xfDcnIPv6ipAddressPrefixLength InetAddressPrefixLength,
                  xfDcnIPv6ipAddressOrigin       IpAddressOriginTC,
                  xfDcnIPv6ipAddressStatus       IpAddressStatusTC,
                  xfDcnIPv6ipAddressCreated      TimeStamp,
                  xfDcnIPv6ipAddressLastChanged  TimeStamp,
                  xfDcnIPv6ipAddressRowStatus    RowStatus,
                  xfDcnIPv6ipAddressStorageType  StorageType
              }

          xfDcnIPv6ipAddressAddrType OBJECT-TYPE
              SYNTAX     InetAddressType
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The address type of xfDcnIPv6ipAddressAddr."
              ::= { xfDcnIPv6ipAddressEntry 1 }

          xfDcnIPv6ipAddressAddr OBJECT-TYPE
              SYNTAX     InetAddress
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The IP address to which this entry's addressing information
                      pertains. The address type of this object is specified in
                      xfDcnIPv6ipAddressAddrType.

                      Implementors need to be aware that if the size of
                      xfDcnIPv6ipAddressAddr exceeds 116 octets, then OIDS of instances of
                      columns in this row will have more than 128 sub-identifiers
                      and cannot be accessed using SNMPv1, SNMPv2c, or SNMPv3."
              ::= { xfDcnIPv6ipAddressEntry 2 }

          xfDcnIPv6ipAddressIfIndex OBJECT-TYPE
              SYNTAX     InterfaceIndex
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The index value that uniquely identifies the interface to
                      which this entry is applicable.  The interface identified by
                      a particular value of this index is the same interface as
                      identified by the same value of the IF-MIB's ifIndex."
              ::= { xfDcnIPv6ipAddressEntry 3 }

          xfDcnIPv6ipAddressType OBJECT-TYPE
              SYNTAX     INTEGER {
                           unicast(1),
                           anycast(2),
                           broadcast(3)
              }
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The type of address.  broadcast(3) is not a valid value for
                      IPv6 addresses (RFC 3513)."
              DEFVAL { unicast }
              ::= { xfDcnIPv6ipAddressEntry 4 }

          xfDcnIPv6ipAddressPrefixPrefix OBJECT-TYPE
              SYNTAX     InetAddress
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The address prefix.  The address type of this object is
                      specified in xfDcnIPv6ipAddressAddrType.  The length of this object
                      is the standard length for objects of that type (4 or 16
                      bytes).  Any bits after xfDcnIPv6ipAddressPrefixLength must be zero."
              ::= { xfDcnIPv6ipAddressEntry 5 }

          xfDcnIPv6ipAddressPrefixLength OBJECT-TYPE
              SYNTAX     InetAddressPrefixLength
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The prefix length associated with this prefix."
              ::= { xfDcnIPv6ipAddressEntry 6 }

          xfDcnIPv6ipAddressOrigin OBJECT-TYPE
              SYNTAX     IpAddressOriginTC
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The origin of the address."
              ::= { xfDcnIPv6ipAddressEntry 7 }

          xfDcnIPv6ipAddressStatus OBJECT-TYPE
              SYNTAX     IpAddressStatusTC
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The status of the address, describing if the address can be
                      used for communication.

                      In the absence of other information, an IPv4 address is
                      always preferred(1)."
              DEFVAL { preferred }
              ::= { xfDcnIPv6ipAddressEntry 8 }

          xfDcnIPv6ipAddressCreated OBJECT-TYPE
              SYNTAX     TimeStamp
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The value of sysUpTime at the time this entry was created.
                      If this entry was created prior to the last re-
                      initialization of the local network management subsystem,
                      then this object contains a zero value."
              ::= { xfDcnIPv6ipAddressEntry 9 }

          xfDcnIPv6ipAddressLastChanged OBJECT-TYPE
              SYNTAX     TimeStamp
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The value of sysUpTime at the time this entry was last
                      updated.  If this entry was updated prior to the last re-
                      initialization of the local network management subsystem,
                      then this object contains a zero value."
              ::= { xfDcnIPv6ipAddressEntry 10 }

          xfDcnIPv6ipAddressRowStatus OBJECT-TYPE
              SYNTAX     RowStatus
              MAX-ACCESS read-create
              STATUS     current
              DESCRIPTION
                     "The status of this conceptual row.

                      The RowStatus TC requires that this DESCRIPTIONclause
                      states under which circumstances other objects in this row
                      can be modified.  The value of this object has no effect on
                      whether other objects in this conceptual row can be
                      modified.

                      A conceptual row can not be made active until the
                      xfDcnIPv6ipAddressIfIndex has been set to a valid index."
              ::= { xfDcnIPv6ipAddressEntry 11 }

          xfDcnIPv6ipAddressStorageType OBJECT-TYPE
              SYNTAX     StorageType
              MAX-ACCESS read-only
              STATUS     current
              DESCRIPTION
                     "The storage type for this conceptual row.  If this object
                      has a value of 'permanent', then no other objects are
                      required to be able to be modified."
              DEFVAL { volatile }
              ::= { xfDcnIPv6ipAddressEntry 12 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6
        xfDcnIPv6tcp      OBJECT IDENTIFIER ::= { xfDcnIPv6mib-2 6 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.5
          xfDcnIPv6tcpActiveOpens OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The number of times TCP over IPv6 connections have made a
                      direct transition to the SYN-SENT state from the
                      CLOSED state."
              ::= { xfDcnIPv6tcp 5 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.6
		  xfDcnIPv6tcpPassiveOpens OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The number of times TCP over IPv6 connections have made a
                      direct transition to the SYN-RCVD state from the
                      LISTEN state."
              ::= { xfDcnIPv6tcp 6 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.7
          xfDcnIPv6tcpAttemptFails OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The number of times TCP over IPv6 connections have made a
                      direct transition to the CLOSED state from either
                      the SYN-SENT state or the SYN-RCVD state, plus the
                      number of times TCP connections have made a direct
                      transition to the LISTEN state from the SYN-RCVD
                      state."
              ::= { xfDcnIPv6tcp 7 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.8
          xfDcnIPv6tcpEstabResets OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The number of times TCP over IPv6 connections have made a
                      direct transition to the CLOSED state from either
                      the ESTABLISHED state or the CLOSE-WAIT state."
              ::= { xfDcnIPv6tcp 8 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.9
          xfDcnIPv6tcpCurrEstab OBJECT-TYPE
              SYNTAX  Gauge32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The number of TCP over IPv6 connections for which the
                      current state is either ESTABLISHED or CLOSE-
                      WAIT."
              ::= { xfDcnIPv6tcp 9 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.10
          xfDcnIPv6tcpInSegs OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The total number of segments received, including
                      those received in error.  This count includes
                      segments received on currently established
                      connections."
              ::= { xfDcnIPv6tcp 10 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.11
          xfDcnIPv6tcpOutSegs OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The total number of segments sent, including
                      those on current connections but excluding those
                      containing only retransmitted octets."
              ::= { xfDcnIPv6tcp 11 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.12
          xfDcnIPv6tcpRetransSegs OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The total number of segments retransmitted - that
                      is, the number of TCP over IPv6 segments transmitted
                      containing one or more previously transmitted
                      octets."
              ::= { xfDcnIPv6tcp 12 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.14
          xfDcnIPv6tcpInErrs OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The total number of segments received in error
                      (e.g., bad TCP over IPv6 checksums)."
              ::= { xfDcnIPv6tcp 14 }

		-- 1.3.6.1.4.1.193.81.2.8.16.6.15
          xfDcnIPv6tcpOutRsts OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The number of TCP over IPv6 segments sent containing the
                      RST flag."
              ::= { xfDcnIPv6tcp 15 }


        -- 1.3.6.1.4.1.193.81.2.8.16.7
		xfDcnIPv6udp      OBJECT IDENTIFIER ::= { xfDcnIPv6mib-2 7 }

		-- 1.3.6.1.4.1.193.81.2.8.16.7.1
        xfDcnIPv6udpInDatagrams OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The total number of UDP over IPv6 datagrams delivered to
                      UDP users."
              ::= { xfDcnIPv6udp 1 }

		-- 1.3.6.1.4.1.193.81.2.8.16.7.2
		xfDcnIPv6udpNoPorts OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The total number of received UDP UDP over IPv6 datagrams for
                      which there was no application at the destination
                      port."
              ::= { xfDcnIPv6udp 2 }

		-- 1.3.6.1.4.1.193.81.2.8.16.7.3
		xfDcnIPv6udpInErrors OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The number of received UDP UDP over IPv6 datagrams that could
                      not be delivered for reasons other than the lack
                      of an application at the destination port."
              ::= { xfDcnIPv6udp 3 }

		-- 1.3.6.1.4.1.193.81.2.8.16.7.4
		xfDcnIPv6udpOutDatagrams OBJECT-TYPE
              SYNTAX  Counter32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "The total number of UDP UDP over IPv6 datagrams sent from this
                      entity."
              ::= { xfDcnIPv6udp 4 }

	-- 1.3.6.1.4.1.193.81.2.8.17
	xfDcnIPv6Ethernet OBJECT IDENTIFIER ::= { xfDcnMIB 17 }

		-- 1.3.6.1.4.1.193.81.2.8.17.1
		xfDcnIPv6EthernetConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfDcnIPv6EthernetConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in this table is created for each Ethernet
				interface. In this table it is possible to change
				the IPv6 address and prefix length of the interface."
			::= { xfDcnIPv6Ethernet 1 }

		-- 1.3.6.1.4.1.193.81.2.8.17.1.1
		xfDcnIPv6EthernetConfigEntry OBJECT-TYPE
			SYNTAX XfDcnIPv6EthernetConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table."
			INDEX { ifIndex }
			::= { xfDcnIPv6EthernetConfigTable 1 }

		XfDcnIPv6EthernetConfigEntry ::=
			SEQUENCE {
				xfDcnIPv6EthernetConfigIpAddr
					InetAddressIPv6,
				xfDcnIPv6EthernetConfigPrefixLength
					INTEGER,
				xfDcnIPv6EthernetConfigLock
					INTEGER,
				xfDcnIPv6EthernetConfigPort
					INTEGER,
				xfDcnIPv6EthernetConfigRxTx
					INTEGER,
				xfDcnIPv6EthernetConfigPriorityQueues
					INTEGER,
				xfDcnIPv6EthernetConfigAutoNeg
					INTEGER,
				xfDcnIPv6EthernetConfigAlarmStatus
					BITS,
				xfDcnIPv6EthernetConfigVrfName
					DisplayString
			}

		-- 1.3.6.1.4.1.193.81.2.8.17.1.1.1
		xfDcnIPv6EthernetConfigIpAddr OBJECT-TYPE
			SYNTAX InetAddressIPv6
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to set the IPv6 address of
				the interface."
			::= { xfDcnIPv6EthernetConfigEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.8.17.1.1.2
		xfDcnIPv6EthernetConfigPrefixLength OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Prefix length for this ethernet interface."
			::= { xfDcnIPv6EthernetConfigEntry 2 }

		-- 1.3.6.1.4.1.193.81.2.8.17.1.1.3
		xfDcnIPv6EthernetConfigLock OBJECT-TYPE
			SYNTAX INTEGER
				{
				lockToNode(1),
				noLock(2)
				}
			MAX-ACCESS read-write
			STATUS obsolete
			DESCRIPTION
				"This object can be used to indicate whether this ethernet interface
				should have the same IPv6 Address and Prefix Length as the node
				(host address).
				
				If xfDcnIPv6EthernetConfigLock = lockToNode(1), the xfDcnIPv6EthernetConfigIpAddr
				and xfDcnIPv6EthernetConfigPrefixLength is not writable.
				
				Note: This object has been obsoleted, value is always noLock(2).
				"
			::= { xfDcnIPv6EthernetConfigEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.8.17.1.1.4
		xfDcnIPv6EthernetConfigPort OBJECT-TYPE
			SYNTAX INTEGER
				{
				autoDetect(1),
				fullDuplex100(2),
				halfDuplex100(3),
				fullDuplex10(4),
				halfDuplex10(5)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to set the Ethernet speed.
				If xfDcnIPv6EthernetConfigAutoNeg=off(1) setting this object to autoDetect(1)
				will result in a inConsistentValue error response.
				If xfDcnIPv6EthernetConfigPort = autoDetect(1) when xfDcnIPv6EthernetConfigAutoNeg
				is set from on(1) to off(2), xfDcnIPv6EthernetConfigPort will be set to
				fullDuplex100(2).
				"
			::= { xfDcnIPv6EthernetConfigEntry 4 }

		-- 1.3.6.1.4.1.193.81.2.8.17.1.1.5
		xfDcnIPv6EthernetConfigRxTx OBJECT-TYPE
			SYNTAX INTEGER
				{
				auto(1),
				manualNormal(2),
				manualSwitched(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates RX/TX configuration of this Ethernet port.
				
				Valid values:
				auto(1)            : Auto-MDIX
				manualNormal(2)    : MDI
				manualSwitched(3)  : MDIX
				"
			::= { xfDcnIPv6EthernetConfigEntry 5 }

		-- 1.3.6.1.4.1.193.81.2.8.17.1.1.6
		xfDcnIPv6EthernetConfigPriorityQueues OBJECT-TYPE
			SYNTAX INTEGER
				{
				disable(1),
				enable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
