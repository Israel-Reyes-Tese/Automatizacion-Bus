    SEQUENCE {
        sonusSwLoadAdmnIndex            Integer32,
        sonusSwLoadAdmnImageName        DisplayString,
        sonusSwLoadAdmnHwType           ServerTypeID,
        sonusSwLoadAdmnRowStatus        RowStatus
    }

