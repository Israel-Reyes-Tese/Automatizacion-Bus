        ptnpgwNPGWConfigurationExecutionTrace     INTEGER,
        ptnpgwNPGWConfigurationDistributionManagement             INTEGER,
        ptnpgwNPGWConfigurationActions  INTEGER,
        ptnpgwNPGWConfigurationParentOID OBJECT IDENTIFIER,
        ptnpgwNPGWConfigurationRowStatus RowStatus,
        ptnpgwNPGWConfigurationIndex    INTEGER
    }

ptnpgwNPGWConfigurationINAPGateway OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "INAP Gateway feature license (UNLICENSED/LICENSED). \nIf all gateways are unlicensed, the SS7 traffic cannot be activated."
    ::= { ptnpgwNPGWConfigurationEntry 1 }

ptnpgwNPGWConfigurationPSXNPGateway OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "PSX NP Gateway feature license (UNLICENSED/LICENSED). \nIf the gateway is unlicensed, the SS7 traffic cannot be activated."
    ::= { ptnpgwNPGWConfigurationEntry 2 }

ptnpgwNPGWConfigurationMTP3TrafficStatus OBJECT-TYPE
    SYNTAX          INTEGER { inactive(0), notConnected(1), connected(2), 
                        partiallyConnected(3) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "MTP3 connectivity status (INACTIVE, NOT CONNECTED, CONNECTED). \nBased on the per NA connectivity status. CONNECTED will be displayed as soon as an MTP3 NA on any slot is connected. \nAssociated to alarm 1617 (NPGW MTP3 Down)."
    ::= { ptnpgwNPGWConfigurationEntry 3 }

ptnpgwNPGWConfigurationSCCPTrafficStatus OBJECT-TYPE
    SYNTAX          INTEGER { inactive(0), notConnected(1), connected(2), 
                        partiallyConnected(3) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "SCCP connectivity status (INACTIVE, NOT CONNECTED, CONNECTED). \nBased on the per NA connectivity status. CONNECTED will be displayed as soon as an SCCP NA on any slot is connected. \nAssociated to alarm 1601 (NPGW SCCP Down)."
    ::= { ptnpgwNPGWConfigurationEntry 4 }

ptnpgwNPGWConfigurationINAPGatewayE164 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "INAP Gateway E.164 number. \nMust be unique on each mate-STP. \nThe generated INAP Initial-DP query will include these digits in the SCCP Calling Party Address parameter. \nMust be defined to activate traffic."
    ::= { ptnpgwNPGWConfigurationEntry 5 }

ptnpgwNPGWConfigurationINAPGatewayTimeout OBJECT-TYPE
    SYNTAX          INTEGER (1000..10000)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "INAP Gateway Transaction Timeout (1000 to 10000). \nPending transaction timeout for the INAP queries in milliseconds (ms). \nDefault: 2000 ms."
    ::= { ptnpgwNPGWConfigurationEntry 6 }

ptnpgwNPGWConfigurationTransactionTimeout OBJECT-TYPE
    SYNTAX          INTEGER (100..5000)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "PSX NP Gateway Transaction Timeout (100 to 5000). \nPending transaction timeout for the PSX NP service queries in milliseconds (ms). \nNote: A fallback lifetime timeout will also be initialized based on the Transaction Timeout plus 500 ms. \nDefault: 1000 ms."
    ::= { ptnpgwNPGWConfigurationEntry 7 }

