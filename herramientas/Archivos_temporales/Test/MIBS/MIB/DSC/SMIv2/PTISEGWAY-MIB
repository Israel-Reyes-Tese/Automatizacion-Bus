    SYNTAX          PtDSCIMSIMobileCodeEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of DSCIMSIMobileCode attributes."
    INDEX           { ptConnGroupIndex, ptDSCIMSIMobileCodeDSCIndex, 
                      ptDSCIMSIMobileCodeDSCDefinitionsIndex, 
                      ptDSCIMSIMobileCodeMobileCodesDefinitionsIndex, 
                      ptDSCIMSIMobileCodeIndex }
    ::= { ptDSCIMSIMobileCodeTable 1 }

PtDSCIMSIMobileCodeEntry ::=
    SEQUENCE {
        ptDSCIMSIMobileCodeMCCMNC       DisplayString,
        ptDSCIMSIMobileCodeDSCIndex     Integer32,
        ptDSCIMSIMobileCodeDSCDefinitionsIndex      Integer32,
        ptDSCIMSIMobileCodeMobileCodesDefinitionsIndex              Integer32,
        ptDSCIMSIMobileCodeParentOID    OBJECT IDENTIFIER,
        ptDSCIMSIMobileCodeRowStatus    RowStatus,
        ptDSCIMSIMobileCodeIndex        Integer32
    }

ptDSCIMSIMobileCodeMCCMNC OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Mobile Country Code/Mobile Network Code combination \nA 3 digits MCCMNC value defines and groups all MCC entries that have only 3 digits MNC values. \nA 6 digits MCCMNC value defines MCC MNC entieis that have both 2 and 3 digits MNC values by \nincluding the exact 3 digits MNC value. \nBy default, an MCCMNC entry that does not exist in this table is assumed to have 2 digits MNC values. \nExample: 123456 (MCC is 132 and MNC is 456, other IMSI values that start with 123 have 2 digits MNC). \n789 (all IMSI values that start with 789 have 3 digits MNCs). \nValid values: 3 digits or 6 digits integer."
    ::= { ptDSCIMSIMobileCodeEntry 1 }

ptDSCIMSIMobileCodeDSCIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSC."
    ::= { ptDSCIMSIMobileCodeEntry 242 }

ptDSCIMSIMobileCodeDSCDefinitionsIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSCDefinitions."
    ::= { ptDSCIMSIMobileCodeEntry 243 }

ptDSCIMSIMobileCodeMobileCodesDefinitionsIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is MobileCodesDefinitions."
    ::= { ptDSCIMSIMobileCodeEntry 244 }

ptDSCIMSIMobileCodeParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptDSCIMSIMobileCodeEntry 253 }

