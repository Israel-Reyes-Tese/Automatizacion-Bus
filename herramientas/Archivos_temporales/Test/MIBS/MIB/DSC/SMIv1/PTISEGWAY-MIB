        ptGTTGTSearchUtilityOPC         INTEGER,
        ptGTTGTSearchUtilityCalledSearchRecord      DisplayString,
        ptGTTGTSearchUtilityCallingSearchRecord       DisplayString,
        ptGTTGTSearchUtilityOPCSearchRecord   DisplayString,
        ptGTTGTSearchUtilityDiagnosticMessage     DisplayString,
        ptGTTGTSearchUtilityAppliedCalledGTModName          DisplayString,
        ptGTTGTSearchUtilityAppliedCallingGTModName           DisplayString,
        ptGTTGTSearchUtilityPCListResult DisplayString,
        ptGTTGTSearchUtilityCalledSSN   INTEGER,
        ptGTTGTSearchUtilityCallingSSN  INTEGER,
        ptGTTGTSearchUtilityGTTDatabaseIndex    INTEGER,
        ptGTTGTSearchUtilityActions     INTEGER,
        ptGTTGTSearchUtilityParentOID   OBJECT IDENTIFIER,
        ptGTTGTSearchUtilityRowStatus   RowStatus,
        ptGTTGTSearchUtilityIndex       INTEGER
    }

