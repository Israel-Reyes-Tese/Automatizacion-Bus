sonusDsx3MIBNotifications OBJECT IDENTIFIER
    ::= { sonusDsx3MIB 2 }

sonusDsx3MIBNotificationsPrefix OBJECT IDENTIFIER
    ::= { sonusDsx3MIBNotifications 0 }

sonusDsx3MIBNotificationsObjects OBJECT IDENTIFIER
    ::= { sonusDsx3MIBNotifications 1 }

sonusDs3OutOfServiceReason OBJECT-TYPE
    SYNTAX          INTEGER { maintenance(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The reason for DS3 going out of service"
    ::= { sonusDsx3MIBNotificationsObjects 1 }

sonusDs3OutOfServiceType OBJECT-TYPE
    SYNTAX          INTEGER { admin(1), hwFailure(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type of operation which caused DS3 to go out of service"
    ::= { sonusDsx3MIBNotificationsObjects 2 }

sonusDs3ThresholdType OBJECT-TYPE
    SYNTAX          INTEGER { currentLineCodeViolation(1), 
                        currentPbitParity(2), currentCbitParity(3), 
                        currentSeverelyErroredFramingSecs(4), 
                        currentLineErrorSeconds(5), 
                        currentPbitParityErroredSeconds(6), 
                        currentCbitParityErroredSeconds(7), 
                        currentPbitSeverelyErroredSeconds(8), 
                        currentLineSeverelyErroredSeconds(9), 
                        currentUnavailableSeconds(10), 
                        totalLineCodeViolation(11), totalPbitParity(12), 
                        totalCbitParity(13), 
                        totalSeverelyErroredFramingSecs(14), 
                        totalLineErrorSeconds(15), 
                        totalPbitParityErroredSeconds(16), 
                        totalCbitParityErroredSeconds(17), 
                        totalPbitSeverelyErroredSeconds(18), 
                        totalLineSeverelyErroredSeconds(19), 
                        totalUnavailableSeconds(20), 
                        currentCbitSeverelyErroredSeconds(21), 
                        currentPbitUnavailableSeconds(22), 
                        currentCbitUnavailableSeconds(23), 
                        totalCbitSeverelyErroredSeconds(24), 
                        totalPbitUnavailableSeconds(25), 
                        totalCbitUnavailableSeconds(26), 
                        currentFarEndCbitParity(27), 
                        currentFarEndCbitParityErroredSeconds(28), 
                        currentFarEndCbitSeverelyErroredSeconds(29), 
                        currentFarEndCbitParityUnavailableSeconds(30), 
                        totalFarEndCbitParity(31), 
                        totalFarEndCbitParityErroredSeconds(32), 
                        totalFarEndCbitSeverelyErroredSeconds(33), 
                        totalFarEndCbitParityUnavailableSeconds(34) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Type of threshold that was crossed which caused a threshold crossing trap"
    ::= { sonusDsx3MIBNotificationsObjects 3 }

sonusDs3LineState OBJECT-TYPE
    SYNTAX          INTEGER { green(1), red(2), rai(3), ais(4), 
                        hwfailure(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type of Alarm or failure which caused DS3 to go out of service"
    ::= { sonusDsx3MIBNotificationsObjects 4 }

sonusDs3AdmnState OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2), deleted(3), 
                        inservice(4), dryingUp(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The administrative state of the DS3."
    ::= { sonusDsx3MIBNotificationsObjects 5 }

sonusDsx3AdminChangeNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs3AdmnState, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "A sonusDsx3AdminChangeNotification trap indicates the DS3 administration 
             changes."
    ::= { sonusDsx3MIBNotificationsPrefix 1 }

sonusDsx3OutOfServiceNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs3OutOfServiceReason, 
                      sonusDs3OutOfServiceType, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "A sonusDsx3OutOfServiceNotification trap indicates that a DS3
             went out of service. The reason indicates the reason why and
             type indicates the type of failure."
    ::= { sonusDsx3MIBNotificationsPrefix 2 }

sonusDsx3LineStatusChangeNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs3LineState, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "A sonusDsx3LineStatusChangeNotification trap indicates the DS3 state changes. 
             It is used to notify DS3 Alarms states"
    ::= { sonusDsx3MIBNotificationsPrefix 4 }

END -- end of module SONUS-DS3-MIB.
