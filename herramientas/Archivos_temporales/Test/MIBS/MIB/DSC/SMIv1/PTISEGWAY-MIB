    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Link Name based on the Linkset Name."
    ::= { ptMTPM2PALinkEntry 44 }

ptMTPM2PALinkGR310VirtualLink OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Indicates whether the M2PA Link represents GR-310-CORE M2PA virtual link (ENABLED/DISABLED).\n This attribute is only used for GR-310-CORE provisioning and is not relevant for non SNAMI licensed deployments. There is no impact on the CMU and TL1 provisioning interfaces.\n The GR-310-CORE provisioning model splits the creation of M2PA links into two entities, an M2PA virtual link (M2PAVLK) and a signaling link (SLK), where the M2PAVLK must be created first. In this implementation, the M2PAVLK parameters are stored in the M2PA Link object. In order to support the GR-310-CORE provisioning model, this attribute indicates whether this M2PA Link object should be treated as a full SLK with an associated M2PAVLK, or as simply an M2PAVLK. If this attribute is set to ENABLED then this object is not considered a full link from GR-310-CORE prespective.\n Default value: DISABLED."
    ::= { ptMTPM2PALinkEntry 45 }

ptMTPM2PALinkTxMSUCount OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Transmitted MSU count, per second."
    ::= { ptMTPM2PALinkEntry 46 }

ptMTPM2PALinkAlarms OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Alarms affect the alarms associated with the Link. DISABLED mode: Alarms will not be raised. ENABLED mode: Alarms will be raised. \nValid values: ENABLED and DISABLED. \nDefault value: ENABLED."
    ::= { ptMTPM2PALinkEntry 47 }

ptMTPM2PALinkSendINIT OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "When ENABLED, the local endpoint sends SCTP INIT to the remote endpoint. \n When DISABLED, the local endpoint does NOT send SCTP INIT to the remote endpoint. \n This attribute can be modified only when the SCTP association is down."
    ::= { ptMTPM2PALinkEntry 48 }

ptMTPM2PALinkRxMSUCount OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Received MSU count, per second."
    ::= { ptMTPM2PALinkEntry 49 }

ptMTPM2PALinkMTP3NAManagerIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is MTP3NAManager."
    ::= { ptMTPM2PALinkEntry 242 }

