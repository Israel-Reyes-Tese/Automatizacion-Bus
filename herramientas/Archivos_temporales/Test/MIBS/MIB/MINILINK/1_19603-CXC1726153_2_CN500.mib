			DESCRIPTION
				"Ethernet entry"
				
			INDEX{     ifIndex    }
			::= { xfCNEthernetTable 1 }

		
		XfCNEthernetEntry ::=
			SEQUENCE {
				xfCNVlanId  				Integer32,
				xfCNEthernetLoop 		INTEGER,
				xfCNVlanTPId				INTEGER,
				xfCNVlanPcp					Integer32,
				xfCNIpDscp					Integer32				
			}

		-- 1.3.6.1.4.1.193.81.7.1.1.4.1.1.1
		xfCNVlanId OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"VLAN ID for the in-band management.
				A number between 0 - 4094"
			::= { xfCNEthernetEntry 1 }

		-- 1.3.6.1.4.1.193.81.7.1.1.4.1.1.2
		xfCNEthernetLoop OBJECT-TYPE
			SYNTAX INTEGER {
					other (1),
					enable (2),
					disable (3)          
      }
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Ethernet loop"
			DEFVAL { disable }
			::= { xfCNEthernetEntry 2 }      

    -- 1.3.6.1.4.1.193.81.7.1.1.4.1.1.3
    	xfCNVlanTPId OBJECT-TYPE
			SYNTAX INTEGER {
					other (1),
					etherType0x8100 (2),
					etherType0x88a8 (3)          
      }	
      MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"VLAN Tag Protocol Identifier for the in-band management over LAN and WAN"
			DEFVAL { etherType0x8100 }
			::= { xfCNEthernetEntry 3 }			

    -- 1.3.6.1.4.1.193.81.7.1.1.4.1.1.4
    	xfCNVlanPcp OBJECT-TYPE
			SYNTAX Integer32 
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"
					Priority Code Point
					It indicates the frame priority level. Values are from 0 (best effort) to 7 (highest); User Priority 1 represents the lowest value in IEEE 802.1 Q and therefore shall be send to TC0.
					PCP	Network priority	Traffic characteristics
					1          	0        	(lowest)	Background 
					0          	1        	Best Effort
					2          	2        	Excellent Effort (Default Value)
					3          	3        	Critical Applications
					4          	4        	Video, < 100 ms latency
					5          	5        	Voice, < 10 ms latency
					6          	6        	Internetwork Control
					7          	7        	(highest)	Network Control
				"
			DEFVAL { 2 }
			::= { xfCNEthernetEntry 4 }

    -- 1.3.6.1.4.1.193.81.7.1.1.4.1.1.5
    	xfCNIpDscp OBJECT-TYPE
			SYNTAX Integer32 	
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"
				Differentiated Services Code Point (DSCP) field in the header of DCN IP packets coming from the internal management port used for packet classification purposes northbound from the PT1.
				DSCP allowed values between 0-63.
				"
			DEFVAL { 0 }	
			::= { xfCNEthernetEntry 5 }			
			

