ptSGM3UAASPAssociationTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtSGM3UAASPAssociationEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptSGM3UAASPAssociation 1 }

ptSGM3UAASPAssociationEntry OBJECT-TYPE
    SYNTAX          PtSGM3UAASPAssociationEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of SGM3UAASPAssociation attributes."
    INDEX           { ptConnGroupIndex, ptSGM3UAASPAssociationM3UAIndex, 
                      ptSGM3UAASPAssociationM3UAConfiguredASPsIndex, 
                      ptSGM3UAASPAssociationM3UAMultiAssociationASPIndex, 
                      ptSGM3UAASPAssociationIndex }
    ::= { ptSGM3UAASPAssociationTable 1 }

PtSGM3UAASPAssociationEntry ::=
    SEQUENCE {
        ptSGM3UAASPAssociationAssociationID   Integer32,
        ptSGM3UAASPAssociationSlot      Integer32,
        ptSGM3UAASPAssociationRemoteIPAddress1      DisplayString,
        ptSGM3UAASPAssociationRemoteIPAddress2      DisplayString,
        ptSGM3UAASPAssociationRemotePort Integer32,
        ptSGM3UAASPAssociationLocalIPAddress1     DisplayString,
        ptSGM3UAASPAssociationLocalIPAddress2     DisplayString,
        ptSGM3UAASPAssociationLocalPort Integer32,
        ptSGM3UAASPAssociationUserActivated   INTEGER,
        ptSGM3UAASPAssociationSCTPState INTEGER,
        ptSGM3UAASPAssociationM3UAState INTEGER,
        ptSGM3UAASPAssociationRxMSGCount Integer32,
        ptSGM3UAASPAssociationTxMSGCount Integer32,
        ptSGM3UAASPAssociationM3UAIndex Integer32,
        ptSGM3UAASPAssociationM3UAConfiguredASPsIndex             Integer32,
        ptSGM3UAASPAssociationM3UAMultiAssociationASPIndex                  Integer32,
        ptSGM3UAASPAssociationActions   INTEGER,
        ptSGM3UAASPAssociationParentOID OBJECT IDENTIFIER,
        ptSGM3UAASPAssociationRowStatus RowStatus,
        ptSGM3UAASPAssociationIndex     Integer32
    }

