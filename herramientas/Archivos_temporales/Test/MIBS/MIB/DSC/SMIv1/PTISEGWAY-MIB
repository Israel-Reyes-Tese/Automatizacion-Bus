ptGWSTGWSSearchUtilityMessageType OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Message type of the MSU based on SI (0 to 255). \n For SI 0, set to the H1H0 value (Decimal). \n For SI 3, set to the SCCP message type: 9 (UDT), 10 (UDTS), 17 (XUDT) or 18 (XUDTS). \n For SI 4, set to the TUP message type: 17 (IAM), 49 (SAM), etc... \n For SI 5, set to the ISUP message type: 1 (IAM), 2 (SAM), etc..."
    ::= { ptGWSTGWSSearchUtilityEntry 7 }

ptGWSTGWSSearchUtilityAffectedPC OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Affected Point Code of the MSU (optional). \n For SI != 0, set to UNSET as not applicable. \n For SI = 0, applicable to only certain H1H0 message types else set to UNSET."
    ::= { ptGWSTGWSSearchUtilityEntry 8 }

ptGWSTGWSSearchUtilitySCCPClass OBJECT-TYPE
    SYNTAX          INTEGER { class0(0), class1(1), class2(2), class3(3), 
                        notPresent(4), none(5) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Protocol Class of the MSU (optional). \n Set to CLASS 0, CLASS 1, CLASS 2, CLASS 3, NOT PRESENT as required else set to NONE when not applicable. \n Only applicable and mandatory for SI 3."
    ::= { ptGWSTGWSSearchUtilityEntry 9 }

