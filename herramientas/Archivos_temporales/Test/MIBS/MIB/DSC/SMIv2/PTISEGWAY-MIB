    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptGWSTGWSSearchUtility 1 }

ptGWSTGWSSearchUtilityEntry OBJECT-TYPE
    SYNTAX          PtGWSTGWSSearchUtilityEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of GWSTGWSSearchUtility attributes."
    INDEX           { ptConnGroupIndex, 
                      ptGWSTGWSSearchUtilityGWSTTablesIndex, 
                      ptGWSTGWSSearchUtilityIndex }
