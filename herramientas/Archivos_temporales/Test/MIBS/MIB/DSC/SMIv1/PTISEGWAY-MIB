    SEQUENCE {
        ptIOHWSerialChannelIOManagerIndex INTEGER,
        ptIOHWSerialChannelSerialChannelsIndex      INTEGER,
        ptIOHWSerialChannelParentOID    OBJECT IDENTIFIER,
        ptIOHWSerialChannelRowStatus    RowStatus,
        ptIOHWSerialChannelIndex        INTEGER
    }

