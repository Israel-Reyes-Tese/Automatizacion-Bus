        -- 1.3.6.1.4.1.193.81.4.1.2.1.17.1
        xfLldpPortConfigTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfLldpPortConfigEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table extends lldpV2PortConfigTableV2 to allow dynamic
                creation and deletion of LLDP agents on a particular LAN/WAN interface
                and destination MAC address. The NE has no rows in this table by default.
                When creating a new row in this table, rows are also created in
                corresponding LLDP-V2-MIB tables automatically.

                This table adds per port and destination MAC address configuration
                in addition to those found in lldpV2PortConfigTableV2.
                "
            ::= { xfLldp 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.17.1.1
        xfLldpPortConfigEntry OBJECT-TYPE
            SYNTAX XfLldpPortConfigEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Entry for an LLDP agent."
            AUGMENTS {
                lldpV2PortConfigEntryV2
            }
            ::= { xfLldpPortConfigTable 1 }

        XfLldpPortConfigEntry ::=
            SEQUENCE {
                xfLldpPortRowStatus            RowStatus,
                xfLldpPortTLVsTxEnable         BITS,
                xfLldpPortClearCounters        INTEGER
            }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.17.1.1.1
        xfLldpPortRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Used to create or destroy an LLDP agent for the port and
                destination MAC address specified in the index of this entry."
            ::= { xfLldpPortConfigEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.17.1.1.2
        xfLldpPortTLVsTxEnable OBJECT-TYPE
            SYNTAX BITS {
                manAddrIPv4(0),
                manAddrIPv6(1)
            }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "The xfLldpPortTLVsTxEnable, defined as a bitmap, includes
                the set of LLDP TLVs whose transmission is allowed on the
                local LLDP agent by the network management in addition to the
                basic optional TLVs specified in lldpV2PortConfigTLVsTxEnableV2.

                Each bit in the bitmap corresponds to a TLV type associated
                with a specific optional TLV. Organizationally defined TLVs
                can be added in the future.

                The bits 'manAddrIPv4(0)' and 'manAddrIPv6(1)' indicate that
                the LLDP agent should transmit 'Management Address TLV' with
                the IPv4 and/or the IPv6 management address of the NE.
                "
            DEFVAL  { { manAddrIPv4 } }
            ::= { xfLldpPortConfigEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.17.1.1.3
        xfLldpPortClearCounters OBJECT-TYPE
            SYNTAX INTEGER { lldpPortClearCounters(1) }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Set this object to clear the LLDP counters in lldpV2StatsTxPortTable
                 and lldpV2StatsRxPortTable belonging to this LLDP agent."
            ::= { xfLldpPortConfigEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.17.2
        xfLldpNotificationEnable OBJECT-TYPE
            SYNTAX INTEGER {
                enable(1),
                disable(2)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object controls, on a per NE basis, whether or not the NE
                sends event notifications to the NMS when the LLDP remote system data
                on any of the LLDP enabled ports is changing. The notification rate
                is limited by the value of lldpV2NotificationInterval."
            DEFVAL { disable }
            ::= { xfLldp 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.17.3
        xfLldpClearCounters OBJECT-TYPE
            SYNTAX InterfaceIndexOrZero
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Set this object to clear LLDP counters on a specific LAN/WAN interface.
                Setting value zero clears LLDP counters on all interfaces and the global LLDP counters."
            ::= { xfLldp 3 }

