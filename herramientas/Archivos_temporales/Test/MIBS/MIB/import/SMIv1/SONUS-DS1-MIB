    SEQUENCE {
        sonusDsx1IntervalShelfIndex     SonusShelfIndex,
        sonusDsx1IntervalSlotIndex      SonusSlotIndex,
        sonusDsx1IntervalPortIndex      INTEGER,
        sonusDsx1IntervalDs1Index       INTEGER,
        sonusDsx1IntervalNumber         INTEGER,
        sonusDsx1IntervalLCVs           PerfCurrentCount,
        sonusDsx1IntervalLESs           PerfCurrentCount,
        sonusDsx1IntervalLSESs          PerfCurrentCount,
        sonusDsx1IntervalPCVs           PerfCurrentCount,
        sonusDsx1IntervalPESs           PerfCurrentCount,
        sonusDsx1IntervalPSESs          PerfCurrentCount,
        sonusDsx1IntervalSASs           PerfCurrentCount,
        sonusDsx1IntervalCSSs           PerfCurrentCount,
        sonusDsx1IntervalUASs           PerfCurrentCount,
        sonusDsx1IntervalLineIDF        TruthValue,
        sonusDsx1IntervalPathIDF        TruthValue,
        sonusDsx1IntervalFC             INTEGER
    }

