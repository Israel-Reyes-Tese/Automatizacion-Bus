    }

ptDSCResultTableTableName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Table Name to uniquely identify the table. \nThe Table Name must be unique across all table types. \nLeading and trailing whitespaces are trimmed. \nValid values: alphanumeric characters, dashes, underscores, spaces, and dots (maximum size of 64). Cannot start the name with reserved dsc_ or pt_ (case insensitive)."
    ::= { ptDSCResultTableEntry 1 }

ptDSCResultTableDSCNodeID OBJECT-TYPE
    SYNTAX          INTEGER { any(0) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Associated DSC Node Internal ID. \nObsolete as of release 14.3. \nValid values: existing DSC Node Internal ID."
    ::= { ptDSCResultTableEntry 2 }

ptDSCResultTableDSCNodeName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Associated DSC Node name. \nValid values: existing DSC Node name."
    ::= { ptDSCResultTableEntry 3 }

ptDSCResultTableLoadBalancing OBJECT-TYPE
    SYNTAX          INTEGER { roundRobinWeight(0), leastLoaded(1), 
                        roundRobinCost(2), hash(3) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Load Balancing \nWhen Round Robin Cost is selected then the lowest cost record is chosen as the routing destination. \nIf more than one record has the same cost then messages are \nload shared in a round robin manner across the available destinations. \n Valid costs are values 1 to 99. \nWhen Round Robin Weight is selected, the weights represent the ratio of messages delivered to each destination. \nFor example: Candidate A has weight = 10, B = 20 and C = 5. \nIn a group of messages, the first 10 messages are sent to A, then the next 20 to B and the next 5 to C. The process is then restarted at A for the next messages. \n With Hash based the routing candidate is selected by hashing a defined AVP value in the message. \nWhen Least Loaded is selected, the weights indicate the relative message capacity of each server (in messages per second). \nDefault Value: ROUND ROBIN WEIGHT."
    ::= { ptDSCResultTableEntry 4 }

ptDSCResultTableAVPAlias OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The name of an AVP Alias. This AVP is used by the hashing algorithm to perform traffic load balancing. \nValid values: Must be an AVP Alias which has been created in the DSC AVP Alias Definitions tables. If the AVP is not present in the message, then the hop-by-hop header value is used."
    ::= { ptDSCResultTableEntry 5 }

ptDSCResultTableDSCIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSC."
    ::= { ptDSCResultTableEntry 242 }

ptDSCResultTableDSCRoutingTablesIndex OBJECT-TYPE
