    ::= { sonusSipSignallingMIBObjects 13 }

sonusCpcToSipCauseMappingProfile OBJECT IDENTIFIER
    ::= { sonusCpcToSipCauseMappingObject 1 }

sonusCpcToSipCauseMappingProfileNextIndex OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The next valid index to use when creating a new sonusCpcToSipCauseMappingProfileTable"
    ::= { sonusCpcToSipCauseMappingProfile 1 }

sonusCpcToSipCauseMappingProfileTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusCpcToSipCauseMappingProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains information about each CPC to SIP cause Profile
             configured to be a member of the node."
    ::= { sonusCpcToSipCauseMappingProfile 2 }

sonusCpcToSipCauseMappingProfileEntry OBJECT-TYPE
    SYNTAX          SonusCpcToSipCauseMappingProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table describes the SIP-to-CPC cause mapping that are configured."
    INDEX           { sonusCpcToSipCauseMappingProfileIndex }
    ::= { sonusCpcToSipCauseMappingProfileTable 1 }

SonusCpcToSipCauseMappingProfileEntry ::=
    SEQUENCE {
        sonusCpcToSipCauseMappingProfileIndex     INTEGER,
        sonusCpcToSipCauseMappingProfileName    SonusName,
        sonusCpcToSipCauseMappingBaseProfile    INTEGER,
        sonusCpcToSipCauseMappingProfileRowStatus         RowStatus,
        sonusCpcToSipCauseMappingIndexProfileVersion            INTEGER,
        sonusCpcToSipCauseMappingProfileUnrecAction           INTEGER,
        sonusCpcToSipCauseMappingProfileIncludeQ850Reason                 SonusAdminState
    }

sonusCpcToSipCauseMappingProfileIndex OBJECT-TYPE
    SYNTAX          INTEGER (1..128)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the SIP-To-CPC cause mapping Profile."
    ::= { sonusCpcToSipCauseMappingProfileEntry 1 }

sonusCpcToSipCauseMappingProfileName OBJECT-TYPE
    SYNTAX          SonusName
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The user name of the CpcToSipCauseMapping Profile."
    ::= { sonusCpcToSipCauseMappingProfileEntry 2 }

sonusCpcToSipCauseMappingBaseProfile OBJECT-TYPE
    SYNTAX          INTEGER { defaultCpcSip(1), defaultQ1912CpcSip(2), 
                        defaultRfc3398CpcSip(3), defaultTs29163CpcSip(4) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The name of the CPCtoSIP mapping profile on which the new profile is based."
    ::= { sonusCpcToSipCauseMappingProfileEntry 3 }

sonusCpcToSipCauseMappingProfileRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            ""
    ::= { sonusCpcToSipCauseMappingProfileEntry 4 }

sonusCpcToSipCauseMappingIndexProfileVersion OBJECT-TYPE
    SYNTAX          INTEGER (1..64)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the CPC-to-SIP cause mapping Profile Index Version."
    ::= { sonusCpcToSipCauseMappingProfileEntry 5 }

sonusCpcToSipCauseMappingProfileUnrecAction OBJECT-TYPE
    SYNTAX          INTEGER { q1912Procedure(1), rfc3398Procedure(2), 
                        ts29163Procedure(3) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The cause procedure to be used when an unrecognised cause is received"
    ::= { sonusCpcToSipCauseMappingProfileEntry 6 }

sonusCpcToSipCauseMappingProfileIncludeQ850Reason OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field indicates if to include Q.850 value in Reason header of SIP messages."
    DEFVAL          { disabled }
    ::= { sonusCpcToSipCauseMappingProfileEntry 7 }

