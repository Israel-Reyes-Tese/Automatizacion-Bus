SonusSipRegActRegNameStatusEntry ::=
    SEQUENCE {
        sonusSipRegActRegNameStatusShelfIndex     SonusShelfIndex,
        sonusSipRegActRegNameStatusSlotIndex    SonusSlotIndex,
        sonusSipRegActRegNameStatusRegId INTEGER,
        sonusSipRegActRegNameStatusAorName  DisplayString,
        sonusSipRegActRegNameStatusState INTEGER,
        sonusSipRegActRegNameStatusContactURI     DisplayString,
        sonusSipRegActRegNameStatusNextHopIpAddress           IpAddress,
        sonusSipRegActRegNameStatusNextHopPortNum         INTEGER,
        sonusSipRegActRegNameStatusRegistrarIpAddress             IpAddress,
        sonusSipRegActRegNameStatusRegistrarPortNum           INTEGER,
        sonusSipRegActRegNameStatusExtExpirationTime            INTEGER,
        sonusSipRegActRegNameStatusIntExpirationTime            INTEGER,
        sonusSipRegActRegNameStatusCreationTime       DateAndTime,
        sonusSipRegActRegNameStatusRegistrarDomainName              DisplayString,
        sonusSipRegActRegNameStatusNextHopIp6Address            Ipv6Address,
        sonusSipRegActRegNameStatusRegistrarIp6Address              Ipv6Address,
        sonusSipRegActRegNameStatusNextHopIpVersion           INTEGER,
        sonusSipRegActRegNameStatusRegistrarIpVersion             INTEGER
    }

