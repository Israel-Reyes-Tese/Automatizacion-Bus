siFsMonObjects OBJECT IDENTIFIER
    ::= { siFsMonitor 1 }

siFsMonGlobalPollInterval OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The period, in seconds, when the filesystems should be 
             checked to see if any exceed the threshold.  A value of
             0 indicates not to poll based on this value."
    ::= { siFsMonObjects 1 }

siFsMonGlobalPollNow OBJECT-TYPE
    SYNTAX          TruthValue
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Setting this object to true(1) will cause the filesystems 
             to be checked to see if they exceed a threshold. The primary
             purpose of this variable is for use with the disman schedule
             MIB."
    ::= { siFsMonObjects 2 }

siFsMonitorTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SiFsMonitorEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A table to configure which filesystems are to be monitored."
    ::= { siFsMonObjects 10 }

siFsMonitorEntry OBJECT-TYPE
    SYNTAX          SiFsMonitorEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "An entry in the siFsMonitorTable."
    INDEX           { siFsMonIndex }
    ::= { siFsMonitorTable 1 }

