siFsMonNotifications OBJECT IDENTIFIER
    ::= { siFsMonitor 2 }

siFsAboveWarningThreshold TRAP-TYPE
    ENTERPRISE      siFsMonNotifications
    VARIABLES       { siFsMonName, siFsMonThreshold, 
                      siFsMonCurrentUtilization }
--     STATUS          mandatory
    DESCRIPTION     
            "A trap indicating that the threshold was exceeded in a
             row where the severity is set to warning."
    ::= 1

siFsAboveCriticalThreshold TRAP-TYPE
    ENTERPRISE      siFsMonNotifications
    VARIABLES       { siFsMonName, siFsMonThreshold, 
                      siFsMonCurrentUtilization }
--     STATUS          mandatory
    DESCRIPTION     
            "A trap indicating that the threshold was exceeded in a
