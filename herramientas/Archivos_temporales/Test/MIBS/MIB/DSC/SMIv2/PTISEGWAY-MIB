pthwmonHardwareMonitorEntry OBJECT-TYPE
    SYNTAX          PthwmonHardwareMonitorEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of hwmonHardwareMonitor attributes."
    INDEX           { ptConnGroupIndex, pthwmonHardwareMonitorIndex }
    ::= { pthwmonHardwareMonitorTable 1 }

PthwmonHardwareMonitorEntry ::=
    SEQUENCE {
        pthwmonHardwareMonitorStatus    DisplayString,
        pthwmonHardwareMonitorDebugLogging  INTEGER,
        pthwmonHardwareMonitorMinorTelcoAlarm     INTEGER,
        pthwmonHardwareMonitorMajorTelcoAlarm     INTEGER,
        pthwmonHardwareMonitorCriticalTelcoAlarm        INTEGER,
        pthwmonHardwareMonitorActiveMCH DisplayString,
        pthwmonHardwareMonitorHardwareMonitorSlot         DisplayString,
        pthwmonHardwareMonitorActions   INTEGER,
        pthwmonHardwareMonitorParentOID OBJECT IDENTIFIER,
        pthwmonHardwareMonitorRowStatus RowStatus,
        pthwmonHardwareMonitorIndex     Integer32
    }

pthwmonHardwareMonitorStatus OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Hardware monitor status."
    ::= { pthwmonHardwareMonitorEntry 1 }

pthwmonHardwareMonitorDebugLogging OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable/disable debug logging."
    ::= { pthwmonHardwareMonitorEntry 2 }

pthwmonHardwareMonitorMinorTelcoAlarm OBJECT-TYPE
    SYNTAX          INTEGER { alUnset(0), alSet(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Alarm state indicated by MCH ! light."
    ::= { pthwmonHardwareMonitorEntry 3 }

pthwmonHardwareMonitorMajorTelcoAlarm OBJECT-TYPE
    SYNTAX          INTEGER { alUnset(0), alSet(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Alarm state indicated by MCH !! light."
    ::= { pthwmonHardwareMonitorEntry 4 }

pthwmonHardwareMonitorCriticalTelcoAlarm OBJECT-TYPE
    SYNTAX          INTEGER { alUnset(0), alSet(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Alarm state indicated by MCH !!! light."
    ::= { pthwmonHardwareMonitorEntry 5 }

pthwmonHardwareMonitorActiveMCH OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the MCH that is actively processing IPMC commands and controlling the Telco Alarm Panel."
    ::= { pthwmonHardwareMonitorEntry 6 }

pthwmonHardwareMonitorHardwareMonitorSlot OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Slot HWMON is running on."
    ::= { pthwmonHardwareMonitorEntry 7 }

pthwmonHardwareMonitorActions OBJECT-TYPE
    SYNTAX          INTEGER { resetTelcoAlarm(1), 
                        restoreDebugDefaults(2) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of all possible actions for this instance"
    ::= { pthwmonHardwareMonitorEntry 252 }

pthwmonHardwareMonitorParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { pthwmonHardwareMonitorEntry 253 }

pthwmonHardwareMonitorRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The row status for the hwmonHardwareMonitor table"
    ::= { pthwmonHardwareMonitorEntry 254 }

pthwmonHardwareMonitorIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The row number for the hwmonHardwareMonitor table"
    ::= { pthwmonHardwareMonitorEntry 255 }

pthwmonSlot OBJECT IDENTIFIER
