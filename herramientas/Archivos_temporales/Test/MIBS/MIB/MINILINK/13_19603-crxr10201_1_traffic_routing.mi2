					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.2.6.5.1.1.1
		xfBuskIfFIFOStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				fifoOK(1),
				fifoError(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates whether there was a FIFO error,
				i.e. overflow/underflow. This objects is set as
				soon as an error occures and reset by a get operation.
				So FIFO errors since last poll can be checked."
			::= { xfBuskIfEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.2.6.5.1.1.2
		xfBuskIfBIP2 OBJECT-TYPE
			SYNTAX INTEGER
				{
				bipOk(1),
				bipError(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object reflects the BIP2 status of a BUSK 
				interface.A bipError is only interesting in 
				case the interface is cross-connected.
				This objects is set as
				soon as an error occures and reset by a get operation.
				So BIP errors since last poll can be checked."
			::= { xfBuskIfEntry 2 }

		
