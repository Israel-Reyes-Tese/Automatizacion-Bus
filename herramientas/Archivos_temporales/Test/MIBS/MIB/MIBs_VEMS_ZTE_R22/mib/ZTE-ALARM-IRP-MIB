--======================================================================
-- CONFORMANCE INFORMATION
--======================================================================

alarmCompliances OBJECT IDENTIFIER ::= { alarmConformance 1 }
alarmGroups      OBJECT IDENTIFIER ::= { alarmConformance 2 }


--======================================================================
-- COMPLIANCE STATEMENTS
--======================================================================

alarmCompliance MODULE-COMPLIANCE
        STATUS current
        DESCRIPTION 
                "A collection of objects that are required for
                 compliance to the ALARM-IRP."

        MODULE -- This module
           MANDATORY-GROUPS { alarmAdminMandatoryGroup,
                              alarmMandatoryGroup,
                              alarmNotificationMandatoryGroup
                               }

           GROUP alarmAdminOptionalGroup
           DESCRIPTION
                   "This group is not optional. A SNMP entity may 
                    support one or several of the objects in the group."
                    
           GROUP alarmNumberGroup
           DESCRIPTION
                   "This group is not optional."
           

        ::= { alarmCompliances 1 }


--======================================================================
-- GROUPS
--======================================================================

alarmAdminMandatoryGroup OBJECT-GROUP  
        OBJECTS {   alarmIrpVersion        			
        	}
        STATUS      current
        DESCRIPTION 
                "A collection of mandatory objects holding administrative
                information."
        ::= { alarmGroups 1 }

alarmAdminOptionalGroup OBJECT-GROUP  
        OBJECTS {  
        	 systemDN
        	 }
        STATUS      current
        DESCRIPTION 
                "A collection of optional objects holding administrative
                information."
        ::= { alarmGroups 2 }

alarmNumberGroup OBJECT-GROUP
        OBJECTS {   alarmNumber,
                    alarmIndeterminateNumber,
                    alarmCriticalNumber,
                    alarmMajorNumber,
                    alarmMinorNumber,
                    alarmWarningNumber }
        STATUS      current
        DESCRIPTION 
                "A collection of optional objects for retrieval of 
                 the number of currently active alarms."
        ::= { alarmGroups 3 }

alarmMandatoryGroup OBJECT-GROUP
        OBJECTS {   alarmIndex,
                    alarmId,
                    alarmManagedObjectInstance,
                    alarmEventTime,
                    alarmEventType,
                    alarmProbableCause,
                    alarmPerceivedSeverity,
                    alarmSpecificProblem,
                    alarmAdditionalText }
        STATUS      current
        DESCRIPTION 
                "A collection of objects that represents mandatory 
                 alarm attributes."
        ::= { alarmGroups 4 }

alarmNotificationMandatoryGroup NOTIFICATION-GROUP
        NOTIFICATIONS { alarmNew,
                        alarmCleared,
                        alarmAckChange,
                        alarmCommentChange,
                        alarmListRebuild,
                        alarmSync,
                        messageInfo,
                        alarmSeverityChange,
                        alarmManagedObjectInstanceNameChange,
                        alarmServiceChange,
                        alarmSyncStart,
                        alarmSyncEnd
                        }
        STATUS      current
        DESCRIPTION 
                "A collection of mandatory alarm notifications."
        ::= { alarmGroups 5 }

END
