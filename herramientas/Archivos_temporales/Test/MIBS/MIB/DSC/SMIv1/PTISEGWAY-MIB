        ptSGM3UASCTPServerMaximumPathRetrans    INTEGER,
        ptSGM3UASCTPServerBundling      INTEGER,
        ptSGM3UASCTPServerBundlingTimeout INTEGER,
        ptSGM3UASCTPServerDelayedAckTimeout   INTEGER,
        ptSGM3UASCTPServerChecksum      INTEGER,
