        sonusDsx3FarEndCurrentDayElapsedTime    Integer32,
        sonusDsx3FarEndCurrentDayCVCPs  PerfCurrentCount,
        sonusDsx3FarEndCurrentDayESCPs  PerfCurrentCount,
        sonusDsx3FarEndCurrentDaySESCPs PerfCurrentCount,
        sonusDsx3FarEndCurrentDaySASCPs PerfCurrentCount,
        sonusDsx3FarEndCurrentDayUASCPs PerfCurrentCount,
        sonusDsx3FarEndCurrentDayFCCP   Integer32,
        sonusDsx3FarEndCurrentDayTcaCVCP TruthValue,
        sonusDsx3FarEndCurrentDayTcaESCP TruthValue,
        sonusDsx3FarEndCurrentDayTcaSESCP TruthValue,
        sonusDsx3FarEndCurrentDayTcaUASCP TruthValue,
        sonusDsx3FarEndCurrentDayMinorTcaCVCP     TruthValue,
        sonusDsx3FarEndCurrentDayMinorTcaESCP     TruthValue,
        sonusDsx3FarEndCurrentDayMinorTcaSESCP      TruthValue,
        sonusDsx3FarEndCurrentDayMinorTcaUASCP      TruthValue,
        sonusDsx3FarEndCurrentDayMajorTcaCVCP     TruthValue,
