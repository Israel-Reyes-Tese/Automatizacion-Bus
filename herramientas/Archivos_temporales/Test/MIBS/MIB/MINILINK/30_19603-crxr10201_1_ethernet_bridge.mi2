        -- 1.3.6.1.4.1.193.81.4.1.2.1.19.1.1.6
        xfMacLoopbackDstMac OBJECT-TYPE
            SYNTAX MacAddress
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies the unicast destination MAC address of the test traffic.
                Looped back frames will have this address in the source MAC address field."
            ::= { xfMacLoopbackEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.19.1.1.7
        xfMacLoopbackEnable OBJECT-TYPE
            SYNTAX INTEGER {
                enable(1),
                disable(2)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Set this object to enable or disable the MAC loopback.
                The loopback will automatically revert to disabled after a predefined timeout.
                Reading this object returns the actual activation state."
            DEFVAL { disable }
            ::= { xfMacLoopbackEntry 7 }


