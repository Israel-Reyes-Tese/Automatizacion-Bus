    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Common Language Location Identifier string for this channel."
    ::= { ptIOHWLSLChannelEntry 11 }

ptIOHWLSLChannelSupportDebug OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Information for debug support."
    ::= { ptIOHWLSLChannelEntry 20 }

ptIOHWLSLChannelL3Use OBJECT-TYPE
    SYNTAX          INTEGER { inUse(-1), available(0) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
