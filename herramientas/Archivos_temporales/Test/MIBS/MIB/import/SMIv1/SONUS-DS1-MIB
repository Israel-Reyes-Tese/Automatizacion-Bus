    ::= { sonusDsx1MIBNotificationsObjects 10 }

sonusDsx1AdminChangeNotification TRAP-TYPE
    ENTERPRISE      sonusDsx1MIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs1Index, sonusDs1AdmnState, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "A sonusDsx1AdminChangeNotification trap indicates the DS1 admin changes. 
             It is used to notify out-of-service, in-service, enabled and
             	 disabled."
    ::= 1

sonusDsx1OutOfServiceNotification TRAP-TYPE
    ENTERPRISE      sonusDsx1MIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs1Index, 
                      sonusDs1OutOfServiceReason, 
                      sonusDs1OutOfServiceType, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "A sonusDsx1OutOfServiceNotification trap indicates that a T1/E1 span
             went out of service. The reason indicates the reason why and
             type indicates the type of failure or alarm."
    ::= 2

sonusDsx1ThresholdCrossingNotification TRAP-TYPE
    ENTERPRISE      sonusDsx1MIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs1Index, 
                      sonusDs1ThresholdType, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "A sonusDsx1ThresholdCrossingNotification trap indicates that a T1/E1 span
             threshold has been crossed. The type indicates the type of threshold."
    ::= 3

sonusDsx1LineStatusChangeNotification TRAP-TYPE
    ENTERPRISE      sonusDsx1MIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs1Index, sonusDs1LineState, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "A sonusDsx1LineStatusChangeNotification trap indicates the T1/E1 span state
             changes. It is used to notify T1/E1 span Alarms states."
    ::= 4

sonusDsx1AdminStateChangeNotification TRAP-TYPE
    ENTERPRISE      sonusDsx1MIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs1Mask, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "A sonusDsx1AdminStateChangeNotification trap indicates one or more
             DS1s in the specified Port have changed admin State. sonusDs1Mask
             is a bitmask indicating which DS1s are Enabled; a '1' in a bit
             position indicates the corresponding DS1 is Enabled, a '0' in a bit
             position indicates the corresponding DS1 is Disabled."
    ::= 5

sonusDsx1AdminModeChangeNotification TRAP-TYPE
    ENTERPRISE      sonusDsx1MIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs1Mask, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime, sonusDs1ModeChangeReason }
--     STATUS          mandatory
    DESCRIPTION     
            "A sonusDsx1AdminModeChangeNotification trap indicates one or more
             DS1s in the specified Port have changed admin Mode. sonusDs1Mask
             is a bitmask indicating which DS1s are In Service; a '1' in a bit
             position indicates the corresponding DS1 is In Service, a '0' in a bit
             position indicates the corresponding DS1 is Out of Service."
    ::= 6

sonusDsx1AdminDryupNotification TRAP-TYPE
    ENTERPRISE      sonusDsx1MIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs1Mask, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime, sonusDs1ModeChangeReason }
--     STATUS          mandatory
    DESCRIPTION     
            "A sonusDsx1AdminDryupNotification trap indicates one or more
             DS1s in the specified Port are Drying Up. sonusDs1Mask
             is a bitmask indicating which DS1s are Drying; a '1' in a bit
             position indicates the corresponding DS1 is Drying Up."
    ::= 7

sonusDsx1AlarmStateChangeNotification TRAP-TYPE
    ENTERPRISE      sonusDsx1MIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs1Index, sonusDs1LineState, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "A sonusDsx1AlarmStateChangeNotification trap indicates the alarm state of
             a T1/E1 span has changed."
    ::= 9

sonusDsx1Q50DcmeTrmStateChangeNotification TRAP-TYPE
    ENTERPRISE      sonusDsx1MIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs1Index, sonusDs1Q50TrmState, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            " "
    ::= 11

END -- end of module SONUS-DS1-MIB.
