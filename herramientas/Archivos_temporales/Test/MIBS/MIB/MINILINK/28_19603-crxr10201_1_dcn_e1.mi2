--
-- Node definitions
--

		-- 1.3.6.1.4.1.193.81.2.8.10.1
		xfDcnE1Objects OBJECT IDENTIFIER ::= { xfDcnE1 1 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.1
		xfDcnE1ConfigObjects OBJECT IDENTIFIER ::= { xfDcnE1Objects 1 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.1.1
		xfDcnE1Mode OBJECT-TYPE
			SYNTAX INTEGER
				{
				dcnE1NotSupported(1),
				dcnE1NotConfigured(2),
				dcnOverDedicatedE1(3),
				dcnOverChanE1PassThrough(4)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Applies only for NPU's 'DCN over Traffic'.
				
				Select which DCN E1 mode is used by the node.
				
				  dcnE1NotSupported        : DCN E1 is not supported on this node.
				
				  dcnE1NotConfigured       : Not configured yet.
				
				  dcnOverDedicatedE1       : DCN over a dedicated E1
				                             (can be unframed or framed/channelized)
				
				  dncOverChanE1PassThrough : DCN over a channelized E1 with traffic
				                             pass through. This is referred as fractional
				                             or robbed time slots in some documents.
				
				Changing a mode will reset NPU's 'DCN over Traffic' rows within xfDcnE1Table
				and the operator configured values for NPU's 'DCN over Traffic' are lost.
				When this object has the value 'dcnE1NotSupported', all write
				access to this object shall return 'wrongValue'.
				
				It is not allowed to set this object to 'dcnE1NotSupported'.
				
				ML66: This object is not supported."
			::= { xfDcnE1ConfigObjects 1 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.1.2
		xfDcnE1RdiMode OBJECT-TYPE
			SYNTAX INTEGER
				{
				transparent(1),
				terminated(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Applies only for NPU's 'DCN over Traffic'.
				
				Select which Rdi mode is used by the node. This can only be set when
				xfDcnE1Mode is 'dcnOverChanE1PassThrough'. Otherwise 'inconsistentValue'
				is returned.
				
				Default is 'terminated'
				
				ML66: This object is not supported."
			::= { xfDcnE1ConfigObjects 2 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2
		xfDcnE1PppObjects OBJECT IDENTIFIER ::= { xfDcnE1Objects 2 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1
		xfDcnE1Table OBJECT-TYPE
			SYNTAX SEQUENCE OF XfDcnE1Entry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A table that shows the E1 interfaces involved in DCN, the higher layer interface using
				the E1 and the time slots (in case of DCN over channelized E1 with traffic pass through)
				allocated for the higher layer.
				
				For NPU's 'DCN over Traffic':
				     xfDcnE1Mode should be set either to 'dcnOverDedicatedE1' or
				    'dcnOverChanE1PassThrough' prior to using this table for NPU's
				    DCN over Traffic.
				    The configured values of NPU's 'DCN over traffic' rows on this
				    table are reset(lost) if the xfDcnE1Mode is changed.
				
				ML66: This object is not supported."
			::= { xfDcnE1PppObjects 1 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1
		xfDcnE1Entry OBJECT-TYPE
			SYNTAX XfDcnE1Entry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Information about a particular E1 used for DCN.
				
				ML66: This object is not supported."
			INDEX { xfDcnE1Index }
			::= { xfDcnE1Table 1 }


		XfDcnE1Entry ::=
			SEQUENCE {
				xfDcnE1Index
					Integer32,
				xfDcnE1IfIndexOrZero
					Integer32,
				xfDcnE1PppIfIndexOrZero
					Integer32,
				xfDcnE1TsPossible
					E1TimeslotsBitmap,
				xfDcnE1TsUsage
					E1TimeslotsBitmap,
				xfDcnE1ErrorAIS
					INTEGER,
				xfDcnE1IdlePattern
					OCTET STRING,
				xfDcnE1Status
					INTEGER,
				xfDcnE1HigherLayerIfIndexOrZero
					Integer32
			 }

		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1.1
		xfDcnE1Index OBJECT-TYPE
			SYNTAX Integer32 (1..2147483647)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An index used to distinguish the two rows of the table.
				
				For NPU's 'DCN over Traffic' only:
				  The two rows will have index 1 and 2 respectively.
				
				For others:
				   Indexing according to the rules of static if-indexing
				   setting all fields but ifRack, ifSlot and ifOffset to
				   zero -> 14/1056-2/CSH 109 32/1, Basic Node Traffic
				  (TN Timeslot function): Chap 1.1.1 'Indexing of the rows'.
				
				ML66: This object is not supported."
			::= { xfDcnE1Entry 1 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1.2
		xfDcnE1IfIndexOrZero OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The interface index of the E1 interface involved in DCN or zero if no E1
				is yet configured for DCN.
				
				Writing to this object when xfDcnE1Mode is 'noDcnE1', results in the
				SNMP error code 'inConsistentValue'.
				
				ML66: This object is not supported."
			::= { xfDcnE1Entry 2 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1.3
		xfDcnE1PppIfIndexOrZero OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				"The interface index of the PPP that uses E1 interface or zero if
				no such PPP is available.
				
				ML66: This object is not supported."
			::= { xfDcnE1Entry 3 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1.4
		xfDcnE1TsPossible OBJECT-TYPE
			SYNTAX E1TimeslotsBitmap
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A bitmap that shows which timeslots are available for 'DCN over Traffic'
				
				For NPU's 'DCN over Traffic' only:
				  A bitmap that shows which timeslots are available for PPP when xfDcnE1Mode
				  is 'dcnOverChanE1PassThrough'.
				  
				  Otherwise it is set to 0x0000.
				
				ML66: This object is not supported."
			::= { xfDcnE1Entry 4 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1.5
		xfDcnE1TsUsage OBJECT-TYPE
			SYNTAX E1TimeslotsBitmap
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"A bitmap to select 'DCN over Traffic' timeslots
				Default value: 0x00000000 (no timeslot is yet selected)
				
				For NPU's 'DCN over Traffic' only:
				  A bitmap to select PPP timeslots from a channelized/framed E1.
				
				  Default values:
				 
				     xfDcnE1Mode = 'dcnOverChanE1PassThrough' : 0x00000000 (no timeslot is yet selected)
				     xfDcnE1Mode = 'dcnOverDedicatedE1        : 0xFFFFFFFF (all timeslots are used)
				     xfDcnE1Mode = 'noDcnE1'                  : 0x00000000
				
				   This object can only be written when xfDcnE1Mode is 'dcnOverChanE1PassThrough'.
				   Otherwise, 'inconsistentValue' is returned.
				
				ML66: This object is not supported."
			::= { xfDcnE1Entry 5 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1.6
		xfDcnE1ErrorAIS OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"To enable or disable AIS on the E1 interface.
				
				For NPU's 'DCN over Traffic' only:
				  This object can only be written  when xfDcnE1Mode is 'dcnOverChanE1PassThrough'.
				  Otherwise 'inconsistentValue' is returned.
				
				  Default value is 'disable'.
				
				ML66: This object is not supported."
			::= { xfDcnE1Entry 6 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1.7
		xfDcnE1IdlePattern OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..1))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Select idle pattern used by E1 involved in DCN E1.
				
				For NPU's 'DCN over Traffic' only:
				  This object can only be set when
				
				    xfDcnE1Mode is 'dcnOverDedicatedE1' and the E1 is channelized/framed
				    xfDcnE1Mode is 'dcnOverChanE1PassThrough'
				
				   Otherwise 'inconsistentValue' is returned.
				
				   Default is 0xFF.
				
				ML66: This object is not supported."
			::= { xfDcnE1Entry 7 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1.8
		xfDcnE1Status OBJECT-TYPE
			SYNTAX INTEGER
				{
				up(1),
				down(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Shows the current status of the row.
				The status will be 'down' if any one of the following occurs:
				
				  - xfDcnE1IfIndexOrZero is zero    (no E1 selected)
				  - xfDcnE1HigherLayerIfIndexOrZero is zero (no PPP that can use E1 is available)
				
				   It will be up if none of the obove occurs.
				
				
				For NPU's 'DCN over Traffic' only:
				
				  The status will be 'down' if any one of the following occurs:
				
				    - xfDcnE1Mode is set to 'noDcnE1'
				    - xfDcnE1IfIndexOrZero is zero    (no E1 selected)
				    - xfDcnE1PppIfIndexOrZero is zero (no PPP that can use E1 is available)
				  
				   It will be up if none of the above occurs.
				
				ML66: This object is not supported."
			::= { xfDcnE1Entry 8 }


		-- 1.3.6.1.4.1.193.81.2.8.10.1.2.1.1.9
		xfDcnE1HigherLayerIfIndexOrZero OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The interface index of the service layer interface that uses E1 interface or zero if
				no such service layer interface  is available.
				
				This object is to replace xfDcnE1PppIfIndexOrZero so that this table is not only used for PPP
				but for any service.
				
				ML66: This object is not supported."
			::= { xfDcnE1Entry 9 }


		-- 1.3.6.1.4.1.193.81.2.8.10.2
		xfDcnE1Conformance OBJECT IDENTIFIER ::= { xfDcnE1 2 }


		-- 1.3.6.1.4.1.193.81.2.8.10.2.1
		xfDcnE1Compliances OBJECT IDENTIFIER ::= { xfDcnE1Conformance 1 }


		-- 1.3.6.1.4.1.193.81.2.8.10.2.1.1
		xfDcnE1FullCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION
				"The compliance statement for SNMP entities which
				implement everything in this module."
			MODULE -- this module
				MANDATORY-GROUPS { xfDcnE1Group }
			::= { xfDcnE1Compliances 1 }


		-- 1.3.6.1.4.1.193.81.2.8.10.3
		xfDcnE1Groups OBJECT IDENTIFIER ::= { xfDcnE1 3 }


		-- 1.3.6.1.4.1.193.81.2.8.10.3.1
		xfDcnE1Group OBJECT-GROUP
			OBJECTS { xfDcnE1Mode, xfDcnE1RdiMode, xfDcnE1IdlePattern, xfDcnE1IfIndexOrZero, xfDcnE1TsPossible,
				xfDcnE1TsUsage, xfDcnE1ErrorAIS, xfDcnE1HigherLayerIfIndexOrZero, xfDcnE1Status }
			STATUS current
			DESCRIPTION
				"A group of all xfDcnE1 MIB objects."
			::= { xfDcnE1Groups 1 }


		-- 1.3.6.1.4.1.193.81.2.8.10.3.2
		xfDcnE1DeprecatedGroup OBJECT-GROUP
			OBJECTS { xfDcnE1PppIfIndexOrZero }
			STATUS deprecated
			DESCRIPTION
				"Description."
			::= { xfDcnE1Groups 2 }


	END

