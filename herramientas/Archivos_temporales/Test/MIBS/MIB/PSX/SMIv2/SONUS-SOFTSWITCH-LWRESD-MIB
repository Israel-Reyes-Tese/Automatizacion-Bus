    OBJECTS         { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "Normal DNS response for a query that previously used expired TTL RR"
    ::= { sonusSoftSwitchLwresdMIBNotificationsPrefix 6 }

