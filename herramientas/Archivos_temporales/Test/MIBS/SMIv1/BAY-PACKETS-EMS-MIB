rsiUserTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiUserEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table stores the information about all users 
             accessing the RSI solution"
    ::= { rsiCommonMib 12 }

rsiUserEntry OBJECT-TYPE
    SYNTAX          RsiUserEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of an RSI Subsystem"
    INDEX           { rsiUserId }
    ::= { rsiUserTable 1 }

RsiUserEntry ::=
    SEQUENCE {
        rsiUserId                       OCTET STRING,
        rsiUserFirstName                OCTET STRING,
        rsiUserMiddleName               OCTET STRING,
        rsiUserLastName                 OCTET STRING,
        rsiUserActivationDate           DisplayString,
        rsiUserExpiryDate               DisplayString,
        rsiUserStatus                   INTEGER,
        rsiUserPolicyId                 INTEGER,
        rsiUserRoles                    OCTET STRING,
        rsiLoggingEnableOption          INTEGER,
        rsiUserRowStatus                RowStatus
    }

rsiUserId OBJECT-TYPE
    SYNTAX          OCTET STRING
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is the unique id of a user"
    ::= { rsiUserEntry 1 }

rsiUserFirstName OBJECT-TYPE
    SYNTAX          OCTET STRING
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the First Name of the user. 
             Mandatory field for creating a new user"
    ::= { rsiUserEntry 2 }

rsiUserMiddleName OBJECT-TYPE
    SYNTAX          OCTET STRING
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the Middle Name of the user.
             Mandatory field for creating a new user"
    ::= { rsiUserEntry 3 }

rsiUserLastName OBJECT-TYPE
    SYNTAX          OCTET STRING
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the Last Name of the user.
             Mandatory field for creating a new user"
    ::= { rsiUserEntry 4 }

rsiUserActivationDate OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(19))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is the time at which the user account was created.
             format is 'YYYY:MM:DD HH:MM:SS'.
             This attribute cannot be modified."
    ::= { rsiUserEntry 5 }

rsiUserExpiryDate OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(19))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is the time at which the user account will expire.
             format is 'YYYY:MM:DD HH:MM:SS'. 
             This attribute cannot be modified."
    ::= { rsiUserEntry 6 }

rsiUserStatus OBJECT-TYPE
    SYNTAX          INTEGER { new(0), active(1), locked(2), inactive(3), 
                        deleted(4) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the user account status.
             This attribute is not needed while creating a user,
             but can be modified.
             This field holds no importance for creating a new user,
             because by default the value of status will be new."
    ::= { rsiUserEntry 7 }

rsiUserPolicyId OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This identifies the security policy opted for the user.
             Mandatory field for creating a new user"
    ::= { rsiUserEntry 8 }

rsiUserRoles OBJECT-TYPE
    SYNTAX          OCTET STRING
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is the list of role ids assigned to a given user. 
             This will be a comma seperated list, and could have
             been described as sequence of integers.
             This attribute cannot be modified.
             By default when a user is created, he is assigned a
             role, and if a new role needs to be assigned to a
             user, then entries should be manipulated at the
             user-role table (rsiUserRoleTable)"
    ::= { rsiUserEntry 9 }

rsiLoggingEnableOption OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "To Enable logging set the LSB to 1 and to disable logging set the LSB to 0."
    ::= { rsiUserEntry 10 }

rsiUserRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is for dynamic addition/deletion of users.
             For creating a new user entry, only 'createAndGo' is
             supported. For deleting an existing row, 'delete'
             is supported"
    ::= { rsiUserEntry 11 }

rsiUserRoleTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiUserRoleEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains the list of component Ids,
             which have been assigned to a given user for a
             given role. "
    ::= { rsiCommonMib 13 }

rsiUserRoleEntry OBJECT-TYPE
    SYNTAX          RsiUserRoleEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of a user-role association. Note UserId
             and RoleId are index of foreign tables"
    INDEX           { rsiUserId, rsiRoleId }
    ::= { rsiUserRoleTable 1 }

RsiUserRoleEntry ::=
    SEQUENCE {
        rsiComponentIdList              OCTET STRING,
        rsiUserRoleRowStatus            RowStatus
    }

