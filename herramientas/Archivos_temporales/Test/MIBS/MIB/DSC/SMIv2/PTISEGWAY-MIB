        ptSCCPSCCPNAKeepAlive           INTEGER,
        ptSCCPSCCPNASSTDelay            Integer32,
        ptSCCPSCCPNAXUDTReassembly      Integer32,
        ptSCCPSCCPNAMTPAudit            INTEGER,
        ptSCCPSCCPNAMTPAuditLifetime    INTEGER,
        ptSCCPSCCPNAConnectionEstablish Integer32,
        ptSCCPSCCPNAConnectionRelease   Integer32,
        ptSCCPSCCPNAReceiveInactive     Integer32,
        ptSCCPSCCPNASendInactive        Integer32,
        ptSCCPSCCPNAReferenceFreeze     Integer32,
        ptSCCPSCCPNASLSIncrementation   INTEGER,
        ptSCCPSCCPNAOldEncodingOfAI     INTEGER,
        ptSCCPSCCPNAOldEncodingOfCLGPAPC INTEGER,
        ptSCCPSCCPNAOldMTPAvailability  INTEGER,
        ptSCCPSCCPNAOldPointCodeFormat  INTEGER,
        ptSCCPSCCPNASetOPCToLPCNonMgmt  INTEGER,
        ptSCCPSCCPNASetOPCToLPCMgmt     INTEGER,
        ptSCCPSCCPNANUNITDATAQoSSupport INTEGER,
        ptSCCPSCCPNASSNZeroUserSupport  INTEGER,
        ptSCCPSCCPNAGTTIncomingRerouting INTEGER,
        ptSCCPSCCPNAGTTIncomingCLGPC    INTEGER,
        ptSCCPSCCPNASSTOPCFromSSP       INTEGER,
        ptSCCPSCCPNASetOPCToLPC         INTEGER,
        ptSCCPSCCPNASetAffectedPCToLPC  INTEGER,
        ptSCCPSCCPNAEncodingOfCLGPAPC   INTEGER,
        ptSCCPSCCPNAForcedSegmentation  Integer32,
        ptSCCPSCCPNALPCRegistration     INTEGER,
        ptSCCPSCCPNAUnequippedUser      INTEGER,
        ptSCCPSCCPNASCCPNAManagerIndex  Integer32,
        ptSCCPSCCPNAActions             INTEGER,
        ptSCCPSCCPNAParentOID           OBJECT IDENTIFIER,
        ptSCCPSCCPNARowStatus           RowStatus,
        ptSCCPSCCPNAIndex               Integer32
    }

ptSCCPSCCPNAContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This attribute contains system generated identifier that is used to identify this SCCP Network Appearance in log messages."
    ::= { ptSCCPSCCPNAEntry 1 }

ptSCCPSCCPNALocalPointCode OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The local point code value is obtained from the MTP3 and is provided here for informational purposes only."
    ::= { ptSCCPSCCPNAEntry 2 }

ptSCCPSCCPNASS7Variant OBJECT-TYPE
    SYNTAX          INTEGER { undefined(255), ansi(0), itu(1), china(2), 
                        ntt(4), ttc(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The SS7 Variant value is obtained from the MTP3."
    ::= { ptSCCPSCCPNAEntry 3 }

