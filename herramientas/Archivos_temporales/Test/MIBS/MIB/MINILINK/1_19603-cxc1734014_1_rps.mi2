--
-- Textual Conventions
--

RpsConfigSlots ::= TEXTUAL-CONVENTION
    STATUS current
    DESCRIPTION
        "The bit corresponding to an MMUs that can be/is included in the N+1 group is set to 1"
    SYNTAX BITS
    {
      slot0(0), 
      slot1(1), 
      slot2(2), 
      slot3(3), 
      slot4(4), 
      slot5(5), 
      slot6(6), 
      slot7(7), 
      slot8(8), 
      slot9(9), 
      slot10(10), 
      slot11(11), 
      slot12(12), 
      slot13(13), 
      slot14(14), 
      slot15(15), 
      slot16(16), 
      slot17(17), 
      slot18(18), 
      slot19(19), 
      slot20(20) 	  
    }
 
RpsCapability ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
        "This TC enumerates the Rps cap."
    SYNTAX BITS
    {
      preferredChannelSupport(0), 
      occasionalSTM1(1),
      occasionalEoS(2)
    }
 
 
 
