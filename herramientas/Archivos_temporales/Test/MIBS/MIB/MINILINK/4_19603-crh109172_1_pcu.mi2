--
-- Node definitions
--  

	xfPCUObjects OBJECT IDENTIFIER ::= { xfPCUMIB 1 }
	xfPCUConformance OBJECT IDENTIFIER ::= { xfPCUMIB 2 }

------------------------------------------------------------------------------
-- xfPCUTable
------------------------------------------------------------------------------
	xfPCUTable OBJECT-TYPE
		SYNTAX SEQUENCE OF XfPCUEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"
			Table for SMU2 managed objects. The table is indexed with 
			entPhysicalIndex.
			Note that some MOs' validity is depending on xfPCUMode (cosite/protection).
			Note that objects might not be accessible if xfMCRBusy 
			in XF-MCR-MIB is busy(1).
			"
		::= { xfPCUObjects 1 }
		
	xfPCUEntry OBJECT-TYPE
		SYNTAX XfPCUEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"An entry in the xfPCUTable"
		INDEX { entPhysicalIndex}
		::= { xfPCUTable 1 }
	
	
	XfPCUEntry ::=
		SEQUENCE
		{
			xfPCUTerminalBoards	XfMCRTerminalBoards,
			xfPCUOMMode			XfMCROMMode,
			xfPCUOMTimeout		XfMCROMTimeout,
			xfPCUMode			XfMCRMode,
			xfPCUUartBaudrate	XfMCRUartBaudrate,
			xfPCUOnceRunning	XfMCROnceRunning,
			xfPCUSeverityTrapEnable XfMCRSeverityTrapEnable,
			xfPCUSeverityLevel		XfMCRSeverityLevel,
			xfPCUCapacity		XfMCRCapacity,
			xfPCUDcnClock		XfMCRDcnClock,
			xfPCUDcnClockCapability	XfMCRDcnClockCapability
		} 
			
--
-- The xfPCUTable MOs
--				
	xfPCUTerminalBoards OBJECT-TYPE
		SYNTAX XfMCRTerminalBoards
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"
			This object sets the number of boards in a MCR Terminal.
			In current release only valid values are oneBoard and threeBoards.
			In a 1+1 configuration this MO sets the MO xfTMUTerminalBoards
			on the MMU2 boards in the 1+1 terminal.  		
			If the SMU2 is configured as a co-siting board, this MO is set to other(1).
			" 
		DEFVAL { oneBoard }
        ::= { xfPCUEntry 1 } 
     
	xfPCUOMMode OBJECT-TYPE
		SYNTAX XfMCROMMode          
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object controls the O&M-switch on the SMU2-board.
			"
		::= {xfPCUEntry 3}

	xfPCUOMTimeout OBJECT-TYPE
		SYNTAX XfMCROMTimeout         
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object is the time out for OM_REQUEST.
 			"	
		::= { xfPCUEntry 4 }
	
	xfPCUMode OBJECT-TYPE
		SYNTAX XfMCRMode         
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"
			This objects sets the working mode of the PCU board.
			"
		DEFVAL { notConfigured }
		::= {xfPCUEntry 5}
	
	xfPCUUartBaudrate OBJECT-TYPE
		SYNTAX XfMCRUartBaudrate      
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object control the baudrate of the PCI Uart.
			"
		::= {xfPCUEntry 6}	
			
	xfPCUOnceRunning OBJECT-TYPE
		SYNTAX XfMCROnceRunning      
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object tells if the MCR Link hop once was up and running.
			If the SMU2 is configured as a co-siting board, this MO is set to other(1).
			"
		::= {xfPCUEntry 7}
		
	xfPCUSeverityTrapEnable OBJECT-TYPE
		SYNTAX XfMCRSeverityTrapEnable       
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"
			This object control whether to send a trap or not if the xfPCUSeverityLevel changes.
			If the SMU2 is configured as a co-siting board, this MO is set to disable(2).
			"
		DEFVAL { enable }			
		::= { xfPCUEntry 8 }	
			
	xfPCUSeverityLevel OBJECT-TYPE
		SYNTAX XfMCRSeverityLevel       
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object tells the severity level of the CSS alarm. 
			It's the Alarm Status Indicator for the CSS alarm.
			If the SMU2 is configured as a co-siting board, this MO is set to clear(000).
			"			
		::= { xfPCUEntry 9 }
			
	xfPCUCapacity OBJECT-TYPE
		SYNTAX XfMCRCapacity  
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"
			This object control the Capacity of the PCU co-site.
			If the SMU2 is configured as protection board, this MO is set to other(1).
			"
		::= { xfPCUEntry 10 }

	xfPCUDcnClock OBJECT-TYPE
		SYNTAX XfMCRDcnClock
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"
			This object control the DCN Clock of the PCU in co-site.
			"
		::= { xfPCUEntry 11 }

	xfPCUDcnClockCapability OBJECT-TYPE
		SYNTAX XfMCRDcnClockCapability
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object shows the the DCN clock capability of the PCU in co-site.
			"
		::= { xfPCUEntry 12 }
			
 
--
-- Conformance
--

	xfPCUCompliances OBJECT IDENTIFIER ::= { xfPCUConformance 1 }

	xfPCUGroups OBJECT IDENTIFIER ::= { xfPCUConformance 2 }

   	xfPCUFullCompliance MODULE-COMPLIANCE
       		STATUS      current
       		DESCRIPTION
           		"The compliance statement for SNMP entities which
            		implement everything."
       		MODULE -- This Module
           	MANDATORY-GROUPS { xfPCUCompleteGroup }
       		::= { xfPCUCompliances 1 }

	xfPCUCompleteGroup OBJECT-GROUP
       		OBJECTS {
        		xfPCUTerminalBoards,
        		xfPCUOMMode,
        		xfPCUOMTimeout,
        		xfPCUMode,
        		xfPCUUartBaudrate,
        		xfPCUOnceRunning,
        		xfPCUSeverityTrapEnable,
        		xfPCUSeverityLevel,
        		xfPCUCapacity,
        		xfPCUDcnClock,
        		xfPCUDcnClockCapability
		}
		STATUS      current
       		DESCRIPTION
           		"A collection of all current objects in this MIB module."
		::= { xfPCUGroups 1 }
					
	END
