    SYNTAX          DisplayString (SIZE(19))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is the time at which this user logged in, and is in
             'YYYY:MM:DD HH:MM:SS' format. "
    ::= { rsiLoggedInUsersEntry 1 }

