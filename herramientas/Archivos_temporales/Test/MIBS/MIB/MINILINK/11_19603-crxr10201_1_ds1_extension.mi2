		-- 1.3.6.1.4.1.193.81.3.1.1.3
		xfE0Objects OBJECT IDENTIFIER ::= { xfDs1ExtensionMIB 3 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.3.1
		xfE0ConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfE0ConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table for confiuration of E0s. At the moment only 
				ServiceChannels for MCR are supported."
			::= { xfE0Objects 1 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.3.1.1
		xfE0ConfigEntry OBJECT-TYPE
			SYNTAX XfE0ConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in table."
			INDEX { ifIndex }
			::= { xfE0ConfigTable 1 }

		
		XfE0ConfigEntry ::=
			SEQUENCE { 
				xfE0ConfigLoopBackConfig
					INTEGER,
				xfE0ConfigLoopBackStatus
					BITS,
				xfE0ConfigLineStatus
					BITS
			 }

		-- 1.3.6.1.4.1.193.81.3.1.1.3.1.1.1
		xfE0ConfigLoopBackConfig OBJECT-TYPE
			SYNTAX INTEGER
				{
				e0NoLoop(1),
				e0LineLoop(3),
				e0InwardLoop(5)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to set loops on an E0.
				e0NoLoop(1)      
				  No loop is set
				
				e0LineLoop(2)    
				  The received signal is looped directly back and AIS 
				  is sent inwards.
				
				e0InwardLoop(3)  
				   Also called local loop. The transmitted signal is 
				   loop back into the node and AIS is transmitted outward."
			DEFVAL { e0NoLoop }
			::= { xfE0ConfigEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.3.1.1.2
		xfE0ConfigLoopBackStatus OBJECT-TYPE
			SYNTAX BITS
				{
				e0NoLoop(0),
				e0NotUsed1(1),
				e0LineLoop(2),
				e0NotUsed2(3),
				e0InwardLoop(4)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates which loops are set."
			::= { xfE0ConfigEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.3.1.1.3
		xfE0ConfigLineStatus OBJECT-TYPE
			SYNTAX BITS
				{
				notUsed0(0),
				notUsed1(1),
				notUsed2(2),
				e0LOS0(3),
				e0LOS1(4),
				e0LOS2(5)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Gives information on the alarm status of a E1 interface.
				 	Severity is indicated by three bits per alarm:
				 	bits 012
				 	     000   clear
				 	     001   undetermined
				 	     010   warning
				 	     011   minor
				 	     100   major
				 	     101   critical
				
				LOS is critical.
				"
			::= { xfE0ConfigEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.4
		xfE0Conformance OBJECT IDENTIFIER ::= { xfDs1ExtensionMIB 4 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.4.1
		xfE0Compliance OBJECT IDENTIFIER ::= { xfE0Conformance 1 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.4.1.1
		xfE0FullCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"The compliance statement for SNMP entities which
				implement everything related to E0."
			MODULE -- this module
				MANDATORY-GROUPS { xfE0Group }
			::= { xfE0Compliance 1 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.4.2
		xfE0Groups OBJECT IDENTIFIER ::= { xfE0Conformance 2 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.4.2.1
		xfE0Group OBJECT-GROUP
			OBJECTS { xfE0ConfigLoopBackConfig, xfE0ConfigLoopBackStatus, xfE0ConfigLineStatus, xfDsx1InputImpedance }
			STATUS current
			DESCRIPTION 
				"A group of all E0 objects."
			::= { xfE0Groups 1 }

		
	
	END
