XF-ETHERNET-BRIDGE-ALARM-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MacAddress, RowStatus
            FROM SNMPv2-TC
        OBJECT-GROUP
            FROM SNMPv2-CONF
        OBJECT-TYPE, MODULE-IDENTITY, Integer32
            FROM SNMPv2-SMI
        xfEthernetBridge            
            FROM XF-TOP-MIB
        PortList, VlanIndex,dot1qVlanIndex
            FROM Q-BRIDGE-MIB;

        -- 1.3.6.1.4.1.193.81.4.1.4
        xfEthernetBridgeAlarmMIB MODULE-IDENTITY 
            LAST-UPDATED "201408261100Z"           -- Aug. 26, 2014 at 11:00 GMT
            ORGANIZATION 
                "Ericsson"
            CONTACT-INFO 
                " "
            DESCRIPTION                           
                "This MIB describes the Ethernet Bridge Alarm Functionality in the MINI-LINK
                Traffic Node. It has MIB objects on bridge level."
            REVISION "201408261100Z"               -- Aug. 26, 2014 at 11:00 GMT
            DESCRIPTION                           
                "A :   EPTEVIL                     - Added missing comments and imports for smilit"
            REVISION "201408021100Z"               -- Aug. 02, 2014 at 11:00 GMT
            DESCRIPTION                           
                "PA1 : EPTEVIL                     - Mib creation
                added:
                -xfBridgeAlarmRowStatus scalar
                    -xfEthernetBridgeObjectMac      read-write
                    -xfEthernetBridgeAlarmThreshold read-write
                    -xfEthernetBridgeAlarmMembers   read-write
                -xfEthernetBridgeVlanLoopTable
                    -xfBridgeVlanLoopMember         read-write
                    -xfBridgeVlanLoopRowStatus      read-create
                -xfEthernetBridgeVlanLoopTable
                    -xfBridgeAlarmType              read-create
                    -xfBridgeAlarmRowStatus         read-create"
            ::= { xfEthernetBridge 4 }
--
-- Node definitions
--                             
                             
	-- 1.3.6.1.4.1.193.81.4.1.4.1
    xfBridgeAlarmObject OBJECT IDENTIFIER ::= { xfEthernetBridgeAlarmMIB 1 }
	                             
	-- 1.3.6.1.4.1.193.81.4.1.4.1.1
	xfEthernetBridgeAlarmConfig OBJECT IDENTIFIER ::= { xfEthernetBridgeAlarmMIB 1 }

-- -------------------------------------------------------------
-- The Loop Alarm Scalars
-- -------------------------------------------------------------
    
	-- 1.3.6.1.4.1.193.81.4.1.4.1.1.1
	xfEthernetBridgeLoopAlarms OBJECT IDENTIFIER ::= { xfEthernetBridgeAlarmConfig 1 }
	
	-- 1.3.6.1.4.1.193.81.4.1.4.1.1.1.1
	xfEthernetBridgeObjectMac OBJECT-TYPE
            SYNTAX  MacAddress
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                 "A 6-octet read-only value carrying the individual
				 MAC address assigned to the Aggregator."
			REFERENCE
				 "6.3.1.1.9"
            ::= { xfEthernetBridgeLoopAlarms 1 }

				
	-- 1.3.6.1.4.1.193.81.4.1.4.1.1.1.2
	xfEthernetBridgeAlarmThreshold OBJECT-TYPE
            SYNTAX Integer32 (10..200)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Set the Threshold.....
                Default value is 10."
            ::= { xfEthernetBridgeLoopAlarms 2 }

-- 1.3.6.1.4.1.193.81.4.1.4.1.1.1.3
	xfEthernetBridgeAlarmMembers OBJECT-TYPE
            SYNTAX PortList (SIZE (4))
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "A portlist that shows all members of the group. The bit position of the 
                master port is always set and can not be modified."
            ::= {xfEthernetBridgeLoopAlarms 3 }            

-- -------------------------------------------------------------
-- The Loop Detection Enabled VLAN Table
-- -------------------------------------------------------------

    -- 1.3.6.1.4.1.193.81.4.1.4.1.1.2
    xfEthernetBridgeVlanLoopTable OBJECT-TYPE
    SYNTAX SEQUENCE OF XfEthernetBridgeVlanLoopEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table containing Loop detection per-port status
        information for VLAN configuration in the device."
            ::= { xfEthernetBridgeAlarmConfig 2 }

    -- 1.3.6.1.4.1.193.81.4.1.4.1.1.2.1
    xfEthernetBridgeVlanLoopEntry OBJECT-TYPE
        SYNTAX XfEthernetBridgeVlanLoopEntry
           MAX-ACCESS not-accessible
           STATUS current
           DESCRIPTION
               "Information controlling VLAN Loops configuration for ports."
        INDEX { dot1qVlanIndex }
           ::= { xfEthernetBridgeVlanLoopTable 1  }

    XfEthernetBridgeVlanLoopEntry ::=
        SEQUENCE { 
          xfBridgeVlanLoopMember
              VlanIndex,
          xfBridgeVlanLoopRowStatus
              RowStatus }
              
    -- 1.3.6.1.4.1.193.81.4.1.4.1.1.2.1.1
    xfBridgeVlanLoopMember OBJECT-TYPE
        SYNTAX VlanIndex
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "A VLAN ports where Loop detection can run"
        ::= { xfEthernetBridgeVlanLoopEntry 1 } 
	
    -- 1.3.6.1.4.1.193.81.4.1.4.1.1.1.2
	   xfBridgeVlanLoopRowStatus OBJECT-TYPE
          SYNTAX RowStatus
           MAX-ACCESS read-create
           STATUS current
           DESCRIPTION
               "A bridge Vlan Loop RowStatus."
           ::= { xfEthernetBridgeVlanLoopEntry 2}        
  
-- -------------------------------------------------------------
-- The Loop Alarm Table
-- -------------------------------------------------------------

  
    -- 1.3.6.1.4.1.193.81.4.1.4.1.2
	xfEthernetBridgeAlarmTable OBJECT-TYPE
		SYNTAX SEQUENCE OF XfEthernetBridgeAlarmEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table of current Bridge Alarms."
            ::= { xfEthernetBridgeAlarmMIB 2  }

	-- 1.3.6.1.4.1.193.81.4.1.4.1.2.1
 	xfEthernetBridgeAlarmEntry OBJECT-TYPE
		SYNTAX XfEthernetBridgeAlarmEntry
           MAX-ACCESS not-accessible
           STATUS current
           DESCRIPTION
               "Entry of a Bridge Alarm."
        INDEX { xfBridgeAlarmType }
           ::= { xfEthernetBridgeAlarmTable 1  }
            
            
    XfEthernetBridgeAlarmEntry ::=
          SEQUENCE { 
              xfBridgeAlarmType
                  INTEGER,
              xfBridgeAlarmRowStatus
                  RowStatus
           }

	-- 1.3.6.1.4.1.193.81.4.1.4.1.2.1.1
       xfBridgeAlarmType OBJECT-TYPE
          SYNTAX INTEGER
			{	
				loop(1)
			}
          MAX-ACCESS read-create
          STATUS current
          DESCRIPTION
              "A bridge Alarm Type."
          ::= { xfEthernetBridgeAlarmEntry 1 }
		   
	-- 1.3.6.1.4.1.193.81.4.1.4.1.2.1.2
	   xfBridgeAlarmRowStatus OBJECT-TYPE
          SYNTAX RowStatus
           MAX-ACCESS read-create
           STATUS current
           DESCRIPTION
               "A bridge Alarm RowStatus."
           ::= { xfEthernetBridgeAlarmEntry 2}

        -- 1.3.6.1.4.1.193.81.4.1.4.1.3
        xfEthernetBridgeAlarmConformance OBJECT IDENTIFIER ::= { xfEthernetBridgeAlarmMIB 3 }

        -- 1.3.6.1.4.1.193.81.4.1.4.1.3.1
        xfEthernetBridgeAlarmGroups OBJECT IDENTIFIER ::= { xfEthernetBridgeAlarmConformance 1 }

       -- 1.3.6.1.4.1.193.81.4.1.4.1.3.1.1
       xfBridgeAlarmGeneralGroup OBJECT-GROUP
       OBJECTS {xfEthernetBridgeObjectMac,xfEthernetBridgeAlarmThreshold,xfEthernetBridgeAlarmMembers,
                xfBridgeVlanLoopMember,xfBridgeVlanLoopRowStatus, xfBridgeAlarmType,xfBridgeAlarmRowStatus}
       STATUS current
       DESCRIPTION 
            "Description."
       ::= { xfEthernetBridgeAlarmGroups 1 }  
END
