    ::= 2

siFsBelowWarningThreshold TRAP-TYPE
    ENTERPRISE      siFsMonNotifications
    VARIABLES       { siFsMonName, siFsMonThreshold, 
                      siFsMonCurrentUtilization }
--     STATUS          mandatory
    DESCRIPTION     
            "A trap indicating that the percent utilization has dropped
             back below the threshold in a row where the severity is set 
             to warning."
    ::= 3

siFsBelowCriticalThreshold TRAP-TYPE
    ENTERPRISE      siFsMonNotifications
    VARIABLES       { siFsMonName, siFsMonThreshold, 
                      siFsMonCurrentUtilization }
--     STATUS          mandatory
    DESCRIPTION     
            "A trap indicating that the percent utilization has dropped
