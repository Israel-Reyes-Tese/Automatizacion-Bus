ptMTPLSLink OBJECT IDENTIFIER
    ::= { ptMTPLinkset 55 }

ptMTPLSLinkTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtMTPLSLinkEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of Link attributes."
    ::= { ptMTPLSLink 1 }

ptMTPLSLinkEntry OBJECT-TYPE
    SYNTAX          PtMTPLSLinkEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of MTPLSLink attributes."
    INDEX           { ptConnGroupIndex, ptMTPLSLinkMTP3NAManagerIndex, 
                      ptMTPLSLinkMTP3NAIndex, ptMTPLSLinkLinksetsIndex, 
                      ptMTPLSLinkLinksetIndex, ptMTPLSLinkIndex }
    ::= { ptMTPLSLinkTable 1 }

PtMTPLSLinkEntry ::=
    SEQUENCE {
        ptMTPLSLinkContext              DisplayString,
        ptMTPLSLinkAPC                  Integer32,
        ptMTPLSLinkSLC                  Integer32,
        ptMTPLSLinkDREID                Integer32,
        ptMTPLSLinkCage                 Integer32,
        ptMTPLSLinkChassisSlot          Integer32,
        ptMTPLSLinkChannel              Integer32,
        ptMTPLSLinkL2State              INTEGER,
        ptMTPLSLinkL3State              INTEGER,
        ptMTPLSLinkCongestionLevel      INTEGER,
        ptMTPLSLinkDiagnosticMessage    DisplayString,
        ptMTPLSLinkL2DebugLevel         Integer32,
        ptMTPLSLinkUserActivated        INTEGER,
        ptMTPLSLinkLinkName             DisplayString,
        ptMTPLSLinkAlarms               INTEGER,
        ptMTPLSLinkRxMSUCount           Integer32,
        ptMTPLSLinkTxMSUCount           Integer32,
        ptMTPLSLinkMTP3NAManagerIndex   Integer32,
        ptMTPLSLinkMTP3NAIndex          Integer32,
        ptMTPLSLinkLinksetsIndex        Integer32,
        ptMTPLSLinkLinksetIndex         Integer32,
        ptMTPLSLinkActions              INTEGER,
        ptMTPLSLinkParentOID            OBJECT IDENTIFIER,
        ptMTPLSLinkRowStatus            RowStatus,
        ptMTPLSLinkIndex                Integer32
    }

ptMTPLSLinkContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A condensed textual representation of the link object."
    ::= { ptMTPLSLinkEntry 1 }

ptMTPLSLinkAPC OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Adjacent SS7 point code of this link."
    ::= { ptMTPLSLinkEntry 2 }

ptMTPLSLinkSLC OBJECT-TYPE
    SYNTAX          Integer32 (0..15)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Signaling Link Code for this link. \nIf AB Plane Bit is set to A or B, valid values are 0-7. \nIf AB Plane Bit is set to SLC ENCODED, valid values are 0-15."
    ::= { ptMTPLSLinkEntry 3 }

ptMTPLSLinkDREID OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "DRE to own this link."
    ::= { ptMTPLSLinkEntry 4 }

ptMTPLSLinkCage OBJECT-TYPE
    SYNTAX          Integer32 (-1..64)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Cage used by this link"
    ::= { ptMTPLSLinkEntry 5 }

ptMTPLSLinkChassisSlot OBJECT-TYPE
    SYNTAX          Integer32 (-1..255)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Chassis Slot used by this link"
    ::= { ptMTPLSLinkEntry 6 }

ptMTPLSLinkChannel OBJECT-TYPE
    SYNTAX          Integer32 (-1 | 1..65535)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Channel used by this link"
    ::= { ptMTPLSLinkEntry 7 }

ptMTPLSLinkL2State OBJECT-TYPE
    SYNTAX          INTEGER { deactivated(0), proving(1), inService(2), 
                        outOfService(3), failed(4), localProcOutage(5), 
                        remProcOutage(6), boProcOutage(7), 
                        localLoopback(8), notEnabled(9), 
                        notInitialized(10), userShutdown(11) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of MTP Level 2 for this link. Deactivated, Proving, In-Service, Out of Service, Failed, Local Processor Outage, Remote Processor Outage, Both Processor Outage, Local Loopback mode, Not Enabled, Not Initialized"
    ::= { ptMTPLSLinkEntry 8 }

ptMTPLSLinkL3State OBJECT-TYPE
    SYNTAX          INTEGER { deactivated(0), proving(1), inService(2), 
                        outOfService(3), locallyInhibited(4), 
                        remInhibited(5), bothInhibited(6), blocked(7), 
                        blockedInhibited(8), localProcOutage(9) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of MTP Level 3 for this link. Deactivated, Proving, In-Service, Out of Service, Locally Inhibited, Remotely Inhibited, Both Inhibited, Blocked, Blocked and inhibited, Local Processor Outage"
    ::= { ptMTPLSLinkEntry 9 }

ptMTPLSLinkCongestionLevel OBJECT-TYPE
    SYNTAX          INTEGER { false(0), level1(1), level2(2), level3(3), 
                        true(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Congestion level of this link. \n For ANSI, TTC and NTT networks the values range from 1 to 3 or FALSE if not congested. \n For ITU networks the value is either TRUE or FALSE."
    ::= { ptMTPLSLinkEntry 10 }

ptMTPLSLinkDiagnosticMessage OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Link management diagnostic message"
    ::= { ptMTPLSLinkEntry 11 }

ptMTPLSLinkL2DebugLevel OBJECT-TYPE
    SYNTAX          Integer32 (0..3)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "MTP Level 2 Debug Level. \nThe recommended value for this attribute is 0."
    ::= { ptMTPLSLinkEntry 12 }

ptMTPLSLinkUserActivated OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Link activation flag. Used on stack restart for alignment attempt."
    ::= { ptMTPLSLinkEntry 13 }

ptMTPLSLinkLinkName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Link Name based on the Linkset Name."
    ::= { ptMTPLSLinkEntry 14 }

ptMTPLSLinkAlarms OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Alarms affect the alarms associated with the Link. DISABLED mode: Alarms will not be raised. ENABLED mode: Alarms will be raised. \nValid values: ENABLED and DISABLED. \nDefault value: ENABLED."
    ::= { ptMTPLSLinkEntry 15 }

ptMTPLSLinkRxMSUCount OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Received MSU count, per second."
    ::= { ptMTPLSLinkEntry 16 }

