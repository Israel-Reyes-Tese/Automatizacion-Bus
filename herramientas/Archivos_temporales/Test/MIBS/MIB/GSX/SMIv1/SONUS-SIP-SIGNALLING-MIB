    SEQUENCE {
        sonusSipRegActRegStatusShelfIndex SonusShelfIndex,
        sonusSipRegActRegStatusSlotIndex SonusSlotIndex,
        sonusSipRegActRegStatusRegId    INTEGER,
        sonusSipRegActRegStatusAorName  DisplayString,
        sonusSipRegActRegStatusState    INTEGER,
        sonusSipRegActRegStatusContactURI DisplayString,
        sonusSipRegActRegStatusNextHopIpAddress       IpAddress,
        sonusSipRegActRegStatusNextHopPortNum     INTEGER,
        sonusSipRegActRegStatusRegistrarIpAddress         IpAddress,
        sonusSipRegActRegStatusRegistrarPortNum       INTEGER,
        sonusSipRegActRegStatusExtExpirationTime        INTEGER,
        sonusSipRegActRegStatusIntExpirationTime        INTEGER,
        sonusSipRegActRegStatusCreationTime   DateAndTime,
        sonusSipRegActRegStatusRegistrarDomainName          DisplayString,
        sonusSipRegActRegStatusNextHopIp6Address        Ipv6Address,
        sonusSipRegActRegStatusRegistrarIp6Address          Ipv6Address
    }

