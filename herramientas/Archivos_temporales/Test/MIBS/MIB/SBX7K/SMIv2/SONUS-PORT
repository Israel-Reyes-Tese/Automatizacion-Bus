        sonusEthernetPortEthernetPktPortStatusIntervalStatisticsAvgRxActualBW                                     Unsigned32,
        sonusEthernetPortEthernetPktPortStatusIntervalStatisticsAvgTxActualBW                                     Unsigned32,
