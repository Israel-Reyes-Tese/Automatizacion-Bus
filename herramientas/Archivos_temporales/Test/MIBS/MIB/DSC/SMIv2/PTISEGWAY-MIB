    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Slot number of the switch board."
    ::= { ptSWMExternalPortEntry 1 }

ptSWMExternalPortPortNumber OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This value corresponds to the number of the port on the switch."
    ::= { ptSWMExternalPortEntry 2 }

ptSWMExternalPortState OBJECT-TYPE
    SYNTAX          INTEGER { querying(0), extracted(1), available(2), 
                        pendingExtractionRequest(3), 
                        readyToBeExtracted(4), lostConnection(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Current status for this slot. Can be one of the following states; AVAILABLE: slot is communicating normally. UNKNOWN_STATE: slot is not present. LOST_CONNECTION: Switch Monitor has lost connection to this slot."
    ::= { ptSWMExternalPortEntry 3 }

ptSWMExternalPortNotification OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Specifies if notifications are to be created on port up/down or not. DISABLED: Events are not created for this port ENABLED: Events are created for this port."
    ::= { ptSWMExternalPortEntry 4 }

ptSWMExternalPortPortName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
