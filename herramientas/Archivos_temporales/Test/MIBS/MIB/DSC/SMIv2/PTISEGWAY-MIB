    SYNTAX          SEQUENCE OF PtMTPM2PALinkEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of Link attributes."
    ::= { ptMTPM2PALink 1 }

ptMTPM2PALinkEntry OBJECT-TYPE
    SYNTAX          PtMTPM2PALinkEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of MTPM2PALink attributes."
    INDEX           { ptConnGroupIndex, ptMTPM2PALinkMTP3NAManagerIndex, 
                      ptMTPM2PALinkMTP3NAIndex, 
                      ptMTPM2PALinkLinksetsIndex, 
                      ptMTPM2PALinkLinksetIndex, ptMTPM2PALinkIndex }
    ::= { ptMTPM2PALinkTable 1 }

PtMTPM2PALinkEntry ::=
    SEQUENCE {
        ptMTPM2PALinkContext            DisplayString,
        ptMTPM2PALinkAPC                Integer32,
        ptMTPM2PALinkSLC                Integer32,
        ptMTPM2PALinkDREID              Integer32,
        ptMTPM2PALinkLocalIPAddress1    DisplayString,
        ptMTPM2PALinkLocalIPAddress2    DisplayString,
        ptMTPM2PALinkLocalPort          Integer32,
        ptMTPM2PALinkRemoteIPAddress1   DisplayString,
        ptMTPM2PALinkRemoteIPAddress2   DisplayString,
        ptMTPM2PALinkRemotePort         Integer32,
        ptMTPM2PALinkL2State            INTEGER,
        ptMTPM2PALinkL3State            INTEGER,
        ptMTPM2PALinkCongestionLevel    INTEGER,
        ptMTPM2PALinkDiagnosticMessage  DisplayString,
        ptMTPM2PALinkDataDebugLevel     Integer32,
        ptMTPM2PALinkProtocolDebugLevel Integer32,
        ptMTPM2PALinkSoftwareDebugLevel Integer32,
        ptMTPM2PALinkSCTPDebugLevel     Integer32,
        ptMTPM2PALinkUserActivated      INTEGER,
        ptMTPM2PALinkTXAbatementL1      Integer32,
        ptMTPM2PALinkTXOnsetL1          Integer32,
        ptMTPM2PALinkTXAbatementL2      Integer32,
        ptMTPM2PALinkTXOnsetL2          Integer32,
        ptMTPM2PALinkTXAbatementL3      Integer32,
        ptMTPM2PALinkTXOnsetL3          Integer32,
        ptMTPM2PALinkRXAbatementBusy    Integer32,
        ptMTPM2PALinkRXOnsetBusy        Integer32,
        ptMTPM2PALinkTXQueueSize        Integer32,
        ptMTPM2PALinkLinksetSCTPValues  INTEGER,
        ptMTPM2PALinkHeartbeat          INTEGER,
        ptMTPM2PALinkHeartbeatInterval  Integer32,
        ptMTPM2PALinkRTOInitial         Integer32,
        ptMTPM2PALinkRTOMinimum         Integer32,
        ptMTPM2PALinkRTOMaximum         Integer32,
        ptMTPM2PALinkMaximumPathRetrans Integer32,
        ptMTPM2PALinkMaximumAssocRetrans Integer32,
        ptMTPM2PALinkBundling           INTEGER,
        ptMTPM2PALinkBundlingTO         Integer32,
        ptMTPM2PALinkDelayedACKTO       Integer32,
        ptMTPM2PALinkFastRetransThreshold Integer32,
        ptMTPM2PALinkChecksum           INTEGER,
        ptMTPM2PALinkM2PAVersion        INTEGER,
        ptMTPM2PALinkDataLinkBandwidth  Integer32,
        ptMTPM2PALinkLinkName           DisplayString,
        ptMTPM2PALinkGR310VirtualLink   INTEGER,
        ptMTPM2PALinkTxMSUCount         Integer32,
        ptMTPM2PALinkAlarms             INTEGER,
        ptMTPM2PALinkSendINIT           INTEGER,
        ptMTPM2PALinkRxMSUCount         Integer32,
        ptMTPM2PALinkMTP3NAManagerIndex Integer32,
        ptMTPM2PALinkMTP3NAIndex        Integer32,
        ptMTPM2PALinkLinksetsIndex      Integer32,
        ptMTPM2PALinkLinksetIndex       Integer32,
        ptMTPM2PALinkActions            INTEGER,
        ptMTPM2PALinkParentOID          OBJECT IDENTIFIER,
        ptMTPM2PALinkRowStatus          RowStatus,
        ptMTPM2PALinkIndex              Integer32
    }

ptMTPM2PALinkContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A condensed textual representation of the link object."
    ::= { ptMTPM2PALinkEntry 1 }

ptMTPM2PALinkAPC OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Adjacent SS7 point code of this link."
    ::= { ptMTPM2PALinkEntry 2 }

ptMTPM2PALinkSLC OBJECT-TYPE
    SYNTAX          Integer32 (0..15)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Signaling Link Code for this link. Value of 0-15."
    ::= { ptMTPM2PALinkEntry 3 }

ptMTPM2PALinkDREID OBJECT-TYPE
    SYNTAX          Integer32 (1..64)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "DRE ID of the DRE that owns this link. Since the corresponding DRE process runs on a specific slot, this ID implies the slot where the M2PA terminates locally, and hence the choice of valid local IP addresses for this link."
    ::= { ptMTPM2PALinkEntry 4 }

ptMTPM2PALinkLocalIPAddress1 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Primary local address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned in /etc/hosts and should resolve to a subset of the local IP Addresses configured for the slot where the link terminates. The slot where the link terminates is indirectly implied by the DRE ID."
    ::= { ptMTPM2PALinkEntry 5 }

ptMTPM2PALinkLocalIPAddress2 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Secondary local address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned in /etc/hosts and should resolve to a subset of the local IP Addresses configured for the slot where the link terminates. The slot where the link terminates is indirectly implied by the DRE ID."
    ::= { ptMTPM2PALinkEntry 6 }

ptMTPM2PALinkLocalPort OBJECT-TYPE
    SYNTAX          Integer32 (0..65535)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "M2PA Local Port."
    ::= { ptMTPM2PALinkEntry 7 }

ptMTPM2PALinkRemoteIPAddress1 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Primary remote address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned in /etc/hosts. This value is used as the SCTP Primary path to the remote SCTP endpoint."
    ::= { ptMTPM2PALinkEntry 8 }

ptMTPM2PALinkRemoteIPAddress2 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Secondary remote address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned in /etc/hosts."
    ::= { ptMTPM2PALinkEntry 9 }

