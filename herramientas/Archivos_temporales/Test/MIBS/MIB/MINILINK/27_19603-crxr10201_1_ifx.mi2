        -- 1.3.6.1.4.1.193.81.2.12.1
        xfIfXPacket OBJECT IDENTIFIER ::= { xfIfXMib 1 }
-- Tables and objects for packet based interfaces only.

        -- 1.3.6.1.4.1.193.81.2.12.1.1
        ifXPacketTable OBJECT-TYPE
            SYNTAX SEQUENCE OF IfXPacketEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "In this table there are only entries for packet based interfaces."
            ::= { xfIfXPacket 1 }


        -- 1.3.6.1.4.1.193.81.2.12.1.1.1
        ifXPacketEntry OBJECT-TYPE
            SYNTAX IfXPacketEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Entry in the table."
            INDEX { ifIndex }
            ::= { ifXPacketTable 1 }


        IfXPacketEntry ::=
            SEQUENCE {
                ifCountersClear
                    INTEGER,
                ifPacketSize
                    Integer32,
                ifPacketMaxSize
                    Integer32,
                ifPacketMinSize
                    Integer32
            }

        -- 1.3.6.1.4.1.193.81.2.12.1.1.1.1
        ifCountersClear OBJECT-TYPE
            SYNTAX INTEGER { reset(1) }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object resets all packet related counters in ifTable and ifXTable."
            ::= { ifXPacketEntry 1 }


        -- 1.3.6.1.4.1.193.81.2.12.1.1.1.2
        ifPacketSize OBJECT-TYPE
            SYNTAX Integer32
            UNITS "bytes"
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object can be used to configure:
                
                - MTU size for Site-LAN, LAN-DCN interfaces
                - MRU size for PPP interfaces
                
                Default value is 1500."
            ::= { ifXPacketEntry 2 }

        -- 1.3.6.1.4.1.193.81.2.12.1.1.1.3
        ifPacketMaxSize OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "This object holds the maximum value accepted for the ifPacketSize"
            ::= { ifXPacketEntry 3 }

        -- 1.3.6.1.4.1.193.81.2.12.1.1.1.4
        ifPacketMinSize OBJECT-TYPE
            SYNTAX Integer32 
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "This object holds the minimum value accepted for the ifPacketSize"
            ::= { ifXPacketEntry 4 }


