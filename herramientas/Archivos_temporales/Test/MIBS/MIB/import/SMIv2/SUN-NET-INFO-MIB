    OBJECTS         { sunNimNotificationEventId, sunNimNotificationTime, 
                      sunNimNotificationObject, 
                      sunNimNotificationAdditionalInfo, 
                      sunNimNotificationAdditionalText, 
                      sunNimNotificationPerceivedSeverity, 
                      sunNimNotificationProbableCause, 
                      sunNimNotificationSpecificProblem, 
                      sunNimNotificationRepairAction }
    STATUS          current
    DESCRIPTION     
            "Indicates that an equipment alarm has occurred as defined
             in X.721, sec. 13.4."
    ::= { sunNimMIBTrapPrefix 5 }

