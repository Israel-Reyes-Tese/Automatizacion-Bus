        ptDSCDSCNodesParentOID          OBJECT IDENTIFIER,
        ptDSCDSCNodesRowStatus          RowStatus,
        ptDSCDSCNodesIndex              INTEGER
    }

ptDSCDSCNodesRoutingHopCounter OBJECT-TYPE
    SYNTAX          INTEGER (1..15)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Routing Hop Counter (1 to 15). \nIf a request is processed by more than the defined DSC and/or UA Nodes, it is discarded as unroutable."
    ::= { ptDSCDSCNodesEntry 1 }

ptDSCDSCNodesDSCLicense OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "DSC license (UNLICENSED/LICENSED). \nThis license must be available to configure the DSC functionality."
    ::= { ptDSCDSCNodesEntry 2 }

ptDSCDSCNodesErrors OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable the logging of DSC Nodes error messages to the debuglog (NOT LOGGED/LOGGED). \nThe logging of DSC Nodes errors should be set to LOGGED for all production systems. \nDefault value: LOGGED."
    ::= { ptDSCDSCNodesEntry 3 }

ptDSCDSCNodesSetupManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable the logging of startup and setup information to the debuglog (NOT LOGGED/LOGGED). \nThese logs are not related to traffic processing and have no impact on the performance of a running system. \nThe logging of DSC startup and setup information should be set to LOGGED for most production systems. \nDefault value: LOGGED."
    ::= { ptDSCDSCNodesEntry 4 }

ptDSCDSCNodesDistributionManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable the logging of inter-DSC process communication to the debuglog (NOT LOGGED/LOGGED). \nThe resultant log messages are intended for Sonus Networks engineering staff and are not intended for customer use. \nThe logging of DSC inter-process information should be set to NOT_LOGGED on all production systems so that the CPU is not unduly taxed. Default value: NOT LOGGED."
    ::= { ptDSCDSCNodesEntry 5 }

ptDSCDSCNodesMaximumQueuedKBytes OBJECT-TYPE
    SYNTAX          INTEGER (2000..8000000)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Maximum number of message bytes in all queues in a DSC process. \nObsolete as of release 17.0, but unused and hidden prior to that. \nExcludes overhead such as list of nodes and routing information. \nRange: 2000-8000000 with default is 2000000 (~2GB)."
    ::= { ptDSCDSCNodesEntry 6 }

ptDSCDSCNodesCapacityParameterSet OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Capacity parameter set for nodal congestion."
    ::= { ptDSCDSCNodesEntry 7 }

ptDSCDSCNodesRequestLifespan OBJECT-TYPE
    SYNTAX          INTEGER (10..240)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Request message lifespan in seconds with return error on expiry. \nRegardless of how many times the request may have been retransmitted, on lifespan expiry, an answer with error UNABLE_TO_DELIVER is returned. \nValid values: 10 to 240 seconds. \nDefault value: 60 seconds."
    ::= { ptDSCDSCNodesEntry 9 }

ptDSCDSCNodesIMFLicense OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IMF license (UNLICENSED/LICENSED). \nThis license must be available to configure the IMF functionality on a per ADN basis."
    ::= { ptDSCDSCNodesEntry 10 }

ptDSCDSCNodesTPSLicense OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Transactions Per Second license (count). \nThe TPS license has an associated MSA for threshold alarms."
    ::= { ptDSCDSCNodesEntry 11 }

ptDSCDSCNodesTopologyMappingLicense OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Topology Mapping license (UNLICENSED/LICENSED). \nThis license must be available to configure the Topology Mapping functionality on a per DSC Node basis."
    ::= { ptDSCDSCNodesEntry 12 }

ptDSCDSCNodesRetransmitLimit OBJECT-TYPE
    SYNTAX          INTEGER { unlimited(-1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Limit to retry individual node messages upon encountering a failure condition (not including redirect attempts). \nValid values: UNLIMITED (-1), 0 to 50 attempts. \nDefault value: 10 attempts. \nChoosing 0 does not allow for any retries. \nChoosing UNLIMITED provides an unlimited number of retries."
    ::= { ptDSCDSCNodesEntry 13 }

ptDSCDSCNodesSystemTPSLicense OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "System Transactions Per Second license (count). \nThe System TPS license has an associated System TPS MSA for threshold alarms."
    ::= { ptDSCDSCNodesEntry 15 }

ptDSCDSCNodesCurrentDaySystemTPSHighWaterMark OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Current Day System Transactions Per Second High Water Mark (count). \nThe current System TPS daily data will be moved to previous day data and then reset at 12:00 a.m. every day."
    ::= { ptDSCDSCNodesEntry 16 }

ptDSCDSCNodesCurrentWeekSystemTPSHighWaterMark OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Current Week System Transactions Per Second High Water Mark (count) from Sunday to Saturday. \nThe current System TPS weekly data will be moved to previous week data and reset at 12:00 a.m. every Sunday morning."
    ::= { ptDSCDSCNodesEntry 17 }

ptDSCDSCNodesCurrentMonthSystemTPSHighWaterMark OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Current Month System Transactions Per Second High Water Mark (count). \nThe current System TPS monthly data will be moved to previous month data and reset on the 1st day of each month."
    ::= { ptDSCDSCNodesEntry 18 }

ptDSCDSCNodesCurrentYearSystemTPSHighWaterMark OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Current Year System Transactions Per Second High Water Mark (count). \nThe current System TPS yearly data will be moved to previous year and reset on the 1st day of each year."
    ::= { ptDSCDSCNodesEntry 19 }

ptDSCDSCNodesPreviousDaySystemTPSHighWaterMark OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Previous Day System Transactions Per Second High Water Mark (count). \nThe previous System TPS daily data will be updated with current daily data at 12:00 a.m. every day. If today is Tuesday, this will represent Monday TPS high water mark data."
    ::= { ptDSCDSCNodesEntry 20 }

ptDSCDSCNodesPreviousWeekSystemTPSHighWaterMark OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Previous Week System Transactions Per Second High Water Mark (count) from Sunday to Saturday. \nThe previous System TPS weekly data will be updated with current weekly data at 12:00 a.m. every Sunday morning."
    ::= { ptDSCDSCNodesEntry 21 }

ptDSCDSCNodesPreviousMonthSystemTPSHighWaterMark OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Previous Month System Transactions Per Second High Water Mark (count). \nThe previous System TPS monthly data will be updated with current monthly data on the 1st day of each month."
    ::= { ptDSCDSCNodesEntry 22 }

ptDSCDSCNodesPreviousYearSystemTPSHighWaterMark OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Previous Year System Transactions Per Second High Water Mark (count). \nThe previous System TPS yearly data will be updated with current yearly data on the 1st day of each year."
    ::= { ptDSCDSCNodesEntry 23 }

ptDSCDSCNodesRealmStatsName OBJECT-TYPE
    SYNTAX          INTEGER { name(0), alias(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Realm information used to form the Instances name in statistics files. \nThe Alias is used for configuration purposes and by default will also be used in Realm Accounting statistics reporting. Since the Realm Name is typically more static in nature, some customers may prefer to use the that value, especially in cases where scripts are used to monitor the data for other purposes. \nValid values: NAME (0), ALIAS (1). \nDefault value: ALIAS"
    ::= { ptDSCDSCNodesEntry 25 }

ptDSCDSCNodesLocalOverloadParameters OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Capacity parameter set for general Local Overload detection. \nThe indicated capacity parameter set specifies the message delay \nand queue size limits for the incoming messages that are used to \ndefine the severify levels of the local overload condition. \nThe fields used are: \n Queue Size in Seconds \n Queue Size in Messages \n Congestion Onset 1,2,3 \n Congestion Abatement 1,2,3 \n Overload Handling 1,2,3 \nValid values: Empty (disabled) or existing Capacity Parameter Set \nDefault value: Empty."
    ::= { ptDSCDSCNodesEntry 26 }

ptDSCDSCNodesLDLDebugLevel OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The LDL Debug Level should be set to 2 (default) or lower on all production systems, unless directed by support. Otherwise the system performance may be impacted by an undue amount of time logging messages. Valid range is (0 [NOT LOGGED], 1 to 10)."
    ::= { ptDSCDSCNodesEntry 27 }

ptDSCDSCNodesQueueSize OBJECT-TYPE
    SYNTAX          INTEGER { auto(0) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Queue Size in megabytes. \nThe maximum number of message bytes that may be queued, measured in megabytes. \nRange: 1 to 50000."
    ::= { ptDSCDSCNodesEntry 28 }

ptDSCDSCNodesQueueUsageThreshold1 OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Queue Usage Alarm Threshold 1. \nIf the message queue is used more than this percentage an alarm is raised. \nRange: 1 to 100."
    ::= { ptDSCDSCNodesEntry 29 }

