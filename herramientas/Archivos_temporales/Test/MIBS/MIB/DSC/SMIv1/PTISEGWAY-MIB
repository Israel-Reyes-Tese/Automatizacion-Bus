PtLBTargetEntry ::=
    SEQUENCE {
        ptLBTargetName                  DisplayString,
        ptLBTargetLoadBalancing         INTEGER,
        ptLBTargetInternalID            INTEGER,
        ptLBTargetStatus                DisplayString,
        ptLBTargetLoadBalancerIndex     INTEGER,
        ptLBTargetTargetsIndex          INTEGER,
        ptLBTargetActions               INTEGER,
        ptLBTargetParentOID             OBJECT IDENTIFIER,
        ptLBTargetRowStatus             RowStatus,
        ptLBTargetIndex                 INTEGER
    }

ptLBTargetName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Load Balancer Target Name. \n Valid values: alphanumeric characters, dashes, and underscores (maximum size of 32)."
    ::= { ptLBTargetEntry 1 }

ptLBTargetLoadBalancing OBJECT-TYPE
    SYNTAX          INTEGER { staticMapping(0), roundRobin(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Select the load balancing algorithm that applies to this target. \nValid values: ROUND ROBIN: Assigns each new connection sequentially around the target slots but assigns more connections to slots with greater capacity. \nCapacity is indicated by the user-assigned weight factor. \nSTATIC MAPPING: The user configures the mappings between connections and the target slots. \nDefault value: STATIC MAPPING."
    ::= { ptLBTargetEntry 2 }

ptLBTargetInternalID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Target unique internal ID. \nReserved for internal use."
    ::= { ptLBTargetEntry 3 }

ptLBTargetStatus OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Configuration status. \nIndicates the status of the most recent configuration modification \nor that configuration is in progress. \nBUSY indicates that new configuration is temporarily blocked."
    ::= { ptLBTargetEntry 4 }

ptLBTargetLoadBalancerIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is LoadBalancer."
    ::= { ptLBTargetEntry 242 }

ptLBTargetTargetsIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is Targets."
    ::= { ptLBTargetEntry 243 }

ptLBTargetActions OBJECT-TYPE
    SYNTAX          INTEGER { forceRebalancing(1), ignoreRebalancing(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The OID of all possible actions for this instance"
    ::= { ptLBTargetEntry 252 }

