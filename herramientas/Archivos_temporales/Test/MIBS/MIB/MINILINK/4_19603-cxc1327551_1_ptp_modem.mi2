------------------------------------------------------------------------------
-- xfMultiCarrierMmuTable
------------------------------------------------------------------------------
    xfMultiCarrierMmuTable OBJECT-TYPE
        SYNTAX SEQUENCE OF XfMultiCarrierMmuEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
            Table for Radio Link Multi Carrier MMU managed objects.
            The table is indexed with entPhysicalIndex.
            "
        ::= { xfRadioLinkPtpModemObjects 6 }

    xfMultiCarrierMmuEntry OBJECT-TYPE
        SYNTAX XfMultiCarrierMmuEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An entry in the xfMultiCarrierMmuTable."
        INDEX { entPhysicalIndex}
        ::= { xfMultiCarrierMmuTable 1 }

    XfMultiCarrierMmuEntry ::=
        SEQUENCE
        {
            xfMmuAlarmStatus                BITS,
            xfMmuSlotPosition               Integer32,
            xfMmuAtpcVersion                INTEGER,
            xfMmuInsuffPowerResourcesReset  INTEGER,
            xfMmuCapability                 BITS
        }

--
-- xfMultiCarrierMmuTable MOs
--

    xfMmuAlarmStatus OBJECT-TYPE
        SYNTAX BITS
        {
            icc0(0),
            icc1(1),
            icc2(2),
            insufficientPower0(3),
            insufficientPower1(4),
            insufficientPower2(5),
            xpicLos0(6),
            xpicLos1(7),
            xpicLos2(8),
            xpicCableDisconnected0(9),
            xpicCableDisconnected1(10),
            xpicCableDisconnected2(11)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
            This object shows the MMU alarm status.
            "
        ::= { xfMultiCarrierMmuEntry 1 }

    xfMmuSlotPosition OBJECT-TYPE
        SYNTAX Integer32 (0..21)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
            The AMM slot position in which the MMU is inserted in.
            0 = unknown, 1-21 = slot position.
            Position 1 is reserved for PFU in AMM 20p.
            "
        ::= { xfMultiCarrierMmuEntry 2 }

    xfMmuAtpcVersion OBJECT-TYPE
        SYNTAX INTEGER
        {
            other(1),
            version1(2),
            version2(3),
            version3(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
            This object represents the MMU ATPC version.
            "
        ::= { xfMultiCarrierMmuEntry 3 }

    xfMmuInsuffPowerResourcesReset OBJECT-TYPE
        SYNTAX INTEGER
        {
            other(1),
            noReset(2),
            reset(3)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
            This object resets 'Insufficient Power Resources' alarm.
            The RAU is powered, but RAU Tx will be still OFF:
            it's in charge of the operator to switch it ON again.
            "
        ::= { xfMultiCarrierMmuEntry 4 }

    xfMmuCapability OBJECT-TYPE
        SYNTAX BITS
        {
           capTDM(0),
           capTDMTwoDirectionsHw(1),
           capMLHC(2),
           capRLProtection(3),
           capRAUDuplex(4),
           capTDMTwoDirectionsSw(5),
           capMLHCTwoDirectionsHw(6),
           capRLEncryption(7),
           capMIMO(8)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
            A bitmap that shows supported MMU capabilities.
            capTDM(0)                 : The MMU supports TDM
            capTDMTwoDirections(1)    : The MMU Hw supports TDM in 2 directions
            capMLHC(2)                : The MMU supports MLHC
            capRLProtection(3)        : The MMU supports RLP
            capRAUDuplex(4)           : The MMU supports RAU Duplex
            capTDMTwoDirectionsSw(5)  : The Sw supports TDM in 2 directions
            capMLHCTwoDirectionsHw(6) : The MMU Hw supports MLHC in 2 directions
            capRLEncryption(7)        : The MMU Hw supports Radio Link Encryption
            capMIMO(8)                : The MMU Hw supports MIMO
            "
        ::= { xfMultiCarrierMmuEntry 5 }

