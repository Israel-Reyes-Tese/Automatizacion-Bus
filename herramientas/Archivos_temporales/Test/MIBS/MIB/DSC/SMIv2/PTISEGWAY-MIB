PtDSCResultRecordEntry ::=
    SEQUENCE {
        ptDSCResultRecordRecordType     INTEGER,
        ptDSCResultRecordCost           Integer32,
        ptDSCResultRecordRecordValue    DisplayString,
        ptDSCResultRecordWeight         Integer32,
        ptDSCResultRecordDSCIndex       Integer32,
        ptDSCResultRecordDSCRoutingTablesIndex      Integer32,
        ptDSCResultRecordResultTableIndex Integer32,
        ptDSCResultRecordParentOID      OBJECT IDENTIFIER,
        ptDSCResultRecordRowStatus      RowStatus,
        ptDSCResultRecordIndex          Integer32
    }

ptDSCResultRecordRecordType OBJECT-TYPE
    SYNTAX          INTEGER { relay(0), redirect(1), realmRedirect(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Record Type (RELAY/REDIRECT/REALM_REDIRECT). \nRELAY: The message is forwarded (relayed) to the ADN represented by Record Value. \nREDIRECT: An answer message is returned to the originator instructing it to try the peer given in Record Value. \nREALM_REDIRECT: An answer message is returned to the originator instructing it to try the realm given in Record Value. \nDefault value: RELAY (reference only)."
    ::= { ptDSCResultRecordEntry 1 }

ptDSCResultRecordCost OBJECT-TYPE
    SYNTAX          Integer32 (1..99)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Cost of the result record (1 to 99). \nLowest available record is used for routing. \nIf more than one record have the lowest available cost, loadsharing is performed."
    ::= { ptDSCResultRecordEntry 2 }

ptDSCResultRecordRecordValue OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Record Value representing an ADN for RELAY, URI for REDIRECT and Realm Definition for REALM_REDIRECT type. \n \nFor RELAY records, if a FQDN Diameter ID is specified, the value will be replaced by the associated ADN name. \nValid values: existing ADN Diameter ID (FQDN format) or ADN name (RELAY). \n \n URI format (REDIRECT). \nExamples: URI format \n NO Transport security URI format - \n aaa://FQDN [port] [transport] [protocol] \n aaa://host.example.com:1813;transport=tcp;protocol=diameter \n Transport security URI format - \n aaas://FQDN [port] [transport] [protocol] \n aaas://host.example.com:1813;transport=tcp;protocol=diameter \n \nFor REALM_REDIRECT records, the realm record value must be the Alias of an existing Realm Definition. "
    ::= { ptDSCResultRecordEntry 3 }

ptDSCResultRecordWeight OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Weight of the Diameter ID (0 to 400000). \nThe Diameter ID Weight is used to indicate the relative message capacity of this Diameter ID when load sharing on the server pool. \nValid values: 0 to 100 (ROUND ROBIN) \n 0 to 4000000 (LEAST LOADED). \nDefault value: 10 (ADN) and 0 (non-ADN) (ROUND ROBIN) \n 2000 (ADN) and 0 (non-ADN) (LEAST LOADED)"
    ::= { ptDSCResultRecordEntry 4 }

ptDSCResultRecordDSCIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSC."
    ::= { ptDSCResultRecordEntry 242 }

ptDSCResultRecordDSCRoutingTablesIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSCRoutingTables."
    ::= { ptDSCResultRecordEntry 243 }

ptDSCResultRecordResultTableIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is ResultTable."
    ::= { ptDSCResultRecordEntry 244 }

