sonusDsx1FarEndIntervalDayTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDsx1FarEndIntervalDayEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The DS1 Near End Interval Table contains various
             far end statistics collected by each DS1 interface
             over the previous 24 hours of operation."
    ::= { sonusDsx1MIBObjects 12 }

sonusDsx1FarEndIntervalDayEntry OBJECT-TYPE
    SYNTAX          SonusDsx1FarEndIntervalDayEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "An entry in the DS1 Current table."
    INDEX           { sonusDsx1FarEndIntervalDayShelfIndex, 
                      sonusDsx1FarEndIntervalDaySlotIndex, 
                      sonusDsx1FarEndIntervalDayPortIndex, 
                      sonusDsx1FarEndIntervalDayDs1Index }
    ::= { sonusDsx1FarEndIntervalDayTable 1 }

SonusDsx1FarEndIntervalDayEntry ::=
    SEQUENCE {
        sonusDsx1FarEndIntervalDayShelfIndex    SonusShelfIndex,
        sonusDsx1FarEndIntervalDaySlotIndex   SonusSlotIndex,
        sonusDsx1FarEndIntervalDayPortIndex   INTEGER,
        sonusDsx1FarEndIntervalDayDs1Index  INTEGER,
        sonusDsx1FarEndIntervalDayLESs  PerfCurrentCount,
        sonusDsx1FarEndIntervalDayPCVs  PerfCurrentCount,
        sonusDsx1FarEndIntervalDayPESs  PerfCurrentCount,
        sonusDsx1FarEndIntervalDayPSESs PerfCurrentCount,
        sonusDsx1FarEndIntervalDaySEFSs PerfCurrentCount,
        sonusDsx1FarEndIntervalDayCSSs  PerfCurrentCount,
        sonusDsx1FarEndIntervalDayUASs  PerfCurrentCount,
        sonusDsx1FarEndIntervalDayLineIDF TruthValue,
        sonusDsx1FarEndIntervalDayPathIDF TruthValue,
        sonusDsx1FarEndIntervalDayFC    INTEGER
    }

