    SYNTAX          OCTET STRING
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is a comma separated list of component Ids
             which can be managed by the given user with the
             given role.
             This attribute is not modifiable"
    ::= { rsiUserRoleEntry 1 }

rsiUserRoleRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is for dynamic addition/deletion of roles
             associated with a user.
             For creating a new user-role entry, only 'createAndGo' 
             is supported. For deleting an existing row, 'delete'
             is supported"
    ::= { rsiUserRoleEntry 2 }

rsiPerformanceHistoryTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiPerformanceHistoryEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains the performance history of all
             the components."
    ::= { rsiCommonMib 14 }

rsiPerformanceHistoryEntry OBJECT-TYPE
    SYNTAX          RsiPerformanceHistoryEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of the performance oid and its value for
             a given component at a give time. 
             This table will give the value of a performance OID
             for a given component, at the given time.
             Note: rsiCompId is index of a foreign table."
    INDEX           { rsiCompId, rsiPerfOid, rsiPerfTimestamp }
    ::= { rsiPerformanceHistoryTable 1 }

RsiPerformanceHistoryEntry ::=
    SEQUENCE {
        rsiPerfTimestamp                INTEGER,
        rsiPerfOid                      OBJECT IDENTIFIER,
        rsiPerfValue                    DisplayString
    }

rsiPerfTimestamp OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Time at which the performance was measured.
             This is the time in seconds since the Epoch 
             (00:00:00 UTC, January 1, 1970)."
    ::= { rsiPerformanceHistoryEntry 1 }

rsiPerfOid OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "OID in MIB for which the performance was measured."
    ::= { rsiPerformanceHistoryEntry 2 }

rsiPerfValue OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "value of the OID for which performance was measured."
    ::= { rsiPerformanceHistoryEntry 3 }

rsiPerformanceThresholdTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiPerformanceThresholdEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains the performance thresholds for all
             the OIDs."
    ::= { rsiCommonMib 15 }

rsiPerformanceThresholdEntry OBJECT-TYPE
    SYNTAX          RsiPerformanceThresholdEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of the performance oid and its rising
             and falling threshold values, along with the alarm
             severity of the alarm to be raised."
    INDEX           { rsiPerfThresholdOid, rsiPerfThresholdId }
    ::= { rsiPerformanceThresholdTable 1 }

RsiPerformanceThresholdEntry ::=
    SEQUENCE {
        rsiPerfThresholdOid             OBJECT IDENTIFIER,
        rsiPerfThresholdId              INTEGER,
        rsiPerfThresholdRisingVal       INTEGER,
        rsiPerfThresholdFallingVal      INTEGER,
        rsiPerfThresholdSeverity        INTEGER,
        rsiPerfThresholdRowStatus       RowStatus
    }

rsiPerfThresholdOid OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "OID in MIB for which the performance threshold has
             been defined."
    ::= { rsiPerformanceThresholdEntry 1 }

rsiPerfThresholdId OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Unique identifier of the given performance threshold."
    ::= { rsiPerformanceThresholdEntry 2 }

rsiPerfThresholdRisingVal OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "High Water Mark for the given threshold"
    ::= { rsiPerformanceThresholdEntry 3 }

rsiPerfThresholdFallingVal OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Low Water Mark for the given threshold"
    ::= { rsiPerformanceThresholdEntry 4 }

rsiPerfThresholdSeverity OBJECT-TYPE
    SYNTAX          INTEGER { critical(0), major(1), minor(2), 
                        warning(3), normal(4) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Severity of the alarm to be raised on crossing the
             threshold."
    ::= { rsiPerformanceThresholdEntry 5 }

rsiPerfThresholdRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is for dynamic addition/deletion of performance
             thresholds.
             For creating a new threshold entry, only 'createAndGo' is
             supported. For deleting an existing row, 'destroy'
             is supported"
    ::= { rsiPerformanceThresholdEntry 6 }

rsiAlarmPoliciesTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiAlarmPoliciesEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains the performance thresholds for all
             the OIDs."
    ::= { rsiCommonMib 16 }

rsiAlarmPoliciesEntry OBJECT-TYPE
    SYNTAX          RsiAlarmPoliciesEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of the alarm policies associated with a 
             severity."
    INDEX           { rsiAlarmPolicySeverity }
    ::= { rsiAlarmPoliciesTable 1 }

RsiAlarmPoliciesEntry ::=
    SEQUENCE {
        rsiAlarmPolicySeverity          INTEGER,
        rsiAlarmSeverityEscalationTimeInterval      INTEGER,
        rsiAlarmAutoAckUpperThresholdLimit  INTEGER,
        rsiAlarmAutoAckRetentionTimeInterval    INTEGER
    }

rsiAlarmPolicySeverity OBJECT-TYPE
    SYNTAX          INTEGER { critical(0), major(1), minor(2), 
                        warning(3), normal(4) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "severity of the alarm for which this policy has 
             been defined."
    ::= { rsiAlarmPoliciesEntry 1 }

rsiAlarmSeverityEscalationTimeInterval OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This signifies the time interval for which an un-acked
             alarms' severity may not be raised to the next level.
             Note the unit of this time interval is hrs.This can not be
             set for critical severity"
    ::= { rsiAlarmPoliciesEntry 2 }

rsiAlarmAutoAckUpperThresholdLimit OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The number of un-acked alarms that are to be retained
             for the given severity at any time."
    ::= { rsiAlarmPoliciesEntry 3 }

rsiAlarmAutoAckRetentionTimeInterval OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This signifies that the time interval for which all
             un-acked alarms are not auto acked.
             Note the unit of this time interval is hrs."
    ::= { rsiAlarmPoliciesEntry 4 }

rsiLoggedInUsersTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiLoggedInUsersEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains the information about the currently
             logged in users. "
    ::= { rsiCommonMib 17 }

