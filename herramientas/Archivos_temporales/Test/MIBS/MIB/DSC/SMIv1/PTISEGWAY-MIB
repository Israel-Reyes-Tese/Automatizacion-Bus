        ptL4CvtrL4CvtrConfigurationLDLDebug   INTEGER,
        ptL4CvtrL4CvtrConfigurationActions  INTEGER,
        ptL4CvtrL4CvtrConfigurationParentOID    OBJECT IDENTIFIER,
        ptL4CvtrL4CvtrConfigurationRowStatus    RowStatus,
        ptL4CvtrL4CvtrConfigurationIndex INTEGER
    }

ptL4CvtrL4CvtrConfigurationActivePeerID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The active peer identifier as defined by the converter LDL file. \nThe converter LDL file (cvtr_ldl.conf) is located in the /opt/cpu_ss7gw/current/data directory."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 1 }

ptL4CvtrL4CvtrConfigurationTrafficStatus OBJECT-TYPE
    SYNTAX          INTEGER { inactive(0), active(1), unlicensed(2) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Traffic status (INACTIVE/ACTIVE/UNLICENSED). \nThe traffic should only be inactive during the initial configuration of the converter system."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 2 }

ptL4CvtrL4CvtrConfigurationReregistrationTimer OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Interval at which reregistration is initiated (in seconds). \nSet to 0 (DISABLED) to disable the reregistration. Default 60 seconds. Valid range is 0 to 999999."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 3 }

ptL4CvtrL4CvtrConfigurationPakistanCBN OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Pakistan fixed Charge Band Number parameter value. \nUsed in the Pakistan-ITU converter only for default settings. Valid range is 0 to 255."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 4 }

ptL4CvtrL4CvtrConfigurationFastAnswerTimer OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Lifespan of Fast Answer record (in seconds). \nUsed in the ANSI-ITU converter only for ACM and ANM conversion. \nSet to 0 (DISABLED) to disable the Fast Answer feature. Default 60 seconds. Valid range is 0 to 999999"
    ::= { ptL4CvtrL4CvtrConfigurationEntry 5 }

ptL4CvtrL4CvtrConfigurationCGAckTimer OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Lifespan of Circuit Group Acknowledgement record (in seconds). \nUsed in the ANSI-ITU converter only for circuit group acknowledgement message management due to circuit range incompatibility between the variants. \nDefault 60 seconds. Valid range is 1 to 999999."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 6 }

ptL4CvtrL4CvtrConfigurationErrors OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable debug logging of the converter errors; \nshould be logged for production systems."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 7 }

ptL4CvtrL4CvtrConfigurationSetupManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable debug logging of the setup management; \nshould be logged for production systems."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 8 }

ptL4CvtrL4CvtrConfigurationExecutionTrace OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable debug logging of the execution trace; \nshould not be logged for production systems."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 9 }

ptL4CvtrL4CvtrConfigurationLayerCommunication OBJECT-TYPE
