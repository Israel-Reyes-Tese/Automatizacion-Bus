rsiTODRuleTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiTODRuleEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains the rules for system access depending upon the time of the day"
    ::= { rsiCommonMib 47 }

rsiTODRuleEntry OBJECT-TYPE
    SYNTAX          RsiTODRuleEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of the rules for system access depending upon the time of the day"
    INDEX           { rsiRuleName }
    ::= { rsiTODRuleTable 1 }

RsiTODRuleEntry ::=
    SEQUENCE {
        rsiRuleName                     DisplayString,
        rsiRuleId                       INTEGER,
        rsiRuleType                     INTEGER,
        rsiStartTime                    DisplayString,
        rsiEndTime                      DisplayString,
        rsiDOW                          DisplayString,
        rsiTODRuleRowStatus             RowStatus
    }

rsiRuleName OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Name for the Rule."
    ::= { rsiTODRuleEntry 1 }

rsiRuleId OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Autogenerated Identifier for the Rule."
    ::= { rsiTODRuleEntry 2 }

rsiRuleType OBJECT-TYPE
    SYNTAX          INTEGER { exactRule(0), dowRule(1), alldays(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Type of the rule. exactRule is on the basis of exact timing and the dowRule is on the basis of day of week and depend on the time of the day. And alldays depend on the time of the day only."
    ::= { rsiTODRuleEntry 3 }

rsiStartTime OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(19))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Start time: For exactRule Format is YYYY:MM:DD HH:MM:SS. For dowRule and alldays Format is XXXX:XX:XX HH:MM:SS"
    ::= { rsiTODRuleEntry 4 }

rsiEndTime OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(19))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "End time: For exactRule Format is YYYY:MM:DD HH:MM:SS. For dowRule and alldays Format is XXXX:XX:XX HH:MM:SS"
    ::= { rsiTODRuleEntry 5 }

rsiDOW OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            " This is a hexadecimal bit mask field depending on the day of the week. The bit mask is as per the rule ALL 0x0, MON 0x0001, TUE 0x0002, WED 0x0004, THU 0x0008, FRI 0x0010, SAT 0x0020, SUN 0x0040"
    ::= { rsiTODRuleEntry 6 }

rsiTODRuleRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is for dynamic addition/deletion of TOD Rules.
             For creating a new TOD Rules, only 'createAndGo' is
             supported. For deleting an existing row, 'delete'
             is supported"
    ::= { rsiTODRuleEntry 7 }

rsiUserRoleTODRuleTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiUserRoleTODRuleEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains the mapping of the User, his role and the rules for system access depending upon the time of the day. rsiUserId id from the table rsiUserTable rsiRoleId is from the table rsiRoleId and rsiRuleId is from the table rsiTODRuleTable"
    ::= { rsiCommonMib 48 }

rsiUserRoleTODRuleEntry OBJECT-TYPE
    SYNTAX          RsiUserRoleTODRuleEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of the mapping of the User, his role and the rules for system access depending upon the time of the day"
    INDEX           { rsiUserId, rsiRoleId, rsiRuleId }
    ::= { rsiUserRoleTODRuleTable 1 }

RsiUserRoleTODRuleEntry ::=
    SEQUENCE {
        rsiUserRoleTODRuleRowStatus     RowStatus
    }

rsiUserRoleTODRuleRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is for dynamic addition/deletion of the mapping of TOD Rules with the User and his role.
             For creating a new TOD Rules, only 'createAndGo' is
             supported. For deleting an existing row, 'delete'
             is supported"
    ::= { rsiUserRoleTODRuleEntry 1 }

