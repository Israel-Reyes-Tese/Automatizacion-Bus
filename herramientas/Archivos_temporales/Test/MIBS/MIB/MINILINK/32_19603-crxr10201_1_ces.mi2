		-- 1.3.6.1.4.1.193.81.4.4.1.2.1.1.8
		xfCesServerMaxJtrBfrDepth OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "microseconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
