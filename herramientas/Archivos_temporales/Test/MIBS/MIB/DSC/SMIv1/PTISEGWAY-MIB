ptiwfIWFSS7 OBJECT IDENTIFIER
    ::= { ptConnGroup 1905 }

ptiwfIWFSS7Table OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtiwfIWFSS7Entry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptiwfIWFSS7 1 }

ptiwfIWFSS7Entry OBJECT-TYPE
    SYNTAX          PtiwfIWFSS7Entry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of iwfIWFSS7 attributes."
    INDEX           { ptConnGroupIndex, ptiwfIWFSS7Index }
    ::= { ptiwfIWFSS7Table 1 }

PtiwfIWFSS7Entry ::=
    SEQUENCE {
        ptiwfIWFSS7SCCPNA               INTEGER,
        ptiwfIWFSS7DPC                  INTEGER,
        ptiwfIWFSS7SCCPPAEncoding       INTEGER,
        ptiwfIWFSS7PCDisplayFormat      DisplayString,
        ptiwfIWFSS7PCRoutingFormat      DisplayString,
        ptiwfIWFSS7SCCPFiller           INTEGER,
        ptiwfIWFSS7ConnectivityStatus   INTEGER,
        ptiwfIWFSS7SGSNSS7Number        DisplayString,
        ptiwfIWFSS7IMSIRealm            DisplayString,
        ptiwfIWFSS7MateSTPLPC           INTEGER,
        ptiwfIWFSS7MateSTPID            INTEGER,
        ptiwfIWFSS7HLRSS7Number         DisplayString,
        ptiwfIWFSS7ParentOID            OBJECT IDENTIFIER,
        ptiwfIWFSS7RowStatus            RowStatus,
        ptiwfIWFSS7Index                INTEGER
    }

ptiwfIWFSS7SCCPNA OBJECT-TYPE
    SYNTAX          INTEGER (1..99)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Associated SCCP NA responsible for the IWF traffic (1 to 99). \nCan only be modified when the IWF traffic is deactivated. \nDefault value: 1"
    ::= { ptiwfIWFSS7Entry 1 }

ptiwfIWFSS7DPC OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "DPC of the SS7/MAP queries initiated by the IWF. \nWhen set, can never be reset to UNSET. \nTraffic cannot be activated if set to UNSET. \nValid values: point code format. \nDefault value: UNSET (start-up only)."
    ::= { ptiwfIWFSS7Entry 2 }

ptiwfIWFSS7SCCPPAEncoding OBJECT-TYPE
    SYNTAX          INTEGER { unset(-1), ansi(0), itu(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Called and Calling Party Address encoding (UNSET, ANSI, ITU). \nUsed in the encoding of the 8th bit of the GTAI octet and the validation of the GTI. \nWhen set, can never be reset to UNSET. \nCan only be modified when the IWF traffic is deactivated. \nTraffic cannot be activated if set to UNSET. \nDefault value: UNSET (start-up only)."
    ::= { ptiwfIWFSS7Entry 3 }

ptiwfIWFSS7PCDisplayFormat OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Point Code Display Format based on the SS7 Variant. \nMust be 14-bit compatible for ITU based SS7 variants, 16-bit for Japan, and 24-bit for ANSI and CHINA. \nWhen set, can never be reset to Undefined. \nCan only be modified when the IWF traffic is deactivated. \nTraffic cannot be activated if set to Undefined. \nValid values: Undefined, 8.8.8, 3.8.3, ff-ff, 4.4.6, 5.4.7, 3.4.7, 4.3.7, 4.6.4, 6.4.4, 4.7.3, 5.4.5, and Decimal. Note that the dash delimiter is also accepted (for example 8-8-8). \nDefault value: Undefined (start-up only)."
    ::= { ptiwfIWFSS7Entry 4 }

ptiwfIWFSS7PCRoutingFormat OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Point Code Routing Format based on the SS7 Variant. \nMust be 14-bit compatible for ITU based SS7 variants, 16-bit for Japan, and 24-bit for ANSI and CHINA. The x-y or x-y-z fields must be 8-bit or less. \nWhen set, can never be reset to Undefined. \nCan only be modified when the IWF traffic is deactivated. \nTraffic cannot be activated if set to Undefined. \nValid values: Undefined, 8.8.8, 3.8.3, 4.4.6, 5.4.7, 3.4.7, 4.3.7, 4.6.4, 6.4.4, 4.7.3, and 5.4.5. \nDefault value: Undefined (start-up only)."
    ::= { ptiwfIWFSS7Entry 5 }

ptiwfIWFSS7SCCPFiller OBJECT-TYPE
    SYNTAX          INTEGER { zero(0), f(15) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP digits filler for Called and Calling PA encoding of odd length (ZERO, F). \nDefault value: F."
    ::= { ptiwfIWFSS7Entry 6 }

ptiwfIWFSS7ConnectivityStatus OBJECT-TYPE
    SYNTAX          INTEGER { inactive(0), notConnected(1), connected(2) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "SCCP NA Connectivity Status (INACTIVE, NOT CONNECTED, CONNECTED). \nBased on the per slot connectivity status. \nCONNECTED will be displayed as soon as an SCCP connection on any slot is connected. \nAssociated to alarm 1907 (IWF SS7 Down)."
    ::= { ptiwfIWFSS7Entry 7 }

ptiwfIWFSS7SGSNSS7Number OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Digit representation of the SGSN. \nUsed as the MAP PurgeMS argument for sgsn-Number encoding in scenario 4.1 and the MAP UpdateGrpsLocation argument for sgsn-Number encoding in scenario 2.1 and 8.1. \nWhen set, can never be reset to an empty string. \nTraffic cannot be activated if set to an empty string. \nValid values: hexadecimal digit string (maximum size of 32). \nDefault value: empty string (start-up only)."
    ::= { ptiwfIWFSS7Entry 8 }

ptiwfIWFSS7IMSIRealm OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "User-Name AVP encoded as per NAI format <imsi>@<IMF IMSI Realm>. \nIf set to an empty string, the IMSI is set directly to User-Name AVP. \nValid values: FQDN format (as per the DSC ADN Diameter ID validation)). \nDefault value: empty string (start-up only)."
    ::= { ptiwfIWFSS7Entry 9 }

ptiwfIWFSS7MateSTPLPC OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Local Point Code of the mate STP. \nValid values: point code format. \nDefault value: UNSET."
    ::= { ptiwfIWFSS7Entry 10 }

ptiwfIWFSS7MateSTPID OBJECT-TYPE
    SYNTAX          INTEGER { notApplicable(-1), stp2(0), stp1(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Identifier of the mate STP (NOT APPLICABLE, STP1, STP2). \nBy default, set to NOT APPLICABLE for a non mated-pair deployments. \nIn a mated-pair IWF deployment, the ID must be unique to differentiate between the 2 systems. \nDefault value: NOT APPLICABLE."
    ::= { ptiwfIWFSS7Entry 11 }

ptiwfIWFSS7HLRSS7Number OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Digit representation of the HLR. \nUsed as the MAP Reset argument for the sendingNodenumber hlr-Number encoding in scenario 7.2 and as the MAP UpdateGprsLocation response for the hlr-Number encoding in scenario 2.2 and 8.2. \nWhen set, can never be reset to an empty string. \nTraffic cannot be activated if set to an empty string. \nValid values: hexadecimal digit string (maximum size of 32). \nDefault value: empty string (start-up only)."
    ::= { ptiwfIWFSS7Entry 12 }

ptiwfIWFSS7ParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptiwfIWFSS7Entry 253 }

