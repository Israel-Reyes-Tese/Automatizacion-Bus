--======================================================================
-- IRP VERSION INFORMATION
--======================================================================

alarmIrpVersion OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..20))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object represents the version of the Alarm IRP
                 SNMPv2 Solution Set supported by the agent."
        ::= { alarmIRPInfo 1 }


--======================================================================
-- GENERIC EVENT ATTRIBUTES SPECIFIED BY NOTIFICATION IRP
--======================================================================

sendNotificationId OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  accessible-for-notify
        STATUS      current
        DESCRIPTION
                "This object represent the notification identifier
                of current send notification ( AlarmNew,AlarmCleared,AlarmAckChange,AlarmCommentChange,AlarmSync)
                
                The agent shall store the notification identifier
                of the last send FM notification in this object
                immediately after sending the notification.
                
                The manager can retrieve the current value of this
                object to detect lost notifications. This mechanism
                can be used by the manager when no notification
                is received for a certain time (e.g. 10 minutes)
                to evaluate whether a synchronization should be performed. "
        ::= { alarmIRPInfo 2 }


systemDN OBJECT-TYPE
        SYNTAX      ManagedObjectInstance
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object carries the Distinguished Name (DN) of the system 
                 which generates the Notification.
                 The combination of notificationId and systemDN uniquely 
                 identifies each notification."
        ::= { alarmIRPInfo 3 }
        
lastSendNotificationId OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  accessible-for-notify
        STATUS      current
        DESCRIPTION
                "This object represent the notification identifier
                of last send notification ( AlarmNew, AlarmCleared,AlarmAckChange,AlarmCommentChange)
                
                The agent shall store the notification identifier
                of the last send FM notification in this object
                immediately after sending the notification.
                
                The manager can retrieve the current value of this
                object to detect lost notifications. This mechanism
                can be used by the manager when no notification
                is received for a certain time (e.g. 10 minutes)
                to evaluate whether a synchronization should be performed. "
        ::= { alarmIRPInfo 4 }
        
        
messageCode OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  accessible-for-notify
        STATUS      current
        DESCRIPTION        
                "the ems error code."
       ::= { alarmIRPInfo 5 }

syncUniqueId OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  accessible-for-notify
        STATUS      current
        DESCRIPTION        
                "This object represent the sync identifier of the same alarm sync operation. "
       ::= { alarmIRPInfo 6 }

--======================================================================
-- NUMBER OF ALARMS
--======================================================================

alarmIndeterminateNumber OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object determines the number of currently 
                 active alarms with perceived severity 'indeterminate'."
        ::= { alarmCount 1 }

alarmCriticalNumber OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object determines the number of currently 
                 active alarms with perceived severity 'critical'."
        ::= { alarmCount 2 }

alarmMajorNumber OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object determines the number of currently 
                 active alarms with perceived severity 'major'."
        ::= { alarmCount 3 }

alarmMinorNumber OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object determines the number of currently 
                 active alarms with perceived severity 'minor'."
        ::= { alarmCount 4 }

alarmWarningNumber OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object determines the number of currently 
                 active alarms with perceived severity 'warning'."
        ::= { alarmCount 5 }

alarmNumber OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object determines the total number of 
                 currently active alarms, i.e. the total number
                 of entries in the Active Alarm Table."
        ::= { alarmCount 6 }
        
--======================================================================
-- CURRENT ALARM TABLE
--======================================================================

currentAlarmTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF AlarmEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "Table with information about the current alarms in the system."
        ::= { alarmIRP 3 }

currentAlarmEntry OBJECT-TYPE
        SYNTAX       AlarmEntry
        MAX-ACCESS   not-accessible
        STATUS       current
        DESCRIPTION
                "One entry in the table holds one current alarm."
        INDEX { alarmIndex }
::= { currentAlarmTable 1 }

--**************************
-- define report field
--*************************

AlarmEntry ::= SEQUENCE {
		alarmIndex		    		Integer32,
        alarmId                     DisplayString,
        alarmManagedObjectInstance  ManagedObjectInstance,
        alarmEventTime              DateAndTime,
        alarmEventType              TypeOfEvent,
        alarmProbableCause          ProbableCause,
        alarmPerceivedSeverity      PerceivedSeverity,
        alarmSpecificProblem        DisplayString,
        alarmAdditionalText         AdditionalText,
        alarmComment                DisplayString,
        alarmCode                   DisplayString,
        alarmNetype                 DisplayString,
        rowStatus                   RowStatus,
        alarmCodeName               DisplayString,
        alarmManagedObjectInstanceName  DisplayString,
        alarmSystemType     		DisplayString,
        alarmNeIP                   DisplayString,
        alarmAck                    AckStatus,
        cleiCode                    DisplayString,
        timeZoneID            	    DisplayString,
        timeZoneOffset          		Integer32,
        dSTSaving               		Integer32,
        aid                         DisplayString,
        id                          DisplayString,
        alarmOtherInfo              DisplayString,
        alarmMocObjectInstance   DisplayString
        }

alarmId OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..255))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object represents an alarm in EMS using 
                the private format of vendor"
        ::= { currentAlarmEntry 1 }

alarmManagedObjectInstance OBJECT-TYPE
        SYNTAX      ManagedObjectInstance
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object identifies the MO instance of 
                 network resource to which the alarm is 
                 related."
        ::= { currentAlarmEntry 2 }

alarmEventTime OBJECT-TYPE
        SYNTAX      DateAndTime
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION 
                "This object represents the event time of the subject alarm."
        ::= { currentAlarmEntry 3 }
        
alarmEventType OBJECT-TYPE
        SYNTAX      TypeOfEvent
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object represents the type of event."
        ::= { currentAlarmEntry 4 }

alarmProbableCause OBJECT-TYPE
        SYNTAX      ProbableCause
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object represents the probable cause 
                 of the current."
        ::= { currentAlarmEntry 5 }

alarmPerceivedSeverity OBJECT-TYPE
        SYNTAX      PerceivedSeverity
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object represents the perceived severity of 
                 the alarm."
        ::= { currentAlarmEntry 6 }

alarmSpecificProblem OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..255))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object represents the vendor specific problem 
                 ."
        ::= { currentAlarmEntry 7 }

alarmAdditionalText OBJECT-TYPE
        SYNTAX      AdditionalText
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object represents some arbitrary additional 
                 information of the alarm."
        ::= { currentAlarmEntry 8 }

alarmIndex OBJECT-TYPE
        SYNTAX      Integer32(0..2147483647)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object uniquely identifies an entry in the 
                 Active Alarm Table. It increases every time a new alarm 
                 occurs. Due to cleared alarms the index will not be 
                 contiguous. When the maximum is reached of 
                 Integer32, the value of this object rolls over to 1. 
                 The actor must use 'alarmEventTime' to sort on 
                 chronological order.
                 
                 The alarmIndex object is read-only ,and it
                 is used as index in the Active Alarm Table. 
                 The alarmIndex facilities the way to extract 
                 the corresponding value from a notification where 
                 the object is included."
        ::= { currentAlarmEntry 9 }
    

alarmComment  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..255))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object represents alarm comment."
        ::= { currentAlarmEntry 10 }

alarmCode OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..50))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object represents an alarm code"
        ::= { currentAlarmEntry 11 }
 
alarmNetype OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..50))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object represents an alarm netype"
        ::= { currentAlarmEntry 12 }              
rowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "TControls and reflects the  status  of  rows  in   this table "
        ::= { currentAlarmEntry 13 }  
        
alarmCodeName OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..255))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
                "This object represents an alarm code name"
        ::= { currentAlarmEntry 14 }       
        
