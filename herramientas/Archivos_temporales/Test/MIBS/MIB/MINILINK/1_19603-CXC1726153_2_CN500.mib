		-- 1.3.6.1.4.1.193.81.7.1.1.1.1.1
		xfCNDsx1ConfigXEntry OBJECT-TYPE
			SYNTAX XfCNDsx1ConfigXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Additional information about the configuration of a E1 
				interface."
			AUGMENTS { dsx1ConfigEntry }
			::= { xfCNDsx1ConfigXTable 1 }

		
		XfCNDsx1ConfigXEntry ::=
			SEQUENCE { 
				xfCNDsx1Mode INTEGER

     			}
		
		-- 1.3.6.1.4.1.193.81.7.1.1.1.1.1.1	
 		xfCNDsx1Mode OBJECT-TYPE
          		SYNTAX  INTEGER {
                    pdhTraffic (1),
					syncIn2MHz (2),
					syncIn2Mbps (3),
					syncOut2MHz (4),
					syncOut2Mbps (5)
                     
                 		}
          		MAX-ACCESS  read-write
          		STATUS  current
          		DESCRIPTION

                "xfCNDsx1Mode is used to configure the E1 physical port as SI:
                either input or output and its format, either 2Mbps or 2MHz.
                When SI is configured the interface is used to transport the synchronization signal over the hop.

				When xfCNDsx1Mode is set to pdhTraffic the interface is dedicated for PDH traffic.
				
				This MO is applicable only for the interface instances relative to the 14th, 15th and 16th E1 ports.
				The MO for 16th E1 port is always settable to syncIn2MHz/2Mbps or syncOut2MHz/2Mbps.
				The MO for 15th E1 port is settable to syncOut2MHz/2Mbps only if MO for 16th E1 port has been set to output SI.
				The MO for 14th E1 port is settable to or syncOut2MHz/2Mbps only if MO for 15th E1 port has been set to output SI."
			DEFVAL { pdhTraffic }
                 		
          		::= { xfCNDsx1ConfigXEntry 1 }

		-- 1.3.6.1.4.1.193.81.7.1.1.2
		xfCNSyncMIBObjects OBJECT IDENTIFIER ::= { xfCNMIBObjects 2 }


		-- 1.3.6.1.4.1.193.81.7.1.1.2.1
		xfCNSyncBoardCapability OBJECT-TYPE
			SYNTAX BITS
				{
				twoMbps(0),
				twoMHz(1)			
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Capability bits for Synchronization signal transport of a CN500 board.
                		0	twoMbps		2048kbps synch signal transport
				1	twoMHz		2048kHz synch signal transport
				
				Note: they are not mutually exclusive. 
				The different values are:
				0000.0000 means no sync
				1000.0000 means 2048kbps
				0100.0000 means 2048kHz
				1100.0000 means 2048kbps and 2048kHz
				"
			::= { xfCNSyncMIBObjects 1 }
			
		-- 1.3.6.1.4.1.193.81.7.1.1.2.2
		xfCNSyncEConfig OBJECT-TYPE
			SYNTAX INTEGER {
				other (1),
				syncEDisabled (2),
				syncERingEthernet (3),
				syncERingRadio (4),
				syncEChainEthernet (5),
				syncEChainRadio (6)
          	}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Use case based configuration of SyncE.
				
				SyncEDisabled (2),
				SyncERingEthernet (3),
				SyncERingRadio (4),
				SyncEChainEthernet (5),
				SyncEChainRadio (6).
				
				Select one of the SyncE configurations.
				Other MOs will be affected."
			::= { xfCNSyncMIBObjects 2 }

		-- 1.3.6.1.4.1.193.81.7.1.1.2.3
		xfCNSyncEQLDomain OBJECT-TYPE
			SYNTAX INTEGER {
				other (1),
				syncEANSI (2),
				syncEETSI (3)
          	}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Selects the domain of the QL values.
				Other(1),
				ANSI (2),
				ETSI (3)"
			::= { xfCNSyncMIBObjects 3 }  
			
			
		-- 1.3.6.1.4.1.193.81.7.1.1.2.4
		xfCNSyncQualityCapability OBJECT-TYPE
			SYNTAX BITS {
				synchQualCap (0)
			}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Capability MO stating if the CN500 SW supports notAssigned as default values for the Quality Level when input SI is configured.
	 
				Bits 0:
					 0	not supported 
					 1 	supported 
				"
			::= { xfCNSyncMIBObjects 4 }

				-- 1.3.6.1.4.1.193.81.7.1.1.2.4
		xfCNSyncEAlarmStatus OBJECT-TYPE
			SYNTAX BITS {
		  loesmc0(0),
		  loesmc1(1),
		  loesmc2(2),
		  invalidQLEthernet0(3),
		  invalidQLEthernet1(4),
		  invalidQLEthernet2(5),
		  invalidQLRL0(6),
		  invalidQLRL1(7),
		  invalidQLRL2(8)
			}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The alarms for SyncE:
				
          loesmc      - Critical: Loss of ESMC. No ESMC PDU for the last five seconds. QL-DNU is reported on the interface.
          invalidQLEthernet - Critical: The ESMC QL values from the Ethernet port has invalid values. 
          							    Probably due to wrong QL domain[ANSI, ETSI]. QL-INV is reported on the interface.
          invalidQLRL    - Critical: The SSM QL values from the hop has invalid values. 
                                     Probably due to wrong QL domain[ANSI, ETSI]. QL-INV is reported on the interface.
 
				"
			::= { xfCNSyncMIBObjects 5 }

