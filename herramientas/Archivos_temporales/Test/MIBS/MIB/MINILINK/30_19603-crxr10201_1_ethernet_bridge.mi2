                xfEthernetEplNetworkMappingPriority
                    Integer32,
                xfEthernetEplNetworkMappingTrafficClass
                    Integer32
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.4.1.1
        xfEthernetEplNetworkMappingPriority OBJECT-TYPE
            SYNTAX Integer32 (0..7)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "The assigned network priority value."
            ::= { xfEthernetEplNetworkMappingEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.4.1.2
        xfEthernetEplNetworkMappingTrafficClass OBJECT-TYPE
