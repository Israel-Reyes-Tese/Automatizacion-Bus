PtIOHWHSLATMChannelsEntry ::=
    SEQUENCE {
        ptIOHWHSLATMChannelsVPIMask     Integer32,
        ptIOHWHSLATMChannelsVCIMask     Integer32,
        ptIOHWHSLATMChannelsState       INTEGER,
        ptIOHWHSLATMChannelsLicense     INTEGER,
        ptIOHWHSLATMChannelsIOManagerIndex  Integer32,
        ptIOHWHSLATMChannelsActions     INTEGER,
        ptIOHWHSLATMChannelsParentOID   OBJECT IDENTIFIER,
        ptIOHWHSLATMChannelsRowStatus   RowStatus,
        ptIOHWHSLATMChannelsIndex       Integer32
    }

ptIOHWHSLATMChannelsVPIMask OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Bit mask (in decimal) that determines the valid VPI range for a link. A maximum of 4 out of 8 bits can be used. Valid options are 0 to 240. For example, mask 15 is 00001111 in binary, which gives a range of 0 to 15 for VPI. The maximum is 11110000 (240 decimal)."
    ::= { ptIOHWHSLATMChannelsEntry 2 }

ptIOHWHSLATMChannelsVCIMask OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Bit mask (in decimal) that determines the valid VCI range for a link. A maximum of 10 out of 16 bits can be used. Valid options are 0 to 65472. For example, mask 15 is 00001111 in binary, which gives a range of 0 to 15 for VPI. The maximum is 1111111111000000 (65472 decimal)."
    ::= { ptIOHWHSLATMChannelsEntry 3 }

ptIOHWHSLATMChannelsState OBJECT-TYPE
    SYNTAX          INTEGER { activated(0), deactivated(1), failed(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Link manager state. State of FAILED indicates setup fault."
    ::= { ptIOHWHSLATMChannelsEntry 4 }

ptIOHWHSLATMChannelsLicense OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), disconnected(1), 
                        licensed(2), unknown(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "ATM license state: Licensed, Unlicensed, Disconnected"
    ::= { ptIOHWHSLATMChannelsEntry 18 }

ptIOHWHSLATMChannelsIOManagerIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
