ptLBConnection OBJECT IDENTIFIER
    ::= { ptLBConnections 5006 }

ptLBConnectionTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtLBConnectionEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptLBConnection 1 }

ptLBConnectionEntry OBJECT-TYPE
    SYNTAX          PtLBConnectionEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of LBConnection attributes."
    INDEX           { ptConnGroupIndex, ptLBConnectionLoadBalancerIndex, 
                      ptLBConnectionConnectionsIndex, 
                      ptLBConnectionIndex }
    ::= { ptLBConnectionTable 1 }

PtLBConnectionEntry ::=
    SEQUENCE {
        ptLBConnectionName              DisplayString,
        ptLBConnectionTargetName        DisplayString,
        ptLBConnectionTransportType     INTEGER,
        ptLBConnectionLocalHost         DisplayString,
        ptLBConnectionRemoteHost        DisplayString,
        ptLBConnectionTargetSlot        INTEGER,
        ptLBConnectionConnectionPortsCount  INTEGER,
        ptLBConnectionInternalID        INTEGER,
        ptLBConnectionStatus            DisplayString,
        ptLBConnectionLoadBalancerIndex INTEGER,
        ptLBConnectionConnectionsIndex  INTEGER,
        ptLBConnectionParentOID         OBJECT IDENTIFIER,
        ptLBConnectionRowStatus         RowStatus,
        ptLBConnectionIndex             INTEGER
    }

ptLBConnectionName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Name used to uniquely identify the connection. \nLeading and trailing whitespaces are trimmed. \nValid values: alphanumeric characters, dashes, and underscores (maximum size of 64)."
    ::= { ptLBConnectionEntry 1 }

ptLBConnectionTargetName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The load balancer target hosting this connection. \nValid values: Existing load balancer target."
    ::= { ptLBConnectionEntry 2 }

ptLBConnectionTransportType OBJECT-TYPE
    SYNTAX          INTEGER { sctp(0), tcp(1), ipsec(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Transport Type (SCTP, TCP, IPSec) \nSet to the transport type between the remote host and the local host. \nThe following applies to IPSec only: \no A Connection Ports object is automatically added to the connection. \no The user cannot add or remove Connection Ports. \no The user can delete the connection without deleting the automatically-added Connection Ports object. \nDefault value: SCTP."
    ::= { ptLBConnectionEntry 3 }

ptLBConnectionLocalHost OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Local host for the connection. \nValid values: Hostname or IPv4 address."
    ::= { ptLBConnectionEntry 4 }

ptLBConnectionRemoteHost OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Remote host for the connection. \nValid values: Hostname or IPv4 address."
    ::= { ptLBConnectionEntry 6 }

ptLBConnectionTargetSlot OBJECT-TYPE
    SYNTAX          INTEGER { unset(-1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Target Slot for the connection. \nThe DSC routing slot currently serving this connection. \nCan only be set when the target load balancing algorithm is set to STATIC MAPPING. \nValid values: A DSC slot defined in the target slots or the string UNSET."
    ::= { ptLBConnectionEntry 8 }

ptLBConnectionConnectionPortsCount OBJECT-TYPE
    SYNTAX          INTEGER { undefined(0) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Connection Ports Count. \nThe number of Connection Ports configured in this connection. \nWhen no Connection Ports objects are configured on this connection, the value is set to UNDEFINED."
    ::= { ptLBConnectionEntry 9 }

ptLBConnectionInternalID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Connection unique internal ID. \nReserved for internal use."
    ::= { ptLBConnectionEntry 10 }

ptLBConnectionStatus OBJECT-TYPE
