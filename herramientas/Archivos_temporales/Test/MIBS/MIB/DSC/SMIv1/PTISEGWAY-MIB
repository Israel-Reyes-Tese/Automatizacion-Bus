    SEQUENCE {
        ptMTPVNodeContext               DisplayString,
        ptMTPVNodeVPC                   INTEGER,
        ptMTPVNodeCLLI                  DisplayString,
        ptMTPVNodeDescription           DisplayString,
        ptMTPVNodeState                 INTEGER,
        ptMTPVNodeBroadcastTFA          INTEGER,
        ptMTPVNodeBroadcastTFP          INTEGER,
        ptMTPVNodeBroadcastTFR          INTEGER,
        ptMTPVNodeMateSTPDistribution   INTEGER,
        ptMTPVNodeMTP3NAManagerIndex    INTEGER,
        ptMTPVNodeMTP3NAIndex           INTEGER,
        ptMTPVNodeVNodesIndex           INTEGER,
        ptMTPVNodeActions               INTEGER,
        ptMTPVNodeParentOID             OBJECT IDENTIFIER,
        ptMTPVNodeRowStatus             RowStatus,
        ptMTPVNodeIndex                 INTEGER
    }

ptMTPVNodeContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A condensed textual representation of the virtual node object."
    ::= { ptMTPVNodeEntry 1 }

ptMTPVNodeVPC OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The SS7 point code of this virtual node in the format Network.Cluster.Member. Maximum value is 255.255.255. If the personality is ITU, the format is Zone.Area.Member and the range for Zone and Member is 0 to 7, Area is 0 to 255. "
    ::= { ptMTPVNodeEntry 2 }

ptMTPVNodeCLLI OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Common Language Location Identifier (CLLI) of this virtual node. This is an informational attribute only."
    ::= { ptMTPVNodeEntry 3 }

ptMTPVNodeDescription OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "A textual description of this virtual node. This attribute is for Operator informational purposes only."
    ::= { ptMTPVNodeEntry 4 }

ptMTPVNodeState OBJECT-TYPE
    SYNTAX          INTEGER { inactive(0), active(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The Virtual Node Status indicates if this VNode is available to be used by a registration to register to. It does NOT indicate whether an application has actually registered to the VNode. That information can be found on the Level 4 connections screen."
    ::= { ptMTPVNodeEntry 5 }

