    ::= { ptMTPLSLinkEntry 9 }

ptMTPLSLinkCongestionLevel OBJECT-TYPE
    SYNTAX          INTEGER { false(0), level1(1), level2(2), level3(3), 
                        true(4) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Congestion level of this link. \n For ANSI, TTC and NTT networks the values range from 1 to 3 or FALSE if not congested. \n For ITU networks the value is either TRUE or FALSE."
    ::= { ptMTPLSLinkEntry 10 }

