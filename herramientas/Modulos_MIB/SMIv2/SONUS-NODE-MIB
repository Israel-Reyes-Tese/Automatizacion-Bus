sonusDflushNetworkFailoverDiskUsageStatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDflushNetworkFailoverDiskUsageStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table specifies Disk Usage for ACT SYS PFM and
             TRC directory"
    ::= { sonusDflush 8 }

sonusDflushNetworkFailoverDiskUsageStatEntry OBJECT-TYPE
    SYNTAX          SonusDflushNetworkFailoverDiskUsageStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This object holds the amount of disk
             space used by each log type during
             NFO"
    INDEX           { sonusDflushDiskLogType }
    ::= { sonusDflushNetworkFailoverDiskUsageStatTable 1 }

SonusDflushNetworkFailoverDiskUsageStatEntry ::=
    SEQUENCE {
        sonusDflushDiskLogType          INTEGER,
        sonusDflushActiveDiskUsage      Integer32,
        sonusDflushStandbyDiskUsage     Integer32
    }

sonusDflushDiskLogType OBJECT-TYPE
    SYNTAX          INTEGER { sys(0), dbg(1), trc(2), act(3), pfm(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This object defines the number of ACT files 
             sent by the Network Failover Feature since
             the admin state was set to ENABLED"
    DEFVAL          { 0 }
    ::= { sonusDflushNetworkFailoverDiskUsageStatEntry 1 }

sonusDflushActiveDiskUsage OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This object defines the disk space used by
             directory storing the specified log type on
             the active flash disk"
    DEFVAL          { 0 }
    ::= { sonusDflushNetworkFailoverDiskUsageStatEntry 2 }

sonusDflushStandbyDiskUsage OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This object defines the disk space used by
             directory storing the specified log type on
             the standby flash disk"
    DEFVAL          { 0 }
    ::= { sonusDflushNetworkFailoverDiskUsageStatEntry 3 }

