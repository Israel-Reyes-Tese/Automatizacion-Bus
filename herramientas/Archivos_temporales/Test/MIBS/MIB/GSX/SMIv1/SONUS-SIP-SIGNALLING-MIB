sonusSipCacOffenderTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipCacOffenderEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Table of SIP Signalling Peer statistics."
    ::= { sonusSipSignallingMIBObjects 43 }

sonusSipCacOffenderEntry OBJECT-TYPE
    SYNTAX          SonusSipCacOffenderEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "SIP Peer CAC Offenders."
    INDEX           { sonusSipCacOffenderSlotIndex, 
                      sonusSipCacOffenderListIndex }
    ::= { sonusSipCacOffenderTable 1 }

SonusSipCacOffenderEntry ::=
    SEQUENCE {
        sonusSipCacOffenderSlotIndex    SonusSlotIndex,
        sonusSipCacOffenderListIndex    Gauge,
        sonusSipCacOffenderIpAddress    IpAddress,
        sonusSipCacOffenderZoneName     SonusNameReference,
        sonusSipCacOffenderIngressCallRejects     Gauge,
        sonusSipCacOffenderEgressCallRejects    Gauge,
        sonusSipCacOffenderLastRejectAt DateAndTime,
        sonusSipCacOffenderFirstRejectAt DateAndTime,
        sonusSipCacOffenderPortNum      Gauge,
        sonusSipCacOffenderIngressSubscribeRejects          Gauge,
        sonusSipCacOffenderEgressSubscribeRejects         Gauge,
        sonusSipCacOffenderSubscribeLastRejectAt        DateAndTime,
        sonusSipCacOffenderSubscribeFirstRejectAt         DateAndTime,
        sonusSipCacOffenderIngressReferRejects      Gauge,
        sonusSipCacOffenderEgressReferRejects     Gauge,
        sonusSipCacOffenderReferLastRejectAt    DateAndTime,
        sonusSipCacOffenderReferFirstRejectAt     DateAndTime,
        sonusSipCacOffenderIngressNotifyRejects       Gauge,
        sonusSipCacOffenderEgressNotifyRejects      Gauge,
        sonusSipCacOffenderNotifyLastRejectAt     DateAndTime,
        sonusSipCacOffenderNotifyFirstRejectAt      DateAndTime,
        sonusSipCacOffenderIngressOptionsRejects        Gauge,
        sonusSipCacOffenderEgressOptionsRejects       Gauge,
        sonusSipCacOffenderOptionsLastRejectAt      DateAndTime,
        sonusSipCacOffenderOptionsFirstRejectAt       DateAndTime,
        sonusSipCacOffenderIngressMessageRejects        Gauge,
        sonusSipCacOffenderEgressMessageRejects       Gauge,
        sonusSipCacOffenderMessageLastRejectAt      DateAndTime,
        sonusSipCacOffenderMessageFirstRejectAt       DateAndTime,
        sonusSipCacOffenderIngressOtherReqRejects         Gauge,
        sonusSipCacOffenderEgressOtherReqRejects        Gauge,
        sonusSipCacOffenderOtherReqLastRejectAt       DateAndTime,
        sonusSipCacOffenderOtherReqFirstRejectAt        DateAndTime,
        sonusSipCacOffenderIngressAggrMsgRejects        Gauge,
        sonusSipCacOffenderEgressAggrMsgRejects       Gauge,
        sonusSipCacOffenderAggrMsgLastRejectAt      DateAndTime,
        sonusSipCacOffenderAggrMsgFirstRejectAt       DateAndTime,
        sonusSipCacOffenderIp6Address   Ipv6Address
    }

