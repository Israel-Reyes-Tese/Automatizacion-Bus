--
-- Textual conventions
--
    
        XfEthernetServiceId ::= TEXTUAL-CONVENTION
            STATUS current
            DESCRIPTION
                "Used to identify an Ethernet service
                
                 layer1Epl        : EPL service
                 layer2Bridge     : Bridge service
                
                "
            SYNTAX INTEGER
                {
                layer1Epl(1),
                layer2Bridge(2)
                }

        XfEthernetServiceInstance ::= TEXTUAL-CONVENTION
            STATUS current
            DESCRIPTION
                "Identifies an instance of a service.
                
                For layer1Epl
                       xfEthernetEplInterface1.xfEthernetEplInterface2
                       (e.g 2147361011.2147378101 which identifies an EPL )
                
                For layer2BridgeLevel
                    1  (since we have only one bridge) :
                "
            SYNTAX OBJECT IDENTIFIER

       XfServiceOamPmMeasureSessionId ::= TEXTUAL-CONVENTION
        DISPLAY-HINT "d"
        STATUS      current
        DESCRIPTION
        "Measure Session Identifier: A small    integer, unique over a given NE,
        identifying a measure session.
        "
        SYNTAX      Unsigned32 (1..128)

        XfServiceOamDmPduInterval ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION
        "Indicates the interval between sent protocol data units.
        The possible values are:
        interval1s(0) DMMs are sent out every 1 second.
        interval10s(1)   DMMs are sent every 10 seconds.
        "
        SYNTAX      INTEGER {
                              interval1s      (0),
                              interval10s     (1)
        }

        XfServiceOamLmPduInterval ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION
        "Indicates the interval between sent protocol data units.
        The possible values are:
        interval100ms(0) LMMs are sent out every 100 millisecond.
        "
        SYNTAX      INTEGER {
                              interval100ms      (0)
        }
    
       XfServiceOamTimeStamp ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION
        "It is an 8-octet field whose format is equal to the TimeRepresentation format
         in [IEC 61588]. That is, in network-byte order the first 4 octets contain
         number of seconds while the last 4 octets contain number of nanoseconds.
         It shall be converted to microseconds when displaying.
        "
        SYNTAX      Counter64

       XfHashMode ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION
        "XfHashMode is used to define LAG hashing algorithm in the switch."
        SYNTAX       INTEGER
        {
                               srcMac(1),
                               dstMac(2),
                               srcDstMac(3),
                               srcIp(4),
                               dstIp(5),
                               srcDstIp(6)
        }

    
