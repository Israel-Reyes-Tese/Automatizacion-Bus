        sonusDsx3IntervalLCVs           PerfCurrentCount,
        sonusDsx3IntervalLESs           PerfCurrentCount,
        sonusDsx3IntervalLSESs          PerfCurrentCount,
        sonusDsx3IntervalCVPs           PerfCurrentCount,
        sonusDsx3IntervalCVCPs          PerfCurrentCount,
        sonusDsx3IntervalESPs           PerfCurrentCount,
        sonusDsx3IntervalESCPs          PerfCurrentCount,
        sonusDsx3IntervalSESPs          PerfCurrentCount,
        sonusDsx3IntervalSESCPs         PerfCurrentCount,
        sonusDsx3IntervalSASs           PerfCurrentCount,
        sonusDsx3IntervalUASPs          PerfCurrentCount,
        sonusDsx3IntervalUASCPs         PerfCurrentCount,
        sonusDsx3IntervalLineIDF        TruthValue,
        sonusDsx3IntervalPathIDF        TruthValue,
        sonusDsx3IntervalFC             Integer32
    }

