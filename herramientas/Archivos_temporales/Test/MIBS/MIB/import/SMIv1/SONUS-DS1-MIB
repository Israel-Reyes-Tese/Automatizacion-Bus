    ::= { sonusDsx1MIBNotificationsObjects 5 }

sonusDs1Mask OBJECT-TYPE
    SYNTAX          OCTET STRING
--                       { ds11(0), ds12(1), ds13(2), ds14(3), ds15(4), 
--                         ds16(5), ds17(6), ds18(7), ds19(8), ds110(9), 
--                         ds111(10), ds112(11), ds113(12), ds114(13), 
--                         ds115(14), ds116(15), ds117(16), ds118(17), 
--                         ds119(18), ds120(19), ds121(20), ds122(21), 
--                         ds123(22), ds124(23), ds125(24), ds126(25), 
--                         ds127(26), ds128(27), ds129(28), ds130(29), 
--                         ds131(30), ds132(31), ds133(32), ds134(33), 
--                         ds135(34), ds136(35), ds137(36), ds138(37), 
--                         ds139(38), ds140(39), ds141(40), ds142(41), 
--                         ds143(42), ds144(43), ds145(44), ds146(45), 
--                         ds147(46), ds148(47), ds149(48), ds150(49), 
--                         ds151(50), ds152(51), ds153(52), ds154(53), 
--                         ds155(54), ds156(55), ds157(56), ds158(57), 
--                         ds159(58), ds160(59), ds161(60), ds162(61), 
--                         ds163(62), ds164(63), ds165(64), ds166(65), 
--                         ds167(66), ds168(67), ds169(68), ds170(69), 
--                         ds171(70), ds172(71), ds173(72), ds174(73), 
--                         ds175(74), ds176(75), ds177(76), ds178(77), 
--                         ds179(78), ds180(79), ds181(80), ds182(81), 
--                         ds183(82), ds184(83) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A bitmask of 84 DS1s."
