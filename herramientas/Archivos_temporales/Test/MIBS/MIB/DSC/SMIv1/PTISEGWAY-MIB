        ptDSCDSCRowStatus               RowStatus,
        ptDSCDSCIndex                   INTEGER
    }

ptDSCDSCDSCName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Name used to logically identify the DSC Instance. \nIf more than one DSC Instance is defined, it is recommended that the name be updated to a short and unique value to facilitate logs, alarms, and statistics correlation. \nValid values: alphanumeric characters, dashes, and underscores (maximum size of 64). \nDefault value: DSC_X, where X is the DSC Instance ID."
    ::= { ptDSCDSCEntry 1 }

ptDSCDSCDSCInstanceID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "DSC Instance Identifier (1 to 99). \nIf more than one DSC Instance is defined, the value is overwritten by the associated command line argument on startup. \nDefault value: 1."
    ::= { ptDSCDSCEntry 2 }

ptDSCDSCDSCVersion OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Current DSC application version. \nThis is a hardcoded value as per the DSC application."
    ::= { ptDSCDSCEntry 3 }

ptDSCDSCEncryptionPackage OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "DSC Encryption Provider \nThis product includes software developed by the OpenSSL Project for use in the OpenSSL Toolkit (http://www.openssl.org/)"
    ::= { ptDSCDSCEntry 4 }

