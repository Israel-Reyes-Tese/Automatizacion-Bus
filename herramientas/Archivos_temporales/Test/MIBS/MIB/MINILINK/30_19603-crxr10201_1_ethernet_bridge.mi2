        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.2.1
        xfEthernetEplServiceConfigEntry OBJECT-TYPE
            SYNTAX XfEthernetEplServiceConfigEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "EPL service configurations."
            INDEX { xfEthernetEplInterface1, xfEthernetEplInterface2 }
            ::= { xfEthernetEplServiceConfigTable 1 }

        XfEthernetEplServiceConfigEntry ::=
            SEQUENCE {
                xfEthernetEplServiceConfigTrusted
                    INTEGER,
                xfEthernetEplServiceConfigPriorityMappingType
                    INTEGER,
                xfEthernetEplServiceConfigNetworkPcpSelection
                    INTEGER,
                xfEthernetEplServiceConfigDefaultPriority
                    INTEGER
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.2.1.1
        xfEthernetEplServiceConfigTrusted OBJECT-TYPE
            SYNTAX INTEGER
                {
                untrusted(1),
                dscpIPv4Trusted(2),
                ctagPcpTrusted(3),
                dscpIPv6Trusted(4),
                expTrusted(5),
                stagPcpTrusted(6)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies which priority field in the incoming user packet is trusted 
                (i.e. on the LAN/user side. The WAN/network side is always trusted.)
                
                untrusted(1)        : No priority field on the received packet is trusted.
                                      Default priority is used.
                
                dscpIPv4Trusted(2)  : DSCP field on IPv4 is trusted. If the packet is not IPv4, 
                                      default priority is used.
                
                ctagPcpTrusted(3)   : PCP field on the C-tag is trusted. If the packet is untagged,
                                      default priority is used.
                
                dscpIPv6Trusted(4)  : DSCP field on IPv6 is trusted. If the packet is not IPv6, 
                                      default priority is used.
                
                expTrusted(5)       :  EXP field on MPLS is trusted. If the packet is not MPLS, 
                                       default priority is used.
                
                stagPcpTrusted(6) :  PCP field on the S-tag is trusted. If the packet has no S-tag,
                                     default priority is used.
                
                default untrusted(1).
                
                If this parameter is not available then xfBridgePortTrusted is applied for Layer1 EPL ports."
            ::= { xfEthernetEplServiceConfigEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.2.1.2
        xfEthernetEplServiceConfigPriorityMappingType OBJECT-TYPE
            SYNTAX INTEGER
                {
                dPriorityMapping(1),
                qPriorityMapping(2),
                userDefinedPriorityMapping(3)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies the network priority to traffic class mapping used.
                
                dPriorityMapping(1]             :use priority mapping according D-Standard
                
                qPriorityMapping(2)             : use priority mapping according Q-Standard
                
                userDefinedPriorityMapping(3)   : use priority mapping defined in
                                                  xfEthernetEplNetworkPriorityMappingTable.
                
                Default is dPriorityMapping(1] .
                
                If this parameter is not available then xfEthernetPriorityMappingType is applied for Layer1 EPL ports."
            ::= { xfEthernetEplServiceConfigEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.2.1.3
        xfEthernetEplServiceConfigNetworkPcpSelection OBJECT-TYPE
            SYNTAX INTEGER
                {
                pcpScheme8p0d(1),
                pcpScheme7p1d(2),
                pcpScheme6p2d(3),
                pcpScheme5p3d(4),
                pcpScheme8p8d(5)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies how pcp encode/decode on the network side (WAN) is done.
                
                Reference: 802.1ad-2005[6.7.3 Priority Code Point encoding]
                
                pcpScheme8p0d(1) : 8 priorities, no DEI
                pcpScheme7p1d(2) : 7 priorities, 1 DEI
                pcpScheme6p2d(3) : 6 priorities, 2 DEI
                pcpScheme5p3d(4) : 7 priorities, 3 DEI
                pcpScheme8p8d(5) : 8 priorities and seperate DEI bit.
                
                default is 8p0d(2).
                
                If this parameter is not available then xfEthernetNetworkPcpSelection is applied for Layer1 EPL ports."
            ::= { xfEthernetEplServiceConfigEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.2.1.4
        xfEthernetEplServiceConfigDefaultPriority OBJECT-TYPE
            SYNTAX INTEGER (0..7)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Default priority used if the user priority is not trusted
                (i.e. xfEthernetEplServiceConfigTrusted='untrusted') or 
                the incoming user packet lacks a priority field specified by 
                xfEthernetEplServiceConfigTrusted.
                
                If this parameter is not available then xfBridgePortDefaultCos is applied for Layer1 EPL ports."
            ::= { xfEthernetEplServiceConfigEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.3
        xfEthernetEplUserPriorityMappingTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfEthernetEplUserPriorityMappingEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table is used to map user priority to network priority. 
                The user priority is derived from the incoming user packet
                and is specified by xfEthernetEplServiceConfigTrusted.
                
                The size of this table varies according to xfEthernetEplServiceConfigTrusted.
                
                If this table is not available then xfBridgePortUserPriorityMappingTable is applied for Layer1 EPL ports."
            ::= { xfBridgeConnection 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.3.1
        xfEthernetEplUserPriorityMappingEntry OBJECT-TYPE
