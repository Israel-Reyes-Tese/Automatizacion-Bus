    SYNTAX          RowStatus
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The row status for the snamiPortNAMapping table"
    ::= { ptsnamiPortNAMappingEntry 254 }

ptsnamiPortNAMappingIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The row number for the snamiPortNAMapping table"
    ::= { ptsnamiPortNAMappingEntry 255 }

