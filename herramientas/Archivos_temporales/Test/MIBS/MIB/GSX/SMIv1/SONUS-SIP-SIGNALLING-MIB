sonusSipArsProfile OBJECT IDENTIFIER
    ::= { sonusSipSignallingMIBObjects 21 }

sonusSipArsProfileNextIndex OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The next valid index to use when creating a new sonusSipArsProfileEntry"
    ::= { sonusSipArsProfile 1 }

sonusSipArsProfileTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipArsProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains information about each Address
             Reachability Service Profile configured on the node."
    ::= { sonusSipArsProfile 2 }

sonusSipArsProfileEntry OBJECT-TYPE
    SYNTAX          SonusSipArsProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table describes the Address Reachability Service Profile."
    INDEX           { sonusSipArsProfIndex }
    ::= { sonusSipArsProfileTable 1 }

SonusSipArsProfileEntry ::=
    SEQUENCE {
        sonusSipArsProfIndex            Gauge,
        sonusSipArsProfName             SonusName,
        sonusSipArsProfRowStatus        RowStatus,
        sonusSipArsProfAdminState       SonusAdminState,
        sonusSipArsProfBlkListAlg       INTEGER,
        sonusSipArsProfBlkListAlgTimeoutsType     INTEGER,
        sonusSipArsProfBlkListAlgTimeoutsNumTimeouts            Gauge,
        sonusSipArsProfBlkListAlgTimeoutsDuration         Gauge,
        sonusSipArsProfRecAlg           INTEGER,
        sonusSipArsProfRecAlgTimerDuration  Gauge,
        sonusSipArsProfRecAlgProbeMethod INTEGER,
        sonusSipArsProfRecAlgProbeInterval  Gauge,
        sonusSipArsProfRecAlgProbeNumResponses      Gauge,
        sonusSipArsProfRecAlgProbeDuration  Gauge,
        sonusSipArsProfBlkListAlgorithms OCTET STRING,
        sonusSipArsProfBlkListAlgRetryAfterType       OCTET STRING
    }

sonusSipArsProfIndex OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The index of the SIP Address Reachability Service table entry."
    ::= { sonusSipArsProfileEntry 1 }

sonusSipArsProfName OBJECT-TYPE
    SYNTAX          SonusName
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The name of this SIP Address Reachability Service Profile."
    ::= { sonusSipArsProfileEntry 2 }

sonusSipArsProfRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            ""
    ::= { sonusSipArsProfileEntry 3 }

sonusSipArsProfAdminState OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The admin state of this SIP Address Reachability Profile.
             Some fields cannot be changed until the state is disabled
             while others can be changed. In the former case, the change
             will be effective once the state is set to enabled."
    DEFVAL          { disabled }
    ::= { sonusSipArsProfileEntry 4 }

sonusSipArsProfBlkListAlg OBJECT-TYPE
    SYNTAX          INTEGER { timeouts(1) }
    ACCESS          read-write
    STATUS          obsolete
    DESCRIPTION     
            "This MIB is obsoleted by sonusSipArsProfBlkListAlgorithms.
             The blacklisting algorithm of this Address Reachability Service
             Profile. Currently, can only support timeouts."
    DEFVAL          { timeouts }
    ::= { sonusSipArsProfileEntry 5 }

sonusSipArsProfBlkListAlgTimeoutsType OBJECT-TYPE
    SYNTAX          INTEGER { sip-invite(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This specifies the timeout trigger. SIP-INIVTE timeout is the
             only event that is supported in this release."
    DEFVAL          { sip-invite }
    ::= { sonusSipArsProfileEntry 6 }

sonusSipArsProfBlkListAlgTimeoutsNumTimeouts OBJECT-TYPE
    SYNTAX          Gauge (1..3600)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field specifies the number of timeout events should
             occur before blacking listing the address."
    DEFVAL          { 1 }
    ::= { sonusSipArsProfileEntry 7 }

sonusSipArsProfBlkListAlgTimeoutsDuration OBJECT-TYPE
    SYNTAX          Gauge (1..3600)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field specifies the duration in seconds, in which the
             specified number of timeouts should occur."
    DEFVAL          { 1 }
    ::= { sonusSipArsProfileEntry 8 }

sonusSipArsProfRecAlg OBJECT-TYPE
    SYNTAX          INTEGER { probe(1), timer(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field specifies the recovery algorithm."
    DEFVAL          { probe }
    ::= { sonusSipArsProfileEntry 9 }

sonusSipArsProfRecAlgTimerDuration OBJECT-TYPE
    SYNTAX          Gauge (1..3600)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field specifies the duration in seconds, for which
             the IP address will remain black listed."
    DEFVAL          { 1 }
    ::= { sonusSipArsProfileEntry 10 }

sonusSipArsProfRecAlgProbeMethod OBJECT-TYPE
    SYNTAX          INTEGER { sip-options(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field pecifies the probe method. Only SIP-OPTIONS will
             be supported for this release."
    DEFVAL          { sip-options }
    ::= { sonusSipArsProfileEntry 11 }

sonusSipArsProfRecAlgProbeInterval OBJECT-TYPE
    SYNTAX          Gauge (1..600)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field specifies the probe interval."
    DEFVAL          { 1 }
    ::= { sonusSipArsProfileEntry 12 }

sonusSipArsProfRecAlgProbeNumResponses OBJECT-TYPE
    SYNTAX          Gauge (1..32)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field specifies the number of responses should occur
             before removing the entry from the black list."
    DEFVAL          { 1 }
    ::= { sonusSipArsProfileEntry 13 }

sonusSipArsProfRecAlgProbeDuration OBJECT-TYPE
    SYNTAX          Gauge (1..3600)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This field specifies the duration in seconds, in which the
             specified number of responses should occur."
    DEFVAL          { 1 }
    ::= { sonusSipArsProfileEntry 14 }

sonusSipArsProfBlkListAlgorithms OBJECT-TYPE
    SYNTAX          OCTET STRING
--                       { timeouts(0), retryafter(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This one obsoletes sonusSiipArsProfBlkListAlg.
             The blacklisting algorithms of this Address Reachability Service
             Profile. Currently, can support timeouts and retryafter algorithms
             or both of them.
             bit 0 -- indicate timeouts algorithm
             bit 1 -- indicate retryafter algorithm
             ...
             "
    DEFVAL          { 128 }
    ::= { sonusSipArsProfileEntry 15 }

sonusSipArsProfBlkListAlgRetryAfterType OBJECT-TYPE
    SYNTAX          OCTET STRING
--                       { sip-503(0) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This specifies the sip status code which has a Retry After value.
             503 Retry After is the only event supported in this release.
             bit 0 -- indicate sip 503
             ...
             "
    DEFVAL          { 128 }
    ::= { sonusSipArsProfileEntry 16 }

