ZTE-NOTIFICATION-IRP-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
        enterprises, Integer32
                FROM SNMPv2-SMI
        TEXTUAL-CONVENTION, DisplayString, DateAndTime,RowStatus
                FROM SNMPv2-TC
        OBJECT-GROUP, NOTIFICATION-GROUP, MODULE-COMPLIANCE
                FROM SNMPv2-CONF
        alarm
                FROM ZTE-IRP-MIB  	
        sendNotificationId,lastSendNotificationId,systemDN,ManagedObjectInstance,ProbableCause
                FROM ZTE-ALARM-IRP-MIB;


notificationIRP                  OBJECT IDENTIFIER ::= { alarm 10 }
ntsAttribute                     OBJECT IDENTIFIER ::= { notificationIRP 1 }
ntsNotifications        		 OBJECT IDENTIFIER ::= { notificationIRP 2 }
ntsNotificationProfile  		 OBJECT IDENTIFIER ::= { ntsNotifications 1 }
ntsNotificationFilter            OBJECT IDENTIFIER ::= { notificationIRP 3 }

--======================================================================
-- NOTIFICATION ATTR
--======================================================================
notifyEventTime OBJECT-TYPE
        SYNTAX      DateAndTime
        MAX-ACCESS  accessible-for-notify
        STATUS      current
        DESCRIPTION 
                "This object represents the time of occurrence of the notification."
        ::= { ntsAttribute 1 }

notifyCode OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..50))
        MAX-ACCESS  accessible-for-notify
        STATUS      current
        DESCRIPTION
                "This object represents an notification code"
        ::= { ntsAttribute 2 }
        
notifyManagedObjectInstance OBJECT-TYPE
        SYNTAX      ManagedObjectInstance
        MAX-ACCESS  accessible-for-notify
        STATUS      current
        DESCRIPTION
                "This object identifies the MO instance of network resource to which the notification is related."
        ::= { ntsAttribute 3 }
        
notifyProbableCause OBJECT-TYPE
        SYNTAX      ProbableCause
        MAX-ACCESS  accessible-for-notify
        STATUS      current
        DESCRIPTION
                "This object represents the probable cause of the current."
