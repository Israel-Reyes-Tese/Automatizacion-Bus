--======================================================================
-- ALARM FILTER
--======================================================================
ntsNotificationFilterEnable OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "Notification Filter enable or false,0 means false,while 1 means true."
        ::= { ntsNotificationFilter 1 }
        
ntsNotificationFilterCondition  OBJECT IDENTIFIER ::= { ntsNotificationFilter 2}
--======================================================================
-- NOTIFICATION FILTER
--======================================================================                                           
ntsNotificationCodesIncludeFilter OBJECT-TYPE
        SYNTAX     OCTET STRING (SIZE (0..512))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters notification according to the system type and notification code.  uses ',' as splitter between alarm codes .for example: 10001,10002 means :The alarm of specifical  system type code of 10001 and 10002 will not be sent. "                 
        ::= { ntsNotificationFilterCondition 1 }  
                  
--======================================================================
-- NOTIFICATION RELATED NOTIFICATIONS
--======================================================================

ntsNotificationNew NOTIFICATION-TYPE
        OBJECTS {   notifyEventTime,
                    sendNotificationId,
                    lastSendNotificationId,
                    systemDN,
		      		notifyCode,
		      		notifyManagedObjectInstance,                
                    notifyProbableCause,
                    notifySpecificProblem,
                    notifyAdditionalText,
		      		notifyNetype,
                    notifyCodeName,
                    notifyManagedObjectInstanceName,
                    notifySystemType,
                    notifyNeIP,
		      		      notifyTimeZoneID,
                    notifyTimeZoneOffset,
                    notifyDSTSaving,
                    notifyAid,
                    notifyId,
                    notifyMocObjectInstance,
				    notifyCustomAttr1,                 
				    notifyCustomAttr2,                 
				    notifyCustomAttr3,                 
				    notifyCustomAttr4,                 
				    notifyCustomAttr5,                 
				    notifyCustomAttr6,                 
				    notifyCustomAttr7,                 
				    notifyCustomAttr8,                 
				    notifyCustomAttr9,                 
	                notifyCustomAttr10 
                     }
        STATUS      current
        DESCRIPTION 
                "This notification is sent when a new notification is generated."
        ::= { ntsNotificationProfile 1 }

