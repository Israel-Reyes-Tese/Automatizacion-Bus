    SYNTAX          PtMTPLSLinkEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of MTPLSLink attributes."
    INDEX           { ptConnGroupIndex, ptMTPLSLinkMTP3NAManagerIndex, 
                      ptMTPLSLinkMTP3NAIndex, ptMTPLSLinkLinksetsIndex, 
                      ptMTPLSLinkLinksetIndex, ptMTPLSLinkIndex }
    ::= { ptMTPLSLinkTable 1 }

PtMTPLSLinkEntry ::=
    SEQUENCE {
        ptMTPLSLinkContext              DisplayString,
        ptMTPLSLinkAPC                  INTEGER,
        ptMTPLSLinkSLC                  INTEGER,
        ptMTPLSLinkDREID                INTEGER,
        ptMTPLSLinkCage                 INTEGER,
        ptMTPLSLinkChassisSlot          INTEGER,
        ptMTPLSLinkChannel              INTEGER,
        ptMTPLSLinkL2State              INTEGER,
        ptMTPLSLinkL3State              INTEGER,
        ptMTPLSLinkCongestionLevel      INTEGER,
        ptMTPLSLinkDiagnosticMessage    DisplayString,
        ptMTPLSLinkL2DebugLevel         INTEGER,
        ptMTPLSLinkUserActivated        INTEGER,
        ptMTPLSLinkLinkName             DisplayString,
        ptMTPLSLinkAlarms               INTEGER,
        ptMTPLSLinkRxMSUCount           INTEGER,
        ptMTPLSLinkTxMSUCount           INTEGER,
        ptMTPLSLinkMTP3NAManagerIndex   INTEGER,
        ptMTPLSLinkMTP3NAIndex          INTEGER,
        ptMTPLSLinkLinksetsIndex        INTEGER,
        ptMTPLSLinkLinksetIndex         INTEGER,
        ptMTPLSLinkActions              INTEGER,
        ptMTPLSLinkParentOID            OBJECT IDENTIFIER,
        ptMTPLSLinkRowStatus            RowStatus,
        ptMTPLSLinkIndex                INTEGER
    }

ptMTPLSLinkContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A condensed textual representation of the link object."
    ::= { ptMTPLSLinkEntry 1 }

