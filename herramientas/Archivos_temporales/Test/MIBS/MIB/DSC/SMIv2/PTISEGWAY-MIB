        ptSCCPSCCPNAMTP3IncomingGTAI    INTEGER,
        ptSCCPSCCPNAMTP3IncomingGTZeroLength    INTEGER,
        ptSCCPSCCPNAMTP3IncomingDestination   INTEGER,
