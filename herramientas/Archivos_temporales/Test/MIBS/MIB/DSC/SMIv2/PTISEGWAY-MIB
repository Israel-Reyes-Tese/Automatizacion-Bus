ptDSCADNConnection OBJECT IDENTIFIER
    ::= { ptDSCAdjacentDiameterNode 1840 }

ptDSCADNConnectionTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtDSCADNConnectionEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptDSCADNConnection 1 }

ptDSCADNConnectionEntry OBJECT-TYPE
    SYNTAX          PtDSCADNConnectionEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of DSCADNConnection attributes."
    INDEX           { ptConnGroupIndex, ptDSCADNConnectionDSCIndex, 
                      ptDSCADNConnectionDSCNodesIndex, 
                      ptDSCADNConnectionDSCNodeIndex, 
                      ptDSCADNConnectionAdjacentDiameterNodesIndex, 
                      ptDSCADNConnectionAdjacentDiameterNodeIndex, 
                      ptDSCADNConnectionIndex }
    ::= { ptDSCADNConnectionTable 1 }

PtDSCADNConnectionEntry ::=
    SEQUENCE {
        ptDSCADNConnectionSlot          INTEGER,
        ptDSCADNConnectionTransportType INTEGER,
        ptDSCADNConnectionLocalHost     DisplayString,
        ptDSCADNConnectionRemoteHost    DisplayString,
        ptDSCADNConnectionRemotePort    Integer32,
        ptDSCADNConnectionStatus        INTEGER,
        ptDSCADNConnectionSecurityParameterSet      DisplayString,
        ptDSCADNConnectionCapacityParameterSet      DisplayString,
        ptDSCADNConnectionSCTPParameterSet  DisplayString,
        ptDSCADNConnectionCERParameterSet DisplayString,
        ptDSCADNConnectionRxMessageCount Integer32,
        ptDSCADNConnectionTxMessageCount Integer32,
        ptDSCADNConnectionInternalID    Integer32,
        ptDSCADNConnectionUserActivation INTEGER,
        ptDSCADNConnectionLocalPort     INTEGER,
        ptDSCADNConnectionEncryption    DisplayString,
        ptDSCADNConnectionVendorID      DisplayString,
        ptDSCADNConnectionProductName   DisplayString,
        ptDSCADNConnectionFirmwareRevision  Integer32,
        ptDSCADNConnectionDSCIndex      Integer32,
        ptDSCADNConnectionDSCNodesIndex Integer32,
        ptDSCADNConnectionDSCNodeIndex  Integer32,
        ptDSCADNConnectionAdjacentDiameterNodesIndex            Integer32,
        ptDSCADNConnectionAdjacentDiameterNodeIndex           Integer32,
        ptDSCADNConnectionActions       INTEGER,
        ptDSCADNConnectionParentOID     OBJECT IDENTIFIER,
        ptDSCADNConnectionRowStatus     RowStatus,
        ptDSCADNConnectionIndex         Integer32
    }

ptDSCADNConnectionSlot OBJECT-TYPE
    SYNTAX          INTEGER { any(0) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Adjacent Diameter Node associated DSC slot. \nValid values: Available routing CPU slot number and ANY (0). \nWhen set to ANY, the Adjacent Diameter Node will be associated with any one of the available routing CPU slots that the Local Host attribute can resolve to. All the available routing slots will attempt to bring the connection UP depending on the Transport Type attribute. Once the connection is successful on one of the routing CPUs, the other routing CPUs will stop attempting to bring the connection in service. \nWhen the Lcoal host attribute is set to a hostname that resolves to multiple IP addresses defined on different routing CPUs, then the first routing CPU that is able to bring the connection UP is chosen. Other routing CPUs close their connection and stop initiating and accepting connections. \nIt is not possible to create more than one Adjacent Diameter Node Connection when the Slot attribute is set to ANY."
    ::= { ptDSCADNConnectionEntry 1 }

ptDSCADNConnectionTransportType OBJECT-TYPE
    SYNTAX          INTEGER { sctp(0), tcp(1), tipc(2), serverOnly(100) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Transport Type (SCTP, TCP, TIPC, SERVER ONLY) \nSet to the transport type between the DSC Node and the ADN. \nThe TCP and SCTP types indicate the transport type that will be used for the outgoing connections. Incoming connections may still be accepted by transport servers of either protocol. \nThe TIPC type is reserved for DSC INTERNAL ADNs. \nIf setting the SERVER ONLY option, no outgoing transport connections will be attempted. Make sure an associated Transport Server object is available. \nNote: 3GPP specifications such as TS 29.272 (S6a, S6d, S13, S7, ...) require the use of SCTP. Default value: SCTP."
    ::= { ptDSCADNConnectionEntry 2 }

ptDSCADNConnectionLocalHost OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Local host for the Adjacent Diameter Node connection. \nMandatory or optional based on the connection and transport types. \nValid values: Hostname or IP address. Ignored for TIPC & TCP Transport Types. \nDefault value: slot<Slot>.<DSC Node.Node Diameter ID>"
    ::= { ptDSCADNConnectionEntry 3 }

ptDSCADNConnectionRemoteHost OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Remote host for the Adjacent Diameter Node connection. \nValid values: Hostname or IP address. Ignored for TIPC and SERVER ONLY Transport Types. \nDefault value: ADN.Adjacent Diameter ID."
    ::= { ptDSCADNConnectionEntry 4 }

ptDSCADNConnectionRemotePort OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Remote port for the Adjacent Diameter Node connection (1 to 65535). \nDefault value: 3868."
    ::= { ptDSCADNConnectionEntry 5 }

ptDSCADNConnectionStatus OBJECT-TYPE
    SYNTAX          INTEGER { down(0), up(1), inactive(2), 
                        partiallyUp(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of the Adjacent Diameter Node connection (UP/DOWN/INACTIVE)."
    ::= { ptDSCADNConnectionEntry 6 }

