    SYNTAX          INTEGER (-1 | 1..8)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Channel used by this link, valid values are 1 to 8"
    ::= { ptMTPATMLinkEntry 7 }

ptMTPATMLinkL2State OBJECT-TYPE
    SYNTAX          INTEGER { deactivated(0), proving(1), inService(2), 
                        outOfService(3), failed(4), localProcOutage(5), 
