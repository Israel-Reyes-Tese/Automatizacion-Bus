    SEQUENCE {
        sonusDsx3IntervalDayShelfIndex  SonusShelfIndex,
        sonusDsx3IntervalDaySlotIndex   SonusSlotIndex,
        sonusDsx3IntervalDayPortIndex   Integer32,
        sonusDsx3IntervalDayDs3Index    Integer32,
        sonusDsx3IntervalDayLCVs        PerfCurrentCount,
        sonusDsx3IntervalDayLESs        PerfCurrentCount,
        sonusDsx3IntervalDayLSESs       PerfCurrentCount,
        sonusDsx3IntervalDayCVPs        PerfCurrentCount,
        sonusDsx3IntervalDayCVCPs       PerfCurrentCount,
        sonusDsx3IntervalDayESPs        PerfCurrentCount,
        sonusDsx3IntervalDayESCPs       PerfCurrentCount,
        sonusDsx3IntervalDaySESPs       PerfCurrentCount,
        sonusDsx3IntervalDaySESCPs      PerfCurrentCount,
        sonusDsx3IntervalDaySASs        PerfCurrentCount,
        sonusDsx3IntervalDayUASPs       PerfCurrentCount,
        sonusDsx3IntervalDayUASCPs      PerfCurrentCount,
        sonusDsx3IntervalDayLineIDF     TruthValue,
        sonusDsx3IntervalDayPathIDF     TruthValue,
        sonusDsx3IntervalDayFC          Integer32
    }

