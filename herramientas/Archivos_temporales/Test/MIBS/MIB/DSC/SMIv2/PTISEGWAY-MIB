        ptSGM3UAMultiAssociationASPMaximumPathRetrans             Integer32,
        ptSGM3UAMultiAssociationASPBundling   INTEGER,
        ptSGM3UAMultiAssociationASPBundlingTimeout          Integer32,
        ptSGM3UAMultiAssociationASPDelayedAckTimeout            Integer32,
        ptSGM3UAMultiAssociationASPChecksum   INTEGER,
        ptSGM3UAMultiAssociationASPInStreams    Integer32,
        ptSGM3UAMultiAssociationASPOutStreams     Integer32,
        ptSGM3UAMultiAssociationASPM3UAVersion      INTEGER,
        ptSGM3UAMultiAssociationASPHeartbeat    INTEGER,
        ptSGM3UAMultiAssociationASPNATranslation        INTEGER,
        ptSGM3UAMultiAssociationASPDefaultNetworkAppearance                   INTEGER,
        ptSGM3UAMultiAssociationASPNAInDATAMessage          INTEGER,
        ptSGM3UAMultiAssociationASPIMFSupport     INTEGER,
        ptSGM3UAMultiAssociationASPM3UAIndex    Integer32,
        ptSGM3UAMultiAssociationASPM3UAConfiguredASPsIndex                  Integer32,
        ptSGM3UAMultiAssociationASPParentOID    OBJECT IDENTIFIER,
        ptSGM3UAMultiAssociationASPRowStatus    RowStatus,
        ptSGM3UAMultiAssociationASPIndex Integer32
    }

