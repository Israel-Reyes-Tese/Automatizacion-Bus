ptGWSTBatchOperations OBJECT IDENTIFIER
    ::= { ptGWSTGWSTTables 598 }

ptGWSTBatchOperationsTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtGWSTBatchOperationsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptGWSTBatchOperations 1 }

ptGWSTBatchOperationsEntry OBJECT-TYPE
    SYNTAX          PtGWSTBatchOperationsEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of GWSTBatchOperations attributes."
    INDEX           { ptConnGroupIndex, 
                      ptGWSTBatchOperationsGWSTTablesIndex, 
                      ptGWSTBatchOperationsIndex }
    ::= { ptGWSTBatchOperationsTable 1 }

PtGWSTBatchOperationsEntry ::=
    SEQUENCE {
        ptGWSTBatchOperationsMaxPerInterval   Integer32,
        ptGWSTBatchOperationsBatchInterval  Integer32,
        ptGWSTBatchOperationsGWSTTablesIndex    Integer32,
        ptGWSTBatchOperationsActions    INTEGER,
        ptGWSTBatchOperationsParentOID  OBJECT IDENTIFIER,
        ptGWSTBatchOperationsRowStatus  RowStatus,
        ptGWSTBatchOperationsIndex      Integer32
    }

ptGWSTBatchOperationsMaxPerInterval OBJECT-TYPE
    SYNTAX          Integer32 (1..100)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Maximum number of records processed in each batch operation interval."
    ::= { ptGWSTBatchOperationsEntry 1 }

ptGWSTBatchOperationsBatchInterval OBJECT-TYPE
    SYNTAX          Integer32 (1..60000)
