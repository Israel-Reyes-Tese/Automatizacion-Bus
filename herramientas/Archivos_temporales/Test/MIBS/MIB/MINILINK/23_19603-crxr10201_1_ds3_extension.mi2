	XF-DS3-EXTENSION-MIB DEFINITIONS ::= BEGIN
 
		IMPORTS
			dsx3ConfigEntry			
				FROM DS3-MIB			
			OBJECT-GROUP, MODULE-COMPLIANCE			
				FROM SNMPv2-CONF			
			OBJECT-TYPE, MODULE-IDENTITY			
				FROM SNMPv2-SMI			
			TruthValue			
				FROM SNMPv2-TC			
			xfPDH, Xf24HrsSecondsGauge, Xf15MinSeconds, Xf24HrsSeconds			
				FROM XF-TOP-MIB;
	
	
		xfDs3ExtensionMIB MODULE-IDENTITY 
			LAST-UPDATED "201210101240Z"		-- October 10, 2012 at 12:40 GMT
			ORGANIZATION 
				"Ericsson-Norway"
			CONTACT-INFO 
				" "
			DESCRIPTION 
				"
				This extends the standard DS3-MIB, see IETF RFC2496 with 
				object to support the functionality for XF MCR Link
				"
			REVISION "201210101240Z"		-- October 10, 2012 at 12:40 GMT
			DESCRIPTION 
				"H Fixed minor syntax warnings"
			REVISION "200404021240Z"		-- April 02, 2004 at 12:40 GMT
			DESCRIPTION 
				"G added LOF and RAI to line status
				  made all PM objects obsolete
				  don't use PM  and NetequipmentOoS in 
				  line status"
			REVISION "200306191252Z"		-- June 19, 2003 at 12:52 GMT
			DESCRIPTION 
				"F
				Changed compliance and made
				  pm objects optional.
				Added default thresholds."
			REVISION "200301170000Z"		-- January 17, 2003 at 00:00 GMT
			DESCRIPTION 
				"
				Rev. E
				No functional change. Only transfer of MIB from EAB to ETO.
				"
			REVISION "200211121200Z"		-- November 12, 2002 at 12:00 GMT
			DESCRIPTION 
				"
				Rev. D
				Changed description to include revision.
				"
			REVISION "200208231200Z"		-- August 23, 2002 at 12:00 GMT
			DESCRIPTION 
				"
				Rev. C
				Removed the BITS import from SNMPv2-SMI
				"
			REVISION "200208131200Z"		-- August 13, 2002 at 12:00 GMT
			DESCRIPTION 
				"
				Rev. B	Corrected XfDsx3AISDetection values. 
						Changed description of XfDsx3AISDetection
				"
			REVISION "200206261200Z"		-- June 26, 2002 at 12:00 GMT
			DESCRIPTION 
				"
				Rev. A 		Initial Revision
				"	
			::= { xfPDH 2 }

--
-- Node definitions
--
	
-- 
-- 
		xfDs3MIBXObjects OBJECT IDENTIFIER ::= { xfDs3ExtensionMIB 1 }

		
		xfDsx3ConfigXTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfDsx3ConfigXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"
				This table is an extension to the dsx3ConfigTable.
				"
			::= { xfDs3MIBXObjects 1 }

		
		xfDsx3ConfigXEntry OBJECT-TYPE
			SYNTAX XfDsx3ConfigXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"
				Additional information about the configuration of a E3 
				interface.
				"
			AUGMENTS { dsx3ConfigEntry }
			::= { xfDsx3ConfigXTable 1 }

		
		XfDsx3ConfigXEntry ::=
			SEQUENCE { 
				xfDsx3AISDetection
					INTEGER,
				xfDsx3Current24ES
					Xf24HrsSecondsGauge,
				xfDsx3Current24SES
					Xf24HrsSecondsGauge,
				xfDsx3Current24UAS
					Xf24HrsSecondsGauge,
				xfDsx3ESCurrentThreshold
					Xf15MinSeconds,
				xfDsx3SESCurrentThreshold
					Xf15MinSeconds,
				xfDsx3ESTotalThreshold
					Xf24HrsSeconds,
				xfDsx3SESTotalThreshold
					Xf24HrsSeconds,
				xfDsx3LineStatus
					BITS,
				xfDsx3TotalValid
					TruthValue
			 }

		xfDsx3AISDetection OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"
				Controls AIS detection on E3 interfaces.
				"
			DEFVAL { enable }
			::= { xfDsx3ConfigXEntry 3 }

		
		xfDsx3Current24ES OBJECT-TYPE
			SYNTAX Xf24HrsSecondsGauge
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS obsolete
			DESCRIPTION
				"
				Number of Errored Secconds since the start of the current 
				24 hours interval.
				"
			::= { xfDsx3ConfigXEntry 4 }

		
		xfDsx3Current24SES OBJECT-TYPE
			SYNTAX Xf24HrsSecondsGauge
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS obsolete
			DESCRIPTION
				"
				Number of Severly Errored Secconds since the start of 
				the current 24 hours interval.
				"
			::= { xfDsx3ConfigXEntry 5 }

		
		xfDsx3Current24UAS OBJECT-TYPE
			SYNTAX Xf24HrsSecondsGauge
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS obsolete
			DESCRIPTION
				"
				Number of Unavailable seconds since the start of the
				crrent 24 Hrs PM interval.
				"
			::= { xfDsx3ConfigXEntry 6 }

		
		xfDsx3ESCurrentThreshold OBJECT-TYPE
			SYNTAX Xf15MinSeconds
			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS obsolete
			DESCRIPTION
				"This is the 15 minutes threshold for ES. Does the 
				current ES counter cross this value an alarmNew will be 
				sent.It will also act as a reset value. So if the next 15 
				minutes interval has an ES counter lower than this 
				threshold value a alarmClear trap will be sent.
				"
			DEFVAL { 38 }
			::= { xfDsx3ConfigXEntry 7 }

		
		xfDsx3SESCurrentThreshold OBJECT-TYPE
			SYNTAX Xf15MinSeconds
			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS obsolete
			DESCRIPTION
				"This is the 15 minutes threshold for SES. Does the 
				current SES counter cross this value an alarmNew will be 
				sent.It will also act as a reset value. So if the next 15 
				minutes interval has an SES counter lower than this 
				threshold value a alarmClear trap will be sent."
			DEFVAL { 4 }
			::= { xfDsx3ConfigXEntry 8 }

		
		xfDsx3ESTotalThreshold OBJECT-TYPE
			SYNTAX Xf24HrsSeconds
			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS obsolete
			DESCRIPTION
				"This is the 24 hours threshold for ES. Does the current 
				24 hour ES counter cross this value an alarmNew will be 
				emitted.In that case a alarmClear will be sent at the 
				end of the 24 hour period.
				"
			DEFVAL { 380 }
			::= { xfDsx3ConfigXEntry 9 }

		
		xfDsx3SESTotalThreshold OBJECT-TYPE
			SYNTAX Xf24HrsSeconds
			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS obsolete
			DESCRIPTION
				"This is the 24 hours threshold for SES. Does the current 
				24 hour SES counter cross this value an alarm will be 
				emitted.In that case a alarmClear will be sent at the 
				end of the 24 hour period.
				"
			DEFVAL { 40 }
			::= { xfDsx3ConfigXEntry 10 }

		
		xfDsx3LineStatus OBJECT-TYPE
			SYNTAX BITS
				{
				dsx3AIS0(0),
				dsx3AIS1(1),
				dsx3AIS2(2),
				dsx3LOS0(3),
				dsx3LOS1(4),
				dsx3LOS2(5),
				dsx3UAS0(6),
				dsx3UAS1(7),
				dsx3UAS2(8),
				dsx3ES15m0(9),
				dsx3ES15m1(10),
				dsx3ES15m2(11),
				dsx3SES15m0(12),
				dsx3SES15m1(13),
				dsx3SES15m2(14),
				dsx3ES24h0(15),
				dsx3ES24h1(16),
				dsx3ES24h2(17),
				dsx3SES24h0(18),
				dsx3SES24h1(19),
				dsx3SES24h2(20),
				dsx3NetEquipmentOOS0(21),
				dsx3NetEquipmentOOS1(22),
				dsx3NetEquipmentOOS2(23),
				dsx3LOF0(24),
				dsx3LOF1(25),
				dsx3LOF2(26),
				dsx3RAI0(27),
				dsx3RAI1(28),
				dsx3RAI2(29)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Gives information on the alarm status of a E1 interface.
				Severity is indicated by three bits per alarm:
				bits 012
				     000   clear
				     001   undetermined
				     010   warning
				     011   minor
				     100   major
				     101   critical
				
				All PM status bits, i.e. UAS,ESxx,SESxx, are not used anymore. Also
				NetEquipmentOOS is not used.
				"
			::= { xfDsx3ConfigXEntry 11 }

		
		xfDsx3TotalValid OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-only
			STATUS obsolete
			DESCRIPTION
				"Indicates whether the last 24 hour interval was valid."
			::= { xfDsx3ConfigXEntry 12 }

		
		xfDs3MIBXConformance OBJECT IDENTIFIER ::= { xfDs3ExtensionMIB 2 }

		
		xfDs3MIBXCompliance OBJECT IDENTIFIER ::= { xfDs3MIBXConformance 1 }

		
--   this module
-- this module
		xfDs3MIBXOptionalPMCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"The compliance statement for SNMP entities states that 
				PM objects optional."
			MODULE -- this module
				MANDATORY-GROUPS { xfDs3MIBXGroup }
			::= { xfDs3MIBXCompliance 1 }

		
		xfDs3MIBXGroups OBJECT IDENTIFIER ::= { xfDs3MIBXConformance 2 }

		
		xfDs3MIBXGroup OBJECT-GROUP
			OBJECTS { xfDsx3AISDetection, xfDsx3LineStatus }
			STATUS current
			DESCRIPTION 
				"A group of all mandatory dsx3 extension objects."
			::= { xfDs3MIBXGroups 1 }

		
		xfDs3Obsolete OBJECT-GROUP
			OBJECTS { xfDsx3Current24ES, xfDsx3Current24SES, xfDsx3Current24UAS, xfDsx3ESCurrentThreshold, xfDsx3SESCurrentThreshold, 
				xfDsx3ESTotalThreshold, xfDsx3SESTotalThreshold, xfDsx3TotalValid }
			STATUS obsolete
			DESCRIPTION 
				"Description."
			::= { xfDs3MIBXGroups 3 }

		
	
	END

