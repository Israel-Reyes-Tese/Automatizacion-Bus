					HCPerfCurrentCount,
				xfCesPwPerfCurrentInHCBytes
					HCPerfCurrentCount,
				xfCesPwPerfCurrentOutHCPackets
					HCPerfCurrentCount,
				xfCesPwPerfCurrentOutHCBytes
					HCPerfCurrentCount,
				xfCesPwTDMPerfCurrentMissingPkts
					PerfCurrentCount,
				xfCesPwTDMPerfCurrentPktReorders
					PerfCurrentCount,
				xfCesPwTDMPerfCurrentJtrBufferUnderruns
					PerfCurrentCount,
				xfCesPwTDMPerfCurrentMisOrderDrop
					PerfCurrentCount,
