        sonusE164ProfDigitF             SonusE164ProfileState,
        sonusE164ProfCharNonFirstPlus   SonusE164ProfileState,
        sonusE164ProfCharStar           SonusE164ProfileState,
        sonusE164ProfCharHash           SonusE164ProfileState,
        sonusE164ProfCharHyphen         SonusE164ProfileState,
        sonusE164ProfCharDot            SonusE164ProfileState,
        sonusE164ProfCharLeftCircularBracket    SonusE164ProfileState,
        sonusE164ProfCharRightCircularBracket     SonusE164ProfileState
