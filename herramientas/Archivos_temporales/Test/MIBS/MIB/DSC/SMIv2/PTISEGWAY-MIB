ptSP2KEvent OBJECT IDENTIFIER
    ::= { ptSP2KEventList 322 }

ptSP2KEventTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtSP2KEventEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptSP2KEvent 1 }

ptSP2KEventEntry OBJECT-TYPE
    SYNTAX          PtSP2KEventEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of SP2KEvent attributes."
    INDEX           { ptConnGroupIndex, ptSP2KEventConfigurationIndex, 
                      ptSP2KEventSP2KEventListIndex, ptSP2KEventIndex }
    ::= { ptSP2KEventTable 1 }

PtSP2KEventEntry ::=
    SEQUENCE {
        ptSP2KEventSeverity             INTEGER,
        ptSP2KEventSequenceNumber       Integer32,
        ptSP2KEventDateAndTime          DisplayString,
        ptSP2KEventTrapType             INTEGER,
        ptSP2KEventProblemCause         INTEGER,
        ptSP2KEventClearType            INTEGER,
        ptSP2KEventCorrelationID        DisplayString,
        ptSP2KEventTrapNameAndID        DisplayString,
        ptSP2KEventComponentID          DisplayString,
        ptSP2KEventSpecificProblem      DisplayString,
        ptSP2KEventHWEntityLocation     DisplayString,
        ptSP2KEventDescription          DisplayString,
        ptSP2KEventConfigurationIndex   Integer32,
        ptSP2KEventSP2KEventListIndex   Integer32,
        ptSP2KEventParentOID            OBJECT IDENTIFIER,
        ptSP2KEventRowStatus            RowStatus,
        ptSP2KEventIndex                Integer32
    }

ptSP2KEventSeverity OBJECT-TYPE
    SYNTAX          INTEGER { indetermined(0), critical(1), major(2), 
                        minor(3), warning(4), cleared(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Severity is used in the reporting of traps. When a trap is issued it will have an associated severity.\n\n Possible Severity are: critical, major, minor, warning, cleared."
    ::= { ptSP2KEventEntry 1 }

ptSP2KEventSequenceNumber OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The sequence number of the trap. This number increased by one for each trap sent by the system."
    ::= { ptSP2KEventEntry 2 }

ptSP2KEventDateAndTime OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Date and time value of the occurrence of the trap."
    ::= { ptSP2KEventEntry 3 }

ptSP2KEventTrapType OBJECT-TYPE
    SYNTAX          INTEGER { undefined(0), communicationAlarm(2), 
                        environmentAlarm(3), equipmentAlarm(4), 
                        processingAlarm(10), qualityOfServiceAlarm(11) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type of the trap, follows Ref. X.733 : \n\nCommunication Alarm: An alarm of this type is principally associated with the procedures and/or processes required to convey information from one point to another.\n\nQuality of Service Alarm: An alarm of this type is principally associated with a degradation in the quality of a service.\n\nProcessing Error Alarm: An alarm of this type is principally associated with a software or processing fault.\n\nEquipment Alarm: An alarm of this type is principally associated with equipment fault.\n\nEnvironmental Alarm: An alarm of this type is principally associated with a condition relating to an enclosure in which the equipment resides."
    ::= { ptSP2KEventEntry 4 }

ptSP2KEventProblemCause OBJECT-TYPE
    SYNTAX          INTEGER { undefined(0), 
                        applicationSubsystemFailure(2), 
                        callEstablishmentError(4), 
                        communicationsProtocolError(5), 
                        communicationsSubsystemError(6), 
                        configurationOrCustomizationError(7), 
                        congestion(8), corruptData(9), 
                        cpuCyclesLimitExceeded(10), 
                        datasetOrModemError(11), degradedSignal(12), 
                        dteDceInterfaceError(13), enclosureDoorOpen(14), 
                        equipmentMalfunction(15), fileError(17), 
                        fireDetected(18), floodDetected(19), 
                        framingError(20), inputOutputDeviceError(23), 
                        inputDeviceError(24), lanError(25), 
                        localNodeTransmissionError(27), lossOfFrame(28), 
                        lossOfSignal(29), multiplexerProblem(31), 
                        outOfMemory(32), outputDeviceError(33), 
                        performanceDegraded(34), powerProblem(35), 
                        processorProblem(37), queueSizeExceeded(39), 
                        receiveFailure(40), 
                        remoteNodeTransmissionError(42), 
                        resourceAtOrNearCapacity(43), 
                        responseTimeExcessive(44), 
                        retransmissionRateExcessive(45), 
                        softwareError(46), 
                        softwareProgramAbnormallyTerminated(47), 
                        softwareProgramError(48), 
                        storageCapacityProblem(49), thresholdCrossed(51), 
                        timingProblem(52), transmitFailure(54), 
                        underlyingResourceUnavailable(56), 
                        versionMismatch(57) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The probable cause of the trap, follows Ref. X.733. After we identify the GSN alarms, we will categorize them according to the five types above, find their probable causes, and map the causes, as close as we can, to the X.733 list."
    ::= { ptSP2KEventEntry 5 }

ptSP2KEventClearType OBJECT-TYPE
    SYNTAX          INTEGER { fmic(1), oic(2), undefined(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The Clear type of the trap, which is either FMIC or OIC, where FMIC means that the trap will be cleared by another clear trap, and OIC means that the trap needs to be cleared by the operator."
    ::= { ptSP2KEventEntry 6 }

ptSP2KEventCorrelationID OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This attribute displays the correlation ID for the trap. \n\n It is used to correlate between the active alarm and its clearing event."
    ::= { ptSP2KEventEntry 7 }

ptSP2KEventTrapNameAndID OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This attribute displays the internal Name and ID for the trap as shown in the UI dashboard. \n\n It is used to correlate between the SP2K severity-based trap and the internal trap definition in the dashboard."
    ::= { ptSP2KEventEntry 8 }

ptSP2KEventComponentID OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This attribute gives the name and the OID of the item type from which the variable names and values are taken.\n\n The variable names and values (listed in the specificProblem field) should clarify which item (if there are many of the same type) issued the trap and may give more information as to why the trap has been issued."
    ::= { ptSP2KEventEntry 9 }

ptSP2KEventSpecificProblem OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This attribute lists the names and the values of all variables inserted in the trap up to the maximum length of 255 characters. Along with the component ID attribute, this gives either a way to know which item issued the trap or may give more information as to why the trap has been issued.\n\n "
    ::= { ptSP2KEventEntry 10 }

ptSP2KEventHWEntityLocation OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This attribute provides the Frame/Shelf/Slot/Sub-Slot information. This info helps locate the management card issuing SP2K traps."
    ::= { ptSP2KEventEntry 11 }

ptSP2KEventDescription OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This attribute provides a textual description of the condition reported in the trap."
    ::= { ptSP2KEventEntry 12 }

ptSP2KEventConfigurationIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is Configuration."
    ::= { ptSP2KEventEntry 242 }

