    ::= { ptGTTAppIDRegistrationEntry 4 }

ptGTTAppIDRegistrationRetainOrigination OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Application ID Registration Retain Origination. The Retain Origination attribute provides a mechanism where by the GTT notifies the SCCP to retain the Originating OPC in the Routing Label of the MSU when forwarding the MSU to to its new destination. Only MSUs redirected to GTT via the Gateway Screening App ID redirection table are affected by this attribute. By default, the Retain Origination attribute when set to disabled, maintains the standard GTT Routing implementation."
    ::= { ptGTTAppIDRegistrationEntry 5 }

ptGTTAppIDRegistrationGTTNAManagerIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
