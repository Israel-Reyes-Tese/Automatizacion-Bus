PthwmonSlotEntry ::=
    SEQUENCE {
        pthwmonSlotSlotNumber           Integer32,
        pthwmonSlotBoardManufactureDate DisplayString,
        pthwmonSlotBoardManufacturer    DisplayString,
        pthwmonSlotBoardProductName     DisplayString,
        pthwmonSlotBoardSerialNumber    DisplayString,
        pthwmonSlotBoardPartNumber      DisplayString,
        pthwmonSlotBoardFRUFileID       DisplayString,
        pthwmonSlotProductManufacturer  DisplayString,
        pthwmonSlotProductName          DisplayString,
        pthwmonSlotProductModelNumber   DisplayString,
        pthwmonSlotProductVersion       DisplayString,
        pthwmonSlotProductSerialNumber  DisplayString,
        pthwmonSlotProductFRUFileID     DisplayString,
        pthwmonSlotMMCFirmwareVersion   DisplayString,
        pthwmonSlotBootcodeFirmwareVersion  DisplayString,
        pthwmonSlotHardwareMonitorIndex Integer32,
        pthwmonSlotParentOID            OBJECT IDENTIFIER,
        pthwmonSlotRowStatus            RowStatus,
        pthwmonSlotIndex                Integer32
    }

