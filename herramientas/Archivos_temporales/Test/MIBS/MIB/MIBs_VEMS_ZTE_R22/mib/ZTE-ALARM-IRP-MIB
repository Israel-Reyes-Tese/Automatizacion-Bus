--======================================================================
-- ALARM FILTER
--======================================================================
alarmFilterEnable OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "Alarm Filter enable or false,0 means false,while 1 means true."
        ::= { alarmFilter 1 }
        
alarmFilterCondition  OBJECT IDENTIFIER ::= { alarmFilter 3}
--======================================================================
-- ALARM FILTER
--======================================================================
        
 
alarmTypeFilter OBJECT-TYPE
        SYNTAX      TypeFilter
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters alarm according to the alarm type.The alarm of specifical type will be sent."                 
        ::= { alarmFilterCondition 4 }  
alarmSeverityFilter OBJECT-TYPE
        SYNTAX      SeverityFilter
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters alarm according to the alarm severity.The alarm of specifical severity will be sent."                 
        ::= { alarmFilterCondition 5 }  
                                            

alarmCodesExcludeFilter OBJECT-TYPE
        SYNTAX     OCTET STRING (SIZE (0..512))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters alarm according to the alarm code.  uses ',' as splitter between alarm codes .for example: 10001,10002 means :The alarm of specifical  code of 10001 and 10002 will not be sent. "                 
        ::= { alarmFilterCondition 6 }  

alarmTypeExcludeFilter OBJECT-TYPE
        SYNTAX     OCTET STRING (SIZE (0..512))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters alarm according to the alarm type.  uses ',' as splitter between alarm types .for example: 1,2 means :The alarms of specifical type  of 1 and 2,those will not be sent. "                 
        ::= { alarmFilterCondition 7 }  

alarmServerityExcludeFilter OBJECT-TYPE
        SYNTAX     OCTET STRING (SIZE (0..512))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters alarm according to the alarm serverity.  uses ',' as splitter between alarm serverity .for example: 1,2 means : The alarms of specifical  serverity of 1 and 2 will not  be sent. "                 
        ::= { alarmFilterCondition 8 }  

alarmSystemTypeCodeExcludeFilter OBJECT-TYPE
        SYNTAX     OCTET STRING (SIZE (0..512))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters alarm according to the alarm system type and alarm code.  uses '[]' as splitter between alarm system type and alarm code .for example: [6,1000][11,1200] means : System type is 6 and the alarm code for 1000, system type 11 and alarm code of 1200 is not sent."                 
        ::= { alarmFilterCondition 9 }  

alarmPosition1OrPostion2ExcludeFilter OBJECT-TYPE
        SYNTAX     OCTET STRING (SIZE (0..1024))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters alarm according to the alarm position1 or position2.  uses ',' as splitter between alarm position1 or poistion2. for example: CommonTopoObject=107-1417765074750,CommonTopoObject=100-1416791386500 means : Position1 is CommonTopoObject=107-1417765074750 and the position2 for CommonTopoObject=100-1416791386500, position1 CommonTopoObject=107-1417765074750 or position2 of CommonTopoObject=100-1416791386500 is not sent."                 
        ::= { alarmFilterCondition 10 }  
   
alarmSystemTypeCauseExcludeFilter OBJECT-TYPE
        SYNTAX     OCTET STRING (SIZE (0..512))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters alarm according to the alarm system type and northbound alarm code.  uses '[]' as splitter between alarm system type and northbound alarm code .for example: [6,716][11,351] means : System type is 6 and the northbound alarm code for 716, system type 11 and northbound alarm code of 351 is not sent."                 
        ::= { alarmFilterCondition 11 }     
        
                        
syncAlarm OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE (0..1024))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object sync alarm ,once successfully set,ems will send alarm.The format is :'position:a,c;', 'a' and 'c' is Moc's Id."             
              
        ::= {alarmOperation 1 }
        
alarmTableFilter OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE (0..1024))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object filters alarm according to serverity,type,northbound alarm code, alarm code, systemtype, position. The format is : 'PerceivedSeverity:1,3; TypeOfEvent:1,2; ProbableCause:1001,1002;SystemTypeAndCause:[6,716];SystemTypeAndCode:[6,1000];Position1OrPostion2:COMM:EMS=0806164728805,CommonTopoObject=123;Position:COMM:EMS=0806164728805,CommonTopoObject=123'"             
              
        ::= {alarmOperation 2 }   
        
alarmTrapEnable  OBJECT-TYPE
         SYNTAX INTEGER {
         off(0),
         on(1),
         error(3)
        } 
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object enable alarm trap.1 means enable alarm trap,while 0 means disable alarm trap."             
              
        ::= {alarmOperation 3 }

clearAlarm  OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE (0..1024))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
                "This object clear alarm ,use alarm's alarmPerceivedSeverity and id. for example 'PerceivedSeverity:2;Id:123' or  'PerceivedSeverity:3,4;' "             
              
        ::= {alarmOperation 4 }          
                 
--======================================================================
-- ALARM RELATED NOTIFICATIONS
--======================================================================

alarmNew NOTIFICATION-TYPE
        OBJECTS {   alarmEventTime,
                    sendNotificationId,
                    lastSendNotificationId,
                    systemDN,
                    alarmCode,
                    alarmManagedObjectInstance,
                    alarmEventType,
                    alarmProbableCause,
                    alarmPerceivedSeverity,
                    alarmSpecificProblem ,
                    alarmAdditionalText,                    
                    alarmNetype,
                    alarmIndex,
                    alarmId,
                    alarmCodeName,
                    alarmManagedObjectInstanceName,
                    alarmSystemType,
                    alarmNeIP,
                    alarmAck,
                    cleiCode,
                    timeZoneID,
                    timeZoneOffset,
                    dSTSaving,
                    aid,
                    id,
                    alarmMocObjectInstance,
                    alarmComment,
                    alarmOtherInfo
                     }
        STATUS      current
        DESCRIPTION 
                "This notification is sent when a new Alarm is 
                 generated and stored in the Active Alarm Table. 
                 Note that the notification does not include all
                 objects for the corresponding entry in the Alarm 
                 Table. The reason is that some environments may have 
                 problems with large PDUs. Thus, the notification 
                 receiver must get the missing objects from the
                 Active Alarm Table.
                 
                 The value 'cleared' for the PerceivedSeverity
                 is not allowed for an alarm notification."
        ::= { alarmNotificationProfile 1 }


alarmCleared NOTIFICATION-TYPE
        OBJECTS {   alarmEventTime,
                    sendNotificationId,
                    lastSendNotificationId,   
                    systemDN,                    
                    alarmCode,                    
                    alarmManagedObjectInstance,
                    alarmEventType,
                    alarmProbableCause,
                    alarmPerceivedSeverity,
                    alarmSpecificProblem,
                    alarmAdditionalText,     
                    alarmNetype,
                    alarmIndex,
                    alarmId,
                    alarmCodeName,
                    alarmManagedObjectInstanceName,
                    alarmSystemType,
                    alarmNeIP,
                    alarmAck,
                    cleiCode,
                    timeZoneID,
                    timeZoneOffset,
                    dSTSaving,
                    aid,
                    id,
                    alarmMocObjectInstance,
                    alarmComment,
                    alarmOtherInfo
                    }
        STATUS      current
        DESCRIPTION 
                "This notification is generated when an alarm is 
                 cleared. A cleared alarm is removed from the 
                 Active Alarm Table and is no longer accessible.
                 
                 It's impossible to retrieve an cleared alarm from
                 Active Alarm Table any more."
        ::= { alarmNotificationProfile 2 }
        
