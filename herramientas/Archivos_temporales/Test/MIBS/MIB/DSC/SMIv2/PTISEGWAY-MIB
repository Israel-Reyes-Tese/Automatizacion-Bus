ptDSCAdjacentDiameterNode OBJECT IDENTIFIER
    ::= { ptDSCAdjacentDiameterNodes 1807 }

ptDSCAdjacentDiameterNodeTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtDSCAdjacentDiameterNodeEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptDSCAdjacentDiameterNode 1 }

ptDSCAdjacentDiameterNodeEntry OBJECT-TYPE
    SYNTAX          PtDSCAdjacentDiameterNodeEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of DSCAdjacentDiameterNode attributes."
    INDEX           { ptConnGroupIndex, 
                      ptDSCAdjacentDiameterNodeDSCIndex, 
                      ptDSCAdjacentDiameterNodeDSCNodesIndex, 
                      ptDSCAdjacentDiameterNodeDSCNodeIndex, 
                      ptDSCAdjacentDiameterNodeAdjacentDiameterNodesIndex, 
                      ptDSCAdjacentDiameterNodeIndex }
    ::= { ptDSCAdjacentDiameterNodeTable 1 }

PtDSCAdjacentDiameterNodeEntry ::=
    SEQUENCE {
        ptDSCAdjacentDiameterNodeType   INTEGER,
        ptDSCAdjacentDiameterNodeAdjacentDiameterID           DisplayString,
        ptDSCAdjacentDiameterNodeStatus INTEGER,
        ptDSCAdjacentDiameterNodeRealm  DisplayString,
        ptDSCAdjacentDiameterNodeTwTimer INTEGER,
        ptDSCAdjacentDiameterNodeStrictApplicationIDMatching                    INTEGER,
        ptDSCAdjacentDiameterNodeInternalID   Integer32,
        ptDSCAdjacentDiameterNodeTcTimer Integer32,
        ptDSCAdjacentDiameterNodeIMFSupport   INTEGER,
        ptDSCAdjacentDiameterNodeRemoteIPValidation           INTEGER,
        ptDSCAdjacentDiameterNodeDIDHost DisplayString,
        ptDSCAdjacentDiameterNodeName   DisplayString,
        ptDSCAdjacentDiameterNodeDPRDisconnectCause           INTEGER,
        ptDSCAdjacentDiameterNodeDiameterProtocolParameterSet                     DisplayString,
        ptDSCAdjacentDiameterNodeOutgoingAnswerRoutingTable                   DisplayString,
        ptDSCAdjacentDiameterNodeMaximumAcceptedAdvertisedCapabilities                              INTEGER,
        ptDSCAdjacentDiameterNodeOutgoingRequestRoutingTable                    DisplayString,
        ptDSCAdjacentDiameterNodeDSCIndex Integer32,
        ptDSCAdjacentDiameterNodeDSCNodesIndex      Integer32,
        ptDSCAdjacentDiameterNodeDSCNodeIndex     Integer32,
        ptDSCAdjacentDiameterNodeAdjacentDiameterNodesIndex                   Integer32,
        ptDSCAdjacentDiameterNodeActions INTEGER,
        ptDSCAdjacentDiameterNodeParentOID  OBJECT IDENTIFIER,
        ptDSCAdjacentDiameterNodeRowStatus  RowStatus,
        ptDSCAdjacentDiameterNodeIndex  Integer32
    }

ptDSCAdjacentDiameterNodeType OBJECT-TYPE
    SYNTAX          INTEGER { dscInternal(0), ua(1), diameter(2), 
                        uaXml(3), dscDb(4) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Adjacent Diameter Node Type (DSC INTERNAL, UA, DIAMETER, UA XML, DSC DB). \nThe type is required to specify traffic restriction to the ADN. \nDIAMETER type is used for standard DSC Node connectivity. \nUA type is used for Sonus applications accepting PT enterprise AVPs such as the Sonus IWF feature. \nUA XML type is used for applications using the Sonus DSC XML protocol which may be configured to accept PT enterprise AVPs. \nDSC DB type is reserved for Diameter databases such as the Sonus DRA feature. \nDSC INTERNAL type is reserved for inter-process communication."
    ::= { ptDSCAdjacentDiameterNodeEntry 1 }

ptDSCAdjacentDiameterNodeAdjacentDiameterID OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Adjacent Diameter Node ID. \nThe attribute is optional when creating an Adjacent Diameter Node with a DID Host. \nIf not specified, the value will be set to a concatenation of the DID Host and Realm. \nNote: This is the value set in the Origin-Host/Destination-Host AVPs. \nValid values: FQDN format or empty string."
    ::= { ptDSCAdjacentDiameterNodeEntry 2 }

ptDSCAdjacentDiameterNodeStatus OBJECT-TYPE
    SYNTAX          INTEGER { down(0), up(1), inactive(2), 
                        partiallyUp(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of the Adjacent Diameter Node (UP/DOWN/INACTIVE). \nADN status is set to UP when at least one ADN connection is UP."
    ::= { ptDSCAdjacentDiameterNodeEntry 3 }

ptDSCAdjacentDiameterNodeRealm OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Adjacent Diameter Node associated realm. \nIf a FQDN is specified, a Realm Definition will be automatically created if none exists. If a Realm Definition is already defined, the FQDN value will be replaced by the associated alias. \nValid values: FQDN format or existing Realm Definition alias."
    ::= { ptDSCAdjacentDiameterNodeEntry 6 }

ptDSCAdjacentDiameterNodeTwTimer OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Device Watchdog Request timer initial value (DISABLED (0), 6000 to 120000 ms). \Initial timer value as per RFC3539. The value is adjusted internally higher or lower by a small random amount. \nSet to 0 to disable. If disabled, received DWR are still answered. \nDefault value: 30000 ms."
    ::= { ptDSCAdjacentDiameterNodeEntry 7 }

ptDSCAdjacentDiameterNodeStrictApplicationIDMatching OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Validate application ID of incoming request messages (DISABLED/ENABLED). \nIf enabled, only send requests to peers for whom the application ID is authorized. \nIf the validation fails, an answer message is returned with error UNABLE_TO_DELIVER. \nDefault value: ENABLED."
    ::= { ptDSCAdjacentDiameterNodeEntry 8 }

ptDSCAdjacentDiameterNodeInternalID OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Adjacent Diameter Node unique internal ID. \nReserved for internal use."
    ::= { ptDSCAdjacentDiameterNodeEntry 9 }

ptDSCAdjacentDiameterNodeTcTimer OBJECT-TYPE
    SYNTAX          Integer32 (1000..60000)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Reconnect timer initial value (1000 to 60000 ms). \Initial timer value as per RFC6733 Section 2.1. The value is adjusted internally higher or lower by a small random amount. \nDefault value: 30000 ms."
    ::= { ptDSCAdjacentDiameterNodeEntry 10 }

ptDSCAdjacentDiameterNodeIMFSupport OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Integrated Monitoring Feed feature support for DSC (DISABLED/ENABLED). \nIMF must be licensed to enable this attribute. \nDefault value: DISABLED."
    ::= { ptDSCAdjacentDiameterNodeEntry 11 }

ptDSCAdjacentDiameterNodeRemoteIPValidation OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Validate remote IP addresses of incoming connections (DISABLED/ENABLED). \nIf enabled, the resolved values of the Remote Host of the ADN Connection must \nmatch the source address(es) of received TCP or SCTP packets. If that fails \nthen an answer message is returned with error DIAMETER_UNKNOWN_PEER. \nDefault value: ENABLED."
    ::= { ptDSCAdjacentDiameterNodeEntry 12 }

ptDSCAdjacentDiameterNodeDIDHost OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "DSC Node Host within the realm. \nThis attribute is optional when creating an Adjacent Diameter Node with a Adjacent Diameter Node ID. \nIf specified, the value will be prepended to the Realm to create the Adjacent Node Diameter ID. \nValid values: FQDN format or empty string."
    ::= { ptDSCAdjacentDiameterNodeEntry 13 }

ptDSCAdjacentDiameterNodeName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Name used to logically identify the Adjacent Diameter Node. \nThe name must be unique within the DSC Instance. \nIt is recommended that the name be set to a short and unique value to facilitate logs and alarms correlation. \nIf no Name is entered on creation, or if the Name is modified to an empty string, the Adjacent Diameter ID will be used. \nValid values: alphanumeric characters, dashes, and underscores (maximum size of 32). The name is allowed to contain dots (.) and exceed maximum size only when it matches the Adjacent Diameter ID."
    ::= { ptDSCAdjacentDiameterNodeEntry 14 }

ptDSCAdjacentDiameterNodeDPRDisconnectCause OBJECT-TYPE
    SYNTAX          INTEGER { rebooting(0), busy(1), 
                        doNotWantToTalkToYou(2) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Disconnect-Cause AVP value sent within the Disconnect-Peer-Request (as per [RFC6733]). \nThe RFC specifies the following values: REBOOTING (0), BUSY (1), DO NOT WANT TO TALK TO YOU (2) \nDefault value is: BUSY (1) \nValid values: Integer in the range 0-32"
    ::= { ptDSCAdjacentDiameterNodeEntry 15 }

ptDSCAdjacentDiameterNodeDiameterProtocolParameterSet OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Diameter Protocol Parameter Set associated with this ADN. \nValid values: Existing Diameter Protocol Parameter Set. \nDefault value: default."
    ::= { ptDSCAdjacentDiameterNodeEntry 16 }

ptDSCAdjacentDiameterNodeOutgoingAnswerRoutingTable OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Associated optional Routing Table to be applied to the answer message. \nRFC6733 states that a relay or a proxy agent MUST NOT modify the contents of an answer Result-Code AVP indicating a failure. \nIt is recommended to not change this attribute from the default value of dsc_default. \nValid values: dsc_default and an existing Routing Table name. \nDefault value: dsc_default."
    ::= { ptDSCAdjacentDiameterNodeEntry 17 }

ptDSCAdjacentDiameterNodeMaximumAcceptedAdvertisedCapabilities OBJECT-TYPE
    SYNTAX          INTEGER { unlimited(-1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Maximum number of Accepted Advertised Capabilities associated with this ADN. \nValid values: (200 to UNLIMITED). \nDefault value: 200"
    ::= { ptDSCAdjacentDiameterNodeEntry 18 }

