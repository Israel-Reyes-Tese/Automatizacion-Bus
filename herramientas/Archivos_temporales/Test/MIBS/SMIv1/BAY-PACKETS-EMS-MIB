    SYNTAX          OCTET STRING
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Version Id of the software installed on this Subsystem"
    ::= { rsiSubsystemEntry 2 }

rsiSubsystemInstallationDateTime OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Date of installation of the current sofware for this 
             Subsystem"
    ::= { rsiSubsystemEntry 3 }

rsiSubsystemActiveSince OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Date since which this Subsystem is active"
    ::= { rsiSubsystemEntry 4 }

rsiSubsystemRole OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "whether this Subsystem is active/standby"
    ::= { rsiSubsystemEntry 5 }

rsiUserSecurityPolicyTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiUserSecurityPolicyEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table stores the information about the 
             security policies available for a user "
    ::= { rsiCommonMib 9 }

rsiUserSecurityPolicyEntry OBJECT-TYPE
    SYNTAX          RsiUserSecurityPolicyEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of a User Security Policy Entry"
    INDEX           { rsiUserSecurityPolicyName }
    ::= { rsiUserSecurityPolicyTable 1 }

RsiUserSecurityPolicyEntry ::=
    SEQUENCE {
        rsiUserSecurityPolicyName       OCTET STRING,
        rsiUserSecurityPolicyId         INTEGER,
        rsiUserSecurityPasswdExpiryDays INTEGER,
        rsiUserSecurityMaxFailedAttempts INTEGER,
        rsiUserSecurityMinPasswordLen   INTEGER,
        rsiUserSecurityPasswdRecylingPolicy   INTEGER,
        rsiUserSecurityPasswdRecylingMinDaysToUse         INTEGER,
        rsiUserSecurityPasswdRecylingNumOfPasswds         INTEGER,
        rsiUserSecurityMinNumberOfNumericCharInPasswd             INTEGER,
        rsiUserSecurityMinNumberOfSpecialCharInPasswd             INTEGER,
        rsiNumberOfLoginsAllowed        INTEGER,
        rsiUserSecurityPolicyRowStatus  RowStatus
    }

rsiUserSecurityPolicyName OBJECT-TYPE
    SYNTAX          OCTET STRING
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is the human readable name for this policy"
    ::= { rsiUserSecurityPolicyEntry 1 }

rsiUserSecurityPolicyId OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is the unique id associated with this policy"
    ::= { rsiUserSecurityPolicyEntry 2 }

rsiUserSecurityPasswdExpiryDays OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Days after which the currentpassword would expire.
             Default Value - 30 days."
    ::= { rsiUserSecurityPolicyEntry 3 }

rsiUserSecurityMaxFailedAttempts OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Maximum no. of failed login attempts at one go, 
             after which the user account will be locked. 
             Default Value - 3 attempts."
    ::= { rsiUserSecurityPolicyEntry 4 }

rsiUserSecurityMinPasswordLen OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Maximum no. of failed login attempts at one go, 
             after which the user account will be locked. 
             Default Value - 6 characters."
    ::= { rsiUserSecurityPolicyEntry 5 }

rsiUserSecurityPasswdRecylingPolicy OBJECT-TYPE
    SYNTAX          INTEGER { timeBased(0), numberBased(1), 
                        timeAndNumberBased(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Old passwords of a user are saved, and based 
             on the recycling policy, the old password can be 
             reused. 
             Time Based Password Recycling: no. of days after 
             which the new password can have the same value as 
             the old password which had been previously modified. 
             Number Based Password Recycling: no. of passwords 
             that have have been changed, after which an 
             old password can be reused. 
             Default Value - timeAndNumberBased"
    ::= { rsiUserSecurityPolicyEntry 6 }

rsiUserSecurityPasswdRecylingMinDaysToUse OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "If the Password Recycling Policy is Time Based, 
             then the number of days after which the old password 
             can be reused. 
             Default Value - 0."
    ::= { rsiUserSecurityPolicyEntry 7 }

rsiUserSecurityPasswdRecylingNumOfPasswds OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "If the Password Recycling Policy is Number Based, 
             then the number of password changes after which the 
             old password can be reused. 
             Default Value - 0"
    ::= { rsiUserSecurityPolicyEntry 8 }

rsiUserSecurityMinNumberOfNumericCharInPasswd OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This defines the minimum number of numeric characters
             that need to be present in a password.
             Default Value - 0"
    ::= { rsiUserSecurityPolicyEntry 9 }

rsiUserSecurityMinNumberOfSpecialCharInPasswd OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This defines the minimum number of special characters
             that need to be present in a password.
             Default Value - 0"
    ::= { rsiUserSecurityPolicyEntry 10 }

rsiNumberOfLoginsAllowed OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This defines the maximum number of logins allowed simultaneousl"
    ::= { rsiUserSecurityPolicyEntry 11 }

rsiUserSecurityPolicyRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is for dynamic addition/deletion of policies.
             For creating a new policy, only 'createAndGo' is
             supported. For deleting an existing row, 'delete'
             is supported"
    ::= { rsiUserSecurityPolicyEntry 12 }

