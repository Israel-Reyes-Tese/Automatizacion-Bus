ptTrapEvent OBJECT IDENTIFIER
    ::= { ptTrapLog 313 }

ptTrapEventTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtTrapEventEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptTrapEvent 1 }

ptTrapEventEntry OBJECT-TYPE
    SYNTAX          PtTrapEventEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of TrapEvent attributes."
    INDEX           { ptConnGroupIndex, ptTrapEventConfigurationIndex, 
                      ptTrapEventTrapLogIndex, ptTrapEventIndex }
    ::= { ptTrapEventTable 1 }

PtTrapEventEntry ::=
    SEQUENCE {
        ptTrapEventTrapName             DisplayString,
        ptTrapEventSequenceNumber       Integer32,
        ptTrapEventDateAndTime          DisplayString,
        ptTrapEventTrapType             INTEGER,
        ptTrapEventSeverity             INTEGER,
        ptTrapEventProblemCause         INTEGER,
        ptTrapEventClearType            INTEGER,
        ptTrapEventItemType             DisplayString,
        ptTrapEventVariableName1        DisplayString,
        ptTrapEventVariableValue1       DisplayString,
        ptTrapEventVariableName2        DisplayString,
        ptTrapEventVariableValue2       DisplayString,
        ptTrapEventVariableName3        DisplayString,
        ptTrapEventVariableValue3       DisplayString,
        ptTrapEventVariableName4        DisplayString,
        ptTrapEventVariableValue4       DisplayString,
        ptTrapEventVariableName5        DisplayString,
        ptTrapEventVariableValue5       DisplayString,
        ptTrapEventVariableName6        DisplayString,
        ptTrapEventVariableValue6       DisplayString,
        ptTrapEventVariableName7        DisplayString,
        ptTrapEventVariableValue7       DisplayString,
        ptTrapEventVariableName8        DisplayString,
        ptTrapEventVariableValue8       DisplayString,
        ptTrapEventVariableName9        DisplayString,
        ptTrapEventVariableValue9       DisplayString,
        ptTrapEventVariableName10       DisplayString,
        ptTrapEventVariableValue10      DisplayString,
        ptTrapEventItemOID              DisplayString,
        ptTrapEventConfigurationIndex   Integer32,
        ptTrapEventTrapLogIndex         Integer32,
        ptTrapEventParentOID            OBJECT IDENTIFIER,
        ptTrapEventRowStatus            RowStatus,
        ptTrapEventIndex                Integer32
    }

ptTrapEventTrapName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The name of the trap issued. For more information concerning possible traps names, please look at the provided mib files."
    ::= { ptTrapEventEntry 1 }

ptTrapEventSequenceNumber OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The sequence number of the trap\n This number increased by one for each trap sent by the system."
    ::= { ptTrapEventEntry 2 }

ptTrapEventDateAndTime OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Date and time value of the occurrence of the trap."
    ::= { ptTrapEventEntry 3 }

ptTrapEventTrapType OBJECT-TYPE
    SYNTAX          INTEGER { undefined(0), communicationAlarm(2), 
                        environmentAlarm(3), equipmentAlarm(4), 
                        processingAlarm(10), qualityOfServiceAlarm(11) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type of the trap, follows Ref. X.733 : \n\nCommunication Alarm: An alarm of this type is principally associated with the procedures and/or processes required to convey information from one point to another. \n\nQuality of Service Alarm: An alarm of this type is principally associated with a degradation in the quality of a service.\n\nProcessing Error Alarm: An alarm of this type is principally associated with a software or processing fault.\n\nEquipment Alarm: An alarm of this type is principally associated with equipment fault.\n\nEnvironmental Alarm: An alarm of this type is principally associated with a condition relating to an enclosure in which the equipment resides."
    ::= { ptTrapEventEntry 4 }

ptTrapEventSeverity OBJECT-TYPE
    SYNTAX          INTEGER { indetermined(0), critical(1), major(2), 
                        minor(3), warning(4), cleared(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Severity is used in the reporting of traps. When a trap is issued it will have an associated severity.\n\n Possible Severity are: indeterminate, critical, major, minor, warning, cleared."
    ::= { ptTrapEventEntry 5 }

ptTrapEventProblemCause OBJECT-TYPE
    SYNTAX          INTEGER { undefined(0), 
                        applicationSubsystemFailure(2), 
                        callEstablishmentError(4), 
                        communicationsProtocolError(5), 
                        communicationsSubsystemError(6), 
                        configurationOrCustomizationError(7), 
                        congestion(8), corruptData(9), 
                        cpuCyclesLimitExceeded(10), 
                        datasetOrModemError(11), degradedSignal(12), 
                        dteDceInterfaceError(13), enclosureDoorOpen(14), 
                        equipmentMalfunction(15), fileError(17), 
                        fireDetected(18), floodDetected(19), 
                        framingError(20), inputOutputDeviceError(23), 
                        inputDeviceError(24), lanError(25), 
                        localNodeTransmissionError(27), lossOfFrame(28), 
                        lossOfSignal(29), multiplexerProblem(31), 
                        outOfMemory(32), outputDeviceError(33), 
                        performanceDegraded(34), powerProblem(35), 
                        processorProblem(37), queueSizeExceeded(39), 
                        receiveFailure(40), 
                        remoteNodeTransmissionError(42), 
                        resourceAtOrNearCapacity(43), 
                        responseTimeExcessive(44), 
                        retransmissionRateExcessive(45), 
                        softwareError(46), 
                        softwareProgramAbnormallyTerminated(47), 
                        softwareProgramError(48), 
                        storageCapacityProblem(49), thresholdCrossed(51), 
                        timingProblem(52), transmitFailure(54), 
                        underlyingResourceUnavailable(56), 
                        versionMismatch(57) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The probable cause of the trap, follows Ref. X.733. After we identify the GSN alarms, we will categorize them according to the five types above, find their probable causes, and map the causes, as close as we can, to the X.733 list."
    ::= { ptTrapEventEntry 6 }

ptTrapEventClearType OBJECT-TYPE
    SYNTAX          INTEGER { fmic(1), oic(2), undefined(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The Clear type of the trap, which is either FMIC or OIC, where FMIC means that the trap will be cleared by another clear trap, and OIC means that the trap needs to be cleared by the operator."
    ::= { ptTrapEventEntry 7 }

ptTrapEventItemType OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This attribute gives the name of the item type from which the variable names and values are taken.\n\n The variable names and values should clarify which item (if there are many of the same type) issued the trap and may give more information as to why the trap has been issued."
    ::= { ptTrapEventEntry 8 }

ptTrapEventVariableName1 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This attribute gives the name of a variable that was inserted in the trap. Associated with its variable value, this gives either a way to know which item issued the trap or may give more information as to why the trap has been issued.\n\n A maximum of 10 variables might have been included in a trap."
    ::= { ptTrapEventEntry 9 }

ptTrapEventVariableValue1 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
