        sonusSipTranspProfileHdrMethodRegister      INTEGER,
        sonusSipTranspProfileHdrMethodMessage     INTEGER,
        sonusSipTranspProfileHdrMethodUpdate    INTEGER,
        sonusSipTranspProfileHdrMethodPublish     INTEGER,
        sonusSipTranspProfileHdrMethodBye INTEGER
    }

