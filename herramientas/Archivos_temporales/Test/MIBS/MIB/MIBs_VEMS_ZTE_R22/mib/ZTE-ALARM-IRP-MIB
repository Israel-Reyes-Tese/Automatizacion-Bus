                    alarmSystemType,
                    alarmNeIP,
                    alarmAck,
                    cleiCode,
                    timeZoneID,
                    timeZoneOffset,
                    dSTSaving,
                    aid,
                    id,
                    alarmMocObjectInstance,
                    alarmComment,
                    alarmOtherInfo
                    }
        STATUS      current
        DESCRIPTION 
                "This notification is sent when alarm's status was changed,acknowledged or unacknowledged."
        ::= { alarmNotificationProfile 3 }

