ptpeereventProcessEvent OBJECT IDENTIFIER
    ::= { ptpeereventProcessEvents 1132 }

ptpeereventProcessEventTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtpeereventProcessEventEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptpeereventProcessEvent 1 }

ptpeereventProcessEventEntry OBJECT-TYPE
    SYNTAX          PtpeereventProcessEventEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of peereventProcessEvent attributes."
    INDEX           { ptConnGroupIndex, 
                      ptpeereventProcessEventProcessEventsIndex, 
                      ptpeereventProcessEventIndex }
    ::= { ptpeereventProcessEventTable 1 }

PtpeereventProcessEventEntry ::=
    SEQUENCE {
        ptpeereventProcessEventSlot     Integer32,
        ptpeereventProcessEventInitialization     INTEGER,
        ptpeereventProcessEventTrafficStatus    INTEGER,
        ptpeereventProcessEventFileWrite INTEGER,
        ptpeereventProcessEventSlotManager  INTEGER,
        ptpeereventProcessEventStatsManager   INTEGER,
        ptpeereventProcessEventIMFManager INTEGER,
        ptpeereventProcessEventLowerLayer INTEGER,
        ptpeereventProcessEventName     DisplayString,
        ptpeereventProcessEventProcessEventsIndex         Integer32,
        ptpeereventProcessEventParentOID OBJECT IDENTIFIER,
        ptpeereventProcessEventRowStatus RowStatus,
        ptpeereventProcessEventIndex    Integer32
    }

ptpeereventProcessEventSlot OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Process corresponding slot. \nIn certain deployments where an application has more than one process on a given slot, the name attribute must be used to properly identify the process. \nIn PCE, the slot represents the LDL peer ID for cross-mate STP support."
    ::= { ptpeereventProcessEventEntry 1 }

ptpeereventProcessEventInitialization OBJECT-TYPE
    SYNTAX          INTEGER { notApplicable(0), inProgress(1), 
                        completed(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Process initialization status (NOT APPLICABLE/IN PROGRESS/COMPLETED)."
    ::= { ptpeereventProcessEventEntry 2 }

ptpeereventProcessEventTrafficStatus OBJECT-TYPE
    SYNTAX          INTEGER { down(0), notReady(1), ready(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Process traffic status (DOWN/NOT READY/READY). \nAn event is generated when the traffic status is set to NOT READY. The event is cleared when the status leaves the NOT READY state."
    ::= { ptpeereventProcessEventEntry 3 }

ptpeereventProcessEventFileWrite OBJECT-TYPE
    SYNTAX          INTEGER { notApplicable(0), failure(1), success(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Process file write status (FAILURE/SUCCESS). \nAn event is generated when the file write is set to FAILURE. The event is cleared on a reset to SUCCESS."
    ::= { ptpeereventProcessEventEntry 4 }

ptpeereventProcessEventSlotManager OBJECT-TYPE
    SYNTAX          INTEGER { notApplicable(0), notConnected(1), 
                        connected(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Process slot manager connection (NOT APPLICABLE/NOT CONNECTED/CONNECTED). \nAn event is generated when the connection status is set to NOT CONNECTED. The event is cleared on a set to CONNECTED."
    ::= { ptpeereventProcessEventEntry 5 }

ptpeereventProcessEventStatsManager OBJECT-TYPE
    SYNTAX          INTEGER { notApplicable(0), notConnected(1), 
                        connected(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Process stats manager connection (NOT APPLICABLE/NOT CONNECTED/CONNECTED). \nAn event is generated when the connection status is set to NOT CONNECTED. The event is cleared on a set to CONNECTED."
    ::= { ptpeereventProcessEventEntry 6 }

ptpeereventProcessEventIMFManager OBJECT-TYPE
    SYNTAX          INTEGER { notApplicable(0), notConnected(1), 
                        connected(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Process IMF manager connection (NOT APPLICABLE/NOT CONNECTED/CONNECTED). \nAn event is generated when the connection status is set to NOT CONNECTED. The event is cleared on a set to CONNECTED."
    ::= { ptpeereventProcessEventEntry 7 }

ptpeereventProcessEventLowerLayer OBJECT-TYPE
    SYNTAX          INTEGER { notApplicable(0), notConnected(1), 
                        partiallyConnected(2), connected(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Process lower layer connections (NOT APPLICABLE/NOT CONNECTED/PARTIALLY CONNECTED/CONNECTED). \nA major event is generated when the layer connection status is set to PARTIALLY CONNECTED and a critical event is generated when the layer connection status is set to NOT CONNECTED. The critical event is cleared, if defined, on a set to PARTIALLY CONNECTED and the major event is cleared on a set to CONNECTED."
    ::= { ptpeereventProcessEventEntry 8 }

ptpeereventProcessEventName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Process corresponding name."
    ::= { ptpeereventProcessEventEntry 9 }

