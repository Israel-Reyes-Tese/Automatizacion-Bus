sonusLightGrayPolicerDiscardRingIpProtocol OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The IP Protocol of the table entry."
    ::= { sonusLightGrayPolicerDiscardRingEntry 6 }

sonusLightGrayPolicerDiscardRingDiscardCount OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The number of packets discarded by the policer."
    ::= { sonusLightGrayPolicerDiscardRingEntry 7 }

sonusLightGrayPolicerDiscardRingIp6Address OBJECT-TYPE
    SYNTAX          Ipv6Address
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The Stat IPv6 address"
    ::= { sonusLightGrayPolicerDiscardRingEntry 8 }

sonusLightGrayPolicerDiscardRingIpVer OBJECT-TYPE
    SYNTAX          INTEGER { afInet(2), afInet6(24) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IP address version of the peer of this connection"
    ::= { sonusLightGrayPolicerDiscardRingEntry 9 }

sonusVLanPolicerDiscardRingTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusVLanPolicerDiscardRingEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The table of statistics for the white list
             policer discard ring."
    ::= { sonusIpmMIBObjects 15 }

sonusVLanPolicerDiscardRingEntry OBJECT-TYPE
    SYNTAX          SonusVLanPolicerDiscardRingEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The white list discard ring entries."
    INDEX           { sonusVLanPolicerDiscardRingShelfIndex, 
                      sonusVLanPolicerDiscardRingSlotIndex, 
                      sonusVLanPolicerDiscardRingPositionIndex }
    ::= { sonusVLanPolicerDiscardRingTable 1 }

SonusVLanPolicerDiscardRingEntry ::=
    SEQUENCE {
        sonusVLanPolicerDiscardRingShelfIndex     SonusShelfIndex,
        sonusVLanPolicerDiscardRingSlotIndex    SonusSlotIndex,
        sonusVLanPolicerDiscardRingPositionIndex        Gauge,
        sonusVLanPolicerDiscardRingNIFSIFName     SonusNameReference,
        sonusVLanPolicerDiscardRingIpAddress    IpAddress,
        sonusVLanPolicerDiscardRingIpProtocol     Gauge,
        sonusVLanPolicerDiscardRingDiscardCount       Counter,
        sonusVLanPolicerDiscardRingIp6Address     Ipv6Address,
        sonusVLanPolicerDiscardRingIpVer INTEGER
    }

sonusVLanPolicerDiscardRingShelfIndex OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The shelf index into this table."
    ::= { sonusVLanPolicerDiscardRingEntry 1 }

sonusVLanPolicerDiscardRingSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The slot index into this table."
    ::= { sonusVLanPolicerDiscardRingEntry 2 }

sonusVLanPolicerDiscardRingPositionIndex OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The position index into this table ordered by
             worst offender with 1 being the worst."
    ::= { sonusVLanPolicerDiscardRingEntry 3 }

sonusVLanPolicerDiscardRingNIFSIFName OBJECT-TYPE
    SYNTAX          SonusNameReference
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The name of the NIF or sub-interface of 
             table entry."
    ::= { sonusVLanPolicerDiscardRingEntry 4 }

sonusVLanPolicerDiscardRingIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The IPv4 address of the table entry."
    ::= { sonusVLanPolicerDiscardRingEntry 5 }

sonusVLanPolicerDiscardRingIpProtocol OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The IP Protocol of the table entry."
    ::= { sonusVLanPolicerDiscardRingEntry 6 }

sonusVLanPolicerDiscardRingDiscardCount OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The number of packets discarded by the policer."
    ::= { sonusVLanPolicerDiscardRingEntry 7 }

sonusVLanPolicerDiscardRingIp6Address OBJECT-TYPE
    SYNTAX          Ipv6Address
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The Stat IPv6 address"
    ::= { sonusVLanPolicerDiscardRingEntry 8 }

sonusVLanPolicerDiscardRingIpVer OBJECT-TYPE
    SYNTAX          INTEGER { afInet(2), afInet6(24) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IP address version of the peer of this connection"
    ::= { sonusVLanPolicerDiscardRingEntry 9 }

sonusIpmMIBNotifications OBJECT IDENTIFIER
    ::= { sonusIpmMIB 2 }

sonusIpmMIBNotificationsPrefix OBJECT IDENTIFIER
    ::= { sonusIpmMIBNotifications 0 }

sonusIpmMIBNotificationsObjects OBJECT IDENTIFIER
    ::= { sonusIpmMIBNotifications 1 }

sonusPolicerTypeStr OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..32))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The policer type whose alarm state has changed."
    ::= { sonusIpmMIBNotificationsObjects 1 }

sonusPolicerLevelStr OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..8))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The previous policer alarm level reported when the alarm 
             state has changed."
    ::= { sonusIpmMIBNotificationsObjects 2 }

sonusNodePolicerMinorAlarmNotification TRAP-TYPE
    ENTERPRISE      sonusIpmMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusPolicerTypeStr, 
                      sonusPolicerLevelStr, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "The sonusNodePolicerMinorAlarmNotification indicates
             a system policer minor level alarm."
    ::= 1

sonusNodePolicerMajorAlarmNotification TRAP-TYPE
    ENTERPRISE      sonusIpmMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusPolicerTypeStr, 
                      sonusPolicerLevelStr, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "The sonusNodePolicerMajorAlarmNotification indicates
             a system policer major level alarm."
    ::= 2

sonusNodePolicerClearAlarmNotification TRAP-TYPE
    ENTERPRISE      sonusIpmMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusPolicerTypeStr, 
                      sonusPolicerLevelStr, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "The sonusNodePolicerClearAlarmNotification indicates
             a system policer alarm clear."
    ::= 3

sonusServerPolicerMinorAlarmNotification TRAP-TYPE
    ENTERPRISE      sonusIpmMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, 
                      sonusPolicerTypeStr, sonusPolicerLevelStr, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
