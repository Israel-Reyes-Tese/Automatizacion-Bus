ptSlot OBJECT IDENTIFIER
    ::= { ptSlotMonitor 602 }

ptSlotTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtSlotEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptSlot 1 }

ptSlotEntry OBJECT-TYPE
    SYNTAX          PtSlotEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of Slot attributes."
    INDEX           { ptConnGroupIndex, ptSlotSlotMonitorIndex, 
                      ptSlotIndex }
    ::= { ptSlotTable 1 }

PtSlotEntry ::=
    SEQUENCE {
        ptSlotSlotNumber                INTEGER,
        ptSlotSlotType                  INTEGER,
        ptSlotSlotStatus                INTEGER,
        ptSlotPrimaryIP                 DisplayString,
        ptSlotSecondaryIP               DisplayString,
        ptSlotPrimaryIPState            INTEGER,
        ptSlotSecondaryIPState          INTEGER,
        ptSlotMonitorState              INTEGER,
        ptSlotCPUMemoryUsage            INTEGER,
        ptSlotBackupSpaceUsage          INTEGER,
        ptSlotCPULoadUsage              DisplayString,
        ptSlotOMsSpaceUsage             INTEGER,
        ptSlotLastFailedSFTPBackupDestIP DisplayString,
        ptSlotLastFailedSFTPBackupDestUserID    DisplayString,
        ptSlotLastFailedSFTPBackupDestDir DisplayString,
        ptSlotSlotMonitorIndex          INTEGER,
        ptSlotParentOID                 OBJECT IDENTIFIER,
        ptSlotRowStatus                 RowStatus,
        ptSlotIndex                     INTEGER
    }

ptSlotSlotNumber OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Physical slot location of the board in the chassis."
    ::= { ptSlotEntry 1 }

ptSlotSlotType OBJECT-TYPE
    SYNTAX          INTEGER { unknownType(0), rtngCpu(1), mgmtCpu(2), 
                        rtngMgmtCpu(3), ss7Io(4), hardrive(5), switch(6), 
                        ismA(7), ismB(8), applCpu(9), custCpu(10), 
                        vacant(11) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Type of board in the slot number in question. CPU, IO, or ISM."
    ::= { ptSlotEntry 2 }

ptSlotSlotStatus OBJECT-TYPE
    SYNTAX          INTEGER { unknownState(0), extracted(1), 
                        available(2), pendingExtractionRequest(3), 
                        readyToBeExtracted(4), lostConnection(5) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Current status for this slot, as determined by the slot monitoring application. Can be one of the following states; EXTRACTED: slot has been properly extracted. AVAILABLE: slot is communicating normally with the Slot Monitor. UNKNOWN_STATE: slot is not present. PENDING_EXTRACTION_REQUEST: Slot Monitor has sent an extraction request to this slot. READY_TO_BE_EXTRACTED: slot is ready for extraction. LOST_CONNECTION: Slot Monitor has lost connection to this slot."
    ::= { ptSlotEntry 3 }

ptSlotPrimaryIP OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Primary IP address of the slot"
    ::= { ptSlotEntry 4 }

ptSlotSecondaryIP OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Secondary IP address of the slot"
    ::= { ptSlotEntry 5 }

ptSlotPrimaryIPState OBJECT-TYPE
    SYNTAX          INTEGER { noConnection(0), responding(1), notUsed(2) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "State of the primary IP address of the slot"
    ::= { ptSlotEntry 6 }

