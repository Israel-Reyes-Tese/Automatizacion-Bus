    SEQUENCE {
        pthwmonThresholdSensorName      DisplayString,
        pthwmonThresholdSensorSlot      Integer32,
        pthwmonThresholdSensorPortalID  Integer32,
        pthwmonThresholdSensorIPMINumber Integer32,
        pthwmonThresholdSensorUnits     DisplayString,
        pthwmonThresholdSensorAlarmState INTEGER,
        pthwmonThresholdSensorCurrentReading    DisplayString,
        pthwmonThresholdSensorMaximumReading    DisplayString,
        pthwmonThresholdSensorMinimumReading    DisplayString,
        pthwmonThresholdSensorUpperCriticalThresh         DisplayString,
        pthwmonThresholdSensorUpperMajorThresh      DisplayString,
        pthwmonThresholdSensorUpperMinorThresh      DisplayString,
        pthwmonThresholdSensorLowerMinorThresh      DisplayString,
        pthwmonThresholdSensorLowerMajorThresh      DisplayString,
        pthwmonThresholdSensorLowerCriticalThresh         DisplayString,
        pthwmonThresholdSensorHardwareMonitorIndex          Integer32,
        pthwmonThresholdSensorSlotIndex Integer32,
        pthwmonThresholdSensorActions   INTEGER,
        pthwmonThresholdSensorParentOID OBJECT IDENTIFIER,
        pthwmonThresholdSensorRowStatus RowStatus,
        pthwmonThresholdSensorIndex     Integer32
    }

