				xfCesPwTDMPerfCurrentUAS, xfCesPwTDMPerfCurrentFC, xfCesPwPerfCurrentOutHCBytes, xfCesPwPmView, xfCesPwPmMalformedFramesThreshold, 
				xfCesPwPmJitterBufferOverrunThreshold, xfCesPwPerfIntervalNumber, xfCesPwPerfIntervalValidData, xfCesPwPerfIntervalTimeElapsed, xfCesPwPerfIntervalInHCPackets, 
				xfCesPwPerfIntervalInHCBytes, xfCesPwPerfIntervalOutHCPackets, xfCesPwPerfIntervalOutHCBytes, xfCesPwTDMPerfIntervalMissingPkts, xfCesPwTDMPerfIntervalPktReorders, 
				xfCesPwTDMPerfIntervalJtrBufferUnderruns, xfCesPwTDMPerfIntervalMisOrderDrop, xfCesPwTDMPerfIntervalMalformedPkts, xfCesPwTDMPerfIntervalES, xfCesPwTDMPerfIntervalSES, 
				xfCesPwTDMPerfIntervalUAS, xfCesPwTDMPerfIntervalFC, xfCesPwPerfCurrentTimeElapsed, xfCesPwPerf1DayIntervalNumber, xfCesPwPerfCurrentValidData, 
				xfCesPwPerf1DayIntervalValidData, xfCesPwPerf1DayIntervalTimeElapsed, xfCesPwPerf1DayIntervalInHCPackets, xfCesPwPerf1DayIntervalInHCBytes, xfCesPwPerf1DayIntervalOutHCPackets, 
				xfCesPwPerf1DayIntervalOutHCBytes, xfCesPwTDMPerf1DayIntervalMissingPkts, xfCesPwTDMPerf1DayIntervalPktReorders, xfCesPwTDMPerf1DayIntervalJtrBufferUnderruns, xfCesPwTDMPerf1DayIntervalMisOrderDrop, 
