		xfDcnIPv6OSPFConfigEntry OBJECT-TYPE
			SYNTAX XfDcnIPv6OSPFConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table.
				
				ML66: This object is not supported."
			INDEX { xfDcnIPv6OSPFConfigIfIndex }
			::= { xfDcnIPv6OSPFConfigTable 1 }

		XfDcnIPv6OSPFConfigEntry ::=
			SEQUENCE {
				xfDcnIPv6OSPFConfigIfIndex
					InterfaceIndex,
				xfDcnIPv6OSPFConfigAreaId
					IpAddress,
				xfDcnIPv6OSPFConfigRowStatus
					RowStatus
			 }

		-- 1.3.6.1.4.1.193.81.2.8.11.3.1.1
		xfDcnIPv6OSPFConfigIfIndex OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This object indicates the interface index of the
				interface assigned to the xfDcnIPv6OSPFConfigAreaId.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6OSPFConfigEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.8.11.3.1.3
		xfDcnIPv6OSPFConfigAreaId OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Indicates the OSPFv3 area for this interface in dotted decimal format.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6OSPFConfigEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.8.11.3.1.4
		xfDcnIPv6OSPFConfigRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object is used to create, modify and destroy
				rows in this table.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6OSPFConfigEntry 4 }

		-- 1.3.6.1.4.1.193.81.2.8.11.4
		xfDcnIPv6OSPFAreaConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfDcnIPv6OSPFAreaConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table is used to add OSPFv3 area parameters.
				Used to indicate whether the area is a stub area
				or not. An ABR router can also use the no-summary
				parameter on a stub area.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6OSPF 4 }

		-- 1.3.6.1.4.1.193.81.2.8.11.4.1
		xfDcnIPv6OSPFAreaConfigEntry OBJECT-TYPE
			SYNTAX XfDcnIPv6OSPFAreaConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table.
				
				ML66: This object is not supported."
			INDEX { xfDcnIPv6OSPFAreaConfigId }
			::= { xfDcnIPv6OSPFAreaConfigTable 1 }

		XfDcnIPv6OSPFAreaConfigEntry ::=
			SEQUENCE {
				xfDcnIPv6OSPFAreaConfigId
					IpAddress,
				xfDcnIPv6OSPFAreaType
					INTEGER,
				xfDcnIPv6OSPFAreaConfigRowStatus
					RowStatus
			 }

		-- 1.3.6.1.4.1.193.81.2.8.11.4.1.1
		xfDcnIPv6OSPFAreaConfigId OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Indicates the OSPFv3 area identifier.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6OSPFAreaConfigEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.8.11.4.1.2
		xfDcnIPv6OSPFAreaType OBJECT-TYPE
			SYNTAX INTEGER
				{
				stubArea(1),
				totallyStubbyArea(2)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This objects indicates whether the OSPFv3 area is a
				stub area or not. Default is stubArea(1).
				
				ML66: This object is not supported."
			DEFVAL { stubArea }
			::= { xfDcnIPv6OSPFAreaConfigEntry 2 }

		-- 1.3.6.1.4.1.193.81.2.8.11.4.1.3
		xfDcnIPv6OSPFAreaConfigRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object is used to create, modify and destroy
				rows in this table.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6OSPFAreaConfigEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.8.11.5
		xfDcnIPv6OSPFRouterIDMode OBJECT-TYPE
			SYNTAX INTEGER
				{
				manual(1),
				automatic(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicated router ID mode:
				
				  manual - the user has explicitly provided a Router ID,
				  automatic - the user hasn't provided a Router ID, or revoked it, and the ID was generated automatically
				
				ML66: This object is not supported."
			::= { xfDcnIPv6OSPF 5 }

		-- 1.3.6.1.4.1.193.81.2.8.11.6
		xfDcnIPv6OSPFRouterID OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Defines the IPv6 Router ID. In order to trigger an automatic configuration set 0.0.0.0.
				
				ML66: This object is not supported."
			::= { xfDcnIPv6OSPF 6 }

		-- 1.3.6.1.4.1.193.81.2.8.12
		xfDcnIPv6Host OBJECT IDENTIFIER ::= { xfDcnMIB 12 }

		-- 1.3.6.1.4.1.193.81.2.8.12.1
		xfDcnIPv6DomainName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..253))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Indicates domain name of the XF NE set by the operator.
				
				ML66: Setting an empty string will set domain name to default."
			REFERENCE
				"RFC1035, section 2.3.1 Preferred name syntax."
			::= { xfDcnIPv6Host 1 }

		-- 1.3.6.1.4.1.193.81.2.8.12.3
		xfDcnIPv6HostAddress OBJECT-TYPE
			SYNTAX InetAddressIPv6
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Indicates Host IPv6 address of the XF NE.
				
				ML-TN/CN/LH:
				
				This object is the same as xfDcnIPv6LoopbackAddress.
				
				ML66:
				
				Dynamically linked to the address of the following interfaces:
				(in order of precedence)
				
				- DCN Loopback interface address, if it is set to a non-default value
				- DCN Ethernet interface address
				
				In DCN Mode routed(4), this object returns 0, as there is no IPv6 access in this mode.
				"
			::= { xfDcnIPv6Host 3 }

		-- 1.3.6.1.4.1.193.81.2.8.12.4
		xfDcnIPv6HostPrefixLength OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Indicates Host IPv6 prefix length of the XF NE.
				
				Only users with administrative rights can modify this object.
				
				ML-TN/CN/LH:
				
				This object is the same as xfDcnIPv6LoopbackPrefixLength.
				
				ML66:
				
				Dynamically linked to the prefix length of the following interfaces:
				(in order of precedence)
				
				- DCN Loopback interface prefix length, if its address is set to a non-default value
				- DCN Ethernet interface prefix length
				
				In DCN Mode routed(4), this object returns 0, as there is no IPv6 access in this mode.
				"
			::= { xfDcnIPv6Host 4 }

		-- 1.3.6.1.4.1.193.81.2.8.12.5
		xfDcnIPv6DefaultGateway OBJECT-TYPE
			SYNTAX InetAddressIPv6
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object specifies the default gateway for this node.
				
				ML66: This object is effective in DCN Mode vlan(3) only."
			DEFVAL { '00000000'h }
			::= { xfDcnIPv6Host 5 }

		-- 1.3.6.1.4.1.193.81.2.8.12.6
		xfDcnIPv6UsbAddress OBJECT-TYPE
			SYNTAX InetAddressIPv6
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"IPv6 address of the Local Access (USB) interface."
			::= { xfDcnIPv6Host 6 }

		-- 1.3.6.1.4.1.193.81.2.8.12.7
		xfDcnIPv6UsbPrefixLength OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"IPv6 prefix length of the Local Access (USB) interface."
			::= { xfDcnIPv6Host 7 }

		-- 1.3.6.1.4.1.193.81.2.8.12.8
		xfDcnIPv6LoopbackAddress OBJECT-TYPE
			SYNTAX InetAddressIPv6
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"IPv6 address of the DCN Loopback interface."
			::= { xfDcnIPv6Host 8 }

		-- 1.3.6.1.4.1.193.81.2.8.12.9
		xfDcnIPv6LoopbackPrefixLength OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"IPv6 prefix length of the DCN Loopback interface.
				
				Only users with administrative rights can modify this object.
				
				ML66: The only accepted value is 128."
			::= { xfDcnIPv6Host 9 }

		-- 1.3.6.1.4.1.193.81.2.8.12.10
		xfDcnIPv6HostDscp OBJECT-TYPE
			SYNTAX Integer32 (0..63)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"DSCP value for all IPv6 DCN services.
				
				Default value is 0 (best-effort forwarding).
				
				ML-TN/CN/LH: This object is not supported.
				"
			::= { xfDcnIPv6Host 10 }

		-- 1.3.6.1.4.1.193.81.2.8.12.11
		xfDcnIPv6HostInterface OBJECT-TYPE
			SYNTAX InterfaceIndexOrZero
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Index of the IPv6 host interface xfDcnIPv6HostAddress and xfDcnIPv6HostPrefixLength
				is currently linked to.
				
				In DCN Mode routed(4), this object returns 0, as there is no IPv6 access in this mode.
				
				ML-TN/CN/LH: This object is not supported.
				"
			::= { xfDcnIPv6Host 11 }

		-- 1.3.6.1.4.1.193.81.2.8.12.12
		xfDcnIPv6HostInterfaceRouted OBJECT-TYPE
			SYNTAX InterfaceIndexOrZero
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Index of the IPv6 host interface configured for DCN Mode routed.
				This object returns 0, as there is no IPv6 access in DCN Mode routed.
				
				ML-TN/CN/LH: This object is not supported.
				"
			::= { xfDcnIPv6Host 12 }

