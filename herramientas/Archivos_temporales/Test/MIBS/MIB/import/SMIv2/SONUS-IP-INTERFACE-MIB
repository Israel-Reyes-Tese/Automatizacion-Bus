sonusMirrorGrpIntfStatObjects OBJECT IDENTIFIER
    ::= { sonusMirrorGrpObjects 4 }

sonusMirrorGrpIntfStatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusMirrorGrpIntfStatTableEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Table of the Mirror Group Interface statistics data"
    ::= { sonusMirrorGrpIntfStatObjects 1 }

sonusMirrorGrpIntfStatTableEntry OBJECT-TYPE
    SYNTAX          SonusMirrorGrpIntfStatTableEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is to create a Mirror Group Interfaces Statistics table entry."
    INDEX           { sonusMirrorGrpIntfStatGrpIndex, 
                      sonusMirrorGrpIntfStatIfIndex }
    ::= { sonusMirrorGrpIntfStatTable 1 }

SonusMirrorGrpIntfStatTableEntry ::=
    SEQUENCE {
        sonusMirrorGrpIntfStatGrpIndex  Integer32,
        sonusMirrorGrpIntfStatIfIndex   Integer32,
        sonusMirrorGrpIntfStatReset     INTEGER,
        sonusMirrorGrpIntfStatMirrorGrpName   DisplayString,
        sonusMirrorGrpIntfStatMonitoredIfName     DisplayString,
        sonusMirrorGrpIntfInboundPacketsTotal     Counter32,
        sonusMirrorGrpIntfOutboundPacketsTotal      Counter32,
        sonusMirrorGrpIntfInboundDiscardedPacketsTotal              Counter32,
        sonusMirrorGrpIntfOutboundDiscardedPacketsTotal               Counter32,
        sonusMirrorGrpIntfDiscardedPacketsNoBuffer          Counter32,
        sonusMirrorGrpIntfDiscardedPacketsOther       Counter32,
        sonusMirrorGrpIntfDiscardedPacketsPolicing          Counter32,
        sonusMirrorGrpIntfDiscardedPacketsLinkDown          Counter32
    }

sonusMirrorGrpIntfStatGrpIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A unique Mirror Group index for this Mirror Group Interfaces Statistics Table."
    ::= { sonusMirrorGrpIntfStatTableEntry 1 }

sonusMirrorGrpIntfStatIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A unique Monitored LIF index for this Mirror Group Interfaces Statistics Table."
    ::= { sonusMirrorGrpIntfStatTableEntry 2 }

sonusMirrorGrpIntfStatReset OBJECT-TYPE
    SYNTAX          INTEGER { unknown(1), reset(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Reset all statistics data for this Mirror Group Interface Table."
    ::= { sonusMirrorGrpIntfStatTableEntry 3 }

sonusMirrorGrpIntfStatMirrorGrpName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..23))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identify the Mirror Group that this Monitored Interface belongs to"
    ::= { sonusMirrorGrpIntfStatTableEntry 4 }

sonusMirrorGrpIntfStatMonitoredIfName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..23))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identify the Monitored Interface"
    ::= { sonusMirrorGrpIntfStatTableEntry 5 }

sonusMirrorGrpIntfInboundPacketsTotal OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Total inbound packets mirrored for this Monitored Interface."
    ::= { sonusMirrorGrpIntfStatTableEntry 6 }

sonusMirrorGrpIntfOutboundPacketsTotal OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Total outbound packets mirrored for this Monitored Interface."
    ::= { sonusMirrorGrpIntfStatTableEntry 7 }

sonusMirrorGrpIntfInboundDiscardedPacketsTotal OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Total inbound mirrored packets not sent due to congestion or other reasons for this Monitored Interface."
    ::= { sonusMirrorGrpIntfStatTableEntry 8 }

sonusMirrorGrpIntfOutboundDiscardedPacketsTotal OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Total outbound mirrored packets not sent due to congestion or other reasons for this Monitored Interface."
    ::= { sonusMirrorGrpIntfStatTableEntry 9 }

sonusMirrorGrpIntfDiscardedPacketsNoBuffer OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Mirrored Packets not sent due to no packet buffers for this Monitored Interface."
    ::= { sonusMirrorGrpIntfStatTableEntry 10 }

sonusMirrorGrpIntfDiscardedPacketsOther OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Mirrored Packets not sent due to various other internal reasons for this Monitored Interface."
    ::= { sonusMirrorGrpIntfStatTableEntry 11 }

sonusMirrorGrpIntfDiscardedPacketsPolicing OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Mirrored Packets not sent due to policing for this Monitored Interface."
    ::= { sonusMirrorGrpIntfStatTableEntry 12 }

sonusMirrorGrpIntfDiscardedPacketsLinkDown OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Mirrored Packets not sent due to mirror LIF link down."
    ::= { sonusMirrorGrpIntfStatTableEntry 13 }

