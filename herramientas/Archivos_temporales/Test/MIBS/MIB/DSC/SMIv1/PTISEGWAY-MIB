    SYNTAX          INTEGER { itu(0), ansi(1), china(2), ntt(3), ttc(4) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Obsolete as of 9.0. Moved to GTT DB Def."
    ::= { ptGTTGTTConfigurationEntry 2 }

