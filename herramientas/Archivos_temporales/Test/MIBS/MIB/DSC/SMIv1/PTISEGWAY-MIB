        ptL4CvtrL4CvtrConfigurationIPServicePort        INTEGER,
        ptL4CvtrL4CvtrConfigurationCPAMID INTEGER,
        ptL4CvtrL4CvtrConfigurationCPAPID INTEGER,
        ptL4CvtrL4CvtrConfigurationSignalingIndicator             INTEGER,
        ptL4CvtrL4CvtrConfigurationIncomingAppIDPrecedence                  INTEGER,
        ptL4CvtrL4CvtrConfigurationISUPSupport      INTEGER,
        ptL4CvtrL4CvtrConfigurationSCCPSupport      INTEGER,
        ptL4CvtrL4CvtrConfigurationBICCSupport      INTEGER,
