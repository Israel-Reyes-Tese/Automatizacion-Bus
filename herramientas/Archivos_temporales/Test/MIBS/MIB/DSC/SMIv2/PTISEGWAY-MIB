        ptEventNotificationSetupEventName DisplayString,
        ptEventNotificationSetupSNMPTrap INTEGER,
        ptEventNotificationSetupDashboard INTEGER,
        ptEventNotificationSetupConfigurationIndex          Integer32,
        ptEventNotificationSetupEventTypeListIndex          Integer32,
