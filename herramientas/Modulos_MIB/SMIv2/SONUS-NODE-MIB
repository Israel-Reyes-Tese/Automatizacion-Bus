    SYNTAX          SonusShelfIndex
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the target chassis"
    ::= { sonusNfsShelfAdmnEntry 1 }

sonusNfsShelfAdmnFallbackCntrl OBJECT-TYPE
    SYNTAX          INTEGER { nonrevertive(1), revertive(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The fallback control of NFS server redundancy. If the secondary NFS is active,
             revertive indicates that it will automatically revert back to primary NFS after
             the Wait To Revert time when the primary NFS becomes healthy. This setting is 
             common to both management server modules."
    DEFVAL          { nonrevertive }
    ::= { sonusNfsShelfAdmnEntry 2 }

sonusNfsShelfAdmnWaitToRevertTime OBJECT-TYPE
    SYNTAX          Integer32 (5..120)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Wait to Revert Time (in minutes) of NFS server redundancy. This setting is
             applicable only in the revertive mode. In revertive mode, if the secondary NFS is
             active it will automatically revert back to primary NFS after Wait To Revert time
             when the primary NFS becomes healthy. This setting is common to both management 
             server modules."
    DEFVAL          { 5 }
    ::= { sonusNfsShelfAdmnEntry 3 }

