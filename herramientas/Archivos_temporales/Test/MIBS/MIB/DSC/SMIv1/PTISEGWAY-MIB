    ::= { ptMTPATMLinkEntry 14 }

ptMTPATMLinkAlarms OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Alarms affect the alarms associated with the Link. DISABLED mode: Alarms will not be raised. ENABLED mode: Alarms will be raised. \nValid values: ENABLED and DISABLED. \nDefault value: ENABLED."
    ::= { ptMTPATMLinkEntry 15 }

