sonusUserListStatusEntry OBJECT-TYPE
    SYNTAX          SonusUserListStatusEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusUserListStatusIndex }
    ::= { sonusUserListStatusTable 1 }

SonusUserListStatusEntry ::=
    SEQUENCE {
        sonusUserListStatusIndex        Integer32,
        sonusUserListStatusLastConfigChange   DateAndTime,
        sonusUserListStatusInvalidLogins Integer32,
        sonusUserListStatusLoginLockedCount   Integer32,
        sonusUserListStatusLoginState   INTEGER,
        sonusUserListStatusAccountExpirationStr       OCTET STRING,
        sonusUserListStatusPasswdExpirationStr      OCTET STRING
    }

sonusUserListStatusIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..8)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Identifies the target user list status entry."
    ::= { sonusUserListStatusEntry 1 }

sonusUserListStatusLastConfigChange OBJECT-TYPE
    SYNTAX          DateAndTime
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Octet string that identifies the GMT timestamp of last
             successful SET PDU from this CLI user.
             
             field  octects  contents      range
             -----  -------  --------      -----
             1      1-2      year         0..65536
             2       3       month        1..12
             3       4       day          1..31
             4       5       hour         0..23
             5       6       minutes      0..59
             6       7       seconds      0..59
             7       8       deci-sec     0..9
             
             * Notes:
             - the value of year is in network-byte order"
    ::= { sonusUserListStatusEntry 2 }

sonusUserListStatusInvalidLogins OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This object indicates the number of invalid login attempts since last reboot."
    ::= { sonusUserListStatusEntry 3 }

sonusUserListStatusLoginLockedCount OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This object indicates the number of times an account has been locked due to invalid
             login attempts since last reboot."
    ::= { sonusUserListStatusEntry 4 }

sonusUserListStatusLoginState OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This object indicates the login state of an account. The
             login state in locked when the number of invalid login attempts
              has been exceeded."
    ::= { sonusUserListStatusEntry 5 }

sonusUserListStatusAccountExpirationStr OBJECT-TYPE
    SYNTAX          OCTET STRING (SIZE(0..28))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "This object displays the time at when this account expires."
    ::= { sonusUserListStatusEntry 6 }

