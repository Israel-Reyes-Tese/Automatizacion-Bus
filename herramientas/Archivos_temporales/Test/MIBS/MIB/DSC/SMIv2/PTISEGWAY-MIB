        ptIOHWSCTPServerNumberOfAssoc   Integer32,
        ptIOHWSCTPServerDebugLevel      Integer32,
        ptIOHWSCTPServerHeartBeat       INTEGER,
        ptIOHWSCTPServerHeartBeatInterval Integer32,
        ptIOHWSCTPServerRTOInitial      Integer32,
        ptIOHWSCTPServerRTOMinimum      Integer32,
        ptIOHWSCTPServerRTOMaximum      Integer32,
        ptIOHWSCTPServerMaximumPathRetrans  Integer32,
        ptIOHWSCTPServerMaximumAssocRetrans   Integer32,
        ptIOHWSCTPServerBundling        INTEGER,
        ptIOHWSCTPServerBundlingTO      Integer32,
        ptIOHWSCTPServerDelayedAckTO    Integer32,
        ptIOHWSCTPServerFastRetransThreshold    Integer32,
        ptIOHWSCTPServerChecksum        INTEGER,
        ptIOHWSCTPServerProtocol        INTEGER,
        ptIOHWSCTPServerIOManagerIndex  Integer32,
        ptIOHWSCTPServerParentOID       OBJECT IDENTIFIER,
        ptIOHWSCTPServerRowStatus       RowStatus,
        ptIOHWSCTPServerIndex           Integer32
    }

