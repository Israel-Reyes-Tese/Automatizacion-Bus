    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The name of the port contains the value seen on the faceplate that corresponds to this port."
    ::= { ptSWMExternalPortEntry 5 }

