		-- 1.3.6.1.4.1.193.81.2.8.8
		xfDcnPpp OBJECT IDENTIFIER ::= { xfDcnMIB 8 }

		-- 1.3.6.1.4.1.193.81.2.8.8.1
		xfDcnPppConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfDcnPppConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table has an entry for each PPP link and
				is indexed by the ifindex. It can be used to
				configure numbered/unnumbered, an IP address
				for the numbered links and it shows the
				IP address of the far-end."
			::= { xfDcnPpp 1 }

		-- 1.3.6.1.4.1.193.81.2.8.8.1.1
		xfDcnPppConfigEntry OBJECT-TYPE
			SYNTAX XfDcnPppConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table."
			INDEX { ifIndex }
			::= { xfDcnPppConfigTable 1 }

		XfDcnPppConfigEntry ::=
			SEQUENCE {
				xfDcnPppConfigNumbered
					INTEGER,
				xfDcnPppConfigIpAddress
					IpAddress,
				xfDcnPppConfigFarEnd
					IpAddress
			 }

		-- 1.3.6.1.4.1.193.81.2.8.8.1.1.1
		xfDcnPppConfigNumbered OBJECT-TYPE
			SYNTAX INTEGER
				{
				unNumbered(1),
				numbered(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to configure the PPP link
				to be either unNumbered(1) or numbered(2).
				A PPP link is default unNumbered(1)."
			::= { xfDcnPppConfigEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.8.8.1.1.2
		xfDcnPppConfigIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"In case the PPP link is configured as
				numbered, this object specifies the IP
				address of the near-end side of the link."
			::= { xfDcnPppConfigEntry 2 }

		-- 1.3.6.1.4.1.193.81.2.8.8.1.1.3
		xfDcnPppConfigFarEnd OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the IP address of the
				far-end side of the PPP link."
			::= { xfDcnPppConfigEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.8.9
		xfDcnEthernet OBJECT IDENTIFIER ::= { xfDcnMIB 9 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1
		xfDcnEthernetConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfDcnEthernetConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in this table is created for each Ethernet,
				Loopback and L3 VLAN interface. In this table it is possible
				to change the IP address and netmask of the interface."
			::= { xfDcnEthernet 1 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1
		xfDcnEthernetConfigEntry OBJECT-TYPE
			SYNTAX XfDcnEthernetConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table."
			INDEX { ifIndex }
			::= { xfDcnEthernetConfigTable 1 }

		XfDcnEthernetConfigEntry ::=
			SEQUENCE {
				xfDcnEthernetConfigIpAddr
					IpAddress,
				xfDcnEthernetConfigMask
					IpAddress,
				xfDcnEthernetConfigLock
					INTEGER,
				xfDcnEthernetConfigPort
					INTEGER,
				xfDcnEthernetConfigRxTx
					INTEGER,
				xfDcnEthernetConfigPriorityQueues
					INTEGER,
				xfDcnEthernetConfigAutoNeg
					INTEGER,
				xfDcnEthernetConfigAlarmStatus
					BITS,
				xfDcnEthernetConfigVrfName
					DisplayString
			 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1.1
		xfDcnEthernetConfigIpAddr OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to set the IP address of this interface."
			::= { xfDcnEthernetConfigEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1.2
		xfDcnEthernetConfigMask OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to set the IP subnet mask of this interface."
			::= { xfDcnEthernetConfigEntry 2 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1.3
		xfDcnEthernetConfigLock OBJECT-TYPE
			SYNTAX INTEGER
				{
				lockToNode(1),
				noLock(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"ML-TN/CN/LH:
				
				This object is used to indicate whether this ethernet interface
				should have the same IP Address and subnet mask as the node (host address).
				This object has been obsoleted, value is always noLock(2).
				
				ML66:
				
				This object indicates unnumbered status of L3 VLAN subinterfaces. Value of
				lockToNode(1) indicates that the interface inherits it's IP Address and
				subnet mask from the loopback interface of the corresponding Virtual Routing
				and Forwarding (VRF) instance. For other interfaces value is always noLock(2).
				
				If xfDcnEthernetConfigLock = lockToNode(1), the xfDcnEthernetConfigIpAddr
				and xfDcnEthernetConfigMask is not writable.
				"
			::= { xfDcnEthernetConfigEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1.4
		xfDcnEthernetConfigPort OBJECT-TYPE
			SYNTAX INTEGER
				{
				autoDetect(1),
				fullDuplex100(2),
				halfDuplex100(3),
				fullDuplex10(4),
				halfDuplex10(5)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to set the Ethernet speed.
				If xfDcnEthernetConfigAutoNeg=off(1) setting this object to autoDetect(1)
				will result in a inConsistentValue error response.
				If xfDcnEthernetConfigPort = autoDetect(1) when xfDcnEthernetConfigAutoNeg
				is set from on(1) to off(2), xfDcnEthernetConfigPort will be set to
				fullDuplex100(2).
				"
			::= { xfDcnEthernetConfigEntry 4 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1.5
		xfDcnEthernetConfigRxTx OBJECT-TYPE
			SYNTAX INTEGER
				{
				auto(1),
				manualNormal(2),
				manualSwitched(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates RX/TX configuration of this Ethernet port.
				
				Valid values:
				auto(1)            : Auto-MDIX
				manualNormal(2)    : MDI
				manualSwitched(3)  : MDIX
				"
			::= { xfDcnEthernetConfigEntry 5 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1.6
		xfDcnEthernetConfigPriorityQueues OBJECT-TYPE
			SYNTAX INTEGER
				{
				disable(1),
				enable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates whether the VLAN priotity queues are enabled
				or disabled for this Ethernet port."
			::= { xfDcnEthernetConfigEntry 6 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1.7
		xfDcnEthernetConfigAutoNeg OBJECT-TYPE
			SYNTAX INTEGER
				{
				on(1),
				off(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to configure auto-negotiate on the ethernet port.
				If xfDcnEthernetConfigPort = autoDetect(1) when this object is set
				from on(1) to off(2), xfDcnEthernetConfigPort will be set to
				fullDuplex100(2).
				Default is on(1).
				"
			::= { xfDcnEthernetConfigEntry 7 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1.8
		xfDcnEthernetConfigAlarmStatus OBJECT-TYPE
			SYNTAX BITS
				{
				ethernetDown0(0),
				ethernetDown1(1),
				ethernetDown2(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates alarm status of the ethernet port.
				
				ethernetDown : the ethernet port/interface is down.
				
				Severity is indicated by 3 bits per status:
				bits 012
				     000   clear
				     001   undetermined
				     010   warning
				     011   minor
				     100   major
				     101   critical
				"
			::= { xfDcnEthernetConfigEntry 8 }

		-- 1.3.6.1.4.1.193.81.2.8.9.1.1.9
		xfDcnEthernetConfigVrfName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..99))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"ML66: The name of the Virtual Routing and Forwarding (VRF)
				instance the interface is associated with.
				
				ML-TN/CN/LH: This object is not supported.
				"
			::= { xfDcnEthernetConfigEntry 9 }

		-- 1.3.6.1.4.1.193.81.2.8.11
		xfDcnIPv6OSPF OBJECT IDENTIFIER ::= { xfDcnMIB 11 }

