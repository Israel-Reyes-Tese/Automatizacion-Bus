	XF-AUX-IO-MIB DEFINITIONS ::= BEGIN
 
		IMPORTS
			entPhysicalIndex			
				FROM ENTITY-MIB			
			ProbableCause, PerceivedSeverity			
				FROM ERICSSON-ALARM-IRP-MIB			
			SnmpAdminString			
				FROM SNMP-FRAMEWORK-MIB			
			OBJECT-GROUP, MODULE-COMPLIANCE			
				FROM SNMPv2-CONF			
			OBJECT-TYPE, MODULE-IDENTITY			
				FROM SNMPv2-SMI			
			DisplayString, TEXTUAL-CONVENTION			
				FROM SNMPv2-TC			
			xfPlatform			
				FROM XF-TOP-MIB;
	
	
		xfAuxIOMIB MODULE-IDENTITY 
			LAST-UPDATED "200408231518Z"		-- August 23, 2004 at 15:18 GMT
			ORGANIZATION 
				"Ericsson-Norway"
			CONTACT-INFO 
				" "
			DESCRIPTION 
				"This MIB is used for digital Auxiliary 
				input and output ports on a XF-NE."
			REVISION "200408231518Z"		-- August 23, 2004 at 15:18 GMT
			DESCRIPTION 
				"E corrected xfAuxOutSeverity and xfAuxOutStatus"
			REVISION "200306181546Z"		-- June 18, 2003 at 15:46 GMT
			DESCRIPTION 
				"D  Removed value limitations on
				     xfAuxInProbableCause
				   Indeterminate also not allowed 
				     as value for xfAuxInSeverity
				   Updated description of 
				     xfAuxOutSeverity that it can 
				     not be set to zero
				   Added cleared in xfAuxOutSeverity
				   Replaced xfAuxInActive with 
				     xfAuxInConfig
				   Added more text to xfInActive.
				   Clear not supported in xfAuxInSeverity
				   "
			REVISION "200203071336Z"		-- March 07, 2002 at 13:36 GMT
			DESCRIPTION 
				"C
				Added size to strings
				changed from admin to displayString
				for xfAuxInSpecificProblem
				Added conformance uniform with MCR
				Changed contact address"
			REVISION "200110101124Z"		-- October 10, 2001 at 11:24 GMT
			DESCRIPTION 
				"B Update after FS/FD work"
			REVISION "200104020000Z"		-- April 02, 2001 at 00:00 GMT
			DESCRIPTION 
				"Rev. A    etoall
				Initial version "
			::= { xfPlatform 3 }

		
	
--
-- Textual conventions
--
	
		XfIOEnable ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"An enumerated type that indicates whether an I/O is
				enabled or not "
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}

		XfInConfig ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"An enumerated type that indicates the configuartion of 
				an input.
				
				no (Normal Open)  : the input is considered as 'active' when 
				                    current IS flowing. Otherwise 'inactive'.
				
				nc (Normal Closed): the input is considered as 'active' when 
				                    current is NOT flowing. Otherwise 'inactive'.
				
				"
			SYNTAX INTEGER
				{
				no(1),
				nc(2)
				}

		XfIOStatus ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"An enumerated type that shows the current status of an IO."
			SYNTAX INTEGER
				{
				active(1),
				inactive(2)
				}

	
--
-- Node definitions
--
	
		xfAuxIOObjects OBJECT IDENTIFIER ::= { xfAuxIOMIB 1 }

		
		xfAuxInTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfAuxInEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" A table of auxiliary inputs."
			::= { xfAuxIOObjects 1 }

		
		xfAuxInEntry OBJECT-TYPE
			SYNTAX XfAuxInEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" Information about an auxiliary inputs."
			INDEX { entPhysicalIndex }
			::= { xfAuxInTable 1 }

		
		XfAuxInEntry ::=
			SEQUENCE { 
				xfAuxInSpecificProblem
					DisplayString,
				xfAuxInProbableCause
					ProbableCause,
				xfAuxInEnable
					XfIOEnable,
				xfAuxInConfig
					XfInConfig,
				xfAuxInSeverity
					PerceivedSeverity,
				xfAuxInStatus
					XfIOStatus
			 }

		xfAuxInSpecificProblem OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..80))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Defualt value is that of the same as the name of the 
				corresponding entPhysicalName of the physical port 
				in the entPhysicalTable.The name is used in the 
				specific problem of the alarm. This value must survive
				restarts."
			::= { xfAuxInEntry 2 }

		
		xfAuxInProbableCause OBJECT-TYPE
			SYNTAX ProbableCause
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Probable cause sent in the alarm for this input. "
			DEFVAL { m3100Indeterminate }
			::= { xfAuxInEntry 3 }

		
		xfAuxInEnable OBJECT-TYPE
			SYNTAX XfIOEnable
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This shows whether an auxiliary input is enabled or not. 
				The management system can enable or disable an auxiliary 
				input by writing  to this object. In the disable state 
				the agent will still represent the input status on the 
				port but will not send any alarm traps.
				
				If the input status is active and this is object is set to
				disable, the previous alarm is cleared.
				
				
				"
			DEFVAL { disable }
			::= { xfAuxInEntry 4 }

		
		xfAuxInConfig OBJECT-TYPE
			SYNTAX XfInConfig
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates the configuration of an input."
			DEFVAL { nc }
			::= { xfAuxInEntry 5 }

		
		xfAuxInSeverity OBJECT-TYPE
			SYNTAX PerceivedSeverity
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The alarm severity level associated with an input. The 
				severities 'cleared' and 'indeterminate'  are not supported and the agent should 
				return 'wrongValue'"
			DEFVAL { critical }
			::= { xfAuxInEntry 6 }

		
		xfAuxInStatus OBJECT-TYPE
			SYNTAX XfIOStatus
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The current status of the auxiliary input."
			DEFVAL { inactive }
			::= { xfAuxInEntry 7 }

		
		xfAuxOutTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfAuxOutEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" A table of auxiliary outputs."
			::= { xfAuxIOObjects 2 }

		
		xfAuxOutEntry OBJECT-TYPE
			SYNTAX XfAuxOutEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" Information about an auxiliary outputs."
			INDEX { entPhysicalIndex }
			::= { xfAuxOutTable 1 }

		
		XfAuxOutEntry ::=
			SEQUENCE { 
				xfAuxOutName
					SnmpAdminString,
				xfAuxOutEnable
					XfIOEnable,
				xfAuxOutSeverity
					BITS,
				xfAuxOutStatus
					XfIOStatus
			 }

		xfAuxOutName OBJECT-TYPE
			SYNTAX SnmpAdminString (SIZE (0..16))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"A name that distinguishes a particular auxiliary output. 
				This is assigned by the management system. 
				Default value is that of the same as the name of the 
				corresponding entPhysicalName of the physical port 
				in the entPhysicalTable.This value must survive restarts."
			::= { xfAuxOutEntry 2 }

		
		xfAuxOutEnable OBJECT-TYPE
			SYNTAX XfIOEnable
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This shows whether an auxiliary output is enabled or not. 
				The management system can enable or disable an auxiliary
				output by writing to this object. Disable leads to an 
				inactive output. Trying to write to status while disabled
				will lead to inconsistent value error response."
			DEFVAL { disable }
			::= { xfAuxOutEntry 3 }

		
		xfAuxOutSeverity OBJECT-TYPE
			SYNTAX BITS
				{
				remote(0),
				warning(1),
				minor(2),
				major(3),
				critical(4),
				cleared(5)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Bit map indicating the alarm severity level of the NE 
				associated with an output or indicating that the output 
				is controlled directly by the manager. 
				It is possible to set more then one bit with exception of
				bit 0. Bit 0 cannot be 1 at the same time as any other bit
				is 1. In that case an 'wrongValue' error response will be 
				returned.
				Bit 0, remote, indicates that the output is not controlled 
				by the alarm status of the XF-NE but by the manager through
				the xfAuxOutStatus objects.
				
				All bits can not be set to zero at the same time and 
				the agent returns 'wrongValue'."
			DEFVAL { { remote } }
			::= { xfAuxOutEntry 4 }

		
		xfAuxOutStatus OBJECT-TYPE
			SYNTAX XfIOStatus
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The current status of the auxiliary output. A set will 
				return 'inconsistentValue' if xfAuxOutSeverity does not 
				have value remote(0) and xfAuxOutEnable does not have 
				value enable(1). The agent sets this object to inactive when 
				the port is disabled."
			DEFVAL { inactive }
			::= { xfAuxOutEntry 5 }

		
		xfAuxIOConformance OBJECT IDENTIFIER ::= { xfAuxIOMIB 2 }

		
		xfAuxIOCompliances OBJECT IDENTIFIER ::= { xfAuxIOConformance 1 }

		
		xfAuxIOFullCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"The compliance statement for SNMP entities which
				implement everything."
			MODULE -- this module
				MANDATORY-GROUPS { xfAuxIOGroup }
			::= { xfAuxIOCompliances 1 }

		
		xfAuxIOGroups OBJECT IDENTIFIER ::= { xfAuxIOConformance 2 }

		
		xfAuxIOGroup OBJECT-GROUP
			OBJECTS { xfAuxInSpecificProblem, xfAuxInProbableCause, xfAuxInEnable, xfAuxInSeverity, xfAuxInStatus, 
				xfAuxOutName, xfAuxOutEnable, xfAuxOutSeverity, xfAuxOutStatus, xfAuxInConfig
				 }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { xfAuxIOGroups 1 }
--                           
--                    
		
	
	END
