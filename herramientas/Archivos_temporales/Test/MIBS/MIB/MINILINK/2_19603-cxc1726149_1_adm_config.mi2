-- Configuration of the HOA

xfHoaConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF XfHoaConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table lists the configuration of the HOA."
    ::= { xfSdhNeConfig 10 }



xfHoaConfigEntry OBJECT-TYPE
    SYNTAX      XfHoaConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry will be created only when the particular equipment, related
        to this interface has been equipped and the VC4 path is terminated. If created, the VC4TTP is present.
        Its interface index is going to be assigned by the equipment management.
        All tables related to this VC4TTP will be created.

        An entry will be automatically deleted when the particular equipment, related
        to this interface has been de-equipped or the VC4 path is no longer terminated."
    INDEX       { xfVc4TtpIndex }
    ::= { xfHoaConfigTable 1 }

XfHoaConfigEntry ::=
    SEQUENCE  {
        xfVc4TtpIndex           InterfaceIndex,
        xfTug31                 XfTu3CtpOr21Tu12Ctps,
        xfTug32                 XfTu3CtpOr21Tu12Ctps,
        xfTug33                 XfTu3CtpOr21Tu12Ctps
    }


xfVc4TtpIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This VC4TTP will be created by the equipment
        management, if the equipment, supporting this VC4TTP is installed and the VC4 path is terminated.
        It will be deleted, if the equipment is removed or the VC4 path is no longer terminated.
        The interface index of this VC4TTP is going to be assigned automatically.
        The rows of the vc4Tables are going to be created as well.
        "

    ::= { xfHoaConfigEntry 1 }

xfTug31 OBJECT-TYPE
    SYNTAX      XfTu3CtpOr21Tu12Ctps
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Configuration, whether one TU3CTP or 21 TU12CTPs have to be created
        for this TUG3.
        "
    DEFVAL { noSubstructure }
    ::= { xfHoaConfigEntry 2 }

xfTug32 OBJECT-TYPE
    SYNTAX      XfTu3CtpOr21Tu12Ctps
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Configuration, whether one TU3CTP or 21 TU12CTPs have to be created
        for this TUG3.
        "
    DEFVAL { noSubstructure }
    ::= { xfHoaConfigEntry 3 }

xfTug33 OBJECT-TYPE
    SYNTAX      XfTu3CtpOr21Tu12Ctps
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Configuration, whether one TU3CTP or 21 TU12CTPs have to be created
        for this TUG3.
        "
