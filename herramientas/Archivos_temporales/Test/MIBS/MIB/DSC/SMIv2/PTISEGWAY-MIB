PtDSCDiameterProtocolParameterSetEntry ::=
    SEQUENCE {
        ptDSCDiameterProtocolParameterSetName     DisplayString,
        ptDSCDiameterProtocolParameterSetP2PEndToEndIDValidation                        INTEGER,
        ptDSCDiameterProtocolParameterSetP2PHopByHopIDValidation                        INTEGER,
        ptDSCDiameterProtocolParameterSetP2PPBitValidation                  INTEGER,
        ptDSCDiameterProtocolParameterSetP2PUnexpectedCERValidation                           INTEGER,
        ptDSCDiameterProtocolParameterSetP2PUnexpectedCEAValidation                           INTEGER,
        ptDSCDiameterProtocolParameterSetInternalID           Integer32,
        ptDSCDiameterProtocolParameterSetDSCIndex         Integer32,
        ptDSCDiameterProtocolParameterSetDSCParameterSetsIndex                      Integer32,
        ptDSCDiameterProtocolParameterSetParentOID          OBJECT IDENTIFIER,
        ptDSCDiameterProtocolParameterSetRowStatus          RowStatus,
        ptDSCDiameterProtocolParameterSetIndex      Integer32
    }

ptDSCDiameterProtocolParameterSetName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Name to uniquely identify the Parameter Set. \nLeading and trailing whitespaces are trimmed. \nValid values: alphanumeric characters, dashes, underscores, spaces, and dots (maximum size of 64)."
    ::= { ptDSCDiameterProtocolParameterSetEntry 1 }

ptDSCDiameterProtocolParameterSetP2PEndToEndIDValidation OBJECT-TYPE
    SYNTAX          INTEGER { ignore(0), enforce(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Point to Point End-to-End ID Validation for CE, and DW messages. RFC6733 states that 'The same End-to-End identifier in the request is used in the answer.'. Choose whether to ENFORCE or IGNORE this rule. \nValid values: IGNORE (0) or ENFORCE (1). \nDefault value: ENFORCE (1)."
    ::= { ptDSCDiameterProtocolParameterSetEntry 2 }

ptDSCDiameterProtocolParameterSetP2PHopByHopIDValidation OBJECT-TYPE
    SYNTAX          INTEGER { ignore(0), enforce(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Point to Point Hop-by-Hop ID Validation for CE, and DW messages. RFC6733 states that 'The same Hop-by-Hop Identifier in the request is used in the answer.'. Choose whether to ENFORCE or IGNORE this rule. \nValid values: IGNORE (0) or ENFORCE (1). \nDefault value: ENFORCE (1)."
    ::= { ptDSCDiameterProtocolParameterSetEntry 3 }

ptDSCDiameterProtocolParameterSetP2PPBitValidation OBJECT-TYPE
    SYNTAX          INTEGER { ignore(0), enforce(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Point to Point P-Bit Validation for CE, and DW messages. RFC6733 states that 'The 'P' bit is set to the same value as the one in the request.' and 'The CER and CEA messages MUST NOT be proxied, redirected, or relayed.'. Choose whether to ENFORCE or IGNORE this rule. \nValid values: IGNORE (0) or ENFORCE (1). \nDefault value: ENFORCE (1)."
    ::= { ptDSCDiameterProtocolParameterSetEntry 4 }

ptDSCDiameterProtocolParameterSetP2PUnexpectedCERValidation OBJECT-TYPE
    SYNTAX          INTEGER { ignore(0), enforce(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Point to Point Unexpected CER Validation. When this rule is enforced, an error message is sent back when an unexpected CER is received. When ignored, the unexpected CER is not processed and a CEA is sent back. Choose whether to ENFORCE or IGNORE this rule. \nValid values: IGNORE (0) or ENFORCE (1). \nDefault value: ENFORCE (1)."
    ::= { ptDSCDiameterProtocolParameterSetEntry 5 }

ptDSCDiameterProtocolParameterSetP2PUnexpectedCEAValidation OBJECT-TYPE
    SYNTAX          INTEGER { ignore(0), enforce(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Point to Point Unexpected CEA Validation. When this rule is enforced, the unexpected CEA is silently discarded. When ignored, the unexpected CEA is processed. Choose whether to ENFORCE or IGNORE this rule. \nValid values: IGNORE (0) or ENFORCE (1). \nDefault value: ENFORCE (1)."
    ::= { ptDSCDiameterProtocolParameterSetEntry 6 }

ptDSCDiameterProtocolParameterSetInternalID OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Parameter set unique internal ID. \nReserved for internal use."
    ::= { ptDSCDiameterProtocolParameterSetEntry 99 }

ptDSCDiameterProtocolParameterSetDSCIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSC."
    ::= { ptDSCDiameterProtocolParameterSetEntry 242 }

ptDSCDiameterProtocolParameterSetDSCParameterSetsIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
