    SEQUENCE {
        ptMTPVNodeContext               DisplayString,
        ptMTPVNodeVPC                   Integer32,
        ptMTPVNodeCLLI                  DisplayString,
        ptMTPVNodeDescription           DisplayString,
        ptMTPVNodeState                 INTEGER,
        ptMTPVNodeBroadcastTFA          INTEGER,
        ptMTPVNodeBroadcastTFP          INTEGER,
        ptMTPVNodeBroadcastTFR          INTEGER,
        ptMTPVNodeMateSTPDistribution   INTEGER,
        ptMTPVNodeMTP3NAManagerIndex    Integer32,
        ptMTPVNodeMTP3NAIndex           Integer32,
        ptMTPVNodeVNodesIndex           Integer32,
        ptMTPVNodeActions               INTEGER,
        ptMTPVNodeParentOID             OBJECT IDENTIFIER,
        ptMTPVNodeRowStatus             RowStatus,
        ptMTPVNodeIndex                 Integer32
    }

ptMTPVNodeContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A condensed textual representation of the virtual node object."
    ::= { ptMTPVNodeEntry 1 }

ptMTPVNodeVPC OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The SS7 point code of this virtual node in the format Network.Cluster.Member. Maximum value is 255.255.255. If the personality is ITU, the format is Zone.Area.Member and the range for Zone and Member is 0 to 7, Area is 0 to 255. "
    ::= { ptMTPVNodeEntry 2 }

ptMTPVNodeCLLI OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
