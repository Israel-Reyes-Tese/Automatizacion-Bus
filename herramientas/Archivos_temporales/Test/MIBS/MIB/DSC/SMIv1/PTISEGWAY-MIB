ptimfIMFPacketCaptureCaptureTime OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Time in seconds to run the capture. \nWhen the user starts the capture, a timer is started for this many seconds. \nIf the user does not manually stop the capture first, the capture is stopped when the timer finishes. \nPlease wait five seconds after starting the capture before sending the desired traffic."
    ::= { ptimfIMFPacketCaptureEntry 1 }

ptimfIMFPacketCaptureStatus OBJECT-TYPE
    SYNTAX          INTEGER { stopped(0), running(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Current status of the packet capture feature. \nWhen RUNNING, captured packets are saved to the file /var/log/packet_capture.pcap. \nIf that file already exists, it will be overwritten. \nThe file is accessible under File Retrieval on the web interface."
    ::= { ptimfIMFPacketCaptureEntry 2 }

ptimfIMFPacketCaptureActions OBJECT-TYPE
    SYNTAX          INTEGER { start(1), stop(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The OID of all possible actions for this instance"
    ::= { ptimfIMFPacketCaptureEntry 252 }

