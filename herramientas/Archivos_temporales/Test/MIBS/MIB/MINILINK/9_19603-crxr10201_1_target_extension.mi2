	XF-TARGET-MIB-EXTENSION DEFINITIONS ::= BEGIN
 
		IMPORTS
			snmpTargetAddrEntry			
				FROM SNMP-TARGET-MIB			
			OBJECT-GROUP, MODULE-COMPLIANCE			
				FROM SNMPv2-CONF			
			Integer32, OBJECT-TYPE, MODULE-IDENTITY			
				FROM SNMPv2-SMI			
			DateAndTime			
				FROM SNMPv2-TC			
			xfPlatform			
				FROM XF-TOP-MIB;
	
	
		xfSnmpTargetMIBExtension MODULE-IDENTITY 
			LAST-UPDATED "200408260842Z"		-- August 26, 2004 at 08:42 GMT
			ORGANIZATION 
				"Ericsson-Norway"
			CONTACT-INFO 
				" "
			DESCRIPTION 
				"This is an extension of the target MIB module of RFC2573. 
				The snmpTargetAddrTable is extended with two new objects, 
				xfNotificationCounter and xfLastNotification."
			REVISION "200408260842Z"		-- August 26, 2004 at 08:42 GMT
			DESCRIPTION 
				"E Changed header"
			REVISION "200306191017Z"		-- June 19, 2003 at 10:17 GMT
			DESCRIPTION 
				"D
				Minor corrections"
			REVISION "200203071348Z"		-- March 07, 2002 at 13:48 GMT
			DESCRIPTION 
				"C 
				Changed contact info
				xfNotificationCounter has to start from 0
				Added conformance
				"
			REVISION "200110101029Z"		-- October 10, 2001 at 10:29 GMT
			DESCRIPTION 
				"B General improvement"
			REVISION "200104030000Z"		-- April 03, 2001 at 00:00 GMT
			DESCRIPTION 
				"Rev. A    etoall
				Initial version "
			::= { xfPlatform 2 }

		
	
	
--
-- Node definitions
--
	
		xfSnmpTargetAddrExtensionObjects OBJECT IDENTIFIER ::= { xfSnmpTargetMIBExtension 1 }

		
		xfSnmpTargetAddrXTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfSnmpTargetAddrXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" This table is an extension to the snmpTargetAddrTable."
			::= { xfSnmpTargetAddrExtensionObjects 1 }

		
		xfSnmpTargetAddrXEntry OBJECT-TYPE
			SYNTAX XfSnmpTargetAddrXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Adds two objects used to check by the EM whether any traps have been lost."
			AUGMENTS { snmpTargetAddrEntry }
			::= { xfSnmpTargetAddrXTable 1 }

		
		XfSnmpTargetAddrXEntry ::=
			SEQUENCE { 
				xfNotificationCounter
					Integer32,
				xfNotificationTime
					DateAndTime
			 }

		xfNotificationCounter OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object counts the number of notifications sent,
				to this manager,since restart. It can be checked to 
				detect whether any notification have been lost."
			::= { xfSnmpTargetAddrXEntry 1 }

		
		xfNotificationTime OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the time when the last 
				notification, to this manager,was sent. It can be 
				used together with xfNotificationCounter. The manager 
				will probably wait a certain time before it decides that 
				a notification was lost."
			::= { xfSnmpTargetAddrXEntry 2 }

		
		xfSnmpTargetAddrExtensionConformance OBJECT IDENTIFIER ::= { xfSnmpTargetMIBExtension 2 }

		
		xfSnmpTargetAddrExtensionCompliances OBJECT IDENTIFIER ::= { xfSnmpTargetAddrExtensionConformance 1 }

		
		xfSnmpTargetAddrExtensionFullCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"The compliance statement for SNMP entities which
				implement everything."
			MODULE -- this module
				MANDATORY-GROUPS { xfSnmpTargetAddrExtensionGroup }
			::= { xfSnmpTargetAddrExtensionCompliances 1 }

		
		xfSnmpTargetAddrExtensionGroups OBJECT IDENTIFIER ::= { xfSnmpTargetAddrExtensionConformance 2 }

		
		xfSnmpTargetAddrExtensionGroup OBJECT-GROUP
			OBJECTS { xfNotificationCounter, xfNotificationTime }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { xfSnmpTargetAddrExtensionGroups 1 }

		
	
	END

