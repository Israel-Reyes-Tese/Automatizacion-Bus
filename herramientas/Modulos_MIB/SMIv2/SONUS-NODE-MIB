    SYNTAX          INTEGER { success(1), unknown(2), inProgress(3), 
                        badRequest(4), noReply(5), managerNack(6), 
                        timerResources(7), dataTimeout(8), 
                        msgSequence(9), memoryResources(10), 
                        imageChecksum(11), badBlkType(12), 
                        flashErase(13), flashWrite(14), 
                        flashChecksum(15), mgrNack(16), badState(17) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The status of the last FLASH update that was executed."
    ::= { sonusFlashStatusEntry 4 }

