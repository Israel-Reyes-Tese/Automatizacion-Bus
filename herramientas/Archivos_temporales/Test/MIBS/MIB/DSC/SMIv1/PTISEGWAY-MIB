            "A sequence of MTPLocalRedirectRecord attributes."
    INDEX           { ptConnGroupIndex, 
                      ptMTPLocalRedirectRecordMTP3NAManagerIndex, 
                      ptMTPLocalRedirectRecordMTP3NAIndex, 
                      ptMTPLocalRedirectRecordPCMappingsIndex, 
                      ptMTPLocalRedirectRecordPCMappingIndex, 
                      ptMTPLocalRedirectRecordIndex }
    ::= { ptMTPLocalRedirectRecordTable 1 }

PtMTPLocalRedirectRecordEntry ::=
    SEQUENCE {
        ptMTPLocalRedirectRecordDPC     DisplayString,
        ptMTPLocalRedirectRecordMessageCategory       INTEGER,
        ptMTPLocalRedirectRecordNewDPC  DisplayString,
        ptMTPLocalRedirectRecordModifier DisplayString,
        ptMTPLocalRedirectRecordInternalID  INTEGER,
        ptMTPLocalRedirectRecordMTP3NAManagerIndex          INTEGER,
        ptMTPLocalRedirectRecordMTP3NAIndex   INTEGER,
        ptMTPLocalRedirectRecordPCMappingsIndex       INTEGER,
        ptMTPLocalRedirectRecordPCMappingIndex      INTEGER,
        ptMTPLocalRedirectRecordParentOID OBJECT IDENTIFIER,
        ptMTPLocalRedirectRecordRowStatus RowStatus,
        ptMTPLocalRedirectRecordIndex   INTEGER
    }

ptMTPLocalRedirectRecordDPC OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Destination Point Code. \nA string that represents a point code, in the PC Display Format used by the NA."
    ::= { ptMTPLocalRedirectRecordEntry 1 }

ptMTPLocalRedirectRecordMessageCategory OBJECT-TYPE
    SYNTAX          INTEGER { fcm(0), tfc(1), rct(2), ssp(3), ssa(4), 
                        sst(5), sccpData(6), scmg(7) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Message Category. \nThe class of message affected by the local redirect. \nThe valid values are TFC, RCT, SSP, SSA, SST, SCCP Data, FCM, and SCMG. \nSCCP Data means SCCP messages other than SCMG messages (Called Party SSN is not 1). \nSCMG means any type of SCCP management message (e.g. SSA, SSP or SST). \nSST, SCMG and SCCP Data values may not be used with the Duplicate modifier. \nFCM means the flow control MTP management messages (TFC and RCT). \nDefault value: FCM."
    ::= { ptMTPLocalRedirectRecordEntry 2 }

ptMTPLocalRedirectRecordNewDPC OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "New Destination Point Code. \nA string that represents a point code, in the PC Display Format used by the NA."
    ::= { ptMTPLocalRedirectRecordEntry 3 }

ptMTPLocalRedirectRecordModifier OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Modifier. \nThis is a string that describes any modification to the behaviour of this record. \nThis may be the empty string or the string ''Duplicate'' or a string of the form ''Duplicate <max>'' or of the form ''Duplicate <max> <overload behaviour>''. \nIf the max is specified, it must be a positive integer between 1 and 1000 that indicates the number of MSUs per second that may be duplicated. If the overload behaviour is specified, it must be one of the following strings: modify, original or alternate. If the number of duplicated MSUs exceeds the provisioned maximum for the record (per second) then the MSU is not duplicated, instead the behaviour depends on the provisioned overload behaviour. If the overload behaviour is set to ''modify'' then the MSU is modified and processed, if the overload behaviour is set to ''original'' then the MSU is processed unchanged and if the overload behavior is set to ''alternate'' then the behaviour alternates between ''modify'' and ''original''. \nIf the Modifier attribute is set to a Duplicate string form, the Message Category attribute cannot be set to SST, SCMG, or SCCP Data. \nThe default values for the max is 1000 and the default value for the overload behaviour is alternate. \nFor example, ''Duplicate'' is the same as ''Duplicate 1000 alternate''."
    ::= { ptMTPLocalRedirectRecordEntry 4 }

ptMTPLocalRedirectRecordInternalID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Internal ID."
    ::= { ptMTPLocalRedirectRecordEntry 5 }

ptMTPLocalRedirectRecordMTP3NAManagerIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
