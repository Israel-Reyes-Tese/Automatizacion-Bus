SunHwMonInventoryEntry ::=
    SEQUENCE {
        sunHwMonFruIndex                Integer32,
        sunHwMonFruName                 DisplayString,
        sunHwMonFruType                 SunFruOrSensorTypeTC,
        sunHwMonFruDescr                DisplayString,
        sunHwMonFruPartNumber           DisplayString,
        sunHwMonFruSerialNumber         DisplayString,
        sunHwMonFruManufacturer         DisplayString,
        sunHwMonFruStatus               SunAlarmStatusTC,
        sunHwMonParentFruIndex          Integer32,
        sunHwMonParentFruName           DisplayString
