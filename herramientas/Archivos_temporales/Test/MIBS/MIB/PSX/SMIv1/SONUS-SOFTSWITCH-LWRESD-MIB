    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 13 }

sonusSoftswitchLWRESDDNSServerBlacklistedNotification TRAP-TYPE
    ENTERPRISE      sonusSoftSwitchLwresdMIBNotifications
    VARIABLES       { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "DNS Server is Black listed"
    ::= 1

sonusSoftswitchLWRESDDNSServerRecoveredNotification TRAP-TYPE
    ENTERPRISE      sonusSoftSwitchLwresdMIBNotifications
    VARIABLES       { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "DNS Serevr is Recovered"
    ::= 2

sonusSoftswitchLWRESDEDNSResponseFailure TRAP-TYPE
    ENTERPRISE      sonusSoftSwitchLwresdMIBNotifications
    VARIABLES       { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "Failure Response for EDNS query with NOT-IMPLE, FORMERR or SERVFAIL"
    ::= 3

sonusSoftswitchLWRESDEDNSResponseSuccess TRAP-TYPE
    ENTERPRISE      sonusSoftSwitchLwresdMIBNotifications
    VARIABLES       { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "Successful Response for EDNS query which failed earlier for that domain"
    ::= 4

sonusSoftswitchLWRESDTTLExpiredRRFallback TRAP-TYPE
    ENTERPRISE      sonusSoftSwitchLwresdMIBNotifications
    VARIABLES       { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "Fallback to TTL expired RR due to DNS response failure"
    ::= 5

sonusSoftswitchLWRESDNormalDNSResponse TRAP-TYPE
    ENTERPRISE      sonusSoftSwitchLwresdMIBNotifications
    VARIABLES       { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "Normal DNS response for a query that previously used expired TTL RR"
    ::= 6

END -- end of module SONUS-SOFTSWITCH-LWRESD-MIB.
