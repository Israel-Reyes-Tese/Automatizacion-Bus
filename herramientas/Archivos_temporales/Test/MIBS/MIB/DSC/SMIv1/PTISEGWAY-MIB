ptMTPLinkLoadMonitoring OBJECT IDENTIFIER
    ::= { ptMTPLinkset 54 }

ptMTPLinkLoadMonitoringTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtMTPLinkLoadMonitoringEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Monitors the Erlang on each link of the linkset."
    ::= { ptMTPLinkLoadMonitoring 1 }

ptMTPLinkLoadMonitoringEntry OBJECT-TYPE
    SYNTAX          PtMTPLinkLoadMonitoringEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of MTPLinkLoadMonitoring attributes."
    INDEX           { ptConnGroupIndex, 
                      ptMTPLinkLoadMonitoringMTP3NAManagerIndex, 
                      ptMTPLinkLoadMonitoringMTP3NAIndex, 
                      ptMTPLinkLoadMonitoringLinksetsIndex, 
                      ptMTPLinkLoadMonitoringLinksetIndex, 
                      ptMTPLinkLoadMonitoringIndex }
    ::= { ptMTPLinkLoadMonitoringTable 1 }

PtMTPLinkLoadMonitoringEntry ::=
    SEQUENCE {
        ptMTPLinkLoadMonitoringMonitoring INTEGER,
        ptMTPLinkLoadMonitoringSLC00Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC00Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC01Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC01Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC02Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC02Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC03Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC03Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC04Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC04Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC05Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC05Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC06Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC06Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC07Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC07Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC08Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC08Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC09Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC09Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC10Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC10Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC11Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC11Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC12Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC12Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC13Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC13Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC14Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC14Tx  DisplayString,
        ptMTPLinkLoadMonitoringSLC15Rx  DisplayString,
        ptMTPLinkLoadMonitoringSLC15Tx  DisplayString,
        ptMTPLinkLoadMonitoringMTP3NAManagerIndex         INTEGER,
        ptMTPLinkLoadMonitoringMTP3NAIndex  INTEGER,
        ptMTPLinkLoadMonitoringLinksetsIndex    INTEGER,
        ptMTPLinkLoadMonitoringLinksetIndex   INTEGER,
        ptMTPLinkLoadMonitoringActions  INTEGER,
        ptMTPLinkLoadMonitoringParentOID OBJECT IDENTIFIER,
        ptMTPLinkLoadMonitoringRowStatus RowStatus,
        ptMTPLinkLoadMonitoringIndex    INTEGER
    }

