    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Adjacent SS7 point code of this link."
    ::= { ptMTPLSLinkEntry 2 }

ptMTPLSLinkSLC OBJECT-TYPE
    SYNTAX          INTEGER (0..15)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Signaling Link Code for this link. \nIf AB Plane Bit is set to A or B, valid values are 0-7. \nIf AB Plane Bit is set to SLC ENCODED, valid values are 0-15."
    ::= { ptMTPLSLinkEntry 3 }

ptMTPLSLinkDREID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
