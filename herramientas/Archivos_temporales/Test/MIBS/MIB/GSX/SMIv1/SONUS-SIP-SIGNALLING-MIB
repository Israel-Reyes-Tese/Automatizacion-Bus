sonusSipRegCountTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipRegCountEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table presents SIP registration count statistics for the node."
    ::= { sonusSipSignallingMIBObjects 16 }

sonusSipRegCountEntry OBJECT-TYPE
    SYNTAX          SonusSipRegCountEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            ""
    INDEX           { sonusSipRegCountShelfIndex, 
                      sonusSipRegCountSlotIndex }
    ::= { sonusSipRegCountTable 1 }

SonusSipRegCountEntry ::=
    SEQUENCE {
        sonusSipRegCountShelfIndex      SonusShelfIndex,
        sonusSipRegCountSlotIndex       SonusSlotIndex,
        sonusSipRegCountReset           INTEGER,
        sonusSipRegCountAttempts        Counter,
        sonusSipRegCountCumCompletions  Counter,
        sonusSipRegCountPending         Counter,
        sonusSipRegCountStable          Counter,
        sonusSipRegCountTotal           Counter
    }

sonusSipRegCountShelfIndex OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the target shelf to obtain the SIP registration counters."
    ::= { sonusSipRegCountEntry 1 }

sonusSipRegCountSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the target slot."
    ::= { sonusSipRegCountEntry 2 }

sonusSipRegCountReset OBJECT-TYPE
    SYNTAX          INTEGER { unknown(1), reset(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Provides a means to reset a subset of the SIP registration
             counters. Any set operation on this object will result in the
             		 attempt and cummulative completion counters being cleared."
    ::= { sonusSipRegCountEntry 3 }

sonusSipRegCountAttempts OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Indicates the number of SIP registration attempts on this card."
    ::= { sonusSipRegCountEntry 4 }

sonusSipRegCountCumCompletions OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Indicates the accumulative number of completed SIP registration attempts on this card."
    ::= { sonusSipRegCountEntry 5 }

sonusSipRegCountPending OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Indicates the current number of pending SIP registrations on this card."
    ::= { sonusSipRegCountEntry 6 }

sonusSipRegCountStable OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Indicates the current number of stable SIP registrations on this card."
    ::= { sonusSipRegCountEntry 7 }

sonusSipRegCountTotal OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Indicates the total number of current SIP registrations on this card."
    ::= { sonusSipRegCountEntry 8 }

