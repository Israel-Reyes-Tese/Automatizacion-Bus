--
-- Node definitions
--
	
		-- 1.3.6.1.4.1.193.81.3.1.1.1
		xfDs1MIBXObjects OBJECT IDENTIFIER ::= { xfDs1ExtensionMIB 1 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1
		xfDsx1ConfigXTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfDsx1ConfigXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" This table is an extension to the dsx1ConfigTable.
				After release 2.0 threshold objects are no longer used."
			::= { xfDs1MIBXObjects 1 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1
		xfDsx1ConfigXEntry OBJECT-TYPE
			SYNTAX XfDsx1ConfigXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Additional information about the configuration of a E1 
				interface."
			AUGMENTS { dsx1ConfigEntry }
			::= { xfDsx1ConfigXTable 1 }

		
		XfDsx1ConfigXEntry ::=
			SEQUENCE { 
				xfDsx1AISDetection
					INTEGER,
				xfDsx1Current24ES
					Xf24HrsSecondsGauge,
				xfDsx1Current24SES
					Xf24HrsSecondsGauge,
				xfDsx1Current24UAS
					Xf24HrsSecondsGauge,
				xfDsx1ESCurrentThreshold
					Xf15MinSeconds,
				xfDsx1SESCurrentThreshold
					Xf15MinSeconds,
				xfDsx1ESTotalThreshold
					Xf24HrsSeconds,
				xfDsx1SESTotalThreshold
					Xf24HrsSeconds,
				xfDsx1SESCriterium
					Integer32,
				xfDsx1LineStatus
					BITS,
				xfDsx1PPPEnable
					INTEGER,
				xfDsx1TotalValid
					TruthValue,
				xfDsx1PMEnable
					INTEGER,
				xfDsx1RxLoop
					INTEGER,
				xfDsx1Current24PCV
					Integer32,
				xfDsx1LinkUpDownAlarmEnable
					BITS,
				xfDsx1FramingPossible
					BITS,
				xfDsx1ClockSourcePossible
					BITS,
				xfDsx1AlarmEnablePossible
					BITS,
				xfDsx1InputImpedance
					INTEGER,
				xfDsx1SyncClockEnable
					INTEGER,
				xfDsx1Capability
					BITS
			 }

		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.3
		xfDsx1AISDetection OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"When true AIS detection is enabled for unstructured E1s."
			DEFVAL { enable }
			::= { xfDsx1ConfigXEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.4
		xfDsx1Current24ES OBJECT-TYPE
			SYNTAX Xf24HrsSecondsGauge
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Number of Errored Seconds since the start of the current 
				24 hours interval."
			::= { xfDsx1ConfigXEntry 4 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.5
		xfDsx1Current24SES OBJECT-TYPE
			SYNTAX Xf24HrsSecondsGauge
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Number of Severely Errored Seconds since the start of 
				the current 24 hours interval."
			::= { xfDsx1ConfigXEntry 5 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.6
		xfDsx1Current24UAS OBJECT-TYPE
			SYNTAX Xf24HrsSecondsGauge
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Number of Unavailable seconds since the start of the
				current 24 Hrs PM interval."
			::= { xfDsx1ConfigXEntry 6 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.7
		xfDsx1ESCurrentThreshold OBJECT-TYPE
			SYNTAX Xf15MinSeconds
			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This is the 15 minutes threshold for ES. Does the 
				current ES counter cross this value an alarmNew will be 
				sent.It will also act as a reset value. So if the next 15 
				minutes interval has an ES counter lower than this 
				threshold value a alarmClear trap will be sent."
			DEFVAL { 20 }
			::= { xfDsx1ConfigXEntry 7 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.8
		xfDsx1SESCurrentThreshold OBJECT-TYPE
			SYNTAX Xf15MinSeconds
			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"his is the 15 minutes threshold for SES. Does the 
				current SES counter cross this value an alarmNew will be 
				sent.It will also act as a reset value. So if the next 15 
				minutes interval has an SES counter lower than this 
				threshold value a alarmClear trap will be sent."
			DEFVAL { 4 }
			::= { xfDsx1ConfigXEntry 8 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.9
		xfDsx1ESTotalThreshold OBJECT-TYPE
			SYNTAX Xf24HrsSeconds
			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This is the 24 hours threshold for ES. Does the current 
				24 hour ES counter cross this value an alarmNew will be 
				emitted.In that case a alarmClear will be sent at the 
				end of the 24 hour period."
			DEFVAL { 200 }
			::= { xfDsx1ConfigXEntry 9 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.10
		xfDsx1SESTotalThreshold OBJECT-TYPE
			SYNTAX Xf24HrsSeconds
			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This is the 24 hours threshold for SES. Does the current 
				24 hour SES counter cross this value an alarm will be 
				emitted.In that case a alarmClear will be sent at the 
				end of the 24 hour period."
			DEFVAL { 40 }
			::= { xfDsx1ConfigXEntry 10 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.11
		xfDsx1SESCriterium OBJECT-TYPE
			SYNTAX Integer32 (1..2048000)
			UNITS "EB/BPV"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Describes the number of EBs that lead to a SES, in case of 
				structured E1,or number of BPVs for unstructured E1.
				Default 300 for structured and 2048 for unstructured.
				Not used in R1."
			::= { xfDsx1ConfigXEntry 11 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.12
		xfDsx1LineStatus OBJECT-TYPE
			SYNTAX BITS
				{
				dsx1AIS0(0),
				dsx1AIS1(1),
				dsx1AIS2(2),
				dsx1LOS0(3),
				dsx1LOS1(4),
				dsx1LOS2(5),
				dsx1UnavailableState0(6),
				dsx1UnavailableState1(7),
				dsx1UnavailableState2(8),
				dsx1ES15m0(9),
				dsx1ES15m1(10),
				dsx1ES15m2(11),
				dsx1SES15m0(12),
				dsx1SES15m1(13),
				dsx1SES15m2(14),
				dsx1ES24h0(15),
				dsx1ES24h1(16),
				dsx1ES24h2(17),
				dsx1SES24h0(18),
				dsx1SES24h1(19),
				dsx1SES24h2(20),
				dsx1NetEquipmentOOS0(21),
				dsx1NetEquipmentOOS1(22),
				dsx1NetEquipmentOOS2(23),
				dsx1LOF0(24),
				dsx1LOF1(25),
				dsx1LOF2(26),
				dsx1RAI0(27),
				dsx1RAI1(28),
				dsx1RAI2(29),
				dsx1LOMF0(30),
				dsx1LOMF1(31),
				dsx1LOMF2(32)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Gives information on the alarm status of a E1 interface.
				Severity is indicated by three bits per alarm:
				bits 012
				     000   clear
				     001   undetermined
				     010   warning
				     011   minor
				     100   major
				     101   critical
				In release after 2.0 the dsx1NetEquipmentOOS bits are not used."
			::= { xfDsx1ConfigXEntry 12 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.13
		xfDsx1PPPEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				pppEnable(1),
				pppDisable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The availability of this object indicates that a 2MBit
				PPP link is possible over this E1, i.e. can be
				set to be a PPP link or not."
			DEFVAL { pppDisable }
			::= { xfDsx1ConfigXEntry 13 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.14
		xfDsx1TotalValid OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates whether the last 24 hour interval was valid."
			::= { xfDsx1ConfigXEntry 14 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.15
		xfDsx1PMEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				pmEnable(1),
				pmDisable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Object that enables / disables PM view."
			DEFVAL { pmDisable }
			::= { xfDsx1ConfigXEntry 15 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.16
		xfDsx1RxLoop OBJECT-TYPE
			SYNTAX INTEGER
				{
				enabled(1),
				disabled(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object inidcates whether the RX-loop is enabled
				by the Far-End radio."
			::= { xfDsx1ConfigXEntry 16 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.17
		xfDsx1Current24PCV OBJECT-TYPE
			SYNTAX Integer32 (0..1080000)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Number of PCV (Path Code Violations) in the current 24 Hours period. The PCVs are the same as the number
				of BBE (Backgrounf Block Errors) and are not counted during SES."
			::= { xfDsx1ConfigXEntry 17 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.18
		xfDsx1LinkUpDownAlarmEnable OBJECT-TYPE
			SYNTAX BITS
				{
				los(0),
				ais(1),
				lof(2),
				lomf(3),
				rai(4),
				uas(5)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used to filter E1 alarms based on alarm types. The filter applies only when 
				ifLinkUpDownTrapEnable is set to 'enabled'. When ifLinkUpDownTrapEnable is set
				to 'disabled', no alarms are sent for E1 interface at all and hence this mib
				object has no effect.
				"
			::= { xfDsx1ConfigXEntry 18 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.19
		xfDsx1FramingPossible OBJECT-TYPE
			SYNTAX BITS
				{
				dsx1E1Unframed(0),
				dsx1E1(1),
				dsx1E1CRC(2),
				dsx1E1MF(3),
				dsx1E1CRCMF(4),
				dsx1ESF(5),
				dsx1D4(6),
				dsx1Unframed(7),
				dsx1J1ESF(8),
				dsx1J1Unframed(9)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates which framing formats are possible for the E1 interface."
			::= { xfDsx1ConfigXEntry 19 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.20
		xfDsx1ClockSourcePossible OBJECT-TYPE
			SYNTAX BITS
				{
				loopTiming(0),
				localTiming(1),
				throughTiming(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates possible clock sources for the E1 interface."
			::= { xfDsx1ConfigXEntry 20 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.21
		xfDsx1AlarmEnablePossible OBJECT-TYPE
			SYNTAX BITS
				{
				los(0),
				ais(1),
				lof(2),
				lomf(3),
				rai(4),
				uas(5)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates which bits in the xfDsx1LinkUpDownAlarmEnable
				are applicable for a specific E1.
				0 not apllicable
				1 applicable"
			::= { xfDsx1ConfigXEntry 21 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.22
		xfDsx1InputImpedance OBJECT-TYPE
			SYNTAX INTEGER
				{
				imp75Ohm(1),
				imp120Ohm(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The input impedance of the interface.
				
				imp75Ohm(1)      75 Ohm impedance
				imp120Ohm(2)     120 ohm impedance
				
				Default value is imp120Ohm(2)."
			::= { xfDsx1ConfigXEntry 22 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.23
		xfDsx1SyncClockEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to enable an E1 interface as 2MHz clock input/output
				 instead of being an E1.The E1 can only be enable as clock input/output
				 in case it is not already used by any other service like CC, SNCP, BERT, 
				 Loop, ATM etc.
				
				Default value is disable(2).
				"
			::= { xfDsx1ConfigXEntry 23 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.1.1.1.24
		xfDsx1Capability OBJECT-TYPE
			SYNTAX BITS
				{
				syncInput(0),
				syncOutput(1),
				impedance75or120(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A bitmap that shows the sync clock capability of this interface.
				
				syncInput(0)         : This bit position shows sync clock input capability
				                        1  means that sync input is supported at this interface.
				                        0  means that sync input is NOT supported at this interface.
				
				syncOutput(1)        : This bit position shows sync clock output capability
				                        1  means that sync output is supported at this interface.
				                        0  means that sync output is NOT supported at this interface.
				
				impdedance75or120(2) : This bit position shows that the impedance can set to 75 or 120 Ohm
				                        1  means that impdance can be set to 75 or 120 ohm at this interface.
				                        0  means that impdance can NOT be changed at this interface."
			::= { xfDsx1ConfigXEntry 24 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.2
		xfDs1MIBXConformance OBJECT IDENTIFIER ::= { xfDs1ExtensionMIB 2 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.2.1
		xfDs1MIBXCompliance OBJECT IDENTIFIER ::= { xfDs1MIBXConformance 1 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.2.1.1
		xfDs1MIBXFullCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"The compliance statement for SNMP entities which
				implement everything related to E1/DSX1."
			MODULE -- this module
				MANDATORY-GROUPS { xfDs1MIBXGroup }
			::= { xfDs1MIBXCompliance 1 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.2.2
		xfDs1MIBXGroups OBJECT IDENTIFIER ::= { xfDs1MIBXConformance 2 }

		
		-- 1.3.6.1.4.1.193.81.3.1.1.2.2.1
		xfDs1MIBXGroup OBJECT-GROUP
			OBJECTS { xfDsx1AISDetection, xfDsx1Current24ES, xfDsx1Current24SES, xfDsx1ESCurrentThreshold, xfDsx1SESCurrentThreshold, 
				xfDsx1ESTotalThreshold, xfDsx1SESTotalThreshold, xfDsx1SESCriterium, xfDsx1LineStatus, xfDsx1TotalValid, 
				xfDsx1PMEnable, xfDsx1RxLoop, xfDsx1Current24PCV, xfDsx1LinkUpDownAlarmEnable, xfDsx1ClockSourcePossible, 
				xfDsx1FramingPossible, xfDsx1AlarmEnablePossible, xfDsx1SyncClockEnable, xfDsx1Capability, xfDsx1PPPEnable, 
				xfDsx1Current24UAS }
			STATUS current
			DESCRIPTION 
				"A group of all Dsx1 extension objects."
			::= { xfDs1MIBXGroups 1 }
