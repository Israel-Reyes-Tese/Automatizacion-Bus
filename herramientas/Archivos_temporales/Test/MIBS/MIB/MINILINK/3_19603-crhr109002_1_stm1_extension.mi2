            AUGMENTS { sonetSectionCurrentEntry }
            ::= { xfSDHsonetSectionCurrentXTable 1 }

        
        XfSDHsonetSectionCurrentXEntry ::=
            SEQUENCE { 
                xfSDHsonetSectionCurrentFMStatus
                    BITS
             }

        xfSDHsonetSectionCurrentFMStatus OBJECT-TYPE
            SYNTAX BITS
                {
                xfSDHsonetSectionNoDefect(0),
                xfSDHsonetSectionLOS0(1),
                xfSDHsonetSectionLOS1(2),
                xfSDHsonetSectionLOS2(3),
                xfSDHsonetSectionLOF0(4),
                xfSDHsonetSectionLOF1(5),
                xfSDHsonetSectionLOF2(6),
                xfSDHsonetSectionTIM0(7),
                xfSDHsonetSectionTIM1(8),
                xfSDHsonetSectionTIM2(9)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Gives information on the alarm status of a 
                Sonet/SDH Section along with the sonetSectionCurrentStatus 
                in the Sonet/SDH MIB.
                Severity is indicated by three bit per alarm:
                   bits 012 
                        000   clear
                        001   undetermined
                        010   warning
                        011   minor
                        100   major
                        101   critical"
            ::= { xfSDHsonetSectionCurrentXEntry 1 }

        
