        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1
        xfEthernetRingProtectionTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfEthernetRingProtectionEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table for managing Ethernet Ring Protection. Each ring has a dedicated VLAN used
                for R-APS PDUs and two ports going in the east and west directions."
            ::= { xfEthernetRingProtection 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1
        xfEthernetRingProtectionEntry OBJECT-TYPE
            SYNTAX XfEthernetRingProtectionEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry for an Ethernet Ring Protection. The table is indexed by the Ring ID and the
                R-APS VLAN ID together."
            INDEX { xfEthernetRingProtectionRingId, dot1qVlanIndex }
            ::= { xfEthernetRingProtectionTable 1 }

        XfEthernetRingProtectionEntry ::=
            SEQUENCE {
                xfEthernetRingProtectionRingId                  Integer32,
                xfEthernetRingProtectionDescription             OCTET STRING,
                xfEthernetRingProtectionEnabled                 INTEGER,
                xfEthernetRingProtectionHoldoffTime             Unsigned32,
                xfEthernetRingProtectionGuardTime               Unsigned32,
                xfEthernetRingProtectionWaitToBlockTime         Unsigned32,
                xfEthernetRingProtectionWaitToRestoreTime       Unsigned32,
                xfEthernetRingProtectionMode                    INTEGER,
                xfEthernetRingProtectionVlanPriority            Integer32,
                xfEthernetRingProtectionMdl                     Integer32,
                xfEthernetRingProtectionSubringVraps            INTEGER,
                xfEthernetRingProtectionRingStatus              INTEGER,
                xfEthernetRingProtectionCountdown               Unsigned32,
                xfEthernetRingProtectionLinkFailureCount        Unsigned32,
                xfEthernetRingProtectionRplFailureCount         Unsigned32,
                xfEthernetRingProtectionManualSwitchCount       Unsigned32,
                xfEthernetRingProtectionForcedSwitchCount       Unsigned32,
                xfEthernetRingProtectionClearCounters           INTEGER,
                xfEthernetRingProtectionAlarms                  BITS,
                xfEthernetRingProtectionSwitchover              INTEGER,
                xfEthernetRingProtectionRowStatus               RowStatus,
                xfEthernetRingProtectionMainRingId              Integer32,
                xfEthernetRingProtectionPmStartTime             DateAndTime,
                xfEthernetRingProtectionLastSwitchover          DateAndTime,
                xfEthernetRingProtectionLastProtectionDuration  Counter64,
                xfEthernetRingProtectionTotalProtectionDuration Counter64,
                xfEthernetRingProtectionPropagateTopologyChange INTEGER,
                xfEthernetRingProtectionTrafficChannels         OCTET STRING
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.1
        xfEthernetRingProtectionRingId OBJECT-TYPE
            SYNTAX Integer32 (1..254)
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "The ID of the ring. Used for unique identification of a ring on the network
                together with the VLAN ID."
            ::= { xfEthernetRingProtectionEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.2
        xfEthernetRingProtectionDescription OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The textual description of the ERP ring. An administrative tool to help network operators."
            ::= { xfEthernetRingProtectionEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.3
        xfEthernetRingProtectionEnabled OBJECT-TYPE
            SYNTAX INTEGER { erpEnabled(1), erpDisabled(2) }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "A switch to enable or disable the ring. To enable, the ring should have
                at least one of its ports bound to an interface."
            ::= { xfEthernetRingProtectionEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.4
        xfEthernetRingProtectionHoldoffTime OBJECT-TYPE
            SYNTAX Unsigned32 (0..10000)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The holdoff time configured on this port. When a port detects a signal failure, it
                can wait for a specified time before it performs a switchover, allowing the network to
                solve the problem automatically. The time is given in milliseconds."
            ::= { xfEthernetRingProtectionEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.5
        xfEthernetRingProtectionGuardTime OBJECT-TYPE
            SYNTAX Unsigned32 (10..2000)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Guard time for the ring. When a signal failure is cleared, it is possible that
                there are still some R-APS PDUs in the network (possibly from the other end of
                the link) that advertise a signal failure. The node ignores these messages from
                the time it noticed the clearing of the failure until this time has passed. The time
                is given in milliseconds"
            ::= { xfEthernetRingProtectionEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.6
        xfEthernetRingProtectionWaitToBlockTime OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Wait To Block time. This setting is only available if xfEthernetRingProtectionMode
                is set to erpModeReverting. It defines the time the node will automatically revert
                after a manual switch or a forced switch has been cleared. The time is given in milliseconds.
                The minimum time is Guard Time + 5000 ms."
            ::= { xfEthernetRingProtectionEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.7
        xfEthernetRingProtectionWaitToRestoreTime OBJECT-TYPE
            SYNTAX Unsigned32 (1..12)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Wait To Restore time. This setting is only available if xfEthernetRingProtectionMode
                is set to erpModeReverting. It defines the time the node will automatically revert
                after a link failure has been cleared. The time is given in minutes."
            ::= { xfEthernetRingProtectionEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.8
        xfEthernetRingProtectionMode OBJECT-TYPE
            SYNTAX INTEGER { erpModeReverting(1), erpModeNonReverting(2) }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Allow reverting on the ring. This setting is only available if one of the
                ports is set to be the RPL owner thus making the node the RPL owner. If the
                mode is set to reverting, the ring will recover from a cleared failure or
                switchover automatically after a certain time has passed (see
                xfEthernetRingProtectionWaitToRestoreTime and xfEthernetRingProtectionWaitToBlockTime
                for the details). Otherwise, it will only fully recover when it is cleared manually
                using xfEthernetRingProtectionClear."
            ::= { xfEthernetRingProtectionEntry 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.9
        xfEthernetRingProtectionVlanPriority OBJECT-TYPE
            SYNTAX Integer32 (0..7)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The network priority for the R-APS PSUs."
            ::= { xfEthernetRingProtectionEntry 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.10
        xfEthernetRingProtectionMdl OBJECT-TYPE
            SYNTAX Integer32 (0..7)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The maintenance domain level of the R-APS PDUs."
            ::= { xfEthernetRingProtectionEntry 10 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.11
        xfEthernetRingProtectionSubringVraps OBJECT-TYPE
            SYNTAX INTEGER {
                erpSubringVirtualChannel(1),
                erpSubringNoVirtualChannel(2)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Enable or disable the virtual channel for this subring. When a virtual channel is present,
                the ring will send R-APS PDUs toward the main ring using the R-APS VLAN. Otherwise, this
                node will be a dead-end for the R-APS PDUs."
            ::= { xfEthernetRingProtectionEntry 11 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.12
        xfEthernetRingProtectionRingStatus OBJECT-TYPE
            SYNTAX INTEGER {
                    erpStatusIdle(1), erpStatusProtection(2), erpStatusManualSwitch(3),
                    erpStatusForcedSwitch(4), erpStatusPending(5), erpStatusDisabled(6)
            }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The current status of the ring.
                 - erpStatusIdle: the ring is operating normally, the RPL is blocked.
                 - erpStatusProtection: one of the links have failed and it is now blocked. If it wasn't
                       the RPL, some disturbance have occured and the RPL is now active. A second
                       link failure might cause serious traffic disturbance.
                 - erpStatusManualSwitch: a manual switch has been made in the ring and that link
                       is blocked instead of the RPL. A link failure can override this. There can
                       be only 1 manual switch in a given ring at any given time.
                 - erpStatusForcedSwitch: a forced switch has been made in the ring and that link
                       is now blocked. There can be multiple forced switches. In this state the ring
                       ignores link failures.
                 - erpStatusPending: a link failure or a switchover has been cleared, one link is blocked.
                       If the node is revertive, the Wait To Restore or Wait To Block timer is running.
                 - erpStatusDisabled: the ring is disabled."
            ::= { xfEthernetRingProtectionEntry 12 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.13
        xfEthernetRingProtectionCountdown OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The countdown until recovery. This setting is only meaningful if xfEthernetRingProtectionMode
                is set to erpModeReverting and xfEthernetRingProtectionRingStatus is erpStatusPending.
                It gives the time in milliseconds until the ring reverts back to erpStatusIdle."
            ::= { xfEthernetRingProtectionEntry 13 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.14
        xfEthernetRingProtectionLinkFailureCount OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The number of link failures in the ring, excluding the RPL failures."
            ::= { xfEthernetRingProtectionEntry 14 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.15
        xfEthernetRingProtectionRplFailureCount OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The number of RPL failures in the ring. Only known by the RPL owner
                and RPL neighbor nodes."
            ::= { xfEthernetRingProtectionEntry 15 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.16
        xfEthernetRingProtectionManualSwitchCount OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The number of manual switches performed in the ring."
            ::= { xfEthernetRingProtectionEntry 16 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.17
        xfEthernetRingProtectionForcedSwitchCount OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The number of forced switches performed in the ring."
            ::= { xfEthernetRingProtectionEntry 17 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.18
        xfEthernetRingProtectionClearCounters OBJECT-TYPE
            SYNTAX INTEGER { erpClearCounters(1) }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Clear the PM counters."
            ::= { xfEthernetRingProtectionEntry 18 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.19
        xfEthernetRingProtectionAlarms OBJECT-TYPE
            SYNTAX BITS {
                erpLinkLossAlarm(0),
                erpUnableToProtectAlarm(1),
                erpProtocolErrorAlarm(2)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The alarms enabled for this ring.
                 - erpLinkLossAlarm: raises an alarm on the affected nodes in case of a link failure.
                 - erpUnableToProtectAlarm: raises an alarm on all ring nodes if the ring can't handle another link failure.
                 - erpProtocolErrorAlarm: raises an alarm if the ring is badly configured."
            ::= { xfEthernetRingProtectionEntry 19 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.20
        xfEthernetRingProtectionSwitchover OBJECT-TYPE
            SYNTAX INTEGER {
                switchoverClear(1),
                switchoverClearSwitch(2),
                switchoverForcedEast(3),
                switchoverForcedWest(4),
                switchoverManualEast(5),
                switchoverManualWest(6)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Perform or clear a switchover on the ring. All of these commands require the ring to be enabled.
                The available commands:
                 - switchoverClear: Manually revert the ring. This setting is only available if one of the
                   ports is set to be the RPL owner thus making the node the RPL owner.
                 - switchoverClearSwitch: Clear the previously set forced or manual switch.
                 - switchoverForcedEast: Block the east port of this node and unblock any not forced-blocked ports.
                   The port will remain blocked and any link failures will be ignored.
                 - switchoverForcedWest: Block the west port of this node and unblock any not forced-blocked ports.
                   The port will remain blocked and any link failures will be ignored.
                 - switchoverManualEast: Manually block the east port of this node and unblock the RPL.
                 - switchoverManualWest: Manually block the west port of this node and unblock the RPL."
            ::= { xfEthernetRingProtectionEntry 20 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.21
        xfEthernetRingProtectionRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to create or destroy the ring. Only a disabled ring can be removed."
            ::= { xfEthernetRingProtectionEntry 21 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.22
        xfEthernetRingProtectionMainRingId OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS deprecated
            DESCRIPTION
                "The ID of the main ring in case this is a subring. This should be configured on the subring endpoint
                nodes in order to specify which ring this ring has to send notifications in case of a switchover."
            ::= { xfEthernetRingProtectionEntry 22 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.23
        xfEthernetRingProtectionPmStartTime OBJECT-TYPE
            SYNTAX DateAndTime
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The time of the last clearing of the counters for this ring. By default it is the time of the
                creation of the ring."
            ::= { xfEthernetRingProtectionEntry 23 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.24
        xfEthernetRingProtectionLastSwitchover OBJECT-TYPE
            SYNTAX DateAndTime
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The time of the last switchover on this ring. If no event has occured since the last clearing,
                its value is 0. Clearing the counters sets it back to 0."
            ::= { xfEthernetRingProtectionEntry 24 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.25
        xfEthernetRingProtectionLastProtectionDuration OBJECT-TYPE
            SYNTAX Counter64
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The time spent in protection mode during the last switchover in seconds. If no switchover has occured
                since the last clearing, its value is 0. If the ring is in protection mode, it measures the time since
                entering said mode. Clearing the counters sets it back to 0."
            ::= { xfEthernetRingProtectionEntry 25 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.26
        xfEthernetRingProtectionTotalProtectionDuration OBJECT-TYPE
            SYNTAX Counter64
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The time spent in protection mode since the last clearing in seconds. If no switchover has occured
                since then, its value is 0. If the ring is in protection mode, the value increments with each reading.
                Clearing the counters sets it back to 0."
            ::= { xfEthernetRingProtectionEntry 26 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.27
        xfEthernetRingProtectionPropagateTopologyChange OBJECT-TYPE
            SYNTAX INTEGER { enabled(1), disabled(2) }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "When topology change occurs in a subring, the forwarding database is flushed on every ERP instance on 
                the interconnection nodes. If topology change propagataion is enabled on the subring ERP instance running 
                on the interconnection node when the topology change occurs, a transmission of a burst of three R-APS 
                messages is triggered over the R-APS channel of the main ring ERP instance. This causes the flushing of the 
                forwarding databases on the ring nodes of the main ring."
            ::= { xfEthernetRingProtectionEntry 27 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.28
        xfEthernetRingProtectionTrafficChannels OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Contains the list of traffic channels used by the ring instance. It allows for bulk addition and removal.
                It is a comma separated list of the VLANs used by the ring. For example: 1,4-8,100,110-117. Any new VLANs
                added to the list must exist."
            ::= { xfEthernetRingProtectionEntry 28 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2
        xfEthernetRingProtectionPortTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfEthernetRingProtectionPortEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table for managing Ethernet Ring Protection ports."
            ::= { xfEthernetRingProtection 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1
        xfEthernetRingProtectionPortEntry OBJECT-TYPE
            SYNTAX XfEthernetRingProtectionPortEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Entries for the Ethernet Ring Protection ports. Each ring in the xfEthernetRingProtectionTable
                has two corresponding port entries in this table. The table is indexed with the Ring ID,
                the R-APS VLAN ID and the port."
            INDEX { xfEthernetRingProtectionRingId, dot1qVlanIndex, xfEthernetRingProtectionPort }
            ::= { xfEthernetRingProtectionPortTable 1 }

        XfEthernetRingProtectionPortEntry ::=
            SEQUENCE {
                xfEthernetRingProtectionPort              INTEGER,
                xfEthernetRingProtectionPortIndex         InterfaceIndexOrZero,
                xfEthernetRingProtectionPortRole          INTEGER,
                xfEthernetRingProtectionPortRmep          Unsigned32,
                xfEthernetRingProtectionPortBlocked       TruthValue,
                xfEthernetRingProtectionPortSignalFailure TruthValue,
                xfEthernetRingProtectionPortManualSwitch  TruthValue,
                xfEthernetRingProtectionPortForcedSwitch  TruthValue,
                xfEthernetRingProtectionPortError         TruthValue
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1.1
        xfEthernetRingProtectionPort OBJECT-TYPE
            SYNTAX INTEGER { erpPortEast(1), erpPortWest(2) }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The ID of the port."
            ::= { xfEthernetRingProtectionPortEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1.2
        xfEthernetRingProtectionPortIndex OBJECT-TYPE
            SYNTAX InterfaceIndexOrZero
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The bridge port bound to this port. If it is set to 0, the port is
                considered a dead-end and the ring a subring. Changing it requires
                the ring to be disabled."
            ::= { xfEthernetRingProtectionPortEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1.3
        xfEthernetRingProtectionPortRole OBJECT-TYPE
            SYNTAX INTEGER { erpPortRoleNormal(1), erpPortRoleRplNeighbor(2) , erpPortRoleRplOwner(3) }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The role of the port in the ring:
                 - erpPortRoleNormal: the port is part of a normal link and unblocked by default.
                 - erpPortRoleRplNeighbor: the port is the other end of the RPL and blocked by default.
                       Prevents usage of the RPL from the other end.
                 - erpPortRoleRplOwner: the port is the owner of RPL and blocked by default. Configuring
                       a port as such makes the node the RPL owner of the ring.
                 Changing the role requires the ring to be disabled."
            ::= { xfEthernetRingProtectionPortEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1.4
        xfEthernetRingProtectionPortRmep OBJECT-TYPE
            SYNTAX Unsigned32 (0..8192)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The Remote MEP assigned to this port. Using this the ring can detect link failures
                much quicker using SOAM CCMs."
            ::= { xfEthernetRingProtectionPortEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1.5
        xfEthernetRingProtectionPortBlocked OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "True if the port is currently blocked."
            ::= { xfEthernetRingProtectionPortEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1.6
        xfEthernetRingProtectionPortSignalFailure OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "True if the port has detected a signal failure."
            ::= { xfEthernetRingProtectionPortEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1.7
        xfEthernetRingProtectionPortManualSwitch OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "True if the port has been set to manual switch."
            ::= { xfEthernetRingProtectionPortEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1.8
        xfEthernetRingProtectionPortForcedSwitch OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "True if the port has been set to forced switch."
            ::= { xfEthernetRingProtectionPortEntry 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.2.1.9
        xfEthernetRingProtectionPortError OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "True if the port has detected a protocol error by not receiving
                R-APS PDUs for 20 seconds when it should have."
            ::= { xfEthernetRingProtectionPortEntry 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.3
        xfEthernetRingProtectionTrafficChannelTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfEthernetRingProtectionTrafficChannelEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table for managing Ethernet Ring Protection traffic channels."
            ::= { xfEthernetRingProtection 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.3.1
        xfEthernetRingProtectionTrafficChannelEntry OBJECT-TYPE
            SYNTAX XfEthernetRingProtectionTrafficChannelEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Entries for the Ethernet Ring Protection traffic channels. Each ring can contain multiple
                VLANs as traffic channel. This table is indexed by the Ring ID, the R-APS VLAN ID and
                the traffic VLAN ID."
            INDEX { xfEthernetRingProtectionRingId, dot1qVlanIndex, xfEthernetRingProtectionTrafficChannel }
            ::= { xfEthernetRingProtectionTrafficChannelTable 1 }

        XfEthernetRingProtectionTrafficChannelEntry ::=
            SEQUENCE {
                xfEthernetRingProtectionTrafficChannel          Unsigned32,
                xfEthernetRingProtectionTrafficChannelRowStatus RowStatus
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.3.1.1
        xfEthernetRingProtectionTrafficChannel OBJECT-TYPE
            SYNTAX Unsigned32 (1..4094)
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "The ID of the VLAN that is managed by this ring."
            ::= { xfEthernetRingProtectionTrafficChannelEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.3.1.2
        xfEthernetRingProtectionTrafficChannelRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to add and remove traffic channels from a ring. Traffic channels can
                only be added or removed from a disabled ring."
            ::= { xfEthernetRingProtectionTrafficChannelEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.15
        xfL3Vlan OBJECT IDENTIFIER ::= { xfEthernetBridgeObjects 15 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.15.1
        xfL3VlanIfTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfL3VlanIfEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table for managing L3 VLAN subinterfaces.
                
                ML-TN/CN/LH: This object is not supported."
            ::= { xfL3Vlan 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1
        xfL3VlanIfEntry OBJECT-TYPE
            SYNTAX XfL3VlanIfEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry for a L3 VLAN subinterface.
                
                ML-TN/CN/LH: This object is not supported."
            INDEX {
                xfL3VlanIfBaseIfIndex,
                xfL3VlanIfVlanIndex
            }
            ::= { xfL3VlanIfTable 1 }

        XfL3VlanIfEntry ::=
            SEQUENCE {
                xfL3VlanIfBaseIfIndex          InterfaceIndex,
                xfL3VlanIfVlanIndex            VlanIndex,
                xfL3VlanIfUntagged             TruthValue,
                xfL3VlanIfIndex                InterfaceIndex,
                xfL3VlanIfRowStatus            RowStatus
            }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.1
        xfL3VlanIfBaseIfIndex OBJECT-TYPE
            SYNTAX InterfaceIndex
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The interface index of the base ethernet interface.
                
                ML-TN/CN/LH: This object is not supported."
            ::= { xfL3VlanIfEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.2
        xfL3VlanIfVlanIndex OBJECT-TYPE
            SYNTAX VlanIndex
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "VLAN ID of the L3 VLAN subinterface.
                
                ML-TN/CN/LH: This object is not supported."
            ::= { xfL3VlanIfEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.3
        xfL3VlanIfUntagged OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "A flag indicating if the VLAN header is removed from the outgoing packets.
                Incoming untagged packets are routed on this port.
                A value of true(1) means untagged.
                
                ML-TN/CN/LH: This object is not supported."
            ::= { xfL3VlanIfEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.4
        xfL3VlanIfIndex OBJECT-TYPE
            SYNTAX InterfaceIndex
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The interface index of the L3 VLAN interface.
                
                ML-TN/CN/LH: This object is not supported."
            ::= { xfL3VlanIfEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.14.1.1.5
        xfL3VlanIfRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "This is used to create/destroy an entry in this table.
                
                ML-TN/CN/LH: This object is not supported."
            ::= { xfL3VlanIfEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16
        xfL1Bonding OBJECT IDENTIFIER ::= { xfEthernetBridgeObjects 16 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1
        xfL1BondingTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfL1BondingEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table for managing L1 bondings."
            ::= { xfL1Bonding 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1
        xfL1BondingEntry OBJECT-TYPE
            SYNTAX XfL1BondingEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry for a L1 bonding."
            INDEX { xfL1BondingMasterPort }
            ::= { xfL1BondingTable 1 }

        XfL1BondingEntry ::=
            SEQUENCE {
                xfL1BondingMasterPort          Unsigned32,
                xfL1BondingMemberPorts         PortList,
                xfL1BondingAlarmsEnable        BITS,
                xfL1BondingRowStatus           RowStatus,
                xfL1BondingAlarmStatus         PerceivedSeverity,
                xfL1BondingDynamicMappingMode      INTEGER,
                xfL1BondingDynamicMappingStatus    INTEGER,
                xfL1BondingDynamicMappingLeftSec   Unsigned32,
                xfL1BondingDynamicMappingVlanList  OCTET STRING,
                xfL1BondingDynamicMappingLo        Unsigned32,
                xfL1BondingDynamicMappingHi        Unsigned32,
                xfL1BondingDynamicMappingScanSec   Unsigned32,
                xfL1BondingDynamicMappingFreqSec   Unsigned32,
                xfL1BondingDynamicMappingManagedVlanList  OCTET STRING
            }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.1
        xfL1BondingMasterPort OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "The master port of the L1 bonding."
            ::= { xfL1BondingEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.2
        xfL1BondingMemberPorts OBJECT-TYPE
            SYNTAX PortList
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The member ports of the L1 bonding. All ports must have the same port role."
            ::= { xfL1BondingEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.3
        xfL1BondingAlarmsEnable OBJECT-TYPE
            SYNTAX BITS {
                l1BondingDegradedServiceAlarm(0),
                l1BondingNoTrafficAlarm(1),
                l1BondingDynamicMappingAlarm(2)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Alarms enabled for the L1 bonding.
                  - l1BondingDegradedServiceAlarm: raised when at least one port is down, but not all.
                  - l1BondingNoTrafficAlarm: raised when all ports of the L1 bonding is down."
            ::= { xfL1BondingEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.4
        xfL1BondingRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to create or destroy the L1 bonding."
            ::= { xfL1BondingEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.5
        xfL1BondingAlarmStatus OBJECT-TYPE
            SYNTAX PerceivedSeverity
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Contains the highest severity of the alarms present on this L1 bonding."
            ::= { xfL1BondingEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.6
        xfL1BondingDynamicMappingMode OBJECT-TYPE
            SYNTAX INTEGER {
                l1BondingModeDisabled(0),
                l1BondingModeAutomatic(1),
                l1BondingModeManual(2)
            }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Enables or disables dynamic mapping for this L1 bonding. When enabled,
                a scan will take place to measure the capacity and usage of each leg of
                the bonding an then map VLANs to even out the bandwidth load so each leg
                may be used to optimal capacity. The scan is rerun every xfL1BondingDynamicMappingFreqSec
                seconds for xfL1BondingDynamicMappingScanSec seconds. In automatic mode,
                the VLANs are also adjusted immediately, while manual mode generates a
                low-severity warning and waits for the user to trigger mapping by re-setting
                this MO to manual. Setting this value, even without changing its value immediately
                triggers a scan and resets the timers. In each case, the adjusting of VLANs
                can generate traffic loss."
            ::= { xfL1BondingEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.7
        xfL1BondingDynamicMappingStatus OBJECT-TYPE
            SYNTAX INTEGER {
                l1BondingStatusDisabled(0),
                l1BondingStatusIdle(1),
                l1BondingStatusScanning(2),
                l1BondingStatusWaitingForUser(3)
            }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The current status of the dynamic mapping algorithm. Between scans it is 'idle',
                during scans it is 'scanning', and if change is required but the xfL1BondingDynamicMappingMode
                is set to 'manual', it is 'waitingForUser'."
            ::= { xfL1BondingEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.8
        xfL1BondingDynamicMappingLeftSec OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The time remaining for the current state of the dynamic mapping algorithm in seconds."
            ::= { xfL1BondingEntry 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.9
        xfL1BondingDynamicMappingVlanList OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The list of VLANs selected for dynamic mapping for this L1 bonding. An empty list means
                that the algorithm should use all VLANs that contain this L1 bonding as member and there
                are have no other mappings defined to it in xfL1BondingMappingTable for this L1 bonding
                instance. If specified, the format is a comma separated list, ranges can be defined with
                dash ('-'). Example: '1,4,10-13,20'. Each VLAN must exist and must contain this bonding
                as a member."
            ::= { xfL1BondingEntry 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.10
        xfL1BondingDynamicMappingLo OBJECT-TYPE
            SYNTAX Unsigned32(0..100)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The percentage value for traffic load under which the algorithm should declare a link 'underused'.
                Default is 50."
            ::= { xfL1BondingEntry 10 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.1.1.11
        xfL1BondingDynamicMappingHi OBJECT-TYPE
            SYNTAX Unsigned32(0..100)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The percentage value for traffic load over which the algorithm should declare a link 'overloaded'.
                Default is 80."
            ::= { xfL1BondingEntry 11 }

