    SYNTAX          DisplayString (SIZE(1..16))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The event file log directory name."
    ::= { sonusDflushNetworkFailureEventStatEntry 9 }

sonusEventLogMIBNotifications OBJECT IDENTIFIER
    ::= { sonusEventLogMIB 2 }

