ptDSCSecurityParameterSet OBJECT IDENTIFIER
    ::= { ptDSCDSCParameterSets 1823 }

ptDSCSecurityParameterSetTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtDSCSecurityParameterSetEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptDSCSecurityParameterSet 1 }

ptDSCSecurityParameterSetEntry OBJECT-TYPE
    SYNTAX          PtDSCSecurityParameterSetEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of DSCSecurityParameterSet attributes."
    INDEX           { ptConnGroupIndex, 
                      ptDSCSecurityParameterSetDSCIndex, 
                      ptDSCSecurityParameterSetDSCParameterSetsIndex, 
                      ptDSCSecurityParameterSetIndex }
    ::= { ptDSCSecurityParameterSetTable 1 }

PtDSCSecurityParameterSetEntry ::=
    SEQUENCE {
        ptDSCSecurityParameterSetName   DisplayString,
        ptDSCSecurityParameterSetSecurityType     INTEGER,
        ptDSCSecurityParameterSetFileSetName    DisplayString,
        ptDSCSecurityParameterSetPriorityString       DisplayString,
        ptDSCSecurityParameterSetInternalID   INTEGER,
        ptDSCSecurityParameterSetDSCIndex INTEGER,
        ptDSCSecurityParameterSetDSCParameterSetsIndex              INTEGER,
        ptDSCSecurityParameterSetParentOID  OBJECT IDENTIFIER,
        ptDSCSecurityParameterSetRowStatus  RowStatus,
        ptDSCSecurityParameterSetIndex  INTEGER
    }

ptDSCSecurityParameterSetName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Name to uniquely identify the Parameter Set. \nLeading and trailing whitespaces are trimmed. \nValid values: alphanumeric characters, dashes, underscores, spaces, and dots (maximum size of 64)."
    ::= { ptDSCSecurityParameterSetEntry 1 }

ptDSCSecurityParameterSetSecurityType OBJECT-TYPE
    SYNTAX          INTEGER { none(0), tls(1), fullTlsDtls(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Security Type (NONE/TLS/FULL TLS DTLS). \nOnly default and plaintext security parameter sets can be set to NONE. \nDefault value: NONE (default and plaintext) \n TLS (else)."
    ::= { ptDSCSecurityParameterSetEntry 2 }

ptDSCSecurityParameterSetFileSetName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "TLS Prefix file names \If the Security Type is NONE, set to empty string. \nUsed in certificate upload. \nValid value: empty string and file name string. \nDefault value: empty string (for default and plaintext Name) \n parameter set name (else)."
    ::= { ptDSCSecurityParameterSetEntry 3 }

ptDSCSecurityParameterSetPriorityString OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "TLS Priority String \nIf the Security type is NONE, set to empty string. \nLeave as 'NORMAL' unless there are errors. See documentation. \nValid values: empty string, NORMAL, valid OpenSSL cipher list (separated by :). \nDefault value: empty string (for default and plaintext Name) \n NORMAL (else). \nNote that to counter known security vulnerabilities, RC4 and MD5 algorithms have been disabled by default. \nFurthermore, TLS 1.0 and below have also been disabled by default. \nFor Diameter RFC compliance: \n Add RC4-SHA to enable TLS_RSA_WITH_RC4_128_SHA \n Add RC4-MD5 to enable TLS_RSA_WITH_RC4_128_MD5 \nFor compatibility (for example): \n Add !SHA384 to interwork with a remote peer that \n erroneously reports support for SHA384 ciphers \n Add &TLS10 to allow TLS 1.0"
    ::= { ptDSCSecurityParameterSetEntry 4 }

ptDSCSecurityParameterSetInternalID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Parameter set unique internal ID. \nReserved for internal use."
    ::= { ptDSCSecurityParameterSetEntry 99 }

