  XF-RADIOLINK-PTP-NODE-MIB DEFINITIONS ::= BEGIN

    IMPORTS
      MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
      OBJECT-TYPE, MODULE-IDENTITY
        FROM SNMPv2-SMI
      TEXTUAL-CONVENTION, DisplayString
            FROM SNMPv2-TC
      OBJECT-TYPE, MODULE-IDENTITY, Integer32
            FROM SNMPv2-SMI
      xfRadioLink
        FROM XF-TOP-MIB;

    xfRadioLinkPtpNodeMIB MODULE-IDENTITY
      LAST-UPDATED "201701050000Z" -- 05 January 2017
      ORGANIZATION
          "Ericsson AB"
      CONTACT-INFO
          "
          Ericsson AB
          Transmission & Transport Networks
          SE-431 84  Molndal
          Sweden
          "
      DESCRIPTION
          "
          This MIB defines objects for point-to-point
          Radiolink nodes.
          "
      REVISION "201701050000Z" -- 05 January 2017
      DESCRIPTION
          "
          Rev PE1
          Added a new Table for Booster handling.
          "
      REVISION "201506030000Z" -- 03 June 2015

      DESCRIPTION
          "
          Rev PD1
          Minor changes for removal of warnings.
          "

      REVISION "201311220000Z" -- 22 November 2013
      DESCRIPTION
          "
          Rev PC1
          Minor changes for removal of warnings.
          "

      REVISION "201105230000Z"
      DESCRIPTION
          "
          Rev. B
          Official R-state for 4.4 FP.
          "

      REVISION "201011230000Z"
      DESCRIPTION
          "
          Rev. PB1
          Added conformance table.
          "

      REVISION "201011110000Z"
      DESCRIPTION
          "
          Rev. A
          Created according to 93/1597-FCP 101 8082 Uae Rev PA8.
          "

          ::= { xfRadioLink 4 }

--
-- Textual conventions
--
--
-- Node definitions
--
  xfRadioLinkPtpNodeObjects OBJECT IDENTIFIER
          ::= { xfRadioLinkPtpNodeMIB 1 }

  xfRadioLinkPtpNodeConformance OBJECT IDENTIFIER
          ::= { xfRadioLinkPtpNodeMIB 2 }

		  
------------------------------------------------------------------------------
-- xfRadioLinkPtpNodeObjects
------------------------------------------------------------------------------
-- The xfRadioLinkPtpNodeObjects MOs

  xfRadioLinkPowerDcByPass OBJECT-TYPE
    SYNTAX INTEGER
    {
      enable(1),
      disable(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
      "
      RadiolinkNode object. Used for enable/disable Dc Bypass
      Default value: disable
      "
    ::= { xfRadioLinkPtpNodeObjects 1 }

	
------------------------------------------------------------------------------
-- xfRadioLinkBoosterTable
------------------------------------------------------------------------------

  xfRadioLinkBoosterTable OBJECT-TYPE
        SYNTAX SEQUENCE OF XfRadioLinkBoosterEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
            Table for Radiolink Booster managed objects.
            "  
        ::= { xfRadioLinkPtpNodeObjects 2 }

  xfRadioLinkBoosterEntry OBJECT-TYPE
        SYNTAX XfRadioLinkBoosterEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An entry in xfRadioLinkBoosterTable."
        INDEX { xfRadioLinkBoosterIndex }
        ::= { xfRadioLinkBoosterTable 1 }

  XfRadioLinkBoosterEntry ::=
        SEQUENCE
        {
 		  xfRadioLinkBoosterIndex              Integer32,
          xfRadioLinkBoosterProductNumber      DisplayString,
          xfRadioLinkBoosterHWRevision         DisplayString
        }
		
--
-- xfRadioLinkBoosterTable MOs
--

  xfRadioLinkBoosterIndex OBJECT-TYPE
            SYNTAX Integer32 (1..2147483647)
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
            "Points to this Table  xfRadioLinkBoosterTable"
        ::= { xfRadioLinkBoosterEntry 1 }
	
  xfRadioLinkBoosterProductNumber OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..20))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "
			 Product Number of the Booster connected to the specific RF IF 
            "
        ::= {  xfRadioLinkBoosterEntry 2 }
        
  xfRadioLinkBoosterHWRevision OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..20))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "
			 HW Revision of the Booster connected to the specific RF IF 
            "
        ::= {  xfRadioLinkBoosterEntry 3 }

	
--
-- Conformance
--
   xfRadioLinkPtpNodeCompliances OBJECT IDENTIFIER
        ::= { xfRadioLinkPtpNodeConformance 1 }

    xfRadioLinkPtpNodeGroups OBJECT IDENTIFIER
        ::= { xfRadioLinkPtpNodeConformance 2 }

    xfRadioLinkPtpNodeFullCompliance MODULE-COMPLIANCE
        STATUS current
        DESCRIPTION
        "
        The compliance statement for SNMP entities which implement everything.
        "
        MODULE -- This Module
        MANDATORY-GROUPS { xfRadioLinkPtpNodeCompleteGroup }
        ::= { xfRadioLinkPtpNodeCompliances 1 }

    xfRadioLinkPtpNodeCompleteGroup OBJECT-GROUP
        OBJECTS
        {
            xfRadioLinkPowerDcByPass,
			xfRadioLinkBoosterIndex,
            xfRadioLinkBoosterProductNumber,
            xfRadioLinkBoosterHWRevision
        }
        STATUS current
        DESCRIPTION
        "A collection of all current objects in this MIB module."
        ::= { xfRadioLinkPtpNodeGroups 1 }

  END
