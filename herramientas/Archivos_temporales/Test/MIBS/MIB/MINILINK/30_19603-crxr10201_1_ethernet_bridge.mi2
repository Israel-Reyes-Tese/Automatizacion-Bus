        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.1.1.3
        xfEthernetEplLLFEnable OBJECT-TYPE
            SYNTAX INTEGER
                {
                enable(1),
                disable(2)
                }
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Enable/Disable link loss forwarding.
                
                enable(1) : enable Link Loss Forwarding.
                disable(2) : disable Link Loss Forwarding.
                
                Default is disable(2)."
