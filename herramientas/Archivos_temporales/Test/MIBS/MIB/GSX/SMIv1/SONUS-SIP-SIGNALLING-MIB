sonusSipSigServiceGroupSourceAddressFiltering OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This object indicates whether or not the GSX will perform IP source
             address filtering on incoming media packets for this trunk group.
             When ENABLED, the SG will only accept incoming media packets whose
             source address matches the destination address of outbound packets.
             When DISABLED, the GSX does not filter the incoming media stream.
             The source address to filter on is supplied during call signaling."
    DEFVAL          { disabled }
    ::= { sonusSipSigServiceGroupEntry 11 }

sonusSipSigServiceGroupAnsSupervisionTimeout OBJECT-TYPE
    SYNTAX          INTEGER (0..1000)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the time from egress call setup to egress receiving an answer."
    DEFVAL          { 300 }
    ::= { sonusSipSigServiceGroupEntry 12 }

sonusSipSigServiceGroupSigZoneId OBJECT-TYPE
    SYNTAX          INTEGER (0..32)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the Signaling Zone this SIP service group is bound to.
             This ID is automatically generated when a new signaling zone is
             created."
    DEFVAL          { 0 }
    ::= { sonusSipSigServiceGroupEntry 13 }

sonusSipSigServiceGroupMediaNifGroupName OBJECT-TYPE
    SYNTAX          SonusNameReference
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The media NIF Group which this SIP service group is bound to."
    ::= { sonusSipSigServiceGroupEntry 14 }

sonusSipSigServiceGroupNaptForSignaling OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This indicates whether or not the devices matching this trunk group
             require NAPT support for signaling. When ENABLED, the GSX will send
             signaling packets to the learned source address and port of 
             the peer. When DISABLED, the GSX will send signaling packets as
             		 dictated by the SIP signaling from the peer."
    DEFVAL          { disabled }
    ::= { sonusSipSigServiceGroupEntry 15 }

sonusSipSigServiceGroupNaptForMedia OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This indicates whether or not the devices matching this trunk group
             require NAPT support for media. When ENABLED, the GSX will send
             RTP packets to the learned source address and port of the
             inbound RTP stream. When DISABLED, the GSX will send RTP
             packets as negotiated by the SIP signaling from the peer."
    DEFVAL          { disabled }
    ::= { sonusSipSigServiceGroupEntry 16 }

sonusSipSigServiceGroupRegistrationExpires OBJECT-TYPE
    SYNTAX          INTEGER (15..65535)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is maximum refresh registration interval(in seconds) for SIP endpoint
             that is not behind NAT. This is also the default refresh interval if the
             SIP endpoint has not proposed an expires value during registration.
               	 The minimum value is 15 seconds."
    DEFVAL          { 3600 }
    ::= { sonusSipSigServiceGroupEntry 17 }

sonusSipSigServiceGroupParseEmbeddedBgid OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This indicates whether the digits after the '*' should be processed
             separately as BGID. When ENABLED, the PSX will process the BGID 
             separately. When DISABLED, the PSX will process the entire digit
             string as a whole."
    DEFVAL          { disabled }
    ::= { sonusSipSigServiceGroupEntry 18 }

sonusSipSigServiceGroupSigZoneName OBJECT-TYPE
    SYNTAX          SonusNameReference
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The signaling zone which this SIP service group is bound to."
    ::= { sonusSipSigServiceGroupEntry 19 }

sonusSipSigServiceGroupAnsSupervisionTimeoutAction OBJECT-TYPE
    SYNTAX          INTEGER { release(1), trapAndRestart(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the action to take if the answer supervision timer
             expires.  The call may be released or the timer can be restarted
             and a trap fired."
    DEFVAL          { release }
    ::= { sonusSipSigServiceGroupEntry 20 }

sonusSipSigServiceGroupCongRetryAfterMin OBJECT-TYPE
    SYNTAX          INTEGER (0..120)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Retry-After minimum duration for calls rejected due to congestion."
    DEFVAL          { 10 }
    ::= { sonusSipSigServiceGroupEntry 21 }

sonusSipSigServiceGroupCongRetryAfterMax OBJECT-TYPE
    SYNTAX          INTEGER (0..120)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Retry-After maximum duration for calls rejected due to congestion."
    DEFVAL          { 30 }
    ::= { sonusSipSigServiceGroupEntry 22 }

sonusSipSigServiceGroupCongRelTimeout OBJECT-TYPE
    SYNTAX          INTEGER (0..5)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Time to delay sending of release during congestion."
    DEFVAL          { 0 }
    ::= { sonusSipSigServiceGroupEntry 23 }

sonusSipSigServiceGroupCongRejMethod OBJECT-TYPE
    SYNTAX          INTEGER { release(1), ignore(2), discard(3) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Determines how a SIP call is rejected during congestion.
             Release - send 100 Trying followed by 503 Service Unavailable.
                       The 503 can optionally be delayed by the 
                       ConRelTimeout value. It can also have an optional
                       retry after parameter.
             Ignore -  send 100 Trying and then drop the call.
             Discard - drop the call with no response."
    DEFVAL          { release }
    ::= { sonusSipSigServiceGroupEntry 24 }

sonusSipSigServiceGroupTimerT1 OBJECT-TYPE
    SYNTAX          INTEGER (1..10000)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the SIP protocol retransmission timer T1 (in milliseconds).
             T1 is an estimate of the round-trip time (RTT), and it defaults to 500 ms. "
    DEFVAL          { 500 }
    ::= { sonusSipSigServiceGroupEntry 25 }

sonusSipSigServiceGroupTimerT2 OBJECT-TYPE
    SYNTAX          INTEGER (1..65535)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the SIP protocol timer T2 (in milliseconds). This represents
             the maximum retransmit interval for non-INVITE requests and INVITE responses."
    DEFVAL          { 4000 }
    ::= { sonusSipSigServiceGroupEntry 26 }

sonusSipSigServiceGroupTimerSessionKeepalive OBJECT-TYPE
    SYNTAX          INTEGER (0..65535)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the SIP protocol session keep-alive timer (in seconds). 
             When the timer is set to 0 keepalive is turned off"
    DEFVAL          { 1800 }
    ::= { sonusSipSigServiceGroupEntry 27 }

sonusSipSigServiceGroupTimerSessionMinSE OBJECT-TYPE
    SYNTAX          INTEGER (0..65535)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The Min-SE header field indicates the minimum value for the session interval, in units of 
             delta-seconds.  When used in an INVITE or UPDATE request, it indicates the smallest value 
             of the session interval that can be used for that session.  When present in a request or 
             response, its value MUST NOT be less than 90 seconds"
    DEFVAL          { 90 }
    ::= { sonusSipSigServiceGroupEntry 28 }

sonusSipSigServiceGroupTimerSessionTerminationDeltaTime OBJECT-TYPE
    SYNTAX          INTEGER (0..65535)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The time before the session expiration at which a SIP user agent not performing 
             refreshes SHOULD send BYE to the peer to take the session down if that SIP user agent
             hasn't received any refresh request from its peer."
    DEFVAL          { 0 }
    ::= { sonusSipSigServiceGroupEntry 29 }

sonusSipSigServiceGroupGeneralRetryCount OBJECT-TYPE
    SYNTAX          INTEGER (4..12)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This object represents the maximum number of retransmissions 
             that the SIP stack will perform for SIP message."
    DEFVAL          { 7 }
    ::= { sonusSipSigServiceGroupEntry 30 }

sonusSipSigServiceGroupInviteRetryCount OBJECT-TYPE
    SYNTAX          INTEGER (0..6)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This object represents the maximum number of retransmissions 
             that the SIP stack will perform for SIP INVITE message."
    DEFVAL          { 6 }
    ::= { sonusSipSigServiceGroupEntry 31 }

sonusSipSigServiceGroupByeRetryCount OBJECT-TYPE
    SYNTAX          INTEGER (0..12)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This object represents the maximum number of retransmissions 
             that the SIP stack will perform for SIP BYE message."
    DEFVAL          { 3 }
    ::= { sonusSipSigServiceGroupEntry 32 }

sonusSipSigServiceGroupCancelRetryCount OBJECT-TYPE
    SYNTAX          INTEGER (0..12)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This object represents the maximum number of retransmissions 
             that the SIP stack will perform for SIP CANCEL message."
    DEFVAL          { 3 }
    ::= { sonusSipSigServiceGroupEntry 33 }

sonusSipSigServiceGroupUseRouteSet OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), stored(2), received(3), 
                        stored-all(4) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This indicates whether the Path or Service-Route information stored
             in the RCB or the received Route headers are to be used for routing.
             Disabled       - Use default method (PSX query) for routing.
             StoredRouteSet - Use the Path or Service-Route stored in RCB for routing.
             received       - Use the received Route set for routing.
             stored-all     - Use stored route set for all requests including refresh registers"
    DEFVAL          { disabled }
    ::= { sonusSipSigServiceGroupEntry 34 }

