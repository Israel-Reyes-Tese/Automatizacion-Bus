    DESCRIPTION     
            "The name of this Gateway Service Group."
    ::= { sonusGwSigServiceGroupEntry 2 }

sonusGwSigServiceGroupRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            ""
    ::= { sonusGwSigServiceGroupEntry 3 }

sonusGwSigServiceGroupAdminState OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The admin state of the this Gateway Service Group."
    ::= { sonusGwSigServiceGroupEntry 4 }

sonusGwSigServiceGroupMode OBJECT-TYPE
    SYNTAX          SonusServiceState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The mode of operation of this Gateway Service Group."
    ::= { sonusGwSigServiceGroupEntry 5 }

sonusGwSigServiceGroupAction OBJECT-TYPE
    SYNTAX          INTEGER { dryup(1), force(2), undefined(3) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The action taken on calls for this Gateway Service Group."
    DEFVAL          { dryup }
    ::= { sonusGwSigServiceGroupEntry 6 }

sonusGwSigServiceGroupDryupTimeout OBJECT-TYPE
    SYNTAX          INTEGER (0..1440)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The timeout value in minutes for dryup action for this
             Gateway Service Group."
    DEFVAL          { 5 }
    ::= { sonusGwSigServiceGroupEntry 7 }

sonusGwSigServiceGroupTG OBJECT-TYPE
    SYNTAX          SonusNameReference
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The Trunk Group which this Gateway Service Group belongs to."
    ::= { sonusGwSigServiceGroupEntry 8 }

sonusGwSigServiceGroupSourceAddressFiltering OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This object indicates whether or not the GSX will perform IP source
             address filtering on incoming media packets for this trunk group.
             When ENABLED, the SG will only accept incoming media packets whose
             source address matches the destination address of outbound packets.
             When DISABLED, the GSX does not filter the incoming media stream.
             The source address to filter on is supplied during call signaling."
    DEFVAL          { enabled }
    ::= { sonusGwSigServiceGroupEntry 9 }

sonusGwSigServiceGroupMediaNifGroupName OBJECT-TYPE
    SYNTAX          SonusNameReference
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The Media NIF Group associated with this Gateway Service Group."
    ::= { sonusGwSigServiceGroupEntry 10 }

sonusGwSigServiceGroupAccMc1Percent OBJECT-TYPE
    SYNTAX          INTEGER (0..100)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The percentage of calls to continue to send to a remote congested gateway (in MC1)."
    DEFVAL          { 90 }
    ::= { sonusGwSigServiceGroupEntry 11 }

sonusGwSigServiceGroupAccMc2Percent OBJECT-TYPE
    SYNTAX          INTEGER (0..100)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The percentage of calls to continue to send to a remote congested gateway (in MC2)."
    DEFVAL          { 70 }
    ::= { sonusGwSigServiceGroupEntry 12 }

sonusGwSigServiceGroupAccTimer OBJECT-TYPE
    SYNTAX          INTEGER (0..10)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Automatic congestion control timer."
    DEFVAL          { 0 }
    ::= { sonusGwSigServiceGroupEntry 13 }

sonusGwSigServiceGroupDirectMediaAllowed OBJECT-TYPE
    SYNTAX          SonusAdminState
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This indicates whether or not Direct Media should be attempted
             for calls to or from endpoints in this service group. If
             enabled, the GSX will attempt to set up the media path such
             that media flows directly between two Direct-Media-enabled 
             endpoints in the same media zone."
    DEFVAL          { disabled }
    ::= { sonusGwSigServiceGroupEntry 14 }

sonusGwSigServiceGroupMediaZoneName OBJECT-TYPE
    SYNTAX          SonusNameReference
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The Media Zone to which this SIP service group is bound."
    ::= { sonusGwSigServiceGroupEntry 15 }

sonusGwSigServiceGroupMediaZoneId OBJECT-TYPE
    SYNTAX          INTEGER (0..32)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the Media Zone this SIP service group is bound to."
    ::= { sonusGwSigServiceGroupEntry 16 }

sonusGwSigServiceGroupStatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusGwSigServiceGroupStatEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains status information about each Gateway Service
             Group in the GSX9000 Switch node."
    ::= { sonusGwSigServiceGroup 3 }

sonusGwSigServiceGroupStatEntry OBJECT-TYPE
    SYNTAX          SonusGwSigServiceGroupStatEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table describes the status of the Gateway Service Groups."
    INDEX           { sonusGwSigServiceGroupStatListIndex }
    ::= { sonusGwSigServiceGroupStatTable 1 }

SonusGwSigServiceGroupStatEntry ::=
    SEQUENCE {
        sonusGwSigServiceGroupStatListIndex   INTEGER,
        sonusGwSigServiceGroupStatStatus INTEGER
    }

sonusGwSigServiceGroupStatListIndex OBJECT-TYPE
    SYNTAX          INTEGER (1..256)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The index of the Gateway Service Group."
    ::= { sonusGwSigServiceGroupStatEntry 1 }

sonusGwSigServiceGroupStatStatus OBJECT-TYPE
    SYNTAX          INTEGER { available(1), unavailable(2), dryup(3) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The state of the Gateway Service Group."
    ::= { sonusGwSigServiceGroupStatEntry 2 }

sonusGwSigPortPerNifStatusTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusGwSigPortPerNifStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Table of Gateway Signalling Port Per-NIF status."
    ::= { sonusGatewaySignallingMIBObjects 8 }

sonusGwSigPortPerNifStatusEntry OBJECT-TYPE
    SYNTAX          SonusGwSigPortPerNifStatusEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Gateway Signalling Port Per-NIF Status."
    INDEX           { sonusGwSigPortPerNifStatusIndex, 
                      sonusGwSigPortPerNifStatusIfNum }
    ::= { sonusGwSigPortPerNifStatusTable 1 }

SonusGwSigPortPerNifStatusEntry ::=
    SEQUENCE {
        sonusGwSigPortPerNifStatusIndex INTEGER,
        sonusGwSigPortPerNifStatusIfNum INTEGER,
        sonusGwSigPortPerNifStatusNifName SonusNameReference,
        sonusGwSigPortPerNifStatusStatus INTEGER
    }

sonusGwSigPortPerNifStatusIndex OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Gateway signaling port index number."
    ::= { sonusGwSigPortPerNifStatusEntry 1 }

sonusGwSigPortPerNifStatusIfNum OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The NIF's Interface number."
    ::= { sonusGwSigPortPerNifStatusEntry 2 }

sonusGwSigPortPerNifStatusNifName OBJECT-TYPE
    SYNTAX          SonusNameReference
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The NIF's name."
    ::= { sonusGwSigPortPerNifStatusEntry 3 }

sonusGwSigPortPerNifStatusStatus OBJECT-TYPE
    SYNTAX          INTEGER { unavailable(0), available(1), 
                        notApplicable(2) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The availability status of the local address allocated
             to the NIF of the NIF group bound to this signaling port."
    ::= { sonusGwSigPortPerNifStatusEntry 4 }

