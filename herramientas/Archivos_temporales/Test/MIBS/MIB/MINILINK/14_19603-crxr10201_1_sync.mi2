--
-- Textual conventions
--
	
--  Textual conventions
-- 
		XfSyncNominee ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"Number of synchornisation sources is limited to 4."
			SYNTAX Integer32 (1..4)

		XfSyncForcedNominee ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"Used to indicate a synchronisation source or not used."
			SYNTAX INTEGER
				{
				candidate1(1),
				candidate2(2),
				candidate3(3),
				candidate4(4),
				holdover(100),
				clear(101)
				}

		XFSyncQualityLevel ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"Used to indicate the quality level of a sync source.
				
				Code 1-9 is used for NE in mode ETSI
				
				Code 6-17 is used for NE in mode ANSI
				
				qlPrc(1):        Interface is OK and traceable to PRC
				qlSsuA(2):       Interface is OK and traceable to SSU-A
				qlSsuB(3):       Interface is OK and traceable to SSU-B
				qlSec(4):        Interface is OK and traceable to SEC
				qlDnu(5):        Interface is OK but the interface is unsuitable as a sync source.
				qlInv(6):        Interface is providing an invalid SSM value.
				qlNsupp(7):      Interface does not support SSM
				qlFailed(8):     Interface is faulty and thus unsuitable as sync source
				qlUnknown(9):    Interface is OK but the quality is unknown
				qlPrs(10)        Interface is OK and traceable to PRS
				qlStu(11)        Interface is OK but QL not traceable to source
				qlSt2(12)        Interface is OK and traceable to ST2
				qlTnc(13)        Interface is OK and traceable to TNC
				qlSt3e(14)       Interface is OK and traceable to ST3E
				qlSt3(15)        Interface is OK and traceable to ST3
				qlSmc(16)        Interface is OK and traceable to SMC
				qlDus(17)        Interface is OK but unsuitable as a sync source
				"
			SYNTAX INTEGER
				{
				qlPrc(1),
				qlSsuA(2),
				qlSsuB(3),
				qlSec(4),
				qlDnu(5),
				qlInv(6),
				qlNsupp(7),
				qlFailed(8),
				qlUnknown(9),
				qlPrs(10),
				qlStu(11),
				qlSt2(12),
				qlTnc(13),
				qlSt3e(14),
				qlSt3(15),
				qlSmc(16),
				qlDus(17)
				}

		XFSyncAssignedQualityLevel ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"Used to assign the quality level of a sync source.
				
				Code 1-5 is used for NE in mode ETSI
				
				Code 5-12 is used for NE in mode ANSI
				
				qlPrc(1):        Interface is OK and traceable to PRC
				qlSsuA(2):       Interface is OK and traceable to SSU-A
				qlSsuB(3):       Interface is OK and traceable to SSU-B
				qlSec(4):        Interface is OK and traceable to SEC
				notAssigned(5):  Quality level not assigned.
				qlPrs(6)         Interface is OK and traceable to PRS
				qlStu(7)         Interface is OK but QL not traceable to source
				qlSt2(8)         Interface is OK and traceable to ST2
				qlTnc(9)         Interface is OK and traceable to TNC
				qlSt3e(10)       Interface is OK and traceable to ST3E
				qlSt3(11)        Interface is OK and traceable to ST3
				qlSmc(12)        Interface is OK and traceable to SMC
				"
			SYNTAX INTEGER
				{
				qlPrc(1),
				qlSsuA(2),
				qlSsuB(3),
				qlSec(4),
				notAssigned(5),
				qlPrs(6),
				qlStu(7),
				qlSt2(8),
				qlTnc(9),
				qlSt3e(10),
				qlSt3(11),
				qlSmc(12)
				}

		XfSyncNomineeOrZero ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"Used to indicate a synchronisation source or not used."
			SYNTAX Integer32 (0..4)

		XfSyncManualNominee ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"Used to indicate a synchronisation source or not used."
			SYNTAX INTEGER
				{
				candidate1(1),
				candidate2(2),
				candidate3(3),
				candidate4(4)
				}

	
--
-- Node definitions
--
	
--  Node definitions
-- 
		-- 1.3.6.1.4.1.193.81.2.14.1
		xfSyncObjects OBJECT IDENTIFIER ::= { xfSyncMIB 1 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.1
		xfSyncEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to enable or disable the node synchronisation function.
				Default is disbled."
			::= { xfSyncObjects 1 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.2
		xfSyncOperStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				disabled(1),
				freeRunning(2),
				locking(3),
				locked(4),
				holdOver(5),
				failure(6)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates an overall synchronization status with the following values: 
				.  Free running: The node is not synchronized to the network
				.  Locking mode: The node is locking to the network.
				.  Locked mode: The node is locked to the network (normal state with node synch. enabled)
				.  Hold over mode: The node has not got any synchronization source to lock on to.
				.  Failure mode: A HW failure is disabling the node sync functionality
				"
			::= { xfSyncObjects 2 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.3
		xfSyncActiveSource OBJECT-TYPE
			SYNTAX XfSyncNominee
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the synchronisation source in the 
				xfSynchSourceTable that is used for synchronisation."
			::= { xfSyncObjects 3 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.5
		xfSyncForcedSwitch OBJECT-TYPE
			SYNTAX XfSyncForcedNominee
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"It is possible to execute a forced switch of active synchronization source 
				by setting this object to the value of the xfSyncNomId of a nominee.
				It is possible to execute a forced switch to holdover by setting this object
				to the value 100. 
				It is not possible to execute a forced switch to a locked out nominee.
				It is possible to clear the forced switch by setting this object to 101.
				"
			::= { xfSyncObjects 5 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.6
		xfSyncSelectionMode OBJECT-TYPE
			SYNTAX INTEGER
				{
				qlEnableSelection(1),
				qlDisableSelection(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"It is possible to set the sync selection logic in QL-enabled and 
				QL-disabled mode.In QL-enabled mode the selection logic selects 
				the synchronization source according to G.781 chapter 5.12.1.
				This basically selects first on QL, then priority, non-revertive 
				on equal priority.
				
				QL-disabled mode the selection logic selects the synchronization 
				source according to G.781 chapter 5.12.2. This basically selects 
				on priority between interfaces with QL equal to or above  DNU, 
				non-revertive on equal priority. 
				
				Default is q1EnableSelection(1)."
			::= { xfSyncObjects 6 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.7
		xfSyncAvailability OBJECT-TYPE
			SYNTAX INTEGER
				{
				unavailable(0),
				available(1),
				protected(2),
				equipmentError(3)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates the equipment status of network synchronization with respect
				to holdover capability, i.e. the status of boards with FPGA based TDM blocks.
				If one board is operational, the state is 'available'.
				If more than one board are operational, the state is 'protected'.
				"
			::= { xfSyncObjects 7 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.8
		xfSyncSqlch OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"MINI-LINK TN does not support SSM signalling on E1, since 
				this is not generally supported by other equipment e.g. 
				CPP/RBS. Squelching, i.e. AIS propagation, is an 
				alternative way to signal that sync traceability is lost. 
				The function is needed to support a good interoperability 
				of our ATM cross-connect in WCDMA RAN network."
			::= { xfSyncObjects 8 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.10
		xfSyncAlarmStatus OBJECT-TYPE
			SYNTAX BITS
				{
				syncProblem0(0),
				syncProblem1(1),
				syncProblem2(2),
				reducedHoldOver0(3),
				reducedHoldOver1(4),
				reducedHoldOver2(5),
				rsquelch0(6),
				rsquelch1(7),
				rsquelch2(8)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates active alarm status of the node synchronization
				functionaly:
				
				
				syncProblem      
				  minor    more than 1 nominee has been appointed and 
				           the sync function switches to the last non 
				           faulty nominated interface .
				  major    the sync function enters holdover mode .
				  critical the sync function enters free running mode.
				
				reducedHoldOver  
				  minor    the sync function can only provide a 
				           reduced holdover mode.
				
				squelch          
				  critical the QL of the active network reference reaches 
				           the Squelch QL level.
				
				
				Severity is inidicated by 3 bits per status:
				bits 012
				     000   clear
				     001   undetermined
				     010   warning
				     011   minor
				     100   major
				     101   critical
				"
			::= { xfSyncObjects 10 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11
		xfSyncNominatedTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfSyncNominatedEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains  a number of possible synchronisation sources. "
			::= { xfSyncObjects 11 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1
		xfSyncNominatedEntry OBJECT-TYPE
			SYNTAX XfSyncNominatedEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry for each synchonisation source."
			INDEX { xfSyncNomId }
			::= { xfSyncNominatedTable 1 }

		
		XfSyncNominatedEntry ::=
			SEQUENCE { 
				xfSyncNomId
					XfSyncNominee,
				xfSyncNomPriority
					INTEGER,
				xfSyncNomIfIndex
					InterfaceIndexOrZero,
				xfSyncNomQualityLevel
					XFSyncQualityLevel,
				xfSyncNomAssQualityLevel
					XFSyncAssignedQualityLevel,
				xfSyncNomHoldOffTime
					Integer32,
				xfSyncNomLockOut
					INTEGER,
				xfSyncNomSigDeg
					INTEGER,
				xfSyncNomHoldOffWarning
					INTEGER,
				xfSyncNomWaitToRestoreTime
					Integer32,
				xfSyncNomSSMAvailable
					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.1
		xfSyncNomId OBJECT-TYPE
			SYNTAX XfSyncNominee
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Index to the table."
			::= { xfSyncNominatedEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.2
		xfSyncNomPriority OBJECT-TYPE
			SYNTAX INTEGER (1..4)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Indicates priority for synchronisation for the interface. Two sources
				may have the same priority. 
				Default value is 4."
			::= { xfSyncNominatedEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.3
		xfSyncNomIfIndex OBJECT-TYPE
			SYNTAX InterfaceIndexOrZero
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"InterfaceIndex of the interface used as synchronisation source. The 
				value 0 is used to indicate that this source is not used, i.e. has no 
				interfaces related to it.
				Default value is 0."
			::= { xfSyncNominatedEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.4
		xfSyncNomQualityLevel OBJECT-TYPE
			SYNTAX XFSyncQualityLevel
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Status of a synchronization interface."
			::= { xfSyncNominatedEntry 4 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.5
		xfSyncNomAssQualityLevel OBJECT-TYPE
			SYNTAX XFSyncAssignedQualityLevel
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"All interfaces in the nominated sync interface list have the possibility
				to be assigned a fixed quality level by setting this object.
				
				Default value for ETSI qlSsuB(3).
				
				Default value for ANSI qlStu(7)."
			::= { xfSyncNominatedEntry 5 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.6
		xfSyncNomHoldOffTime OBJECT-TYPE
			SYNTAX Integer32 (3..25)
			UNITS "100 msec"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"An interface status which will lead to the quality level of QL-FAILED 
				exists for the hold-off time before the xfSyncNomQualityLevel is set 
				to QL-FAILED according to G781. The filter is implemented with 
				a timer restarting for change in the QL value.
				Default 500 msec"
			::= { xfSyncNominatedEntry 6 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.7
		xfSyncNomLockOut OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"It is possible to exclude a nominated interface from the sync source 
				selection process by setting this object to enable(1), according to G.781 
				chapter 5.11.1.1.
				It is also possible to clear the lockout of a nominated sync interface 
				(according to G.781 chapter 5.11.1.2) and thus return the interface to 
				the consideration by sync source selection process by setting this object 
				to disable(2).
				Default enable."
			::= { xfSyncNominatedEntry 7 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.8
		xfSyncNomSigDeg OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"FAILED condition is possible to enable(1) or disable(2)
				per nominated interface by setting the this object 
				to enable(1) or disable(2) with disable(2) as the default value."
			::= { xfSyncNominatedEntry 8 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.9
		xfSyncNomHoldOffWarning OBJECT-TYPE
			SYNTAX INTEGER
				{
				noWarning(1),
				warning(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"If a nominated interface is a protected interface 
				(SNCP, MSP,..) and the holdoff value is lower for 
				the interface protection than the holdoff set for 
				the nominee then this object indicates warning(2)."
			::= { xfSyncNominatedEntry 9 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.10
		xfSyncNomWaitToRestoreTime OBJECT-TYPE
			SYNTAX Integer32 (0 | 10)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"An interface status which will clear the quality level 
				of QL-FAIL exists for the wait to restore time 
				before the xfSyncNomQualityLevel object is set to a value 
				different from qlFailed."
			::= { xfSyncNominatedEntry 10 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.11.1.11
		xfSyncNomSSMAvailable OBJECT-TYPE
			SYNTAX INTEGER
				{
				available(1),
				notAvailable(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Each nominee indicates if the interface supports SSM. "
			::= { xfSyncNominatedEntry 11 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.12
		xfSyncCandidateTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfSyncCandidateEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table of interfaces that can be used as synchronisation sources."
			::= { xfSyncObjects 12 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.12.1
		xfSyncCandidateEntry OBJECT-TYPE
			SYNTAX XfSyncCandidateEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table. The table is indexed by ifIndex."
			INDEX { ifIndex }
			::= { xfSyncCandidateTable 1 }

		
		XfSyncCandidateEntry ::=
			SEQUENCE { 
				xfSyncCandidateNominee
					XfSyncNomineeOrZero
			 }

		-- 1.3.6.1.4.1.193.81.2.14.1.12.1.1
		xfSyncCandidateNominee OBJECT-TYPE
			SYNTAX XfSyncNomineeOrZero
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object inidcated the corresponding entry in the xfSyncSourceTable. The
				0 value indicates not used as synchronisation source."
			::= { xfSyncCandidateEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.13
		xfSyncNodeClockEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				enable(1),
				disable(2)
				}
			MAX-ACCESS read-write
			STATUS deprecated
			DESCRIPTION
				"This object is used to enable the fixed E1 as 2MHz clock input/output
				 instead of being an E1.The E1 can only be enable as clock input/output
				 in case it is not already used by any other service like CC, SNCP, BERT, 
				Loop, ATM etc.
				
				Default value is disable(2)."
			::= { xfSyncObjects 13 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.14
		xfSyncNodeClockOutputStatus OBJECT-TYPE
			SYNTAX BITS
				{
				notSupported(0),
				noTimeSlotsLeft(1)
				}
			MAX-ACCESS read-only
			STATUS deprecated
			DESCRIPTION
				"A bitmap that shows the status of the node sync clock output.
				
				notSupported(0)   : This bit position shows whether the node supports
				                    sync clock output. '1' means 'not supported' and '0'
				                    means 'supported'.
				
				
				noTimeSlotsLeft(1 ): This bit position shows whether there enough
				                     time slots are available for the node sync 
				                     clock output. '1' means 'no timeslots available'
				                     and '0' means 'timeslots available'.
				                               
				If no bit is set (i.e. 0x00), it means the status of node sync output
				clock is 'OK'."
			::= { xfSyncObjects 14 }

		
		-- 1.3.6.1.4.1.193.81.2.14.1.15
		xfSyncVariant OBJECT-TYPE
			SYNTAX Integer32 (1..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Specifies the current variant of network synchronization feature. This value is incremented whenever the network synchronization feature is modified.
				 		
				This object must be used together with xfNEActiveTelecomStandard as it means different for etsi(1) and ansi(2) mode.
				
				For ETSI 
				  Variant 
				     1 Starting value. (netsync1 ie. TN 4.3 ETSI the same as No MO variant) 
				     2 Same as variant 1, but 2048KHz sync clock output capability moved to E1 level 
				     3 Added enhanced sync (future) 
				     (Selection between basic and performance mode)
				
				For ANSI 
				  Variant 
				    1 Starting value. (no netsync support (ie. no netsync Craft pages are present)) 
				    2 only delivery of 2048kHz output 
				    3 Same as variant 2, but 2048KHz sync clock output capability moved to E1 level. 
				    4 Added (future) basic sync (netsync1) functionality 
				      All input and output supported
				    5 Added enhanced sync (future) 
				      (Selection between basic and performance mode)
				
				"
			::= { xfSyncObjects 15 }

		
