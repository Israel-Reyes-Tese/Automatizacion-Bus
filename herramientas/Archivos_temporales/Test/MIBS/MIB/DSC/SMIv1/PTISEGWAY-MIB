    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The row number for the ActiveTrapList table"
    ::= { ptActiveTrapListEntry 255 }

ptActiveTrap OBJECT IDENTIFIER
    ::= { ptActiveTrapList 324 }

ptActiveTrapTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtActiveTrapEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptActiveTrap 1 }

ptActiveTrapEntry OBJECT-TYPE
    SYNTAX          PtActiveTrapEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of ActiveTrap attributes."
    INDEX           { ptConnGroupIndex, ptActiveTrapConfigurationIndex, 
                      ptActiveTrapActiveTrapListIndex, ptActiveTrapIndex }
    ::= { ptActiveTrapTable 1 }

PtActiveTrapEntry ::=
    SEQUENCE {
        ptActiveTrapTrapName            DisplayString,
        ptActiveTrapSequenceNumber      INTEGER,
        ptActiveTrapDateAndTime         DisplayString,
        ptActiveTrapTrapType            INTEGER,
        ptActiveTrapSeverity            INTEGER,
        ptActiveTrapProblemCause        INTEGER,
        ptActiveTrapClearType           INTEGER,
        ptActiveTrapItemType            DisplayString,
        ptActiveTrapVariableName1       DisplayString,
        ptActiveTrapVariableValue1      DisplayString,
        ptActiveTrapOid1                DisplayString,
        ptActiveTrapOidType1            DisplayString,
        ptActiveTrapVariableName2       DisplayString,
        ptActiveTrapVariableValue2      DisplayString,
        ptActiveTrapOid2                DisplayString,
        ptActiveTrapOidType2            DisplayString,
        ptActiveTrapVariableName3       DisplayString,
        ptActiveTrapVariableValue3      DisplayString,
        ptActiveTrapOid3                DisplayString,
        ptActiveTrapOidType3            DisplayString,
        ptActiveTrapVariableName4       DisplayString,
        ptActiveTrapVariableValue4      DisplayString,
        ptActiveTrapOid4                DisplayString,
        ptActiveTrapOidType4            DisplayString,
        ptActiveTrapVariableName5       DisplayString,
        ptActiveTrapVariableValue5      DisplayString,
        ptActiveTrapOid5                DisplayString,
        ptActiveTrapOidType5            DisplayString,
        ptActiveTrapVariableName6       DisplayString,
        ptActiveTrapVariableValue6      DisplayString,
        ptActiveTrapOid6                DisplayString,
        ptActiveTrapOidType6            DisplayString,
        ptActiveTrapVariableName7       DisplayString,
        ptActiveTrapVariableValue7      DisplayString,
        ptActiveTrapOid7                DisplayString,
        ptActiveTrapOidType7            DisplayString,
        ptActiveTrapVariableName8       DisplayString,
        ptActiveTrapVariableValue8      DisplayString,
        ptActiveTrapOid8                DisplayString,
        ptActiveTrapOidType8            DisplayString,
        ptActiveTrapVariableName9       DisplayString,
        ptActiveTrapVariableValue9      DisplayString,
        ptActiveTrapOid9                DisplayString,
        ptActiveTrapOidType9            DisplayString,
        ptActiveTrapVariableName10      DisplayString,
        ptActiveTrapVariableValue10     DisplayString,
        ptActiveTrapOid10               DisplayString,
        ptActiveTrapOidType10           DisplayString,
        ptActiveTrapItemOID             DisplayString,
        ptActiveTrapConfigurationIndex  INTEGER,
        ptActiveTrapActiveTrapListIndex INTEGER,
        ptActiveTrapParentOID           OBJECT IDENTIFIER,
        ptActiveTrapRowStatus           RowStatus,
        ptActiveTrapIndex               INTEGER
    }

ptActiveTrapTrapName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The name of the trap issued. For more information concerning possible traps names, please look at the provided mib files."
    ::= { ptActiveTrapEntry 1 }

ptActiveTrapSequenceNumber OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The sequence number of the trap\n This number increased by one for each trap sent by the system."
    ::= { ptActiveTrapEntry 2 }

ptActiveTrapDateAndTime OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Date and time value of the occurrence of the trap."
    ::= { ptActiveTrapEntry 3 }

ptActiveTrapTrapType OBJECT-TYPE
    SYNTAX          INTEGER { undefined(0), communicationAlarm(2), 
                        environmentAlarm(3), equipmentAlarm(4), 
                        processingAlarm(10), qualityOfServiceAlarm(11) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The type of the trap, follows Ref. X.733 : \n\nCommunication Alarm: An alarm of this type is principally associated with the procedures and/or processes required to convey information from one point to another. \n\nQuality of Service Alarm: An alarm of this type is principally associated with a degradation in the quality of a service.\n\nProcessing Error Alarm: An alarm of this type is principally associated with a software or processing fault.\n\nEquipment Alarm: An alarm of this type is principally associated with equipment fault.\n\nEnvironmental Alarm: An alarm of this type is principally associated with a condition relating to an enclosure in which the equipment resides."
    ::= { ptActiveTrapEntry 4 }

ptActiveTrapSeverity OBJECT-TYPE
    SYNTAX          INTEGER { indetermined(0), critical(1), major(2), 
                        minor(3), warning(4), cleared(5) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Severity is used in the reporting of traps. When a trap is issued it will have an associated severity.\n\n Possible Severity are: indeterminate, critical, major, minor, warning, cleared."
    ::= { ptActiveTrapEntry 5 }

ptActiveTrapProblemCause OBJECT-TYPE
    SYNTAX          INTEGER { undefined(0), 
                        applicationSubsystemFailure(2), 
                        callEstablishmentError(4), 
                        communicationsProtocolError(5), 
                        communicationsSubsystemError(6), 
                        configurationOrCustomizationError(7), 
                        congestion(8), corruptData(9), 
                        cpuCyclesLimitExceeded(10), 
                        datasetOrModemError(11), degradedSignal(12), 
                        dteDceInterfaceError(13), enclosureDoorOpen(14), 
                        equipmentMalfunction(15), fileError(17), 
                        fireDetected(18), floodDetected(19), 
                        framingError(20), inputOutputDeviceError(23), 
                        inputDeviceError(24), lanError(25), 
                        localNodeTransmissionError(27), lossOfFrame(28), 
                        lossOfSignal(29), multiplexerProblem(31), 
                        outOfMemory(32), outputDeviceError(33), 
                        performanceDegraded(34), powerProblem(35), 
                        processorProblem(37), queueSizeExceeded(39), 
                        receiveFailure(40), 
                        remoteNodeTransmissionError(42), 
                        resourceAtOrNearCapacity(43), 
                        responseTimeExcessive(44), 
                        retransmissionRateExcessive(45), 
                        softwareError(46), 
                        softwareProgramAbnormallyTerminated(47), 
                        softwareProgramError(48), 
                        storageCapacityProblem(49), thresholdCrossed(51), 
                        timingProblem(52), transmitFailure(54), 
                        underlyingResourceUnavailable(56), 
                        versionMismatch(57) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The probable cause of the trap, follows Ref. X.733. After we identify the GSN alarms, we will categorize them according to the five types above, find their probable causes, and map the causes, as close as we can, to the X.733 list."
