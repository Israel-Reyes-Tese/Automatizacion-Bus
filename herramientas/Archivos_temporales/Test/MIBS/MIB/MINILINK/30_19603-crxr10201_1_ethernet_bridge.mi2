                xfEthernetEplUserPriority
                    Integer32,
                xfEthernetEplNetworkPriority
                    Integer32
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.3.1.1
        xfEthernetEplUserPriority OBJECT-TYPE
            SYNTAX Integer32 (0..2147483647)
