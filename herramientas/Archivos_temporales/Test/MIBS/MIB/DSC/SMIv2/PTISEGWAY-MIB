        ptSGM3UASCTPServerMaximumAssocRetrans     Integer32,
        ptSGM3UASCTPServerMaximumPathRetrans    Integer32,
        ptSGM3UASCTPServerBundling      INTEGER,
        ptSGM3UASCTPServerBundlingTimeout Integer32,
        ptSGM3UASCTPServerDelayedAckTimeout   Integer32,
        ptSGM3UASCTPServerChecksum      INTEGER,
        ptSGM3UASCTPServerInStreams     Integer32,
        ptSGM3UASCTPServerOutStreams    Integer32,
        ptSGM3UASCTPServerDefaultSCTPValues   INTEGER,
        ptSGM3UASCTPServerListenStatus  INTEGER,
        ptSGM3UASCTPServerM3UAIndex     Integer32,
        ptSGM3UASCTPServerM3UASCTPServersIndex      Integer32,
        ptSGM3UASCTPServerActions       INTEGER,
        ptSGM3UASCTPServerParentOID     OBJECT IDENTIFIER,
        ptSGM3UASCTPServerRowStatus     RowStatus,
        ptSGM3UASCTPServerIndex         Integer32
    }

