    SYNTAX          INTEGER { notApplicable(0) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This screen is not applicable for SS7 to SS7 traffic deployment."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 14 }

