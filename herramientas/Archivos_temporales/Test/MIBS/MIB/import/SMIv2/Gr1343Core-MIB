    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of times an TFTP session with the component corresponding to
             the sessionCompIndex has been ended due to a TFTP timeout."
    ::= { sessionTftpEntry 6 }

sessionTftpLocAborts OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of times an TFTP session with the component corresponding to
             the sessionCompIndex has been ended due to a local abort."
    ::= { sessionTftpEntry 7 }

sessionTftpRemAborts OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of times an TFTP session with the component corresponding to
             the sessionCompIndex has been ended due to a remote abort."
    ::= { sessionTftpEntry 8 }

sessionTftpEntryStatus OBJECT-TYPE
    SYNTAX          INTEGER { valid(1), invalid(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This object indicates if a table entry is valid or invalid. The value of this object
             is set to 1 when the entry is active and is set to 2 when the entry is inactive."
    ::= { sessionTftpEntry 9 }

