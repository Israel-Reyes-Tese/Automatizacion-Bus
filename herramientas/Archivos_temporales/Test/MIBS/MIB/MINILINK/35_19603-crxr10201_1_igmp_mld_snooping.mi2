	XF-IGMP-MLD-SNOOPING-MIB DEFINITIONS ::= BEGIN
 
		IMPORTS
			dot1dBasePort
				FROM BRIDGE-MIB
			InetAddress, InetAddressType
				FROM INET-ADDRESS-MIB
			dot1qVlanIndex, PortList, VlanIdOrNone
				FROM Q-BRIDGE-MIB
			OBJECT-GROUP
				FROM SNMPv2-CONF
			Integer32, Unsigned32, Gauge32, Counter32, OBJECT-TYPE, 
			MODULE-IDENTITY
				FROM SNMPv2-SMI
			TruthValue, RowStatus
				FROM SNMPv2-TC
			xfEthernetBridge
				FROM XF-TOP-MIB;


		-- 1.3.6.1.4.1.193.81.4.1.5
		xfIgmpMldSnoopingMib MODULE-IDENTITY 
			LAST-UPDATED "201601081000Z"		-- January 08, 2016 at 10:00 GMT
			ORGANIZATION 
				"Ericsson"
			CONTACT-INFO 
				" "
			DESCRIPTION 
				"The MIB module for IGMP snooping (IPv4) and MLD snooping (IPv6) 
				management in MINI-LINK TN/CN/LH."
			REVISION "201601081000Z"		-- January 08, 2016 at 10:00 GMT
			DESCRIPTION 
				"A:  Specified the default value for objects:
				     - xfImsIgmpSnoopingEnabled
				     - xfImsMldSnoopingEnabled
				     - xfIPMcastAlarmEnable
				    Fixed object descriptions."
			REVISION "201508311130Z"		-- August 31, 2015 at 11:30 GMT
			DESCRIPTION 
				"PA2:  Added descriptions and default values."
			REVISION "201503231235Z"		-- March 23, 2015 at 12:35 GMT
			DESCRIPTION 
				"PA1:  Initial revision."
			::= { xfEthernetBridge 5 }


--
-- Node definitions
--

		-- 1.3.6.1.4.1.193.81.4.1.5.1
		xfIgmpMldSnoopingObjects OBJECT IDENTIFIER ::= { xfIgmpMldSnoopingMib 1 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.1
		xfIgmpMldSnoopingMIBScalars OBJECT IDENTIFIER ::= { xfIgmpMldSnoopingObjects 1 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.1.1
		xfImsMibVariant OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The version of xfIgmpMldSnoopingMib that the NE supports.
				
				1: Initial version."
			::= { xfIgmpMldSnoopingMIBScalars 1 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.1.2
		xfImsIgmpSnoopingEnabled OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Use this object to enable IGMP snooping on the NE. When IGMP 
				snooping is enabled, IGMP messages are sent to the switch CPU 
				for processing. Otherwise, the IGMP messages are forwarded 
				as ordinary multicast packets.
				
				Note: while IGMP snooping is disabled on the NE, the VLAN-level 
				IGMP snooping settings in xfImsVlanConfigTable take no effect."
			DEFVAL { false }
			::= { xfIgmpMldSnoopingMIBScalars 2 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.1.3
		xfImsIgmpSnoopingOperStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				enabled(1),
				disabled(2),
				disabledLicense(3)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the operational status of IGMP snooping in the NE:
				
				enabled(1)        : up
				disabled(2)       : down, administratively disabled
				disabledLicense(3): down, feature license missing"
			::= { xfIgmpMldSnoopingMIBScalars 3 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.1.4
		xfImsMldSnoopingEnabled OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Use this object to enable MLD snooping on the NE. When MLD 
				snooping is enabled, MLD messages are sent to the switch CPU 
				for processing. Otherwise, the MLD messages are forwarded as 
				ordinary multicast packets.
				
				Note: while MLD snooping is disabled on the NE, the VLAN-level
				MLD snooping settings in xfImsVlanConfigTable take no effect."
			DEFVAL { false }
			::= { xfIgmpMldSnoopingMIBScalars 4 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.1.5
		xfImsMldSnoopingOperStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				enabled(1),
				disabled(2),
				disabledLicense(3)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the operational status of MLD snooping in the NE:
				
				enabled(1)        : up
				disabled(2)       : down, administratively disabled
				disabledLicense(3): down, feature license missing"
			::= { xfIgmpMldSnoopingMIBScalars 5 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.1.6
		xfImsResetCounters OBJECT-TYPE
			SYNTAX INTEGER { resetCounters(1) }
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Set this object to resetCounters(1) to clear all counters 
				in xfImsVlanPortStatTable. Both IGMP and MLD counters are reset."
			::= { xfIgmpMldSnoopingMIBScalars 6 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.2
		xfIgmpMldSnoopingConfig OBJECT IDENTIFIER ::= { xfIgmpMldSnoopingObjects 2 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.2.1
		xfImsVlanConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfImsVlanConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table is used to enable IGMP snooping and/or MLD snooping
				on individual VLANs. The VLAN can be single-tagged or double-tagged.
				
				This table is indexed by:
				  xfImsVlanAddressType: the snooped protocol, ie. IGMP or MLD.
				    The NE maintains independent state for IGMP and MLD on a
				    particular VLAN.
				
				  dot1qVlanIndex: outer (provider) VLAN ID.
				    Mandatory. Must correspond to one of the entries in 
				    dot1qVlanCurrentTable.
				
				  xfImsVlanInnerTag: inner (customer) VLAN ID.
				    Optional. Set to 0 if IGMP/MLD packets have no inner tag.
				
				Note: the VLAN-level settings of this table do not take effect
				until IGMP, resp. MLD snooping is enabled globally by the
				xfImsIgmpSnoopingEnabled and xfImsMldSnoopingEnabled objects."
			::= { xfIgmpMldSnoopingConfig 1 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.2.1.1
		xfImsVlanConfigEntry OBJECT-TYPE
			SYNTAX XfImsVlanConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry representing a VLAN on which IGMP snooping or 
				MLD snooping is enabled."
			INDEX { xfImsVlanAddressType, dot1qVlanIndex, xfImsVlanInnerTag }
			::= { xfImsVlanConfigTable 1 }


		XfImsVlanConfigEntry ::=
			SEQUENCE { 
				xfImsVlanAddressType
					InetAddressType,
				xfImsVlanInnerTag
					VlanIdOrNone,
				xfImsVlanProxyReporting
					TruthValue,
				xfImsVlanConfQueryInterval
					Unsigned32,
				xfImsVlanConfRobustnessVariable
					Unsigned32,
				xfImsVlanImmediateLeave
					PortList,
				xfImsVlanRowStatus
					RowStatus
			 }

		-- 1.3.6.1.4.1.193.81.4.1.5.1.2.1.1.1
		xfImsVlanAddressType OBJECT-TYPE
			SYNTAX InetAddressType
				{
				ipv4(1),
				ipv6(2)
				}
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The address type of the multicast protocol to which this
				table entry pertains:
				  ipv4(1) : IGMP
				  ipv6(2) : MLD
				
				This object along with dot1qVlanIndex and xfImsVlanInnerTag
				acts as the index to xfImsVlanConfigTable. A VLAN may be
				configured in multiple modes concurrently, e.g., in IPv4
				and IPv6 modes; however, the traffic is considered to be
				logically separate."
			::= { xfImsVlanConfigEntry 1 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.2.1.1.2
		xfImsVlanInnerTag OBJECT-TYPE
			SYNTAX VlanIdOrNone
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The inner C-VLAN tag of the VLAN on which IGMP or MLD snooping 
				is enabled.
				Setting this object to 0 indicates that no customer 
				vlan tag is present (single-tagged VLAN)."
			::= { xfImsVlanConfigEntry 2 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.2.1.1.3
		xfImsVlanProxyReporting OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object specifies whether IGMP or MLD Proxy Reporting 
				is enabled on this VLAN.
				
				When Proxy Reporting is enabled, IGMP/MLD join reports for 
				existing IGMP states are suppressed. Only the first IGMP join 
				message is propagated towards IGMP routers. In addition, 
				IGMP leave messages are forwarded from hosts to the routers 
				only when the last member leaves a group.
				
				When Proxy Reporting is disabled the switch performs snooping 
				by transparently forwarding all IGMP or MLD messages between 
				hosts and routers."
			DEFVAL { false }
			::= { xfImsVlanConfigEntry 3 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.2.1.1.4
		xfImsVlanConfQueryInterval OBJECT-TYPE
			SYNTAX Unsigned32 (1..31744)
			UNITS "seconds"
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object configures the Query Interval to be used on
				this VLAN if the Querier router is IGMPv2 or MLDv1.
				
				If the Querier router is IGMPv3 or MLDv2, the switch uses the
				Query Interval received in the IGMP or MLD General Query
				and this value is ignored.
				Read xfImsVlanQueryInterval for the actual value used."
			DEFVAL { 125 }
			::= { xfImsVlanConfigEntry 4 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.2.1.1.5
		xfImsVlanConfRobustnessVariable OBJECT-TYPE
			SYNTAX Unsigned32 (2..7)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object configures the Robustness Variable to be used on
				this VLAN if the Querier router is IGMPv2 or MLDv1.
				
				If the Querier router is IGMPv3 or MLDv2, the switch uses the
				Robustness Variable received in the IGMP or MLD General Query
				and this value is ignored.
				Read xfImsVlanRobustnessVariable for the actual value used."
			DEFVAL { 2 }
			::= { xfImsVlanConfigEntry 5 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.2.1.1.6
		xfImsVlanImmediateLeave OBJECT-TYPE
			SYNTAX PortList
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Use this object to enable Immediate Leave processing
				on individual ports of this VLAN.
				
				If Immediate Leave handling is enabled: upon receiving an 
				IGMP Leave or MLD Done message, the NE immediately removes 
				the port from the forwarding table entry for that multicast 
				group without sending out an IGMP or MLD group-specific query 
				and waiting for its timeout, to verify if there are other members.
				
				Note: enable immediate-leave only on VLAN ports to which 
				a single host is attached. Otherwise, hosts can be dropped
				unintentionally.
				
				The default value of this object is a string of zeros of
				appropriate length disabling immediate leave processing on
				all ports."
			::= { xfImsVlanConfigEntry 6 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.2.1.1.7
		xfImsVlanRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object is used to create and delete a row
				in xfImsVlanConfigTable."
			::= { xfImsVlanConfigEntry 7 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3
		xfIgmpMldSnoopingStatus OBJECT IDENTIFIER ::= { xfIgmpMldSnoopingObjects 3 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.1
		xfImsVlanStatusTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfImsVlanStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table containing information regarding the IGMP or MLD Querier 
				and the group/mrouter port count in VLANs on which IGMP or MLD 
				snooping is enabled."
			::= { xfIgmpMldSnoopingStatus 1 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.1.1
		xfImsVlanStatusEntry OBJECT-TYPE
			SYNTAX XfImsVlanStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry representing a VLAN on which IGMP snooping or 
				MLD snooping is enabled."
			INDEX { xfImsVlanAddressType, dot1qVlanIndex, xfImsVlanInnerTag }
			::= { xfImsVlanStatusTable 1 }


		XfImsVlanStatusEntry ::=
			SEQUENCE { 
				xfImsVlanQuerierAddress
					InetAddress,
				xfImsVlanQuerierVersion
					INTEGER,
				xfImsVlanQueryInterval
					Unsigned32,
				xfImsVlanQueryMaxResponseTime
					Unsigned32,
				xfImsVlanRobustnessVariable
					Unsigned32,
				xfImsVlanGroupCount
					Gauge32,
				xfImsVlanRouterPortCount
					Gauge32
			 }

		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.1.1.1
		xfImsVlanQuerierAddress OBJECT-TYPE
			SYNTAX InetAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The IP address of the IGMP or MLD Querier on this VLAN.
				The InetAddressType, e.g., IPv4 or IPv6, is indicated by
				the xfImsVlanAddressType in the index of this row."
			::= { xfImsVlanStatusEntry 1 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.1.1.2
		xfImsVlanQuerierVersion OBJECT-TYPE
			SYNTAX INTEGER
				{
				unknown(0),
				v1(1),
				v2(2),
				v3(3)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the IGMP (MLD) protocol version of the
				Querier router for the VLAN. The version of the Querier is
				determined by the type of the IGMP (MLD) General Query
				received by the NE."
			::= { xfImsVlanStatusEntry 2 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.1.1.3
		xfImsVlanQueryInterval OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The frequency at which IGMP or MLD General Query packets are
				transmitted by the Querier router on this VLAN.
				
				The NE uses this value together with
				xfImsVlanRobustnessVariable and xfImsVlanQueryMaxResponseTime
				to calculate the group membership timeout, the length of time
				a port is kept in the forwarding state for a multicast group
				without receiving an IGMP or MLD Join message."
			::= { xfImsVlanStatusEntry 3 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.1.1.4
		xfImsVlanQueryMaxResponseTime OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The maximum Query Response Interval advertised in MLD or IGMP 
				General queries by the Querier router of this VLAN."
			::= { xfImsVlanStatusEntry 4 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.1.1.5
		xfImsVlanRobustnessVariable OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This objects indicates the Robustness Variable used by 
				IGMP or MLD snooping on this VLAN."
			::= { xfImsVlanStatusEntry 5 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.1.1.6
		xfImsVlanGroupCount OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The current number of IPv4 or IPv6 entries for this VLAN 
				in xfImsMulticastGroupTable."
			::= { xfImsVlanStatusEntry 6 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.1.1.7
		xfImsVlanRouterPortCount OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The current number of IPv4 or IPv6 multicast router (mrouter)
				ports for this VLAN in xfImsMulticastRouterPortTable."
			::= { xfImsVlanStatusEntry 7 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.2
		xfImsMulticastRouterPortTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfImsMulticastRouterPortEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table listing the multicast router (mrouter) ports
				on VLANs that have IGMP or MLD snooping enabled.
				
				An mrouter port is a switch port that directly or indirectly
				connects to a multicast router. Mrouter ports are detected
				dynamically by the reception of IGMP/MLD General Query or
				PIMv2 hello."
			::= { xfIgmpMldSnoopingStatus 2 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.2.1
		xfImsMulticastRouterPortEntry OBJECT-TYPE
			SYNTAX XfImsMulticastRouterPortEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry representing a multicast router port."
			INDEX { xfImsVlanAddressType, dot1qVlanIndex, xfImsVlanInnerTag, dot1dBasePort }
			::= { xfImsMulticastRouterPortTable 1 }


		XfImsMulticastRouterPortEntry ::=
			SEQUENCE { 
				xfImsMulticastRouterPortUpTime
					Unsigned32,
				xfImsMulticastRouterPortExpiryTime
					Unsigned32
			 }

		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.2.1.1
		xfImsMulticastRouterPortUpTime OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The time elapsed since this entry was created."
			::= { xfImsMulticastRouterPortEntry 1 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.2.1.2
		xfImsMulticastRouterPortExpiryTime OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "seconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This value represents the time remaining before this entry 
				expires. The timer is reset by the reception of General Query
				or PIMv2 hello messages."
			::= { xfImsMulticastRouterPortEntry 2 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.3
		xfImsMulticastGroupTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfImsMulticastGroupEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The (conceptual) table listing the IP multicast groups and
				channels for which there are active IGMP or MLD receivers.
				Multicast groups/channels are maintained per VLAN.
				
				An entry is automatically created when the NE receives the first
				IGMP or MLD Join message for a (*;G) group or an (S;G) channel.
				The entry is removed after the last host Leaves the group or 
				the entry times out."
			::= { xfIgmpMldSnoopingStatus 3 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.3.1
		xfImsMulticastGroupEntry OBJECT-TYPE
			SYNTAX XfImsMulticastGroupEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry representing an IP multicast group or channel."
			INDEX { xfImsVlanAddressType, dot1qVlanIndex, xfImsVlanInnerTag, xfImsMulticastGroupAddress, xfImsMulticastGroupSourceAddress
				 }
			::= { xfImsMulticastGroupTable 1 }


		XfImsMulticastGroupEntry ::=
			SEQUENCE { 
				xfImsMulticastGroupAddress
					InetAddress,
				xfImsMulticastGroupSourceAddress
					InetAddress,
				xfImsMulticastGroupEgressPorts
					PortList
			 }

		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.3.1.1
		xfImsMulticastGroupAddress OBJECT-TYPE
			SYNTAX InetAddress
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This object specifies the IP address of the multicast group.
				Type of this address is indicated by xfImsVlanAddressType
				in the index of this row."
			::= { xfImsMulticastGroupEntry 1 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.3.1.2
		xfImsMulticastGroupSourceAddress OBJECT-TYPE
			SYNTAX InetAddress
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This object specifies the IP address of the source of the
				multicast traffic for this group. A value of zero for this
				object indicates that the multicast traffic to this group may
				arrive from any source. Type of this address is indicated 
				by xfImsVlanAddressType in the index of this row."
			::= { xfImsMulticastGroupEntry 2 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.3.1.3
		xfImsMulticastGroupEgressPorts OBJECT-TYPE
			SYNTAX PortList
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The set of ports, in this VLAN, to which packets destined
				to this multicast group's IP address (and received from
				xfImsMulticastGroupSourceAddress) are currently being
				explicitly forwarded. Group member ports are learned by the
				reception of IGMP or MLD Join messages.
				
				This object does not include ports, to which this multicast
				group is only implicitly forwarded, i.e., the multicast router
				ports in xfImsMulticastRouterPortTable."
			::= { xfImsMulticastGroupEntry 3 }


		-- 1.3.6.1.4.1.193.81.4.1.5.1.3.4
		xfImsMulticastGroupPortTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfImsMulticastGroupPortEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The (conceptual) table listing the bridge ports that are members 
				of a particular IP multicast group or channel.
				The same port set is indicated in xfImsMulticastGroupEgressPorts."
			::= { xfIgmpMldSnoopingStatus 4 }


