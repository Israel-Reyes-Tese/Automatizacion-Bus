    SYNTAX          INTEGER (0..65535)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The remote port number for this SCTP connection."
    ::= { sonusSctpAssocEntry 7 }

sonusSctpAssocShelf OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "A 32-bit integer identifying the shelf
             that the SCTP Connection Group table applies to"
    ::= { sonusSctpAssocEntry 8 }

