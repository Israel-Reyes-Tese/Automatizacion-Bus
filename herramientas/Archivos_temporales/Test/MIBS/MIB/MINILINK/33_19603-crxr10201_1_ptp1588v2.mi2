				""
			::= { xfPtpClockPortStatsEntry 9 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.2.1.10
		xfPtpClockPortStatsDelayRespTx OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				""
			::= { xfPtpClockPortStatsEntry 10 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.2.1.11
		xfPtpClockPortStatsPdelayReqRx OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				""
			::= { xfPtpClockPortStatsEntry 11 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.2.1.12
		xfPtpClockPortStatsPdelayReqTx OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				""
			::= { xfPtpClockPortStatsEntry 12 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.2.1.13
		xfPtpClockPortStatsPdelayRespRx OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				""
			::= { xfPtpClockPortStatsEntry 13 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.2.1.14
		xfPtpClockPortStatsPdelayRespTx OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				""
			::= { xfPtpClockPortStatsEntry 14 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.2.1.15
		xfPtpClockPortStatsSignalingRx OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				""
			::= { xfPtpClockPortStatsEntry 15 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.2.1.16
		xfPtpClockPortStatsSignalingTx OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				""
			::= { xfPtpClockPortStatsEntry 16 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.3
		xfPtpMulticastConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfPtpMulticastConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table is used to configure the parameters that control how a 
				local PTP port establishes a multicast peering relationship with its 
				neighbors.

				Only those PTP ports, configured to multicast transport mode have an entry 
				in this table. For unicast parameters see xfPtpUnicastConfigTable.
				This table holds no entries if xfPtpClockRole = grandmaster.
				"
			::= { xfPtpTableObjects 3 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.3.1
		xfPtpMulticastConfigEntry OBJECT-TYPE
			SYNTAX XfPtpMulticastConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry for a PTP port in unicast mode."
			INDEX { xfPtpClockPortIndex }
			::= { xfPtpMulticastConfigTable 1 }

		XfPtpMulticastConfigEntry ::=
			SEQUENCE { 
				xfPtpMulticastConfigAnnounceInterval
					Integer32,
				xfPtpMulticastConfigAnnounceRctTimeout
					Integer32,
				xfPtpMulticastConfigSyncInterval
					Integer32,
				xfPtpMulticastConfigMinDelayReqInterval
					Integer32
				}

		-- 1.3.6.1.4.1.193.81.2.15.1.3.3.1.1
		xfPtpMulticastConfigAnnounceInterval OBJECT-TYPE
			SYNTAX Integer32 (-3..4)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Use this command to specify the requested mean time interval between successive Announce messages from the master.
				The logarithm to base 2 of the requested mean period between messages, in seconds. 
				* 4 at every 16 seconds
				* 3 at every 8 seconds
				* 2 at every 4 seconds
				* 1 at 0.5 Hz, that is at every 2 seconds
				* 0 at 1 Hz
				* -1 at 2 Hz
				* -2 at 4 Hz
				* -3 at 8 Hz
				Range: (-3..4)
				Default value: 1

				If xfPtpProfile = g82751 then only value -3(8Hz) is allowed.
				"
			DEFVAL { 1 } 
			::= { xfPtpMulticastConfigEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.3.1.2
		xfPtpMulticastConfigAnnounceRctTimeout OBJECT-TYPE
			SYNTAX Integer32 (2..10)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Use this command to specify the number of announce-intervals that have to pass
				without receipt of an Announce message before a timeout event is generated

				VALUE = Number of announce-intervals. 
				Range: (2..10)
				Default value: 3
				"
			DEFVAL { 3 }
			::= { xfPtpMulticastConfigEntry 2 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.3.1.3
		xfPtpMulticastConfigSyncInterval OBJECT-TYPE
			SYNTAX Integer32 (-6..-3)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Use this command to specify the requested mean time interval between successive
				Sync messages from a master.

				VALUE = Number of announce-intervals. 
				Range: (-6..-3)

				The logarithm to base 2 of the requested mean period in seconds. The valid range is from -6 to -3.
				-3 at 8 Hz
				-4 at 16 Hz
				-5 at 32 Hz
				-6 at 64 Hz
				
				Setting a new value on xfPtpMulticastConfigSyncInterval will automatically adjust
				xfPtpMulticastConfigMinDelayReqInterval to the same value.

				If xfPtpProfile = g82751 then only value -4(16Hz) is allowed.
				"
			DEFVAL { -4 }
			::= { xfPtpMulticastConfigEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.3.1.4
		xfPtpMulticastConfigMinDelayReqInterval OBJECT-TYPE
			SYNTAX Integer32 (-6..5)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Use this command to set the minimum delay request transmission interval
				VALUE = The value of the intervals.The valid range is between -6 and 5, but never more
				frequent than the synchronization (xfPtpMulticastConfigSyncInterval). 

				5 at every 32 seconds
				4 at every 16 seconds
				3 at every 8 seconds
				2 at every 4 seconds
				1 at 0.5 Hz, that is at every 2 seconds
				0 at 1 Hz
				-1 at 2 Hz
				-2 at 4 Hz
				-3 at 8 Hz
				-4 at 16 Hz
				-5 at 32 Hz
				-6 at 64 Hz

				If xfPtpProfile = g82751 then only value -4(16Hz) is allowed.
				"
			DEFVAL { -4 }
			::= { xfPtpMulticastConfigEntry 4 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4
		xfPtpUnicastConfigTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfPtpUnicastConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table is used to configure the parameters that control how a 
				local PTP port establishes a unicast peering relationship with its 
				configured masters.

				Only those PTP ports, configured to unicast transport mode have an entry 
				in this table. For multicast parameters see xfPtpMulticastConfigTable.
				This table holds no entries if xfPtpClockRole = grandmaster.
				"
			::= { xfPtpTableObjects 4 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1
		xfPtpUnicastConfigEntry OBJECT-TYPE
			SYNTAX XfPtpUnicastConfigEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry for a PTP port in unicast mode."
			INDEX { xfPtpClockPortIndex }
			::= { xfPtpUnicastConfigTable 1 }

		XfPtpUnicastConfigEntry ::=
			SEQUENCE { 
				xfPtpUnicastConfigQueryInterval
					Integer32,
				xfPtpUnicastConfigDuration
					Unsigned32,
				xfPtpUnicastConfigAnnounceInterval
					Integer32,
				xfPtpUnicastConfigAnnounceRctTimeout
					Integer32,
				xfPtpUnicastConfigSyncInterval
					Integer32,
				xfPtpUnicastConfigDelayRespInterval
					Integer32,
				xfPtpUnicastConfigSyncRctTimeout
					Unsigned32,
				xfPtpUnicastConfigDelayRespRctTimeout
					Unsigned32,
				xfPtpUnicastConfigWaitToRestoreTime
					Unsigned32
				}

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1.1
		xfPtpUnicastConfigQueryInterval OBJECT-TYPE
			SYNTAX Integer32 (0..7)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The logarithm to the base 2 of the mean interval in seconds between 
				requests from a node for a unicast Announce/Sync/DelayResp message.

				Range: (0..7)
				Default: 0"
			DEFVAL { 0 }
			::= { xfPtpUnicastConfigEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1.2
		xfPtpUnicastConfigDuration OBJECT-TYPE
			SYNTAX Unsigned32 (10..1000)
			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The requested number of seconds for which the requested messages
				shall be transmitted.

				Applicable for Announce, Sync and Delay Response messages.

				Range: (10..1000) seconds
				Default: 300 seconds"
			DEFVAL { 300 }
			::= { xfPtpUnicastConfigEntry 2 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1.3
		xfPtpUnicastConfigAnnounceInterval OBJECT-TYPE
			SYNTAX Integer32 (-3..4)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object specifies the requested Announce message 
				transmission interval from a unicast master.

				The logarithm to base 2 of the requested mean period between messages, in seconds. 

				* 4 at every 16 seconds
				* 3 at every 8 seconds
				* 2 at every 4 seconds
				* 1 at 0.5 Hz, that is at every 2 seconds
				* 0 at 1 Hz
				* -1 at 2 Hz
				* -2 at 4 Hz
				* -3 at 8 Hz

				Range: (-3..4)
				Default: 1"
			DEFVAL { 1 }
			::= { xfPtpUnicastConfigEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1.4
		xfPtpUnicastConfigAnnounceRctTimeout OBJECT-TYPE
			SYNTAX Integer32 (2..10)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Use this command to specify the number of announce-intervals that have to pass 
				without receipt of an Announce message before a timeout event is generated.

				Value: Number of announce-intervals. 

				Range: (2..10)
				Default: 3"
			DEFVAL { 3 }
			::= { xfPtpUnicastConfigEntry 4 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1.5
		xfPtpUnicastConfigSyncInterval OBJECT-TYPE
			SYNTAX Integer32 (-6..-3)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object specifies the requested Sync message 
				transmission interval from a unicast master.

				The value is the logarithm (base 2) of the requested mean period in
				seconds, between the requested messages.
				* -3 at 8 Hz
				* -4 at 16 Hz
				* -5 at 32 Hz
				* -6 at 64 Hz

				Range: (-6..-3)
				Default: -4"
			DEFVAL { -4 }
			::= { xfPtpUnicastConfigEntry 5 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1.6
		xfPtpUnicastConfigDelayRespInterval OBJECT-TYPE
			SYNTAX Integer32 (-6..6)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object specifies the requested Delay Response message 
				transmission interval from a unicast master.
				Unicast Master may ignore the excess Delay Request messages.

				The value is the logarithm (base 2) of the requested mean period in
				seconds, between the requested messages.

				Range: (-6..6)
				Default: -4"
			DEFVAL { -4 }
			::= { xfPtpUnicastConfigEntry 6 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1.7
		xfPtpUnicastConfigSyncRctTimeout OBJECT-TYPE
			SYNTAX Unsigned32 (2..64)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Number of Sync Interval that has to pass without receipt 
				of a Sync message before a timeout event is generated.

				Range: 2..64
				Default: 3"
			DEFVAL { 3 }           
			::= { xfPtpUnicastConfigEntry 7 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1.8
		xfPtpUnicastConfigDelayRespRctTimeout OBJECT-TYPE
			SYNTAX Unsigned32 (2..64)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Number of Delay Response Interval that has to pass without receipt 
				of a Delay Response message before a timeout event is generated.

				Range: 2..64
				Default: 3"
			DEFVAL { 3 }
			::= { xfPtpUnicastConfigEntry 8 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.4.1.9
		xfPtpUnicastConfigWaitToRestoreTime OBJECT-TYPE
			SYNTAX Unsigned32 (0..12)
			UNITS "minutes"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"A previously failed synchronisation source will be considered 
				as available if it is fault-free for a certain period of time.
				That time is specified in this attribute, as number of minutes.

				Range: (0..12)
				Default: 5
				"
			DEFVAL { 5 }
			::= { xfPtpUnicastConfigEntry 9 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.5
		xfPtpUnicastMasterTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfPtpUnicastMasterEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table holds the unicast master candidates.
				Entries can be added and removed by means of xfPtpUnicastMasterRowStatus.
				The table can hold up to a maximum of 20 unicast master candidates.
				
				Entries can only be added if xfPtpClockRole != grandmaster AND 
				xfPtpDeviceType != transparentClock AND xfPtpMessageTransportMode = unicast."
			::= { xfPtpTableObjects 5 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1
		xfPtpUnicastMasterEntry OBJECT-TYPE
			SYNTAX XfPtpUnicastMasterEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in this table represents a unicast master candidate."
			INDEX { xfPtpClockPortIndex, xfPtpUnicastMasterIndex }
			::= { xfPtpUnicastMasterTable 1 }
		
		XfPtpUnicastMasterEntry ::=
			SEQUENCE { 
				xfPtpUnicastMasterIndex
					INTEGER,
				xfPtpUnicastMasterName
					DisplayString,
				xfPtpUnicastMasterNetworkProtocol
					XfPtpNetworkProtocol,
				xfPtpUnicastMasterAddressType
					InetAddressType,
				xfPtpUnicastMasterIpAddress
					InetAddress,
				xfPtpUnicastMasterGateway
					InetAddress,
				xfPtpUnicastMasterMacAddress
					MacAddress,
				xfPtpUnicastMasterLocalPriority
					Unsigned32,
				xfPtpUnicastMasterEnabled
					TruthValue,
				xfPtpUnicastMasterAccessible
					TruthValue,
				xfPtpUnicastMasterClockId
					ClockIdentity,
				xfPtpUnicastMasterTsuCapable
					TruthValue,
				xfPtpUnicastMasterBridgePort
					Unsigned32,
				xfPtpUnicastMasterRowStatus
					RowStatus
			 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.1
		xfPtpUnicastMasterIndex OBJECT-TYPE
			SYNTAX INTEGER (1..20)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This value uniquely identifies this entry among the masters of a clock port.

				When creating a new row in this table xfPtpUnicastMasterIndex in the OID must be set
				to the lowest unused value that belongs to the clock port designated by xfPtpClockPortIndex."
			::= { xfPtpUnicastMasterEntry 1 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.2
		xfPtpUnicastMasterName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..20))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object specifies the unicast master name.
				
				The unicast master name cannot be empty and must be unique for each unicast master.
				The unicast master name cannot be changed after a unicast master has been created."
			DEFVAL { "" }	
			::= { xfPtpUnicastMasterEntry 2 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.3
		xfPtpUnicastMasterNetworkProtocol OBJECT-TYPE
			SYNTAX XfPtpNetworkProtocol
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Protocol that will be used for communication to this master: 
				- UDP over IPv4
				- ethernet"
			DEFVAL { udpIpv4 }
			::= { xfPtpUnicastMasterEntry 3 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.4
		xfPtpUnicastMasterAddressType OBJECT-TYPE
			SYNTAX InetAddressType
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The type of IP addresses in this table. Only ipv4 is supported.			
				
				Applicable only if xfPtpUnicastMasterNetworkProtocol = udpIpv4."
			DEFVAL { ipv4 }
			::= { xfPtpUnicastMasterEntry 4 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.5
		xfPtpUnicastMasterIpAddress OBJECT-TYPE
			SYNTAX InetAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"IP address for a unicast master.
				
				Default value: 0.0.0.0 if xfPtpUnicastMasterAddressType = ipv4.
				
				Applicable only if xfPtpUnicastMasterNetworkProtocol = udpIpv4."
			::= { xfPtpUnicastMasterEntry 5 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.6
		xfPtpUnicastMasterGateway OBJECT-TYPE
			SYNTAX InetAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Gateway for a unicast master.
				
				Default value: 0.0.0.0 if xfPtpUnicastMasterAddressType = ipv4.
				Set this value to 0.0.0.0 if the local and master interface is in the same IP subnet.
				
				Applicable only if xfPtpUnicastMasterNetworkProtocol = udpIpv4." 	
			::= { xfPtpUnicastMasterEntry 6 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.7
		xfPtpUnicastMasterMacAddress OBJECT-TYPE
			SYNTAX MacAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"MAC address for a unicast master.
				
				Default value: 00:00:00:00:00:00
				
				Applicable only if xfPtpUnicastMasterNetworkProtocol = ieee8023.
				"
			::= { xfPtpUnicastMasterEntry 7 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.8
		xfPtpUnicastMasterLocalPriority OBJECT-TYPE
			SYNTAX Unsigned32 (1..16)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Master priority set by the user. Highest priority = 1."
			DEFVAL { 8 }
			::= { xfPtpUnicastMasterEntry 8 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.9
		xfPtpUnicastMasterEnabled OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable the entry in unicast master table."
			DEFVAL { false }
			::= { xfPtpUnicastMasterEntry 9 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.10
		xfPtpUnicastMasterAccessible OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Information about accessibility of this master candidate."
			::= { xfPtpUnicastMasterEntry 10 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.11
		xfPtpUnicastMasterClockId OBJECT-TYPE
			SYNTAX ClockIdentity
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object specifies the unicast master clock identity."
			::= { xfPtpUnicastMasterEntry 11 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.12
		xfPtpUnicastMasterTsuCapable OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object specifies if unicast master supports time stamping."
			::= { xfPtpUnicastMasterEntry 12 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.13
		xfPtpUnicastMasterBridgePort OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object specifies the unicast master bridge port."
			::= { xfPtpUnicastMasterEntry 13 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.5.1.14
		xfPtpUnicastMasterRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object is used to create a new row or modify or 
				delete an existing row in this table."
			::= { xfPtpUnicastMasterEntry 14 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.6
		xfPtpUnicastSlaveTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfPtpUnicastSlaveEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table holds the unicast slaves attached to a local clock port.
				Entries are automatically added or deleted by the PTP stack."
			::= { xfPtpTableObjects 6 }

		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.6.1
		xfPtpUnicastSlaveEntry OBJECT-TYPE
			SYNTAX XfPtpUnicastSlaveEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry for a unicast slave attached to a local clock port.

				Note: earlier implementations of this table support only a single index
				element (xfPtpClockPortIndex) in the OID."
			INDEX { xfPtpClockPortIndex, xfPtpUnicastSlaveIndex }
			::= { xfPtpUnicastSlaveTable 1 }
		
		XfPtpUnicastSlaveEntry ::=
			SEQUENCE { 
				xfPtpUnicastSlaveIndex
					INTEGER, 
				xfPtpUnicastSlaveAddress
					DisplayString,
				xfPtpUnicastSlaveSyncInterval
					Integer32,
				xfPtpUnicastSlaveAnnounceInterval
					Integer32
			 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.6.1.1
		xfPtpUnicastSlaveIndex OBJECT-TYPE
			SYNTAX INTEGER (1..65535)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A unique value, greater than zero, for each instance."
			::= { xfPtpUnicastSlaveEntry 1 }
		
		-- 1.3.6.1.4.1.193.81.2.15.1.3.6.1.2
		xfPtpUnicastSlaveAddress OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..255))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object specifies the unicast slave address.
				
				Depending on configuration this address can be displayed as:
				- IP address	(xfPtpClockPortEncapsulationType = udpIpv4)
				- MAC address	(xfPtpClockPortEncapsulationType = ieee8023)
				"
