        xfL1BondingMappingPcp OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE(1))
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The VLAN tag PCP values mapped to this port. For any given bonding each value should only appear once
                and all values should appear for an interface."
            ::= { xfL1BondingMappingEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.16.2.1.3
        xfL1BondingMappingDscp OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE(8))
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
