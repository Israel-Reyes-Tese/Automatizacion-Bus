			SYNTAX BITS
				{
				unknown(0),
				wrongMacAddress(1),
				linkDegraded(2),
				delayTooHigh(3),
				delayTooLow(4),
				noMultiFrame(5),
				unsupportedProtocol(6),
				noControlFrameReceived(7),
				duplicatedLink(8),
				remoteAcceptDataNotRecived(9),
				rxDataNotReceived(10)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This objects indicates the status of the IM near-end interface."
			::= { xfBridgeIMLinkEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.5.1.4
