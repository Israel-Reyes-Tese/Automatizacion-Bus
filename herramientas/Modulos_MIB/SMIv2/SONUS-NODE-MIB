sonusNodeSlotAdmnTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusNodeSlotAdmnEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The node slot admn table provides physical manipulation
             of a slot location in a shelf."
    ::= { sonusNode 12 }

sonusNodeSlotAdmnEntry OBJECT-TYPE
    SYNTAX          SonusNodeSlotAdmnEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusNodeSlotAdmnShelfIndex, 
                      sonusNodeSlotAdmnSlotIndex }
    ::= { sonusNodeSlotAdmnTable 1 }

SonusNodeSlotAdmnEntry ::=
    SEQUENCE {
        sonusNodeSlotAdmnShelfIndex     SonusShelfIndex,
        sonusNodeSlotAdmnSlotIndex      SonusSlotIndex,
        sonusNodeSlotAdmnReset          INTEGER
    }

