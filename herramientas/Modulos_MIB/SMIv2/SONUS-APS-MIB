    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Textual name for the APS group."
    ::= { sonusApsNameMapEntry 1 }

sonusApsNameMapUserName OBJECT-TYPE
    SYNTAX          SonusName
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "User-assigned textual name for the APS group."
    ::= { sonusApsNameMapEntry 2 }

