    ::= { sonusNrsSubInterfaceEntry 21 }

sonusNrsSubInterfaceVLanPBucketSize OBJECT-TYPE
    SYNTAX          Integer32 (16..65520)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The per Logical Interface/VLAN policer bucket size, in bytes,
             used to policing all signaling traffic at the logical interface level.
             The rate is 16 bytes to 65520 bytes, in 16 byte increments.  0 to 128
             pseudo-packets (512 bytes) in 1/32 packet increments."
    ::= { sonusNrsSubInterfaceEntry 22 }

sonusNrsSubInterfaceVLanPFillRate OBJECT-TYPE
    SYNTAX          Integer32 (256..16776960)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The per Logical Interface/VLAN policer fill rate, in bytes
             per second, used to policing all signaling traffic at the logical
             interface level.  The range is 256 bytes/sec to 16MBytes/second 
             in increments of 256 bytes/sec. 0.5 to 32K pseudo-packets/second
             in increments of 0.5 packets per second."
    ::= { sonusNrsSubInterfaceEntry 23 }

sonusNrsSubInterfaceIpSecPolicyName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..23))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The name of the IPsec Policy assigned to this SIF. Null if IPsec 
             disabled."
    ::= { sonusNrsSubInterfaceEntry 24 }

