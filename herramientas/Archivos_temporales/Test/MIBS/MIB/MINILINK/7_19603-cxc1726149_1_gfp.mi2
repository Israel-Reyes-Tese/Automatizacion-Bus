    --------------------------------------------------------------
    xfGfpStatisticsFrameCountsTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF XfGfpStatisticsFrameCountsEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "This table contains the GFP statistics frame count records of
            GFP interfaces."
        ::= { xfGfpStatistics 1 }

    xfGfpStatisticsFrameCountsEntry OBJECT-TYPE
        SYNTAX     XfGfpStatisticsFrameCountsEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "This entry contains a GFP statistics record for one GFP
            interface."
        INDEX { ifIndex }
        ::= { xfGfpStatisticsFrameCountsTable 1 }

    XfGfpStatisticsFrameCountsEntry ::= SEQUENCE
    {
        xfGfpStatisticsClientFramesReceived         Counter64,
        xfGfpStatisticsClientFramesSent             Counter64,
        xfGfpStatisticsMngtFramesReceived           Counter32,
        xfGfpStatisticsMngtFramesSent               Counter32,
        xfGfpStatisticsReset                        INTEGER        
    }

    xfGfpStatisticsClientFramesReceived OBJECT-TYPE
        SYNTAX     Counter64
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
            "This is the count of client frames received."
        ::= { xfGfpStatisticsFrameCountsEntry 1 }

    xfGfpStatisticsClientFramesSent OBJECT-TYPE
        SYNTAX     Counter64
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
            "This is the count of client frames sent."
        ::= { xfGfpStatisticsFrameCountsEntry 2 }

    xfGfpStatisticsMngtFramesReceived OBJECT-TYPE
        SYNTAX     Counter32
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
            "This is the count of management frames received."
        ::= { xfGfpStatisticsFrameCountsEntry 3 }

    xfGfpStatisticsMngtFramesSent OBJECT-TYPE
        SYNTAX     Counter32
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
            "This is the count of management frames sent."
        ::= { xfGfpStatisticsFrameCountsEntry 4 }

    xfGfpStatisticsReset OBJECT-TYPE
        SYNTAX INTEGER { reset(1) }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "This object resets all packet related statistics in xfGfpStatisticsFrameCountsTable."
        ::= { xfGfpStatisticsFrameCountsEntry 5 }

    --------------------------------------------------------------
    -- GFP Performance monitoring configuration table.
    --------------------------------------------------------------
    xfGfpPmConfigTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF XfGfpPmConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "This table contains the GFP configuration entries for all GFP
            interfaces."
        ::= { xfGfpPm 1 }

    xfGfpPmConfigEntry OBJECT-TYPE
        SYNTAX     XfGfpPmConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "The entry contains GFP configuration parameters for one GFP
            interface."
        INDEX { ifIndex }
        ::= { xfGfpPmConfigTable 1 }

    XfGfpPmConfigEntry ::= SEQUENCE
    {
        xfGfpPmEnable                             TruthValue,
    xfGfpPmResetCurrent15MinPerfRegisters       TruthValue,
    xfGfpPmResetCurrent24HourPerfRegisters      TruthValue
    }

    xfGfpPmEnable OBJECT-TYPE
        SYNTAX     TruthValue
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
           "This object is used to enable or disable GFP performance
           monitoring."
        DEFVAL { false }
        ::= { xfGfpPmConfigEntry 1 }


    xfGfpPmResetCurrent15MinPerfRegisters OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "This is a trigger command. If set to on, the values of the current 15 minute registers
        are set to 0. A Get response will always report the value off. Via the MO
	xfGfpPmConfigTableLastChange the agent might check whether this command has worked or not.
        "
    ::= { xfGfpPmConfigEntry 2 }

    xfGfpPmResetCurrent24HourPerfRegisters OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "This is a trigger command. If set to on, the values of the current 15 minute registers
        are set to 0. A Get response will always report the value off. Via the MO
	xfGfpPmConfigTableLastChange the agent might check whether this command has worked or not.
        "
    ::= { xfGfpPmConfigEntry 3 }

    xfGfpPmConfigTableLastChange  OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The value of sysUpTime at the time of the last
        creation, deletion or modification of an entry
        in the xfRsTtpPerfConfigTable. This query is helpful in case of
        resetting the current performance registers. The agent has now
        the possibilty to check whether these trigger commands have worked or not.
        If the xfGfpPerfConfigTable entries has been unchanged
        since the last re-initialization of the local network
        management subsystem, then this object contains a zero value."
    ::= { xfGfpPm 2 }

