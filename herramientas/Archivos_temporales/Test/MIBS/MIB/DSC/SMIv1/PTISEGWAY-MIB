    SEQUENCE {
        ptDSCIncomingADNRoutingRecordRecordType       INTEGER,
        ptDSCIncomingADNRoutingRecordAdjacentDiameterNode                 DisplayString,
        ptDSCIncomingADNRoutingRecordNextTableName          DisplayString,
        ptDSCIncomingADNRoutingRecordModificationTableName                  DisplayString,
        ptDSCIncomingADNRoutingRecordDSCNodeID      INTEGER,
        ptDSCIncomingADNRoutingRecordDSCNodeName        DisplayString,
        ptDSCIncomingADNRoutingRecordDSCIndex     INTEGER,
        ptDSCIncomingADNRoutingRecordDSCRoutingTablesIndex                  INTEGER,
        ptDSCIncomingADNRoutingRecordIncomingADNRoutingTableIndex                         INTEGER,
        ptDSCIncomingADNRoutingRecordParentOID      OBJECT IDENTIFIER,
        ptDSCIncomingADNRoutingRecordRowStatus      RowStatus,
        ptDSCIncomingADNRoutingRecordIndex  INTEGER
    }

ptDSCIncomingADNRoutingRecordRecordType OBJECT-TYPE
    SYNTAX          INTEGER { default(1), value(1000) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Record Type (DEFAULT/VALUE) \nA single non-deleteable DEFAULT record handles routing if no VALUE match is found. \nDefault value: VALUE."
    ::= { ptDSCIncomingADNRoutingRecordEntry 1 }

ptDSCIncomingADNRoutingRecordAdjacentDiameterNode OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Associated Adjacent Diameter Node. \nValid values: empty string (DEFAULT record type) \n FQDN format and empty string (VALUE record type and LONGEST FROM RIGHT ADN search type) \n existing ADN name (VALUE record type and EXACT MATCH ADN search type)."
    ::= { ptDSCIncomingADNRoutingRecordEntry 2 }

ptDSCIncomingADNRoutingRecordNextTableName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Next routing table, result table, default routing, or error handling to route the message. \nDepending on the required routing decision, set to: \n1) The name of the next routing table to continue routing. \n2) The name of the result table to send the message. \n3) dsc_default to apply default routing. \n4) dsc_error to return Result-Code 3002, UNABLE_TO_DELIVER. \n5) dsc_error<error code> to return Result-Code of <error code>. \n6) dsc_error<vendor id>_<error code> to return Experimental-Result with Vendor-Id of <vendor id> and Experimental-Result-Code of <error code>. \n7) dsc_discard to drop the request. \n \nOptional text may be added after a variant of dsc_error, separated by a space, to indicated a desired Error-Message. \nThe next table cannot be a modification table."
    ::= { ptDSCIncomingADNRoutingRecordEntry 3 }

ptDSCIncomingADNRoutingRecordModificationTableName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Associated optional Modification Table to be applied to the message. \nValid values: empty string and existing Modification Table name. \nDefault value: empty string."
    ::= { ptDSCIncomingADNRoutingRecordEntry 4 }

ptDSCIncomingADNRoutingRecordDSCNodeID OBJECT-TYPE
    SYNTAX          INTEGER { any(0) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Internal ID of the associated DSC Node that owns the ADN. \nObsolete as of release 14.3. \nValid values: ANY (DEFAULT record type) \n Existing DSC Node ID (VALUE record type)."
    ::= { ptDSCIncomingADNRoutingRecordEntry 5 }

ptDSCIncomingADNRoutingRecordDSCNodeName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "DSC Node Name of the associated DSC Node which owns the ADN. \nValid values: ANY (DEFAULT record type) \n Existing DSC Node Name (VALUE record type)."
    ::= { ptDSCIncomingADNRoutingRecordEntry 6 }

