            INDEX { dot1dBasePort, dot1qVlanIndex }
            ::= { xfBridgePortVlanGroupMappingTable 1 }

        XfBridgePortVlanGroupMappingEntry ::=
            SEQUENCE {
                xfBridgePortVlanGroup
                    Integer32,
                xfBridgePortVlanGroupMappingRowStatus
                    RowStatus
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.19.1.1
        xfBridgePortVlanGroup OBJECT-TYPE
            SYNTAX Integer32 (1..8)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The VLAN group to which the incoming VLAN is mapped to.

                Default is 1 for all VLANs (i.e. all VLANs are mapped
                to VLAN group 1)."
