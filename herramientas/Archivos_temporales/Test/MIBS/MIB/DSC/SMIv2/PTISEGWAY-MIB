        ptsnamiPortNAMappingPort        Integer32,
        ptsnamiPortNAMappingNA          Integer32,
        ptsnamiPortNAMappingDestinationName   DisplayString,
        ptsnamiPortNAMappingRoutesetTableName     DisplayString,
        ptsnamiPortNAMappingGTTDBDefName DisplayString,
        ptsnamiPortNAMappingPortNAMappingsIndex       Integer32,
        ptsnamiPortNAMappingParentOID   OBJECT IDENTIFIER,
        ptsnamiPortNAMappingRowStatus   RowStatus,
        ptsnamiPortNAMappingIndex       Integer32
    }

ptsnamiPortNAMappingPort OBJECT-TYPE
    SYNTAX          Integer32 (8611..8618)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "TCP/IP port for the SNAMI NA configuration. \nMust be unique. \nValid values: 8611 to 8618."
    ::= { ptsnamiPortNAMappingEntry 1 }

ptsnamiPortNAMappingNA OBJECT-TYPE
    SYNTAX          Integer32 (1..99)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Network Appearance associated with the port (1..99). \nMultiple ports may reuse the same NA."
    ::= { ptsnamiPortNAMappingEntry 2 }

ptsnamiPortNAMappingDestinationName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Destination name associated with the port. \nValid values: uppercase alphanumeric (fixed size of 12)."
    ::= { ptsnamiPortNAMappingEntry 3 }

ptsnamiPortNAMappingRoutesetTableName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Routeset Table name associated with the NA. \nDefault value: STANDARD \nValid values: uppercase alphanumeric (maximum size of 8)."
    ::= { ptsnamiPortNAMappingEntry 4 }

ptsnamiPortNAMappingGTTDBDefName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "GTT DB Def name associated with the Port NA. \nDefault value: STANDARD \nValid values: uppercase alphanumeric (maximum size of 15)."
    ::= { ptsnamiPortNAMappingEntry 5 }

ptsnamiPortNAMappingPortNAMappingsIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is PortNAMappings."
    ::= { ptsnamiPortNAMappingEntry 242 }

ptsnamiPortNAMappingParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptsnamiPortNAMappingEntry 253 }

