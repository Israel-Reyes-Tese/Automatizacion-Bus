sonusDsx1StatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDsx1StatEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Table of Sonus Enterprise Status for all DS1s"
    ::= { sonusDsx1MIBObjects 2 }

sonusDsx1StatEntry OBJECT-TYPE
    SYNTAX          SonusDsx1StatEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Entry in Table consisting of the all the Sonus Enterprise DS1 Status"
    INDEX           { sonusDsx1StatShelfIndex, sonusDsx1StatSlotIndex, 
                      sonusDsx1StatPortIndex, sonusDsx1StatDs3Index, 
                      sonusDsx1StatDs1Index }
    ::= { sonusDsx1StatTable 1 }

SonusDsx1StatEntry ::=
    SEQUENCE {
        sonusDsx1StatShelfIndex         SonusShelfIndex,
        sonusDsx1StatSlotIndex          SonusSlotIndex,
        sonusDsx1StatPortIndex          INTEGER,
        sonusDsx1StatDs3Index           INTEGER,
        sonusDsx1StatDs1Index           INTEGER,
        sonusDsx1StatValidInt           INTEGER,
        sonusDsx1StatRxSlips            INTEGER,
        sonusDsx1StatTxSlips            INTEGER,
        sonusDsx1StatLOF                INTEGER,
        sonusDsx1StatAIS                INTEGER,
        sonusDsx1StatRAI                INTEGER,
        sonusDsx1StatRED                INTEGER,
        sonusDsx1StatCRC                INTEGER,
        sonusDsx1StatCOFA               INTEGER,
        sonusDsx1StatFBE                INTEGER,
        sonusDsx1StatStatus             DisplayString,
        sonusDsx1StatAllocChannels      DisplayString,
        sonusDsx1StatEnabChannels       DisplayString,
        sonusE1StatLOMF                 INTEGER,
        sonusE1StatMFError              INTEGER,
        sonusE1StatFEBE                 INTEGER,
        sonusE1StatOOOF                 INTEGER,
        sonusE1StatC2NCIWCK             INTEGER,
        sonusE1StatRAIContCRC           INTEGER,
        sonusE1StatContFEBE             INTEGER,
        sonusE1StatExcessCRC            INTEGER,
        sonusE1StatFASErr               INTEGER,
        sonusDsx1StatSendPattern        INTEGER,
        sonusDsx1StatStatusCode         INTEGER,
        sonusDsx1StatLoopbackStatus     INTEGER,
        sonusDsx1StatOperStatus         INTEGER,
        sonusDsx1StatValidDayInt        INTEGER,
        sonusDsx1StatDefectAIS          INTEGER,
        sonusDsx1StatDefectRAI          INTEGER,
        sonusDsx1StatE1CasAlarmStatus   INTEGER,
        sonusDsx1StatQ50TrunkState      INTEGER,
        sonusDsx1StatQ50RcvTrmSignal    INTEGER,
        sonusDsx1StatQ50XmtTrmSignal    INTEGER,
        sonusDsx1StatQ50Good3kHzCalls   INTEGER,
        sonusDsx1StatQ50Failed3kHzCalls INTEGER,
        sonusDsx1StatQ50Good64kbpsCalls INTEGER,
        sonusDsx1StatQ50Failed64kbpsCalls INTEGER,
        sonusDsx1StatQ50Alloc3khzChannels DisplayString,
        sonusDsx1StatQ50Alloc64kbpsChannels   DisplayString
    }

