    ::= { ptIOHWLSLChannel 1 }

ptIOHWLSLChannelEntry OBJECT-TYPE
    SYNTAX          PtIOHWLSLChannelEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of IOHWLSLChannel attributes."
    INDEX           { ptConnGroupIndex, ptIOHWLSLChannelIOManagerIndex, 
                      ptIOHWLSLChannelLSLChannelsIndex, 
                      ptIOHWLSLChannelIndex }
    ::= { ptIOHWLSLChannelTable 1 }

PtIOHWLSLChannelEntry ::=
    SEQUENCE {
        ptIOHWLSLChannelChannelNumber   Integer32,
        ptIOHWLSLChannelPort            INTEGER,
        ptIOHWLSLChannelTimeSlot        INTEGER,
        ptIOHWLSLChannelBaudRate        INTEGER,
        ptIOHWLSLChannelLoopbackMode    INTEGER,
        ptIOHWLSLChannelMTP3API         INTEGER,
        ptIOHWLSLChannelLSSU            INTEGER,
        ptIOHWLSLChannelFISUAndMSU      INTEGER,
        ptIOHWLSLChannelCLLI            DisplayString,
        ptIOHWLSLChannelSupportDebug    DisplayString,
        ptIOHWLSLChannelL3Use           INTEGER,
        ptIOHWLSLChannelIOManagerIndex  Integer32,
        ptIOHWLSLChannelLSLChannelsIndex Integer32,
        ptIOHWLSLChannelParentOID       OBJECT IDENTIFIER,
        ptIOHWLSLChannelRowStatus       RowStatus,
        ptIOHWLSLChannelIndex           Integer32
    }

ptIOHWLSLChannelChannelNumber OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of the Signaling Channel being mapped"
    ::= { ptIOHWLSLChannelEntry 1 }

ptIOHWLSLChannelPort OBJECT-TYPE
    SYNTAX          INTEGER { port0(0), port1(1), port2(2), port3(3), 
                        port4(4), port5(5), port6(6), port7(7), 
                        monitorPort(8), notConfigured(9), port8(10) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The number of the I/O port with the physical connection to the link"
    ::= { ptIOHWLSLChannelEntry 2 }

ptIOHWLSLChannelTimeSlot OBJECT-TYPE
    SYNTAX          INTEGER { notConfigured(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Timeslot on the link (1-24 T1, 1-31 E1)"
    ::= { ptIOHWLSLChannelEntry 3 }

ptIOHWLSLChannelBaudRate OBJECT-TYPE
    SYNTAX          INTEGER { ds064k(0), ds0a56k(1), baud48k(2), 
                        baud48kRev(3), baudTtc48k(4) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Baud rate for the timeslot. 64K uses all 8 bits. 56K uses 7 of 8 bits and discards the LSB. 48K uses 6 of 8 bits and discards the 2 LSBs. 48K_REV uses 6 of 8 bits and discards the two MSBs. TTC_48K uses 6 of 8 bits and discards the LSB and MSB."
    ::= { ptIOHWLSLChannelEntry 4 }

ptIOHWLSLChannelLoopbackMode OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Redirects incoming data on port and timeslot back to sender (Remote end L2 alignment may be achieved). ENABLED or DISABLED"
    ::= { ptIOHWLSLChannelEntry 6 }

ptIOHWLSLChannelMTP3API OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1), overridden(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Log MTP3 API related information. Set this value at MTP3. Values: LOGGED, NOT_LOGGED. Value OVERRIDDEN indicates the debug FEATURE is NOT_LOGGED."
    ::= { ptIOHWLSLChannelEntry 8 }

ptIOHWLSLChannelLSSU OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1), overridden(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Log LSSU receipt and state changes. Set this value at MTP3. Values: LOGGED, NOT_LOGGED. Value OVERRIDDEN indicates the debug FEATURE is NOT_LOGGED."
    ::= { ptIOHWLSLChannelEntry 9 }

ptIOHWLSLChannelFISUAndMSU OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1), overridden(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Log Sequence Numbers information. Set this value at MTP3. Values: LOGGED, NOT_LOGGED. Value OVERRIDDEN indicates the debug FEATURE is NOT_LOGGED."
    ::= { ptIOHWLSLChannelEntry 10 }

ptIOHWLSLChannelCLLI OBJECT-TYPE
