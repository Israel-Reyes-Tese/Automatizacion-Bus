    ::= { sonusTransportResourcesMIBNotifications 1 }

sonusNodePolicerSetThresholdNotification TRAP-TYPE
    ENTERPRISE      sonusTransportResourcesMIBNotifications
    VARIABLES       { sonusPolicerShelf, sonusPolicerSlot, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that node policer set threshold has been reached."
    ::= 1

sonusNodePolicerClearThresholdNotification TRAP-TYPE
    ENTERPRISE      sonusTransportResourcesMIBNotifications
    VARIABLES       { sonusPolicerShelf, sonusPolicerSlot, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that node policer clear threshold has been reached."
    ::= 2

sonusDBLNPPoolExhaustNotification TRAP-TYPE
    ENTERPRISE      sonusTransportResourcesMIBNotifications
    VARIABLES       { sonusPolicerShelf, sonusPolicerSlot, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that NP policer pool has run out of resource for DBL"
    ::= 3

sonusDBLAddPolicerThresholdNotify TRAP-TYPE
    ENTERPRISE      sonusTransportResourcesMIBNotifications
    VARIABLES       { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that DBL Threshold set for an endpoint has reached"
    ::= 4

