PtSWMExternalPortEntry ::=
    SEQUENCE {
        ptSWMExternalPortSlotNumber     Integer32,
        ptSWMExternalPortPortNumber     Integer32,
        ptSWMExternalPortState          INTEGER,
        ptSWMExternalPortNotification   INTEGER,
        ptSWMExternalPortPortName       DisplayString,
        ptSWMExternalPortSwitchesIndex  Integer32,
        ptSWMExternalPortSwitchIndex    Integer32,
        ptSWMExternalPortParentOID      OBJECT IDENTIFIER,
        ptSWMExternalPortRowStatus      RowStatus,
        ptSWMExternalPortIndex          Integer32
