rsiSasPriorityCallPerfCounters OBJECT-TYPE
    SYNTAX          SEQUENCE OF RsiSasPriorityCallPerfCountersEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table contains the information of Priority Call Perf measurement
             counters on different subsystems.It contains rsiSubsystemID
             from rsiSubsystemTable."
    ::= { rsiCommonMib 69 }

rsiSasPriorityCallPerfCountersEntry OBJECT-TYPE
    SYNTAX          RsiSasPriorityCallPerfCountersEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Description of a Priority Call measurement counter of a particular subsystem."
    INDEX           { rsiSubsystemId }
    ::= { rsiSasPriorityCallPerfCounters 1 }

RsiSasPriorityCallPerfCountersEntry ::=
    SEQUENCE {
        rsiMeasurementPriCallCounterValueId   DisplayString,
        rsiMeasurementPriCallCounterValue INTEGER
    }

rsiMeasurementPriCallCounterValueId OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Measurement Priority Call Counter Identifier"
    ::= { rsiSasPriorityCallPerfCountersEntry 1 }

rsiMeasurementPriCallCounterValue OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is value of rsiMeasurementCounterID/rsiSystemID/rsiMeasurementCounterValueId"
    ::= { rsiSasPriorityCallPerfCountersEntry 2 }

rsiTraps OBJECT IDENTIFIER
    ::= { rsiMib 10 }

rsiCommunicationFailure TRAP-TYPE
    ENTERPRISE      rsiTraps
    VARIABLES       { rsiAlarmId, rsiRelatedAlarmId, rsiAlarmDateTime, 
                      rsiAlarmErrorCode, rsiAlarmContent, 
                      rsiOriginatingSubsystemId, 
                      rsiOriginatingSubsystemName, 
                      rsiTroublingSubsystemId, rsiTroublingSubsystemName, 
                      rsiAlarmSeverity, rsiOrigSystemId, 
                      rsiOrigSystemName, rsiOrigHostId, rsiOrigHostName }
--     STATUS          mandatory
    DESCRIPTION     
            "This defines the alarms corresponding to communication 
             failure between two subsystems"
    ::= 1

rsiConfigurationError TRAP-TYPE
    ENTERPRISE      rsiTraps
    VARIABLES       { rsiAlarmId, rsiRelatedAlarmId, rsiAlarmDateTime, 
                      rsiAlarmErrorCode, rsiAlarmContent, 
                      rsiOriginatingSubsystemId, 
                      rsiOriginatingSubsystemName, 
                      rsiTroublingSubsystemId, rsiTroublingSubsystemName, 
                      rsiAlarmSeverity, rsiOrigSystemId, 
                      rsiOrigSystemName, rsiOrigHostId, rsiOrigHostName }
--     STATUS          mandatory
    DESCRIPTION     
            "This defines the alarms raised because of configuration
             errors of a subsystem. For ex: a directory not found"
    ::= 2

rsiSystemFaults TRAP-TYPE
    ENTERPRISE      rsiTraps
    VARIABLES       { rsiAlarmId, rsiRelatedAlarmId, rsiAlarmDateTime, 
                      rsiAlarmErrorCode, rsiAlarmContent, 
                      rsiOriginatingSubsystemId, 
                      rsiOriginatingSubsystemName, 
                      rsiTroublingSubsystemId, rsiTroublingSubsystemName, 
                      rsiAlarmSeverity, rsiOrigSystemId, 
                      rsiOrigSystemName, rsiOrigHostId, rsiOrigHostName }
--     STATUS          mandatory
    DESCRIPTION     
            "This defines the alarms rasied by subsystems. For ex:
             subsystem is up/down, etc"
    ::= 3

rsiSystemCapacity TRAP-TYPE
    ENTERPRISE      rsiTraps
    VARIABLES       { rsiAlarmId, rsiRelatedAlarmId, rsiAlarmDateTime, 
                      rsiAlarmErrorCode, rsiAlarmContent, 
                      rsiOriginatingSubsystemId, 
                      rsiOriginatingSubsystemName, 
                      rsiTroublingSubsystemId, rsiTroublingSubsystemName, 
                      rsiAlarmSeverity, rsiOrigSystemId, 
                      rsiOrigSystemName, rsiOrigHostId, rsiOrigHostName }
--     STATUS          mandatory
    DESCRIPTION     
            "This defines the alarms corresponding to system capacity 
             overflow. For ex: buffer overflow, max. no of service
             instances exceeded."
    ::= 4

rsiResourceFailure TRAP-TYPE
    ENTERPRISE      rsiTraps
    VARIABLES       { rsiAlarmId, rsiRelatedAlarmId, rsiAlarmDateTime, 
                      rsiAlarmErrorCode, rsiAlarmContent, 
                      rsiOriginatingSubsystemId, 
                      rsiOriginatingSubsystemName, 
                      rsiTroublingSubsystemId, rsiTroublingSubsystemName, 
                      rsiAlarmSeverity, rsiOrigSystemId, 
                      rsiOrigSystemName, rsiOrigHostId, rsiOrigHostName }
--     STATUS          mandatory
    DESCRIPTION     
            "This defines the alarms corresponding to Resource Failure. 
             For ex: database write failed, file open failed, etc"
    ::= 5

rsiApplicationError TRAP-TYPE
    ENTERPRISE      rsiTraps
    VARIABLES       { rsiAlarmId, rsiRelatedAlarmId, rsiAlarmDateTime, 
                      rsiAlarmErrorCode, rsiAlarmContent, 
                      rsiOriginatingSubsystemId, 
                      rsiOriginatingSubsystemName, 
                      rsiTroublingSubsystemId, rsiTroublingSubsystemName, 
                      rsiAlarmSeverity, rsiOrigSystemId, 
                      rsiOrigSystemName, rsiOrigHostId, rsiOrigHostName }
--     STATUS          mandatory
    DESCRIPTION     
            "This defines the alarms corresponding to application errors. 
             For ex: wrong format of input CDR, incomplete input CDR, etc"
    ::= 6

rsiThresholdAlarm TRAP-TYPE
    ENTERPRISE      rsiTraps
    VARIABLES       { rsiAlarmId, rsiRelatedAlarmId, rsiAlarmDateTime, 
                      rsiAlarmErrorCode, rsiAlarmContent, 
                      rsiOriginatingSubsystemId, 
                      rsiOriginatingSubsystemName, 
                      rsiTroublingSubsystemId, rsiTroublingSubsystemName, 
                      rsiAlarmSeverity, rsiOrigSystemId, 
                      rsiOrigSystemName, rsiOrigHostId, rsiOrigHostName }
--     STATUS          mandatory
    DESCRIPTION     
            "This defines the alarms corresponding to threshold errors. 
             For ex: CPU usage exceeded a given threshold"
    ::= 7

rsiSecurityAlarm TRAP-TYPE
    ENTERPRISE      rsiTraps
    VARIABLES       { rsiAlarmId, rsiRelatedAlarmId, rsiAlarmDateTime, 
                      rsiAlarmErrorCode, rsiAlarmContent, 
                      rsiOriginatingSubsystemId, 
                      rsiOriginatingSubsystemName, 
                      rsiTroublingSubsystemId, rsiTroublingSubsystemName, 
                      rsiAlarmSeverity, rsiOrigSystemId, 
                      rsiOrigSystemName, rsiOrigHostId, rsiOrigHostName, 
                      rsiUserId }
--     STATUS          mandatory
    DESCRIPTION     
            "This defines the security alarms. For ex: if a user tries
             to login in for 3 consecutive times, and enters a wrong
             password, then an alarm would be raised."
    ::= 8

