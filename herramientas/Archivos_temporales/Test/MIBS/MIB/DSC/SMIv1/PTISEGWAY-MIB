                        t1Shorthaul0133ft(4), t1Shorthaul133266ft(5), 
                        t1Shorthaul266399ft(6), t1Shorthaul399533ft(7), 
                        t1Shorthaul533655ft(8), t1Longhaul0db(9), 
                        t1Longhaul7db(10), t1Longhaul15db(11), 
