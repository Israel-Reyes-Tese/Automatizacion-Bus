sonusNfsShelfAdmnTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusNfsShelfAdmnEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table specifies the configurable NFS redundancy options common to 
             both management server modules in a chassis."
    ::= { sonusNfs 3 }

sonusNfsShelfAdmnEntry OBJECT-TYPE
    SYNTAX          SonusNfsShelfAdmnEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusNfsShelfAdmnShelfIndex }
    ::= { sonusNfsShelfAdmnTable 1 }

SonusNfsShelfAdmnEntry ::=
    SEQUENCE {
        sonusNfsShelfAdmnShelfIndex     SonusShelfIndex,
        sonusNfsShelfAdmnFallbackCntrl  INTEGER,
        sonusNfsShelfAdmnWaitToRevertTime Integer32
    }

