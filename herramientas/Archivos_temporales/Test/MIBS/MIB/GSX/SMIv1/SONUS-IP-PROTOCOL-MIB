SonusIpSecSadIpSecStatisticsEntry ::=
    SEQUENCE {
        sonusIpSecSadIpSecStatisticsLocalTunnelIpAddr             IpAddress,
        sonusIpSecSadIpSecStatisticsLocalSpi    INTEGER,
        sonusIpSecSadIpSecStatisticsRemoteTunnelIpAddr              IpAddress,
        sonusIpSecSadIpSecStatisticsRemoteSpi     INTEGER,
        sonusIpSecSadIpSecStatisticsPacketsSent       Counter,
        sonusIpSecSadIpSecStatisticsPacketsReceived           Counter,
        sonusIpSecSadIpSecStatisticsPacketsDiscardedFailedIntegrity                           Counter,
        sonusIpSecSadIpSecStatisticsPacketsDiscardedFailedSelector                          Counter,
        sonusIpSecSadIpSecStatisticsPacketsDiscardedFailedAntiReplay                            Counter,
        sonusIpSecSadIpSecStatisticsLocalTunnelIp6Addr              Ipv6Address,
        sonusIpSecSadIpSecStatisticsRemoteTunnelIp6Addr               Ipv6Address,
