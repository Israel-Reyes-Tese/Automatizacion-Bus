		XfPtpNetworkProtocol ::= TEXTUAL-CONVENTION
			STATUS current
			DESCRIPTION 
				"Description."
			SYNTAX INTEGER
				{
					udpIpv4(1),
					ieee8023(2)
				}

		ClockPortState ::= TEXTUAL-CONVENTION
			STATUS          current
			DESCRIPTION
				"This is the value of the current state of the protocol engine
				associated with this port."
			SYNTAX INTEGER  
				{
					off(0),
					initializing(1),
					faulty(2),
					disabled(3),
					listening(4),
					preMaster(5),
					master(6),
					passive(7),
					uncalibrated(8),
					slave(9)
				}

		ClockIdentity ::= TEXTUAL-CONVENTION
		    DISPLAY-HINT    "255a"
		    STATUS          current
		    DESCRIPTION
		        "The clock Identity is an 8-octet array and will be presented in
		        the form of a character array.  Network byte order is assumed.
		
		        The value of the ClockIdentity should be taken from the
		        IEEE EUI-64 individual assigned numbers as indicated in
		        Section 7.5.2.2.2 of [IEEE 1588-2008].
		
		        The EUI-64 address is divided into the following fields:
		
		             OUI bytes (0-2)
		             Extension identifier bytes (3-7)
		
		        The clock identifier can be constructed from existing EUI-48
		        assignments and here is an abbreviated example extracted from
		        section 7.5.2.2.2 [IEEE 1588-2008].
		
		             Company EUI-48 = 0xACDE4823456716
		             EUI-64 = ACDE48FFFE23456716
		
		        It is important to note the IEEE Registration Authority has
		        deprecated the use of MAC-48 in any new design."
		
		    REFERENCE       "Section 7.5.2.2.1 of [IEEE 1588-2008]"
		    SYNTAX          OCTET STRING (SIZE (1..255))

		ClockQualityClassType ::= TEXTUAL-CONVENTION
		    DISPLAY-HINT    "d"
		    STATUS          current
		    DESCRIPTION
		        "The ClockQuality as specified in section 5.3.7, 7.6.2.4 and
		        Table 5 of [IEEE 1588-2008].
		
		        Value     Description
		        --------  ------------------------------------------------
		               0  Reserved to enable compatibility with future
		                  versions.
		             1-5  Reserved
		               6  Shall designate a clock that is synchronized
		                  to a primary reference time source.  The
		                  timescale distributed shall be PTP.  A
		                  clockClass 6 clock shall not be a slave to
		                  another clock in the domain.
		               7  Shall designate a clock that has previously
		                  been designated as clockClass 6 but that has
		                  lost the ability to synchronize to a primary
		                  reference time source and is in holdover mode
		                  and within holdover specifications. The
		                  timescale distributed shall be PTP.  A
		                  clockClass 7 clock shall not be a slave to
		                  another clock in the domain.
		               8  Reserved.
		            9-10  Reserved to enable compatibility with future
		                  versions.
		           11-12  Reserved.
		              13  Shall designate a clock that is synchronized
		                  to an application-specific source of time.
		                  The timescale distributed shall be ARB.  A
		                  clockClass 13 clock shall not be a slave to
		                  another clock in the domain.
		              14  Shall designate a clock that has previously
		                  been designated as clockClass 13 but that
		                  has lost the ability to synchronize to an
		                  application-specific source of time and is
		                  in holdover mode and within holdover
		                  specifications. The timescale distributed
		                  shall be ARB.  A clockClass 14 clock shall
		                  not be a slave to another clock in the domain.
		           15-51  Reserved.
		              52  Degradation alternative A for a clock of
		                  clockClass 7 that is not within holdover
		                  specification.  A clock of clockClass 52
		                  shall not be a slave to another clock in
		                  the domain.
		           53-57  Reserved.
		              58  Degradation alternative A for a clock of
		                  clockClass 14 that is not within holdover
		                  specification. A clock of clockClass 58 shall
		                  not be a slave to another clock in the domain.
		           59-67  Reserved.
		          68-122  For use by alternate PTP profiles.
		         123-127  Reserved.
		         128-132  Reserved.
		         133-170  For use by alternate PTP profiles.
		         171-186  Reserved.
		
		             187  Degradation alternative B for a clock of
		                  clockClass 7 that is not within holdover
		                  specification. A clock of clockClass 187 may
		                  be a slave to another clock in the domain.
		         188-192  Reserved.
		             193  Degradation alternative B for a clock of
		                  clockClass 14 that is not within holdover
		                  specification. A clock of clockClass 193 may
		                  be a slave to another clock in the domain.
		         194-215  Reserved.
		         216-232  For use by alternate PTP profiles.
		         233-247  Reserved.
		             248  Default. This clockClass shall be used if
		                  none of the other clockClass definitions apply.
		         249-250  Reserved.
		             251  Reserved for version 1 compatibility; see Clause 18.
		         252-254  Reserved.
		             255  Shall be the clockClass of a slave-only clock; see
		                  9.2.2."
		
		    REFERENCE       "Section 5.3.7, 7.6.2.4 and Table 5 of
		                     [IEEE 1588-2008]."
		    SYNTAX          Unsigned32 (0..255)

		ClockQualityAccuracyType ::= TEXTUAL-CONVENTION
		    STATUS          current
		    DESCRIPTION
		        "The ClockQuality as specified in section 5.3.7, 7.6.2.5 and
		        Table 6 of [IEEE 1588-2008].
		
		        The following values are not represented in the enumerated
		        values.
		
		                 0x01-0x1F Reserved
		                 0x32-0x7F Reserved
		
		        It is important to note that section 7.1.1 RFC2578 allows for
		        gaps and enumerate values to start with zero when indicated by
		        the protocol."
		
		    REFERENCE       "Section 5.3.7, 7.6.2.5 and Table 6 of
		                     [IEEE 1588-2008]"
		    SYNTAX          INTEGER  {
		                        reserved00(1),       -- 0x00 to 0x1F  (Note: set to 1 to allign with TN impl.)
		                        nanoSecond25(32),    -- 0x20
		                        nanoSecond100(33),   -- 0x21
		                        nanoSecond250(34),   -- 0x22
		                        microSec1(35),       -- 0x23
		                        microSec2dot5(36),   -- 0x24
		                        microSec10(37),      -- 0x25
		                        microSec25(38),      -- 0x26
		                        microSec100(39),     -- 0x27
		                        microSec250(40),     -- 0x28
		                        milliSec1(41),       -- 0x29
		                        milliSec2dot5(42),   -- 0x2A
		                        milliSec10(43),      -- 0x2B
		                        milliSec25(44),      -- 0x2C
		                        milliSec100(45),     -- 0x2D
		                        milliSec250(46),     -- 0x2E
		                        second1(47),         -- 0x2F
		                        second10(48),        -- 0x30
		                        secondGreater10(49), -- 0x31
		                        unknown(254),        -- 0xFE
		                        reserved255(255)     -- 0xFF
		                    }

		ClockTimeSourceType ::= TEXTUAL-CONVENTION
		    STATUS          current
		    DESCRIPTION
		        "The ClockQuality as specified in section 5.3.7, 7.6.2.6 and
		        Table 7 of [IEEE 1588-2008].
		
		        The following values are not represented in the enumerated
		        values.
		
		        0xF0-0xFE  For use by alternate PTP profiles
		        0xFF       Reserved
		
		        It is important to note that section 7.1.1 RFC2578 allows for
		        gaps and enumerate values to start with zero when indicated by
		        the protocol."
		
		    REFERENCE       "Section 5.3.7, 7.6.2.6 and Table 7 of
		                     [IEEE 1588-2008]."
		    SYNTAX          INTEGER  {
		                        atomicClock(16), -- 0x10
		                        gps(32), -- 0x20
		                        terrestrialRadio(48), -- 0x22
		                        ptp(64), -- 0x40
		                        ntp(80), -- 0x50
		                        handSet(96), -- 0x60
		                        other(144), -- 0x90
		                        internalOscillator(160) -- 0xA0
		                    }

--
-- Node definitions
--
		-- 1.3.6.1.4.1.193.81.2.15.1
		xfPtpObjects OBJECT IDENTIFIER ::= { xfPtpMIB 1 }

		-- 1.3.6.1.4.1.193.81.2.15.1.1
		xfPtpVariant OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates the revision of PTP implementation.
				
				Values:
				-1 (unsigned 4294967295):  NE does not support PTP.
				1:  NE supports only CLI-based configuration of PTP.
				2:  NE supports SNMP-based configuration of PTP in ieee1588 and in g82651 profile.
				3:  Support g82751 profile"
			::= { xfPtpObjects 1 }                      
			                         
		-- 1.3.6.1.4.1.193.81.2.15.1.2
		xfPtpScalarObjects OBJECT IDENTIFIER ::= { xfPtpObjects 2 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.1
		xfPtpAlarmEnable OBJECT-TYPE
			SYNTAX BITS
				{
					freeRunningMode(0),
  					traceabilityLost(1),
					holdoverEntered(2),
					clockProtectionLost(3),
					peerDown(4),
					los1PPSSource(5),
 					incompatibleHardware(6)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used to enable/disable PTP clock alarm notifications.

			 	'0' at a bit position means disable and '1' means enable.

				freeRunningMode(0)      : enable/disable the Phase Free Running Mode Entered alarm
				traceabilityLost(1)     : enable/disable the Loss of Grandmaster Traceability alarm
				holdoverEntered(2)      : enable/disable the Phase Holdover Mode Entered alarm
				clockProtectionLost(3)  : enable/disable the Loss of Master Clock Protection alarm
				peerDown(4)             : enable/disable the IEEE1588 Peer is Down alarm
				los1PPSSource(5)        : enable/disable the Loss of Signal on 1PPS/ToD interface alarm
				incompatibleHardware(6) : enable/disable the IEEE1588 Incompatible Hardware alarm
				"
			::= { xfPtpScalarObjects 1 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.2
		xfPtpAlarmStatus OBJECT-TYPE
			SYNTAX BITS
				{
					freeRunningMode0(0),
					freeRunningMode1(1),
					freeRunningMode2(2),
					traceabilityLost0(3),
					traceabilityLost1(4),
					traceabilityLost2(5),
					holdoverEntered0(6),
					holdoverEntered1(7),
					holdoverEntered2(8),
					clockProtectionLost0(9),
					clockProtectionLost1(10),
					clockProtectionLost2(11),
					peerDown0(12),
					peerDown1(13),
					peerDown2(14),
					los1PPSSource0(15),
					los1PPSSource1(16),
					los1PPSSource2(17),
					incompatibleHardware0(18),
					incompatibleHardware1(19),
					incompatibleHardware2(20)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the alarm status of the PTP clock.

				 Severity is indicated by three bits per alarm:
					bits 012
					     000   clear
					     001   undetermined
					     010   warning
					     011   minor
					     100   major
					     101   critical"
			::= { xfPtpScalarObjects 2 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.3
		xfPtpEventEnable OBJECT-TYPE
			SYNTAX BITS
				{
					switchParentClock(0)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used to enable/disable PTP clock event notifications.
				
				switchParentClock(0)    : Switch of Parent Clock
				"
			::= { xfPtpScalarObjects 3 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.4
		xfPtpClearPortStatistics OBJECT-TYPE
			SYNTAX Unsigned32 (1..65534)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to reset the statistic counters of the given Clock Port.
				
				Setting the value to 1..65534 will clear the counters of the clock port that has 
				the same index in xfPtpClockPortTable.
				"
			::= { xfPtpScalarObjects 4 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.5
		xfPtpProfile OBJECT-TYPE
			SYNTAX INTEGER
				{
					disabled(0),
					ieee1588v2(1),
					g82651(2),
					g82751(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Specifies the PTP profile:
				- disabled   : PTP-based synchronization and timestamping on interfaces is disabled.
				               The Node behaves as a L2 switch for PTP packets.
				- ieee1588v2 : phase/time synchronization according to IEEE 1588-2008.
				- g82651     : frequency synchronization according to ITU-T G.8265.1.
				- g82751     : phase/time synchronization according to ITU-T G.8275.1

				This object can only be configured if there are no clock ports in xfPtpClockPortTable.
				"
			DEFVAL { disabled }
			::= { xfPtpScalarObjects 5 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.6
		xfPtpDeviceType OBJECT-TYPE
			SYNTAX INTEGER
				{
					ordinaryClock(1),
					boundaryClock(2),
					transparentClock(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Specifies the type of the device. It can be configured by the user only if
				xfPtpProfile = ieee1588v2 or xfPtpProfile = g82751.

				In case of xfPtpProfile = g82751 then
				   - xfPtpDeviceType = ordinaryClock then
				        - xfPtpClockRole can be grandmaster or slaveOnly
				   - xfPtpDeviceType = boundaryClock then
				        - xfPtpClockRole can be bmc or grandmaster
				   - xfPtpDeviceType = transparentClock then
				        - xfPtpClockRole is notConfigured
				
				This object can only be configured if there are no clock ports in xfPtpClockPortTable.
				"
			::= { xfPtpScalarObjects 6 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.7
		xfPtpMessageTransportMode OBJECT-TYPE
			SYNTAX INTEGER
				{
					notConfigured(0),
					multicast(1),
					unicast(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Specifies the message transport mode, multicast or unicast.

				For xfPtpProfile = g82651 only the unicast(2)   mode is valid.
				For xfPtpProfile = g82751 only the multicast(1) mode is valid.
				This object can only be configured if there are no clock ports in xfPtpClockPortTable.
				
				Value 0 indicates that no clock has been configured yet. 
				"
			::= { xfPtpScalarObjects 7 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.8
		xfPtpClockRole OBJECT-TYPE
			SYNTAX INTEGER
				{
					notConfigured(0),
					grandmaster(1),
					slaveOnly(2),
					bmc(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Specifies if the node shall be used as a PTP grandmaster, slave-only or in BMC mode.
                
				Not Configured: No clock role has been configured for the PTP node.
                
				Grandmaster: the PTP node is distributing frequency to slave clocks. 
				  It can't become a slave itself.
				  Applicable if xfPtpProfile = g82651 or g82751 or ieee1588 except if deviceType = transparentClock.

				Slave-only: the PTP node is synchronizing its phase or frequency to one or 
				  more master clock(s). It can't become a master for other clocks.
				  Applicable if xfPtpProfile = g82651.
				  Applicable if xfPtpProfile = ieee1588v2 and xfPtPDeviceType = ordinaryClock.
				  Applicable if xfPtpProfile = g82751     and xfPtPDeviceType = ordinaryClock.

				BMC: the PTP clock runs the Best Master Clock algorithm to determine the role 
				  of its clock ports. One PTP port will become slave to a master clock, the 
				  rest of the PTP ports have master or passive roles to other clocks.
				"
			::= { xfPtpScalarObjects 8 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.9
		xfPtpDomain OBJECT-TYPE
			SYNTAX Unsigned32 (0..255)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The PTP domain number.

				This object can only be configured if there are no clock ports in xfPtpClockPortTable.

				The valid settings changes range and default value depending on the xfPtpProfile setting. 
				
				xfPtpProfile = ieee1588v2:
				xfPtpDomain range 0-255, default value: 0
 
				xfPtpProfile = g82651:
				xfPtpDomain range 4-23, default value: 4

				xfPtpProfile = g82751:
				xfPtpDomain range 24-43, default value: 24
				"
			::= { xfPtpScalarObjects 9 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.10
		xfPtpInterfaceTsuAssignmentCommit OBJECT-TYPE
			SYNTAX INTEGER
				{
					tsuAssignCommit(1)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Setting this object to tsuAssignCommit(1) will cause all outstanding 
				 TSU assignment requests to RL-IME interfaces in xfPtpInterfaceTable
				 being committed. See xfPtpInterfaceTsuAssignmentRequest object.

				 The commit results in an NPU cold restart.
				"
			::= { xfPtpScalarObjects 10 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.11
		xfPtpLogClockServoFrequency OBJECT-TYPE
			SYNTAX Unsigned32 (1..1000)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Specifies the frequency of the logging. Every nth sample is saved. The valid range is 1-1000.

				Use this command to record the timestamps of the transmitted and received 1588 packets.
				The recorded samples are available in the local.log.cslog file generated
				by the Bug Reporting Tool within the tn_local_logs_*.tgz file.
				This file contains the timestamps in a compressed format.
				"
			::= { xfPtpScalarObjects 11 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.12
		xfPtpLogClockServoDuration OBJECT-TYPE
			SYNTAX Unsigned32 (0..100000)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Specifies the time duration for the logging in seconds. The valid range is 0-100000. The value 0 stops the logging.

				Use this command to record the timestamps of the transmitted and received 1588 packets.
				The recorded samples are available in the local.log.cslog file generated
				by the Bug Reporting Tool within the tn_local_logs_*.tgz file.
				This file contains the timestamps in a compressed format.
				"
			::= { xfPtpScalarObjects 12 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.13
		xfPtpSyncPortEnable OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"True: the frequency recovered by the PTP protocol is offered to 
				   Network Synchronization to be used as a Sync Source.

				 False: the PTP recovered frequency (if any) is not offered as Sync Source.
				"
			::= { xfPtpScalarObjects 13 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.14
		xfPtpPriority1 OBJECT-TYPE
			SYNTAX Unsigned32 (0..255)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The Priority1 value. The valid range is 0-255.
				Use this command to set the Priority1 value of the local clock.

				Not Applicable if xfPtpProfile = g82751 
				"
			::= { xfPtpScalarObjects 14 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.15
		xfPtpPriority2 OBJECT-TYPE
			SYNTAX Unsigned32 (0..255)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The Priority2 value. The valid range is 0-255.
				Use this command to set the Priority2 value of the local clock.
				"
			::= { xfPtpScalarObjects 15 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.16
		xfPtpHoldoverTimeout OBJECT-TYPE
			SYNTAX Unsigned32 (0..10000)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Phase/time holdover timeout in secundum."
			DEFVAL { 3000 }
			::= { xfPtpScalarObjects 16 }

		-- 1.3.6.1.4.1.193.81.2.15.1.2.17
		xfPtpClockLocalPriority OBJECT-TYPE
			SYNTAX Unsigned32 (1..255)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Applicable only if xfPtpProfile = g82751.
				PTP clock level local-priority"
			DEFVAL { 128 }
			::= { xfPtpScalarObjects 17 }
			
		-- 1.3.6.1.4.1.193.81.2.15.1.2.18
		xfPtpHoldoverTimeoutEnabled OBJECT-TYPE
			SYNTAX INTEGER
				{
					enabled(1),
					disabled(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable/disable Phase/time holdover timeout.
				 Applicable only in g82751 and ieee1588 profile"
			DEFVAL { enabled }
			::= { xfPtpScalarObjects 18 }			

		-- 1.3.6.1.4.1.193.81.2.15.1.3
		xfPtpTableObjects OBJECT IDENTIFIER ::= { xfPtpObjects 3 }  

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1
		xfPtpClockPortTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfPtpClockPortEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table is used to create/destroy/modify PTP clock ports.

				A boundary clock may have one clock port on each bridge port.
				An ordinary clock has one clock port.
				A transparent clock has no clock ports.
				"
			::= { xfPtpTableObjects 1 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1
		xfPtpClockPortEntry OBJECT-TYPE
			SYNTAX XfPtpClockPortEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry for a PTP port."
			INDEX { xfPtpClockPortIndex }
			::= { xfPtpClockPortTable 1 }

		XfPtpClockPortEntry ::=
			SEQUENCE {
				xfPtpClockPortIndex
					INTEGER,
				xfPtpClockPortName
					DisplayString, 
				xfPtpClockPortEnabled
					TruthValue,
				xfPtpClockPortOperStatus
					INTEGER,
				xfPtpClockPortState
					ClockPortState,
				xfPtpClockPortEncapsulationType
					XfPtpNetworkProtocol,
				xfPtpClockPortCustomerVlanTagging
					INTEGER,
				xfPtpClockPortCustomerVlanId
					Integer32,
				xfPtpClockPortCustomerVlanPriority
					Unsigned32,
				xfPtpClockPortTransportVlanPriority
					Unsigned32,
				xfPtpClockPortDot1dBasePort 
					INTEGER,
				xfPtpClockPortDelayMech
					INTEGER,
				xfPtpClockPortDot1qVlanIndex
					Integer32,
				xfPtpClockPortAddressType
					InetAddressType,
				xfPtpClockPortIpAddress
					InetAddress,
				xfPtpClockPortSubnetMask
					InetAddressPrefixLength,
				xfPtpClockPortDefaultGateway
					InetAddress,
				xfPtpClockPortDscp
					Unsigned32,
				xfPtpClockPortPortIdentity
					OCTET STRING,
				xfPtpClockPortPTPVersion
					Unsigned32,
				xfPtpClockPortRowStatus
					RowStatus,
				xfPtpClockPortLocalPriority
					Unsigned32,
				xfPtpClockPortNotSlave
					TruthValue,
				xfPtpClockPortMulticastAddressType
					INTEGER,
				xfPtpClockPortDot1qVlanTagging
					INTEGER,
				xfPtpClockPortInterface
					InterfaceIndexOrZero
			 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.1
		xfPtpClockPortIndex OBJECT-TYPE
			SYNTAX INTEGER (1..65535)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Table Index
				"
			::= { xfPtpClockPortEntry 1 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.2
		xfPtpClockPortName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (1..20))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Clock port name.
				The clock port name cannot be empty and must be unique for each clock port.
				The clock port name cannot be changed after a clock port has been created.
				"
			DEFVAL { "" }
			::= { xfPtpClockPortEntry 2 }   

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.3
		xfPtpClockPortEnabled OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable/disable the PTP port."
			DEFVAL { false }
			::= { xfPtpClockPortEntry 3 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.4
		xfPtpClockPortOperStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
					up(1),
					down(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Operational state of clock port.
				Is either up(1) or down(2)
				"
			::= { xfPtpClockPortEntry 4 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.5
		xfPtpClockPortState OBJECT-TYPE
			SYNTAX ClockPortState
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The port state returned by PTP engine."
			::= { xfPtpClockPortEntry 5 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.6
		xfPtpClockPortEncapsulationType OBJECT-TYPE
			SYNTAX XfPtpNetworkProtocol
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Configure the protocol that will be used for transport of PTP messages:
				- UDP over IPv4
				- ethernet"
			::= { xfPtpClockPortEntry 6 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.7
		xfPtpClockPortCustomerVlanTagging OBJECT-TYPE
			SYNTAX INTEGER
				{
					enabled(1),
					disabled(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable/disable C-VLAN tagging.

				C-VLAN tagging can only be enabled if a Customer VLAN Id is assigned.
				Customer VLAN (inner-tag) is only applicable in Provider Bridge mode.
				"
			DEFVAL { disabled }
			::= { xfPtpClockPortEntry 7 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.8
		xfPtpClockPortCustomerVlanId OBJECT-TYPE
			SYNTAX Integer32 (0..4094)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Customer VLAN Id, assigned to transmitted frames on this PTP-port.

				Applicable only if C-VLAN tagging is enabled and bridge mode is Provider.
				Configurable values:  1..4094
				
				Value 0 indicates that a customer VLAN Id has not been assigned yet, likely
				due to that C-VLAN tagging is disabled.
				"
			DEFVAL { 0 }
			::= { xfPtpClockPortEntry 8 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.9
		xfPtpClockPortCustomerVlanPriority OBJECT-TYPE
			SYNTAX Unsigned32 (0..7)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Customer VLAN priority, assigned to frames transmitted on this PTP port.

				Applicable only if C-VLAN tagging is enabled and bridge mode is Provider.
				"
			DEFVAL { 7 }
			::= { xfPtpClockPortEntry 9 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.10
		xfPtpClockPortTransportVlanPriority OBJECT-TYPE
			SYNTAX Unsigned32 (0..7)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Transport VLAN priority."
			DEFVAL { 7 }
			::= { xfPtpClockPortEntry 10 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.11
		xfPtpClockPortDot1dBasePort OBJECT-TYPE
			SYNTAX INTEGER (0..65535)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Number of the bridge port that the actual PTP port is connected to.
				Applicable only if xfPtpDeviceType = boundaryClock.
				Value 0 indicates that a specific bridge port has not been assigned yet."
			DEFVAL { 0 }
			::= { xfPtpClockPortEntry 11 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.12
		xfPtpClockPortDelayMech OBJECT-TYPE
			SYNTAX INTEGER
				{
					e2e(1)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Use this command to configure the delay mechanism for a port. 
				Configuration of the delay mechanism is allowed only for boundary clock.
				Options:
					e2e	 	Sets delay mechanism to end-to-end.
				"
			::= { xfPtpClockPortEntry 12 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.13
		xfPtpClockPortDot1qVlanIndex OBJECT-TYPE
			SYNTAX Integer32 (0..4094)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The transport VLAN that the actual PTP port is assigned to.
				Value 0 indicates that a specific VLAN has not been assigned yet."
			DEFVAL { 0 }
			::= { xfPtpClockPortEntry 13 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.14
		xfPtpClockPortAddressType OBJECT-TYPE
			SYNTAX InetAddressType
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The type of IP addresses in this table. Only ipv4 is supported." 
			DEFVAL { ipv4 }
			::= { xfPtpClockPortEntry 14 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.15 
		xfPtpClockPortIpAddress OBJECT-TYPE
			SYNTAX InetAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"IP address assigned to the actual PTP port.
				Applicable only if xfPtpClockPortEncapsulationType = udpIpv4."
			::= { xfPtpClockPortEntry 15 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.16
		xfPtpClockPortSubnetMask OBJECT-TYPE
			SYNTAX InetAddressPrefixLength
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"IP subnet mask.
				Encoded as the length of the Internet address prefix.
				A value of n corresponds to an IP address mask that
				has n contiguous 1-bits from the most significant bit,
				with all other bits set to 0.

				i.e. for a IPv4 subnet mask 255.255.255.0 is denoted 24
				as the 24 most significant bits are set to 1.

				Applicable only if xfPtpClockPortEncapsulationType = udpIpv4.
				"
			::= { xfPtpClockPortEntry 16 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.17
		xfPtpClockPortDefaultGateway OBJECT-TYPE
			SYNTAX InetAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The IP address of the default gateway. If this value is
				undefined or unknown, it shall have the value 0.0.0.0.

				Applicable only if xfPtpClockPortEncapsulationType = udpIpv4.
				"
			::= { xfPtpClockPortEntry 17 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.18
		xfPtpClockPortDscp OBJECT-TYPE
			SYNTAX Unsigned32 (0..63)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"DSCP field in the IP header.
				Applicable only if xfPtpClockPortEncapsulationType = udpIpv4."
			DEFVAL { 46 }
			::= { xfPtpClockPortEntry 18 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.19
		xfPtpClockPortPortIdentity OBJECT-TYPE
			SYNTAX          OCTET STRING(SIZE(1..256))
			MAX-ACCESS      read-only
			STATUS          current
			DESCRIPTION
				"This object specifies the PTP clock port Identity."
			::= { xfPtpClockPortEntry 19 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.20
		xfPtpClockPortPTPVersion OBJECT-TYPE
			SYNTAX          Unsigned32
			MAX-ACCESS      read-only
			STATUS          current
			DESCRIPTION
				"This object specifies the PTP version being used.
				"
			::= { xfPtpClockPortEntry 20 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.21
		xfPtpClockPortRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object is used to create a new row or modify or 
				delete an existing row in this table."
			::= { xfPtpClockPortEntry 21 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.22
		xfPtpClockPortLocalPriority OBJECT-TYPE
			SYNTAX Unsigned32 (1..255)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This objects specifies the PTP port level local priority.
				Applicable only if xfPtpProfile = g82751."
			DEFVAL { 128 }
			::= { xfPtpClockPortEntry 22 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.23
		xfPtpClockPortNotSlave OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This objects specifies the notSlave attributes of the PTP port.
				Applicable only if xfPtpProfile = g82751.
				If xfPtpClockRole = grandmaster then xfPtpClockPortNotSlave must be true
				If xfPtpClockRole = slaveOnly   then xfPtpClockPortNotSlave must be false
				If xfPtpClockRole = bmc         then xfPtpClockPortNotSlave can be configurable true or false
				"
			DEFVAL { false }
			::= { xfPtpClockPortEntry 23 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.24
		xfPtpClockPortMulticastAddressType OBJECT-TYPE
			SYNTAX INTEGER
				{
					forwardable(1),
					nonForwardable(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Applicable only if xfPtpMessageTransportMode = multicast then
				      - forwardable:    applicable if xfPtpProfile = g82751 or xfPtpProfile = ieee1588v2
				      - nonForwardable: applicable if xfPtpProfile = g82751
				      "
			DEFVAL { forwardable }
			::= { xfPtpClockPortEntry 24 }
			
		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.25
		xfPtpClockPortDot1qVlanTagging OBJECT-TYPE
			SYNTAX INTEGER
				{
					enabled(1),
					disabled(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				" Enable/disable VLAN tagging. 
				
				 xfPtpClockPortDot1qVlanTagging must be enabled in ieee1588v2 and in g82651
				 xfPtpClockPortDot1qVlanTagging can  be enabled or disabled in g82751
				 "
			DEFVAL { enabled }
			::= { xfPtpClockPortEntry 25 }

		-- 1.3.6.1.4.1.193.81.2.15.1.3.1.1.26
		xfPtpClockPortInterface OBJECT-TYPE
			SYNTAX InterfaceIndexOrZero
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object is used to connect the PTP port to a L2 LAN/WAN or a L3 interface.
				When this object is set, the xfPtpClockPortEncapsulationType MO is updated
				automatically to either Ethernet or UDP/IPv4.
				Value 0 indicates that a specific interface has not been assigned yet.

				Supported in ML66."
			DEFVAL { 0 }
			::= { xfPtpClockPortEntry 26 }

