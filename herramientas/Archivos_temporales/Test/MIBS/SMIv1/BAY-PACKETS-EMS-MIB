    SYNTAX          INTEGER
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is autogenerated Identifier for the Alarm Destination."
    ::= { rsiAlarmDestEntry 1 }

rsiAlarmDestGroupId OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is Alarm Group Id."
    ::= { rsiAlarmDestEntry 2 }

rsiAlarmDestTODStart OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(19))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Start time: For exactRule Format is YYYY:MM:DD HH:MM:SS. For dowRule and alldays Format is XXXX:XX:XX HH:MM:SS"
    ::= { rsiAlarmDestEntry 3 }

rsiAlarmDestTODEnd OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(19))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "End time: For exactRule Format is YYYY:MM:DD HH:MM:SS. For dowRule and alldays Format is XXXX:XX:XX HH:MM:SS"
    ::= { rsiAlarmDestEntry 4 }

rsiAlarmDestDOFWeek OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is a hexadecimal bit mask field depending on the day of the week. The bit mask is as per the rule ALL 0x0, MON 0x0001, TUE 0x0002, WED 0x0004, THU 0x0008, FRI 0x0010, SAT 0x0020, SUN 0x0040"
    ::= { rsiAlarmDestEntry 5 }

rsiAlarmDestEntryType OBJECT-TYPE
    SYNTAX          INTEGER { exactRule(0), dowRule(1), alldays(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is entry type of the rule.exactRule is on the basis of exact timing,dowRule is on the basis of day of week and time of the day and alldays is on the basis of time of the day for all days."
    ::= { rsiAlarmDestEntry 6 }

rsiAlarmDestType OBJECT-TYPE
    SYNTAX          INTEGER { snmpTrap(0), smtpMail(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is Destination Type for the alarm."
    ::= { rsiAlarmDestEntry 7 }

rsiAlarmDest OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This is the alarm destination.For SNMP Trap it will be a IP Address and for SMTP Mail it will be a Mail Id."
    ::= { rsiAlarmDestEntry 8 }

rsiAlarmDestRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "."
    ::= { rsiAlarmDestEntry 10 }

