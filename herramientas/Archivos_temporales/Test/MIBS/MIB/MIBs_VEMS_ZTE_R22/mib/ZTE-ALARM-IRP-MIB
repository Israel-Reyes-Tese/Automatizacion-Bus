ZTE-ALARM-IRP-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
        enterprises, Integer32
                FROM SNMPv2-SMI
        TEXTUAL-CONVENTION, DisplayString, DateAndTime,RowStatus
                FROM SNMPv2-TC
        OBJECT-GROUP, NOTIFICATION-GROUP, MODULE-COMPLIANCE
                FROM SNMPv2-CONF
        alarm
        	FROM ZTE-IRP-MIB;


alarmIRP                  OBJECT IDENTIFIER ::= { alarm 1 }
alarmIRPInfo			  OBJECT IDENTIFIER ::= { alarmIRP 1 }
alarmCount		  		  OBJECT IDENTIFIER ::= { alarmIRP 2 }	
alarmNotifications        OBJECT IDENTIFIER ::= { alarmIRP 4 }
alarmNotificationProfile  OBJECT IDENTIFIER ::= { alarmNotifications 1 }
alarmConformance          OBJECT IDENTIFIER ::= { alarmIRP 5 }
alarmFilter               OBJECT IDENTIFIER ::= { alarmIRP 6 }
alarmOperation            OBJECT IDENTIFIER ::= { alarmIRP 7 }

--======================================================================
-- TEXTUAL CONVENTIONS
--======================================================================
ManagedObjectInstance ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION
                "the unique name of one object instance."
        SYNTAX OCTET STRING (SIZE (0..512))

AdditionalText   ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION
                "This object represents some arbitrary additional 
                 information of the alarm."
        SYNTAX      OCTET STRING (SIZE (0..1000))

TypeOfEvent ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION
                "Event type specified in ITU recommendations 
                 X.733 and X736."
        SYNTAX INTEGER {
                communicationsAlarm(1),
                processingErrorAlarm(2),
                environmentalAlarm(3),
                qualityOfServiceAlarm(4),
                equipmentAlarm(5),
                integrityViolation(6),
                operationalViolation(7),
                physicalViolation(8),
                securityServiceViolation(9),
                timeDomainViolation(10),
                omcAlarm(11)          
        }

PerceivedSeverity ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION        
                "the alarm level including 
                Critical, Major, Minor, Warning, and Indeterminate."
        SYNTAX INTEGER {
                indeterminate(1),
                critical(2),
                major(3),
                minor(4),
                warning(5),
                cleared(6)
        }

SeverityFilter ::=   TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION        
                "the alarm level filter including 
                Critical, Major, Minor, Warning, and Indeterminate."
        SYNTAX INTEGER {
                noFilter(0),
                indeterminate(1),
                critical(2),
                major(3),
                minor(4),
                warning(5),
                cleared(6)
        }    
TypeFilter ::=   TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION        
                "Event type specified in ITU recommendations 
                 X.733 and X736."
        SYNTAX INTEGER {      
                noFilter(0),
                communicationsAlarm(1),
                processingErrorAlarm(2),
                environmentalAlarm(3),
                qualityOfServiceAlarm(4),
                equipmentAlarm(5),
                integrityViolation(6),
                operationalViolation(7),
                physicalViolation(8),
                securityService(9),
                timeDomainViolation(10),
                omcAlarm(11)     
        }            
        
AckStatus ::=   TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION        
                "the alarm ack status including ACKNOWLEDGED and UNACKNOWLEDGED"
        SYNTAX INTEGER {      
                acknowledged(1),
                unAcKnowledged(2)
        }    
        
