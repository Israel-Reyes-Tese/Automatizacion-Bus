ptGWSTGWSTTables OBJECT IDENTIFIER
    ::= { ptConnGroup 573 }

ptGWSTGWSTTablesTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtGWSTGWSTTablesEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptGWSTGWSTTables 1 }

ptGWSTGWSTTablesEntry OBJECT-TYPE
    SYNTAX          PtGWSTGWSTTablesEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of GWSTGWSTTables attributes."
    INDEX           { ptConnGroupIndex, ptGWSTGWSTTablesIndex }
    ::= { ptGWSTGWSTTablesTable 1 }

PtGWSTGWSTTablesEntry ::=
    SEQUENCE {
        ptGWSTGWSTTablesLastModified    DisplayString,
        ptGWSTGWSTTablesLastAction      INTEGER,
        ptGWSTGWSTTablesErrors          Integer32,
        ptGWSTGWSTTablesTotalIncomingLinksets     Integer32,
        ptGWSTGWSTTablesTotalAllowedOPCs Integer32,
        ptGWSTGWSTTablesTotalBlockedOPCs Integer32,
        ptGWSTGWSTTablesTotalAllowedSIOs Integer32,
        ptGWSTGWSTTablesTotalAllowedDPCs Integer32,
        ptGWSTGWSTTablesTotalBlockedDPCs Integer32,
        ptGWSTGWSTTablesTotalAffectedDestinations         Integer32,
        ptGWSTGWSTTablesTotalAllowedISUPMTs   Integer32,
        ptGWSTGWSTTablesTotalAllowedSCCPCLGPAs      Integer32,
        ptGWSTGWSTTablesTotalAllowedSCCPTTs   Integer32,
        ptGWSTGWSTTablesTotalAllowedSCCPCLDPAs      Integer32,
        ptGWSTGWSTTablesTotalAffectedSCCPMgmts      Integer32,
        ptGWSTGWSTTablesTotalAllowedTCAPOpCodes       Integer32,
        ptGWSTGWSTTablesTotalRedirectToAppIDs     Integer32,
        ptGWSTGWSTTablesTotalRedirectToPCs  Integer32,
        ptGWSTGWSTTablesTotalAccountingMTP3Rules        Integer32,
        ptGWSTGWSTTablesTotalAccountingSCCPRules        Integer32,
        ptGWSTGWSTTablesTotalIncomingAppIDs   Integer32,
        ptGWSTGWSTTablesTotalAllowedSCCPCLGDigitsPAs            Integer32,
        ptGWSTGWSTTablesTotalAllowedSCCPCLDDigitsPAs            Integer32,
        ptGWSTGWSTTablesTotalAllowedMAPIMSIs    Integer32,
        ptGWSTGWSTTablesTotalGWSStatisticsRules       Integer32,
        ptGWSTGWSTTablesTotalAllowedTUPMTs  Integer32,
        ptGWSTGWSTTablesActions         INTEGER,
        ptGWSTGWSTTablesParentOID       OBJECT IDENTIFIER,
        ptGWSTGWSTTablesRowStatus       RowStatus,
        ptGWSTGWSTTablesIndex           Integer32
    }

