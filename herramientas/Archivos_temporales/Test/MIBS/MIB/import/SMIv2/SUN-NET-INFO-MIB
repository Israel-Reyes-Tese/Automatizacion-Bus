sunNimStateChange NOTIFICATION-TYPE
    OBJECTS         { sunNimNotificationEventId, sunNimNotificationTime, 
                      sunNimNotificationObject, 
                      sunNimNotificationChangedOID, 
                      sunNimNotificationOldInteger, 
                      sunNimNotificationNewInteger }
    STATUS          current
    DESCRIPTION     
            "Indicates that a state change has occurred on the state attribute
             identified by sunNimNotificationChangedOID. As states are
             enumerations of type INTEGER, the old and new states are passed in
             sunNimNotificationOldInteger and sunNimNotificationNewInteger
             respectively.
             
             This is as defined in X.721, sec. 13.14."
    ::= { sunNimMIBTrapPrefix 7 }

sunNimAttributeChangeInteger NOTIFICATION-TYPE
    OBJECTS         { sunNimNotificationEventId, sunNimNotificationTime, 
                      sunNimNotificationObject, 
                      sunNimNotificationChangedOID, 
                      sunNimNotificationOldInteger, 
                      sunNimNotificationNewInteger }
    STATUS          current
    DESCRIPTION     
            "Indicates that attribute identified by sunNimNotificationChangedOID
             has changed from the value in sunNimNotificationOldInteger to the
             value in sunNimNotificationNewInteger.
             
             This is as defined in X.721, sec. 13.1."
    ::= { sunNimMIBTrapPrefix 8 }

sunNimAttributeChangeString NOTIFICATION-TYPE
    OBJECTS         { sunNimNotificationEventId, sunNimNotificationTime, 
                      sunNimNotificationObject, 
                      sunNimNotificationChangedOID, 
                      sunNimNotificationOldString, 
                      sunNimNotificationNewString }
    STATUS          current
    DESCRIPTION     
            "Indicates that attribute identified by sunNimNotificationChangedOID
             has changed from the value in sunNimNotificationOldString to the
             value in sunNimNotificationNewString.
             
             This is as defined in X.721, sec. 13.1."
    ::= { sunNimMIBTrapPrefix 9 }

sunNimAttributeChangeOID NOTIFICATION-TYPE
    OBJECTS         { sunNimNotificationEventId, sunNimNotificationTime, 
                      sunNimNotificationObject, 
                      sunNimNotificationChangedOID, 
                      sunNimNotificationOldOID, sunNimNotificationNewOID }
    STATUS          current
    DESCRIPTION     
            "Indicates that attribute identified by sunNimNotificationChangedOID
             has changed from the value in sunNimNotificationOldOID to the
             value in sunNimNotificationNewOID.
             
             This is as defined in X.721, sec. 13.1."
    ::= { sunNimMIBTrapPrefix 10 }

