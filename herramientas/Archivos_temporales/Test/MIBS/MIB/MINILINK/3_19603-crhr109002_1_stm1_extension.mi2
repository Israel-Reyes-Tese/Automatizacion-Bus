    XF-STM1-EXTENSION-MIB DEFINITIONS ::= BEGIN
 
        IMPORTS
            OBJECT-GROUP, MODULE-COMPLIANCE            
                FROM SNMPv2-CONF            
            Integer32, OBJECT-TYPE, MODULE-IDENTITY            
                FROM SNMPv2-SMI            
            sonetMediumEntry, sonetSectionCurrentEntry, sonetLineCurrentEntry, sonetLineIntervalEntry, sonetFarEndLineCurrentEntry, 
            sonetFarEndLineIntervalEntry, sonetPathCurrentEntry, sonetPathIntervalEntry, sonetFarEndPathCurrentEntry, sonetFarEndPathIntervalEntry, 
            sonetVTCurrentEntry, sonetVTIntervalEntry, sonetFarEndVTCurrentEntry, sonetFarEndVTIntervalEntry            
                FROM SONET-MIB            
            XfSDH15MinUAVGauge, XfSDHTrailTraceIndentifier            
                FROM XF-STM1-PRIVATE-MIB            
            xfSDH            
                FROM XF-TOP-MIB;
    
    
        xfSDHsonetExtensionMIB MODULE-IDENTITY 
            LAST-UPDATED "200612010000Z"		-- December 1, 2006
            ORGANIZATION 
                "Ericsson AB"
            CONTACT-INFO 
                " "
            DESCRIPTION 
                "Extension to the Sonet/SDH Interface Type MIB. 
                Other related media specific MIBs for the STM-1 TM are:
                    -SONET-MIB, RFC2558
                    -XF-STM1-PRIVATE-MIB,
                     4/15519-CRHR109 002/1 
                                
                Some terminology explained:
                    Section : Regenerator Section in SDH
                    Line    : Multiplexer Section in SDH
                    Path    : Higher Order Path in SDH, i.e. 
                              VC-4 for XF STM-1 TM
                    VT      : Sonet Virtual Tributaries or 
                              Lower Order Path in SDH,i.e. 
                              VC-12 for XF STM-1 TM.
                
                More detailed information can be found in 
                1/155 19-CRH 109 625/1 Uae, Managed objects for Radio Link,
                1/155 19-CRHR 109 002/1, 
                XF STM-1 TM: SNMP Managed Objects,
                and in 
                1/155 17-CRHR 109 002/1, 
                Functional Specification for the STM-1 TM.
                "
                
            REVISION "200612010000Z"		-- December 1, 2006
            DESCRIPTION 
                "PE4
                 Changes according to 1/155 19-CRH 109 625/1 Uae Rev N:
                 Corrected syntax for xfSDHsonetMediumInsertAISOnBER.                 
                
                "
            REVISION "200611130000Z"		-- November 13, 2006
            DESCRIPTION 
                "PE3 
                 Changes according to 1/155 19-CRH 109 625/1 Uae Rev N:
                 xfSDHsonetMediumInsertAISOnJ0Line and xfSDHsonetMediumInsertAISOnJ0Radio
                 replaced by xfSDHsonetMediumInsertAISOnJ0, in XfSDHsonetMediumXEntry.
                 alsManualRestartUserDefined(7) renamed to alsManualRestartRLSpecific(7)
                 in syntax for xfSDHsonetMediumLaserMode, and updated description for this object. 
                "
                
            REVISION "200609290000Z"		-- September 29, 2006
            DESCRIPTION 
                "PE2-PATCH Altered values for xfSDHsonetMediumLaserMode 
                 according to 1/155 19-CRH 109 625/1 Uae Rev M"
            REVISION "200609151232Z"		-- September 15, 2006 at 12:32 GMT
            DESCRIPTION 
                "PE2 some small fixes after review"
            REVISION "200609131423Z"		-- September 13, 2006 at 14:23 GMT
            DESCRIPTION 
                "PE1 Added
                 
                xfSDHsonetMediumInsertAISOnJ0Line	   
                xfSDHsonetMediumInsertAISOnJ0Radio	   
                xfSDHsonetMediumInsertAISOnBER	 
                "
            REVISION "200605291552Z"		-- May 29, 2006 at 15:52 GMT
            DESCRIPTION 
                "D added xfSDHsonetMediumLaserMode, 
                    XfSDHsonetMediumTrailTraceIdMode, 
                    xfSDHsonetMediumTrailTraceC1Expect, 
                    xfSDHsonetMediumTrailTraceC1Receive 
                    and xfSDHsonetMediumTrailTraceC1Transmit "
            REVISION "200303141328Z"		-- March 14, 2003 at 13:28 GMT
            DESCRIPTION 
                "Revision C"
            REVISION "200212121438Z"		-- December 12, 2002 at 14:38 GMT
            DESCRIPTION 
                "Revision B"
            ::= { xfSDH 1 }

        
    
    
--
-- Node definitions
--
    
        xfSDHsonetExtensionObjects OBJECT IDENTIFIER ::= { xfSDHsonetExtensionMIB 1 }

        
        xfSDHsonetExtensionMedium OBJECT IDENTIFIER ::= { xfSDHsonetExtensionObjects 1 }

        
        xfSDHsonetMediumXTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfSDHsonetMediumXEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "The SONET/SDH Medium Extension table.
                The table is indexed by the ifIndex for the layer. 
                All SDH interfaces have one entry in this table.
                Section and Line have one entry since they have the 
                same ifIndex."
            ::= { xfSDHsonetExtensionMedium 1 }

        
        xfSDHsonetMediumXEntry OBJECT-TYPE
            SYNTAX XfSDHsonetMediumXEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry in the SONET/SDH Medium Extension table."
            AUGMENTS { sonetMediumEntry }
            ::= { xfSDHsonetMediumXTable 1 }

        
        XfSDHsonetMediumXEntry ::=
            SEQUENCE { 
                xfSDHsonetMediumTrailTraceExpect
                    XfSDHTrailTraceIndentifier,
                xfSDHsonetMediumTrailTraceReceive
                    XfSDHTrailTraceIndentifier,
                xfSDHsonetMediumTrailTraceTransmit
                    XfSDHTrailTraceIndentifier,
                xfSDHsonetMediumLineAutoDetect
                    INTEGER,
                xfSDHsonetMediumLineOperStatus
                    INTEGER,
                xfSDHsonetMediumLaserMode
                    INTEGER,
                xfSDHsonetMediumTrailTraceIdMode
                    INTEGER,
                xfSDHsonetMediumTrailTraceC1Expect
                    Integer32,
                xfSDHsonetMediumTrailTraceC1Receive
                    Integer32,
                xfSDHsonetMediumTrailTraceC1Transmit
                    Integer32,
                xfSDHsonetMediumInsertAISOnJ0
                    INTEGER,
                xfSDHsonetMediumInsertAISOnBER
                    INTEGER
             }

        xfSDHsonetMediumTrailTraceExpect OBJECT-TYPE
            SYNTAX XfSDHTrailTraceIndentifier
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The Trail Trace Identifier (TTI) value expected from the 
                far end. If the Trail Trace Expected is empty string, the detection 
                of the Trail Identifier Mismatch (TIM) is disabled."
            DEFVAL { "" }
            ::= { xfSDHsonetMediumXEntry 1 }

        
        xfSDHsonetMediumTrailTraceReceive OBJECT-TYPE
            SYNTAX XfSDHTrailTraceIndentifier
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The Trail Trace Identifier (TTI) value received 
                from the far end."
            ::= { xfSDHsonetMediumXEntry 2 }

        
        xfSDHsonetMediumTrailTraceTransmit OBJECT-TYPE
            SYNTAX XfSDHTrailTraceIndentifier
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The Trail Trace Identifier (TTI) to be sent as an 
                identification of the transmitting termination point."
            DEFVAL { "" }
            ::= { xfSDHsonetMediumXEntry 3 }

        
        xfSDHsonetMediumLineAutoDetect OBJECT-TYPE
            SYNTAX INTEGER
                {
                disabled(1),
                enabled(2),
                notApplicable(3)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to determine if the line detected or the line 
                configured by external management is used.
                1 -> disabled
                2 -> enabled 
                3 -> notApplicable
                For Section the default value is enabled, for all 
                other layers the default value is notApplicable.
                "
            ::= { xfSDHsonetMediumXEntry 4 }

        
        xfSDHsonetMediumLineOperStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                noLineSelected(1),
                automaticElectrical(2),
                automaticOptical(3),
                manualElectrical(4),
                manualOptical(5)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Used to show which line that is used.
                The information is only valis for Section layer
                1 -> No Line is selected
                2 -> Automatic detection of Electrical line
                3 -> Automatic detection of Optical line
                4 -> Manual set of Electrical line
                5 -> Manual set of Optical line
                "
            ::= { xfSDHsonetMediumXEntry 5 }

        
        xfSDHsonetMediumLaserMode OBJECT-TYPE
            SYNTAX INTEGER
                {
                unKnown(0),
                other(1),
                manualOn(2),
                manualOff(3),
                alsAuto(4),
                alsManualRestart(5),
                alsManualRestartForTest(6),
                alsManualRestartRLSpecific(7)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "unKnown (0)
                 other (1)
                 manualOn (2) (SDH default)
                 manualOff (3)
                 alsAuto (4) automatic laser shutdown
                 alsManualRestart (5) manual restart, transmitter on for 2 seconds
                 alsManualRestartForTest (6) manual restart for test, transmitter on for 90 seconds
                 alsManualRestartRLSpecific (7) proprietary manual restart for test, transmitter on for 12 seconds
                  
                 ifAdminStatus turns off the laser regardless of xfSDHsonetMediumLaserMode setting.
                 In protection the laser is controlled by the equipment protection logic if ifAdminStatus
                 is up and if xfSDHsonetMediumLaserMode is alsAuto.
                 
                 Only applicable to Line RS.
                "
            ::= { xfSDHsonetMediumXEntry 6 }

        
        xfSDHsonetMediumTrailTraceIdMode OBJECT-TYPE
            SYNTAX INTEGER
                {
                passThrough(0),
                singleValueMode(1),
                stringValueMode(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Mode of the TTI."
            ::= { xfSDHsonetMediumXEntry 7 }

        
        xfSDHsonetMediumTrailTraceC1Expect OBJECT-TYPE
            SYNTAX Integer32 (0..255)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The expected TTI value when in single mode
                Meaningful only when xfSDHsonetMediumTrailTraceIdMode 
                contains the singleValueMode value.
                "
            ::= { xfSDHsonetMediumXEntry 8 }

        
        xfSDHsonetMediumTrailTraceC1Receive OBJECT-TYPE
            SYNTAX Integer32 (0..255)
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The received TTI value when in single mode. Meaningful only when 
                xfSDHsonetMediumTrailTraceIdMode contains the singleValueMode 
                value."
            ::= { xfSDHsonetMediumXEntry 9 }

        
        xfSDHsonetMediumTrailTraceC1Transmit OBJECT-TYPE
            SYNTAX Integer32 (0..255)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The transmitted TTI value when in single mode. Meaningful only when 
                xfSDHsonetMediumTrailTraceIdMode contains the singleValueMode value.
                "
            ::= { xfSDHsonetMediumXEntry 10 }

        
        xfSDHsonetMediumInsertAISOnJ0 OBJECT-TYPE
            SYNTAX INTEGER
                {
                unknown(0),
                other(1),
                disabled(2),
                enabled(3)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Default disabled(2)"
            ::= { xfSDHsonetMediumXEntry 11 }

       
        xfSDHsonetMediumInsertAISOnBER OBJECT-TYPE
            SYNTAX INTEGER
                {
