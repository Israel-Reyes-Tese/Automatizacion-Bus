sonusRogueMediaBySrcPolicerDiscardRingTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusRogueMediaBySrcPolicerDiscardRingEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The table of statistics for the rogue media by source
             policer discard ring. The table is used to identify the
             source VLAN/IP address initiating an attack."
    ::= { sonusIpmMIBObjects 8 }

sonusRogueMediaBySrcPolicerDiscardRingEntry OBJECT-TYPE
    SYNTAX          SonusRogueMediaBySrcPolicerDiscardRingEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The rogue media discard ring entries."
    INDEX           { sonusRogueMediaBySrcPolicerDiscardRingShelfIndex, 
                      sonusRogueMediaBySrcPolicerDiscardRingSlotIndex, 
                      sonusRogueMediaBySrcPolicerDiscardRingPositionIndex }
    ::= { sonusRogueMediaBySrcPolicerDiscardRingTable 1 }

SonusRogueMediaBySrcPolicerDiscardRingEntry ::=
    SEQUENCE {
        sonusRogueMediaBySrcPolicerDiscardRingShelfIndex                SonusShelfIndex,
        sonusRogueMediaBySrcPolicerDiscardRingSlotIndex               SonusSlotIndex,
        sonusRogueMediaBySrcPolicerDiscardRingPositionIndex                   Gauge,
        sonusRogueMediaBySrcPolicerDiscardRingNIFSIFName                SonusNameReference,
        sonusRogueMediaBySrcPolicerDiscardRingSourceIpAddress                     IpAddress,
        sonusRogueMediaBySrcPolicerDiscardRingDestIpAddress                   IpAddress,
        sonusRogueMediaBySrcPolicerDiscardRingDiscardCount                  Counter,
        sonusRogueMediaBySrcPolicerDiscardRingType          INTEGER,
        sonusRogueMediaBySrcPolicerDiscardRingSourceIpPort                  Gauge,
        sonusRogueMediaBySrcPolicerDiscardRingDestIpPort                Gauge,
        sonusRogueMediaBySrcPolicerDiscardRingDestUnique                INTEGER,
        sonusRogueMediaBySrcPolicerDiscardRingSourceIp6Address                      Ipv6Address,
        sonusRogueMediaBySrcPolicerDiscardRingSourceIpVer                 INTEGER,
        sonusRogueMediaBySrcPolicerDiscardRingDestIp6Address                    Ipv6Address,
        sonusRogueMediaBySrcPolicerDiscardRingDestIpVer               INTEGER
    }

sonusRogueMediaBySrcPolicerDiscardRingShelfIndex OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The shelf index into this table."
    ::= { sonusRogueMediaBySrcPolicerDiscardRingEntry 1 }

sonusRogueMediaBySrcPolicerDiscardRingSlotIndex OBJECT-TYPE
