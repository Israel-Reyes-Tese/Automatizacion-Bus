XF-GFP-MIB DEFINITIONS ::= BEGIN
    IMPORTS
        MODULE-IDENTITY,
        OBJECT-TYPE,
        Integer32,
        Counter32,
        Counter64,
        Unsigned32
            FROM SNMPv2-SMI

        TimeStamp, DateAndTime,
        TEXTUAL-CONVENTION,
        TruthValue
            FROM SNMPv2-TC

        ifIndex
            FROM IF-MIB

        PerfCurrentCount, PerfIntervalCount
            FROM PerfHist-TC-MIB

        xfSdhAdm
            FROM XF-TOP-MIB;


    xfGfpMib MODULE-IDENTITY
       LAST-UPDATED    "200802281200Z"    -- Feb. 28, 2008 at 12:00 GMT
       ORGANIZATION    "Ericsson Communications Germany"
       CONTACT-INFO
          "Herbert Leuwer
           Postal: Ericsson
           Gerberstrasse 33
           71522 Backnang
           Germany
           Phone:  +49 7191/13-3676
           Email:  herbert.leuwer@ericsson.com"
       DESCRIPTION
           "This MIB module describes specific objects for Generica Frame
           Procedure (GFP) functionality in the Ericsson TN product."
       REVISION     "200802281200Z"    -- Feb. 28, 2008 at 12:00 GMT
       DESCRIPTION
           "REV PA2: Use official MIB module OID (Jens Engel)."
       REVISION         "200712181000Z"
       DESCRIPTION
           "Cleanup (Jens Engel):
             - xfGfpStatisticsMngtFrames...: Use Counter32 instead Integer32.
             - xfGfpPmCurrent15Min... counts: Use PerfCurrentCount  (was: Integer32).
             - xfGfpPmInterval...     counts: Use PerfIntervalCount (was: Integer32).
             - XfCounterThreshold15Min: Use basetype Unsigned32 (was: Integer32).
            Correction:
             - xfGfpPmIntervalNumber: Revert to range 1.96 (was: 1..16, reintroduced).
           "
       REVISION         "200712061000Z"
       DESCRIPTION
           "Bugfix (Herbert Leuwer):
             - Added reset for current intervals.
             - PM timestamp per interval per interface added
             - Last PM configuration time stamp added.
            Cleanup comitted (Herbert Leuwer).
             - Separated general configuration from PM configuration.
             - ValidData is now replaced by SuspectInterval."
       REVISION         "200711291000Z"
       DESCRIPTION
           "Cleanup (Jens Engel):
            - BOOLEAN-ENUMERITIS: Replace XfYesNo, XfEnabledDisabled with TruthValue.
            - Provide DEFVAL for writable boolean MOs (...TtpConfigLcasEnable).

            MODIFICATION:
             - Change xfGfpPmIntervalNumber range: 1..96 (was: 1..16)"
       REVISION         "200711061000Z"
       DESCRIPTION
           "Cleanup (Jens Engel):
             - Remove unused-imports
             - Remove unused-types (XfImplemented, XfOnOff)
             - Replace type-alias with TEXTUAL-CONVENTION (required by SMIv2).
             - integer-misuse: Use Integer32 instead of INTEGER for non-enum types."
       REVISION         "200707090800Z"
       DESCRIPTION
           "Initial Version 1.1"

     ::= { xfSdhAdm 7 }

    xfGfpMibObjects           OBJECT IDENTIFIER ::= { xfGfpMib 1 }
    xfGfpMibNotifications     OBJECT IDENTIFIER ::= { xfGfpMib 2 }
    xfGfpMibConformance       OBJECT IDENTIFIER ::= { xfGfpMib 3 }



--
-- Local data types
--
     XfGfpUpi ::= TEXTUAL-CONVENTION
         STATUS current
         DESCRIPTION
             "User Payload Identifier UPI."
         SYNTAX INTEGER
         {
           frameMappedEthernet (1)
         }

    XfGfpExi ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION
            "GFP Extension Header Identifier EXI"
        SYNTAX INTEGER {
          nullExtensionHeader (0)
        }

    XfCounterThreshold15Min ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION
            "Threshold for number of events within a 15 minute time period."
        SYNTAX Unsigned32

    TimeElapsed15Min ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION
            "Used for PM data time elapsed in seconds for a 15 minute period."
        SYNTAX Integer32 (0..900)

    TimeElapsed24Hour ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION
            "Used for PM data time elapsed in seconds for a 24 hour period."
        SYNTAX Integer32 (0..86400)


-------------------------------------------------------------------------------
-- MIB Object Definitions
-------------------------------------------------------------------------------

    xfGfpConfig     OBJECT IDENTIFIER ::= { xfGfpMibObjects 1 }
    xfGfpStatus     OBJECT IDENTIFIER ::= { xfGfpMibObjects 2 }
    xfGfpStatistics OBJECT IDENTIFIER ::= { xfGfpMibObjects 3 }
    xfGfpPm         OBJECT IDENTIFIER ::= { xfGfpMibObjects 4 }

    --------------------------------------------------------------
    -- GFP config group tables.
    --------------------------------------------------------------


    xfGfpConfigTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF XfGfpConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "This table contains the GFP configuration entries for all GFP
            interfaces."
        ::= { xfGfpConfig 1 }

    xfGfpConfigEntry OBJECT-TYPE
        SYNTAX     XfGfpConfigEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "The entry contains GFP configuration parameters for one GFP
            interface."
        INDEX { ifIndex }
        ::= { xfGfpConfigTable 1 }

    XfGfpConfigEntry ::= SEQUENCE
    {
        xfGfpConfigUserPayloadIdentifier          XfGfpUpi,
        xfGfpConfigFcsEnable                      TruthValue,
        xfGfpConfigExtensionHeaderIdentifier      XfGfpExi,
        xfGfpConfigDegThresholdPayloadCrcFail     XfCounterThreshold15Min,
        xfGfpConfigDegThresholdHecUncorrelated    XfCounterThreshold15Min,
        xfGfpConfigAlarmStatusEnable              BITS
    }

    xfGfpConfigUserPayloadIdentifier OBJECT-TYPE
        SYNTAX     XfGfpUpi
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
            "This object is used to setup the User Payload Identifier UPI."
        DEFVAL     { frameMappedEthernet }
        ::= { xfGfpConfigEntry 1 }

   xfGfpConfigFcsEnable OBJECT-TYPE
        SYNTAX     TruthValue
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
           "This object is used to enable or disable FCS generation and check on a GFP
           interface."
        DEFVAL { false }
        ::= { xfGfpConfigEntry 2 }

   xfGfpConfigExtensionHeaderIdentifier OBJECT-TYPE
       SYNTAX XfGfpExi
       MAX-ACCESS read-write
       STATUS     current
       DESCRIPTION
           "This object is used to setup the Extension Header Identifier EXI."
       DEFVAL     { nullExtensionHeader }
       ::= { xfGfpConfigEntry 3 }

   xfGfpConfigDegThresholdPayloadCrcFail OBJECT-TYPE
        SYNTAX XfCounterThreshold15Min
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
           "This object contains the threshold for reporting of DEG alarm for
            bad frames received. The reporting is based on payload CRC calculation.
            The threshold is the number of bad frames per 15 minutes before
            reporting DEG alarms.
            Deg alarm goes off when a full 15 minutes period has been passed,
            with the number of bad frames lower than one tenth of this number."
        DEFVAL { 10 }
        ::= { xfGfpConfigEntry 4 }

    xfGfpConfigDegThresholdHecUncorrelated OBJECT-TYPE
       SYNTAX XfCounterThreshold15Min
       MAX-ACCESS read-write
       STATUS     current
       DESCRIPTION
            "This object contains the threshold for reporting of DEG alarm for
            number of HEC uncorrelated errors. The threshold is the number of
            bad frames per 15 minutes before reporting DEG alarms.
            Deg alarm goes off when a full 15 minutes period has been passed, with the
            number of HEC uncorrelated errors lower than one tenth of this number."
        DEFVAL { 10 }
        ::= { xfGfpConfigEntry 5 }

    xfGfpConfigAlarmStatusEnable OBJECT-TYPE
        SYNTAX BITS
        {
          exm(0),
          pfm(1),
          lfd(2),
          ptm(3),
          upm(4),
          clos(5),
          degFCS(6),
          degtHEC(7)
        }
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
           "This object is used to enable or disable alarm reporting for
           GFP related alarms."
        DEFVAL { {} }   -- ZERO: ALL-BITS-CLEARED.
        ::= { xfGfpConfigEntry 6 }

    --------------------------------------------------------------
    -- GFP status group tables.
    --------------------------------------------------------------

    xfGfpStatusTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF XfGfpStatusEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "This table contains the GFP status information for all GFP
            interfaces."
        ::= { xfGfpStatus 1 }

    xfGfpStatusEntry OBJECT-TYPE
        SYNTAX     XfGfpStatusEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "The entry contains GFP status information for one GFP interface."
        INDEX      { ifIndex }
        ::= { xfGfpStatusTable 1 }

    XfGfpStatusEntry ::= SEQUENCE
        {
            xfGfpStatusAlarmStatus BITS
        }

    xfGfpStatusAlarmStatus OBJECT-TYPE
        SYNTAX     BITS
           {
             exm0(0),
             exm1(1),
             exm2(2),
             pfm0(3),
             pfm1(4),
             pfm2(5),
             lfd0(6),
             lfd1(7),
             lfd2(8),
             ptm0(9),
             ptm1(10),
             ptm2(11),
             upm0(12),
             upm1(13),
             upm2(14),
             clos0(15),
             clos1(16),
             clos2(17),
             degFCS0(18),
             degFCS1(19),
             degFCS2(20),
             degtHEC0(21),
             degtHEC1(22),
             degtHEC2(23)
           }
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
            "This objects provides information on the alarm status of a
            GFP interface.
            The severity is indicated by three bits per alarm:
              bits 012
                   000   clear
                   001   undetermined
                   010   warning
                   011   minor
                   100   major
                   101   critical
           The following severities are assigned to the alarms:

           exm     Extended Header mismatch          => major
           pfm     Payload FCS Identifier mismatch   => major
           lfd     Loss of Frame Delineation         => critical
           ptm     Payload Type Identifier mismatch  => major
           upm     User Payload Identifier mismatch  => major
           clos    Client Loss of Signal             => critical
           degFCS  Degrade FCS                       => critical
           degtHEC Degrade HEC                       => critical

           Critical alarms also lead to a linkDown condition.
           "
       ::= { xfGfpStatusEntry 1}

