    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Pakistan fixed Charge Band Number parameter value. \nUsed in the Pakistan-ITU converter only for default settings. Valid range is 0 to 255."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 4 }

ptL4CvtrL4CvtrConfigurationFastAnswerTimer OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Lifespan of Fast Answer record (in seconds). \nUsed in the ANSI-ITU converter only for ACM and ANM conversion. \nSet to 0 (DISABLED) to disable the Fast Answer feature. Default 60 seconds. Valid range is 0 to 999999"
    ::= { ptL4CvtrL4CvtrConfigurationEntry 5 }

ptL4CvtrL4CvtrConfigurationCGAckTimer OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Lifespan of Circuit Group Acknowledgement record (in seconds). \nUsed in the ANSI-ITU converter only for circuit group acknowledgement message management due to circuit range incompatibility between the variants. \nDefault 60 seconds. Valid range is 1 to 999999."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 6 }

