            "SDC capability, including both MMU and RAU."
        ::= { xfModemSdcEntry 1 }

    xfMMUSdcAdminStatus OBJECT-TYPE
        SYNTAX INTEGER
        {
            other(1),
            sdcEnabled(2),
            sdcDisabled(3)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "SDC Admin Status."
        ::= { xfModemSdcEntry 2 }

    xfMMUSdcOperStatus OBJECT-TYPE
        SYNTAX INTEGER
        {
            other(1),
            sdcEnabled(2),
            sdcDisabled(3),
            sdcInProgress(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "SDC Oper Status."
        ::= { xfModemSdcEntry 3 }

    xfMMUSdcDadeControl OBJECT-TYPE
        SYNTAX INTEGER
        {
            other(1),
            sdcDadeStart(2),
            sdcDadeAbort(3)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "SDC DADE procedure control."
        ::= { xfModemSdcEntry 4 }

    xfMMUSdcDadeStatus OBJECT-TYPE
        SYNTAX INTEGER
        {
            other(1),
            sdcCalibrated(2),
            sdcNotCalibrated(3),
            sdcCalInProgress(4),
            sdcCalMismatch(5)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "SDC DADE procedure status."
        ::= { xfModemSdcEntry 5 }

    xfMMUSdcPath OBJECT-TYPE
        SYNTAX INTEGER
        {
            other(1),
            sdcAuto(2),
            sdcMain(3),
            sdcDiv(4)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "SDC selected path (desired value)."
        ::= { xfModemSdcEntry 6 }

    xfTRXSdcDateTimeCalibration OBJECT-TYPE
        SYNTAX DisplayString (SIZE (0..255))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
