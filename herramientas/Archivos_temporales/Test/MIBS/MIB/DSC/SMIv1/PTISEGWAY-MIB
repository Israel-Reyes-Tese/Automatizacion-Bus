    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Diameter ID associated to the Server Pool. \nValid values: FQDN format."
    ::= { ptDSCMemberDiameterIDEntry 1 }

ptDSCMemberDiameterIDWeight OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Weight of the Diameter ID (0 to 400000). \nThe Diameter ID Weight is used to indicate the relative message capacity of this Diameter ID when load sharing on the server pool. \nValid values: 0 to 100 (ROUND ROBIN) \n 0 to 4000000 (LEAST LOADED). \nDefault value: 10 (ADN) and 0 (non-ADN) (ROUND ROBIN) \n 2000 (ADN) and 0 (non-ADN) (LEAST LOADED)"
    ::= { ptDSCMemberDiameterIDEntry 2 }

ptDSCMemberDiameterIDInternalID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Member Diameter ID internal ID. \nReserved for internal use."
    ::= { ptDSCMemberDiameterIDEntry 3 }

ptDSCMemberDiameterIDCost OBJECT-TYPE
    SYNTAX          INTEGER (1..99)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Cost of the result record (1 to 99). \nLowest available record is used for routing. \nIf more than one record have the lowest available cost, loadsharing is performed."
    ::= { ptDSCMemberDiameterIDEntry 4 }

