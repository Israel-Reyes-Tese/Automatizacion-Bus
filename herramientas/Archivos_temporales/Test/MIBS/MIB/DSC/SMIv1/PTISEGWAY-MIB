    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The row number for the DSCMobileCodesDefinitions table"
    ::= { ptDSCMobileCodesDefinitionsEntry 255 }

ptDSCIMSIMobileCode OBJECT IDENTIFIER
    ::= { ptDSCMobileCodesDefinitions 1862 }

