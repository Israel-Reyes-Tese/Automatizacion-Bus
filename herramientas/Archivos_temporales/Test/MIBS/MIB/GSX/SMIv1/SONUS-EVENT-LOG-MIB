    DEFVAL          { 0 }
    ::= { sonusDflushNetworkFailureEventStatEntry 4 }

sonusNfoInterfaceStatReason OBJECT-TYPE
    SYNTAX          INTEGER { nfsOOS(0), lnkFail(1), lnkCong(2) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This object defines the stat reason"
    DEFVAL          { 1 }
    ::= { sonusDflushNetworkFailureEventStatEntry 5 }

sonusNfoInterfaceStatStartTime OBJECT-TYPE
    SYNTAX          DateAndTime
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Time when the NFO started for this type of event
             log"
    ::= { sonusDflushNetworkFailureEventStatEntry 6 }

sonusNfoInterfaceStatStopTime OBJECT-TYPE
    SYNTAX          DateAndTime
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Time when the NFO stopped for this type of event
             log"
    ::= { sonusDflushNetworkFailureEventStatEntry 7 }

