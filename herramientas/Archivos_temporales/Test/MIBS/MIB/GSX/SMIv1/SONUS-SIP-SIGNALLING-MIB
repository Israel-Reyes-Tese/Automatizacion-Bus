--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that the SIP Service Group was put
             into service by maintenance staff."
    ::= 18

sonusSipTranspProfileOOSNotification TRAP-TYPE
    ENTERPRISE      sonusSipSignallingMIBNotifications
    VARIABLES       { sonusSipTranspProfileName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that the SIP Service Group was taken
             out of service by maintenance staff."
    ::= 19

END -- end of module SONUS-SIP-SIGNALLING-MIB.
