                    Integer32
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.1.1.1
        xfEthernetServiceId OBJECT-TYPE
            SYNTAX XfEthernetServiceId
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Identifies a service.
                
                See the textual convention 'XfEthernetServiceId'."
            ::= { xfEthernetServiceTcEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.1.1.2
        xfEthernetServiceInstance OBJECT-TYPE
            SYNTAX XfEthernetServiceInstance
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Identifies an instance of a service.
                
                See the textual convention XfEthernetServiceInstance."
            ::= { xfEthernetServiceTcEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.1.1.3
        xfEthernetServiceTc OBJECT-TYPE
            SYNTAX Integer32 (0..7)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Identifies a traffic class."
            ::= { xfEthernetServiceTcEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.1.1.4
        xfEthernetServiceTcAging OBJECT-TYPE
            SYNTAX Integer32 (0..100)
            UNITS "milliseconds"
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies the aging parameter for a traffic class
                which is used to drop old packets.
                
                If xfEthernetServiceTcAgingEnable is not available, then setting this
                object to 0 turns aging off."
            ::= { xfEthernetServiceTcEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.1.1.5
        xfEthernetServiceTcYellowLimit OBJECT-TYPE
            SYNTAX Integer32 (0..100)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies a threshold for yellow packets. Yellow packets arriving
                after this threshold is reached will be dropped.
                
                It is given in percentage of the buffer size of the traffic class.
                
                Default value is 25%.
                
                For UNI ports/interfaces this will be set to 25%.
                "
            ::= { xfEthernetServiceTcEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.1.1.6
        xfEthernetServiceTcColorDropEnable OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to enable or disable color dropping in a traffic class.
                
                If true color dropping is enable, if false color dropping is
                disabled.
                
                Default is false(2).
                
                For UNI ports/interfaces this will be set to false(2).
                "
            ::= { xfEthernetServiceTcEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.1.1.7
        xfEthernetServiceTcWredEnable OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to enable or disable WRED on a traffic class.
                
                If true(1) WRED is enabled, if false(2) WRED is disabled.
                
                Default is false(i.e. disabled).
                
                For UNI ports/interfaces this will be set to false(2)."
            ::= { xfEthernetServiceTcEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.1.1.8
        xfEthernetServiceTcWredProfile OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Points to WRED parameters in xfEthernetWredProfileTable
                 (xfEthernetWredProfileIndex).
                
                By default this is set to 1, i.e. 'Standard WRED'
                
                For UNI ports/interfaces this will be set to 1,i.e. 'Standard WRED'."
            ::= { xfEthernetServiceTcEntry 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.1.1.9
        xfEthernetServiceTcWfqWeight OBJECT-TYPE
            SYNTAX Integer32 (1..100)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies the weight to be used when the traffic class is
                served as WFQ (i.e. xfTcScheduler='wfq').
                
                Range 1 - 100 in steps of 1.
                
                Default is the value copied from xfEthernetSchedulerDefaultWeight.
                
                For UNI ports/interfaces this is not applicable since they use
                the All strict priority' scheduler profile."
            ::= { xfEthernetServiceTcEntry 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2
        xfEthernetServiceTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfEthernetServiceEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table contains attributes on service level."
            ::= { xfEthernetService 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2.1
        xfEthernetServiceEntry OBJECT-TYPE
            SYNTAX XfEthernetServiceEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of a service."
            INDEX { xfEthernetServiceId, xfEthernetServiceInstance }
            ::= { xfEthernetServiceTable 1 }

        XfEthernetServiceEntry ::=
            SEQUENCE {
                xfEthernetServiceVariant
                    Integer32,
                xfEthernetServiceSchedulerProfile
                    Integer32,
                xfEthernetServiceConsistentConfigured
                    TruthValue,
                xfEthernetServiceProtectionVariant
                    Integer32,
                xfEthernetServiceHqosProfile
                    Integer32,
                xfEthernetServiceHqosDefaultQueueSetProfile
                    Integer32,
                xfEthernetServiceHqosDefaultSchedulerProfile
                    Integer32,
                xfEthernetServiceTcAgingEnable
                    INTEGER
                    }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2.1.1
        xfEthernetServiceVariant OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Shows the variant of an Ethernet Service (e.g. Bridge or L1_EPL).
                E.g. For a Bridge, 2 represents BRIDGEv2.
                
                See the object model diagram in 2/15519-csh10932/1 (Ethernet SNMP IWD) for
                details on the capability of a certain variant of a service. The variant is added
                to the class name,e.g BRIDGEv1, EPLv2 etc.
                
                For Bridge:
                
                  variant 1 : Start value
                  variant 2 : Replaces variant 1
                  variant 3 : Replaces variant 2, Added packet aging
                  variant 4 : Replaces variant 3, Added pcpSelection, policing and jumbo frames
                  variant 5 : Replaces variant 4, Added MSTP
                  variant 6 : Replaces variant 5, Added Provider Bridge
                  variant 7 : Replaces variant 6, Added QinQ for Customer Bridge
                
                For EPL:
                  variant 1 : Start value
                  variant 2 : Replaces variant 1
                  variant 3 : Replaces variant 2, Added packet aging
                  variant 4 : Replaces variant 3, Added pcpSelection, LANv10-12 and WANv4
                  variant 5 : Replaces variant 4, Added L2CP configuration"
            ::= { xfEthernetServiceEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2.1.2
        xfEthernetServiceSchedulerProfile OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Points to scheduler parameters in xfEthernetSchedulerProfileTable
                (xfEthernetSchedulerProfileIndex) and xfEthernetSchedulerTcParamsTable.
                
                By default this is set to 1, i.e. 'All strict priority'.
                
                For UNI ports/interfaces this will be set to 1,i.e. 'All strict priority'."
            ::= { xfEthernetServiceEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2.1.3
        xfEthernetServiceConsistentConfigured OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "This shows whether all members of the service have
                inherited their parameters from service or not.
                
                Boolean.
                
                   - True means  that all members have inherited values from service,
                      i.e. all members are locked to the service
                   - False means that there is one or more service member
                    that has not inherited from service."
            ::= { xfEthernetServiceEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2.1.4
        xfEthernetServiceProtectionVariant OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Shows the variant for Ethernet Service protection.
                
                  variant 1 : support of NPU switch protection"
            ::= { xfEthernetServiceEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2.1.5
        xfEthernetServiceHqosProfile OBJECT-TYPE
            SYNTAX Integer32 (1..32)
            MAX-ACCESS read-write
            STATUS deprecated
            DESCRIPTION
                "Points to hqos-scheduler parameters in xfEthernetHqosProfileTable.
                
                By default this is set to profile 1.
                "
            ::= { xfEthernetServiceEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2.1.6
        xfEthernetServiceHqosDefaultQueueSetProfile OBJECT-TYPE
            SYNTAX Integer32  (1..32)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Points to queue discipline parameters in xfEthernetQueueSetProfileTable.
                
                By default this is set to 1, i.e. 'Taildrop for each class'."
            ::= { xfEthernetServiceEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2.1.7
        xfEthernetServiceHqosDefaultSchedulerProfile OBJECT-TYPE
            SYNTAX Integer32 (1..8)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Points to scheduler parameters in xfEthernetSchedulerProfileTable
                
                By default this is set to 1, i.e. 'All strict priority'.
                
                For UNI ports/interfaces this will be set to 1, i.e. 'All strict priority'."
            ::= { xfEthernetServiceEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.8.2.1.8
        xfEthernetServiceTcAgingEnable OBJECT-TYPE
            SYNTAX INTEGER
                {
                disable(1),
                enable(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Enable/Disable packet aging for all TCs.
                
                disable(1) : disable Packet Aging.
                enable(2) : enable Packet Aging.
                
                Default is disable(1)."
            ::= { xfEthernetServiceEntry 8 }
 
        -- 1.3.6.1.4.1.193.81.4.1.2.1.9
        xfVlan OBJECT IDENTIFIER ::= { xfEthernetBridgeObjects 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.1
        xfVlanTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfVlanEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An extension to VLAN tables."
            ::= { xfVlan 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.1.1
        xfVlanEntry OBJECT-TYPE
            SYNTAX XfVlanEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of a VLAN."
            INDEX { dot1qVlanIndex }
            ::= { xfVlanTable 1 }

        XfVlanEntry ::=
            SEQUENCE {
                xfVlanMstiId                          Integer32,
                xfVlanForwardUnregisteredMulticast    TruthValue,
                xfVlanStaticEgressPortsTest           OCTET STRING,
                xfVlanL3IfEnable                      INTEGER,
                xfVlanL3IfIndex                       InterfaceIndexOrZero,
                xfVlanMacLearningEnable               TruthValue,
                xfVlanStatisticsEnable                TruthValue
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.1.1.1
        xfVlanMstiId OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The MST instance to which the VLAN belongs to."
            ::= { xfVlanEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.1.1.2
        xfVlanForwardUnregisteredMulticast OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies whether multicast group-addressed frames for
                which there is no more specific forwarding information
                are to be forwarded to all members of the VLAN or not.
                
                true(1) means forward, false(2) means don't forward.
                
                Default is true(1)."
            ::= { xfVlanEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.1.1.3
        xfVlanStaticEgressPortsTest OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object behaves as dot1qVlanStaticEgressPorts but
                ONLY TEST PHASE is executed when this object is set
                (i.e. dot1qVlanStaticEgressPorts remains unchanged)."
            ::= { xfVlanEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.1.1.4
        xfVlanL3IfEnable OBJECT-TYPE
            SYNTAX INTEGER
                {
                l3Enabled(1),
                l3Disabled(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object is used to configure the VLAN to create
                or remove corresponding L3 interface.
                
                Default is l3Disabled(2)."
            ::= { xfVlanEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.1.1.5
        xfVlanL3IfIndex OBJECT-TYPE
            SYNTAX InterfaceIndexOrZero
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "This object indicates the L3 interface connected to this VLAN."
            ::= { xfVlanEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.1.1.6
        xfVlanMacLearningEnable OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Enable MAC learning in a specific VLAN.
                
                 By default its value is true(1). If you set it false(2) MAC learning in this VLAN will be disabled."
                
            ::= { xfVlanEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.1.1.7
        xfVlanStatisticsEnable OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object is used to configure the VLAN statistics collection."
                
            ::= { xfVlanEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2
        xfVlanRangeConfigureScalarObjects OBJECT IDENTIFIER ::= { xfVlan 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2.1
        xfVlanRangeIdStart OBJECT-TYPE
            SYNTAX Integer32 (2..4094)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "First VLAN ID in range"
            ::= { xfVlanRangeConfigureScalarObjects 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2.2
        xfVlanRangeIdEnd OBJECT-TYPE
            SYNTAX Integer32 (2..4094)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Last VLAN ID in range"
            ::= { xfVlanRangeConfigureScalarObjects 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2.3
        xfVlanRangeName OBJECT-TYPE
            SYNTAX SnmpAdminString (SIZE (0..32))
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "An administratively assigned string, which may be used
                to identify the VLAN."
            ::= { xfVlanRangeConfigureScalarObjects 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2.4
        xfVlanRangeEgressPorts OBJECT-TYPE
            SYNTAX PortList
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The set of ports that are permanently assigned to the
                 egress list for this VLAN by management. Changes to a
                 bit in this object affect the per-port, per-VLAN
                 Registrar control for Registration Fixed for the
                 relevant GVRP state machine on each port."
            ::= { xfVlanRangeConfigureScalarObjects 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2.5
        xfVlanRangeForwardUnregisteredMulticast OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies whether multicast group-addressed frames for
                 which there is no more specific forwarding information
                 are to be forwarded to all members of the VLAN or not.
                 
                 true(1) means forward, false(2) means don't forward.
                 
                 Default is true(1)."
            DEFVAL { true }
            ::= { xfVlanRangeConfigureScalarObjects 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2.6
        xfVlanRangeCommand OBJECT-TYPE
            SYNTAX INTEGER
                {
                idle(1),
                create(2),
                modify(3),
                remove(4),
                modifyEgressPorts(5)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Setting this to something other than Idle will start the execution.
                 Can't be set while Status is InProgress.

                 idle(1): No VLAN range related configuration will take place.
                 create(2): Create a range of VLANs with the specified parameters.
                 modify(3): Modify a range of VLANs with the specified parameters. This
                 will use every currently configured values (name, egress ports,
                 unregistered multicast) for the modification.
                 remove(4): Remove a range of VLANs.
                 modifyEgressPorts(5): Modify only the egress ports of the VLAN range."
            DEFVAL { idle }
            ::= { xfVlanRangeConfigureScalarObjects 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2.7
        xfVlanRangeStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                inProgress(1),
                done(2),
                failed(3)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Status of current command execution"
            DEFVAL { done }
            ::= { xfVlanRangeConfigureScalarObjects 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2.8
        xfVlanRangeAction OBJECT-TYPE
            SYNTAX INTEGER
                {
                add(1),
                remove(2),
                overwrite(3)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Defines the action that should be executed on the range of VLANs.
                    Possible actions:
                        - Add: Add every interface, specified by xfVlanRangeEgressPorts
                        to the range of VLANs. Only those interfaces will be added to
                        the VLAN, that are not a member of it yet. This means, that if
                        a VLAN has three members, [1, 2, 3] and interfaces [2, 5] are
                        added to it, then the mentioned VLAN would look like [1, 2, 3, 5]
                        after the modification.
                        - Remove: Remove every interface, specified by
                        xfVlanRangeEgressPorts from the range of VLANs. It is not required
                        that every interface, specified by xfVlanRangeEgressPorts should
                        be a member of a VLAN. This means, that if a VLAN has three members,
                        [1, 2, 3] and interfaces [2, 5] are removed from it, then the
                        mentioned VLAN would look like [1, 3] after the modification.
                        - Overwrite: Overwrite every member in the range of VLANs to the
                        ones, specified by xfVlanRangeEgressPorts. If there are 2 VLANs,
                        [1, 2, 3] and [4, 5] and [8, 9] is specified as the value to be
                        overwritten with, then both VLANs will contain [8, 9] as their
                        members."
            DEFVAL { add }
            ::= { xfVlanRangeConfigureScalarObjects 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.9.2.9
        xfVlanRangeIds OBJECT-TYPE
            SYNTAX SnmpAdminString (SIZE (3..100))
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Defines a range of VLANs, where the specified action (xfVlanRangeAction)
                should be executed on. Accepted values are a VLAN, a VLAN range (two number,
                separated by -), or any combination of these, separated by commas. E.g.:
                    - 3-5
                    - 3-5, 8
                    - 3-5, 8, 10-15"
            ::= { xfVlanRangeConfigureScalarObjects 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10
        xfEthernetProfiles OBJECT IDENTIFIER ::= { xfEthernetBridgeObjects 10 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10.1
        xfEthernetBandwidthProfileTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfEthernetBandwidthProfileEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A bandwidth profile table used for policing.
                
                There are two read-only profiles that can not
                be removed or modified.
                
                xfBridgeBandwidthProfileIndex=1:
                       'No rate limiting/drop none' profile
                       CIR=16384 (Mbps), CBS=128 (KB), EIR=0, EBS=128 (KB).
                
                xfBridgeBandwidthProfileIndex=2:
                  'Drop all' profile
                  CIR=0, CBS=128 (KB), EIR=0, EBS=128 (KB)."
            ::= { xfEthernetProfiles 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10.1.1
        xfEthernetBandwidthProfileEntry OBJECT-TYPE
            SYNTAX XfEthernetBandwidthProfileEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A bandwidth profile."
            INDEX { xfEthernetBandwidthProfileIndex }
            ::= { xfEthernetBandwidthProfileTable 1 }

        XfEthernetBandwidthProfileEntry ::=
            SEQUENCE {
                xfEthernetBandwidthProfileIndex
                    Integer32,
                xfEthernetBandwidthProfileName
                    SnmpAdminString,
                xfEthernetBandwidthProfileUsers
                    Integer32,
                xfEthernetBandwidthProfileCir
                    Integer32,
                xfEthernetBandwidthProfileCbs
                    INTEGER,
                xfEthernetBandwidthProfileEir
                    Integer32,
                xfEthernetBandwidthProfileEbs
                    INTEGER,
                xfEthernetBandwidthProfileCm
                    INTEGER,
                xfEthernetBandwidthProfileCf
                    INTEGER,
                xfEthernetBandwidthProfileStorageType
                    StorageType,
                xfEthernetBandwidthProfileRowStatus
                    RowStatus
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10.1.1.1
        xfEthernetBandwidthProfileIndex OBJECT-TYPE
            SYNTAX Integer32 (1..32)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An index to identify a profile."
            ::= { xfEthernetBandwidthProfileEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10.1.1.2
        xfEthernetBandwidthProfileName OBJECT-TYPE
            SYNTAX SnmpAdminString (SIZE (1..80))
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "The name of the profile. This name is unique and non-empty."
            ::= { xfEthernetBandwidthProfileEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10.1.1.3
        xfEthernetBandwidthProfileUsers OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Shows the current number of users of the profile.
                A profile that is currently in use, i.e. this value
                is greater than 0, can not be deleted."
            ::= { xfEthernetBandwidthProfileEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10.1.1.4
        xfEthernetBandwidthProfileCir OBJECT-TYPE
            SYNTAX Integer32 (0..156250)
            UNITS "64 Kbit/second"
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Committed Information Rate expressed as bits per second.
                
                Default value is 156250 (10Gbits/second) for NPU1D,
                15625 (1Gbits/second) on other NPUs, i.e. the same
                as 'drop none' profile."
            ::= { xfEthernetBandwidthProfileEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10.1.1.5
        xfEthernetBandwidthProfileCbs OBJECT-TYPE
            SYNTAX INTEGER
                {
                cbs0(0),
                cbs16K(1),
                cbs32K(2),
                cbs64K(3),
                cbs128K(4),
                cbs256K(5),
                cbs512K(6),
                cbs1024K(7),
                cbs2048K(8),
                cbs4096K(9),
                cbs8192K(10),
                cbs16384K(11)
                }
            UNITS "Bytes"
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Committed Burst Size expressed as bytes.
                
                When CIR>0, this must be greater than or equal to the maximum
                service frame size.
                
                Default cbs128K (same as 'drop none' profile)."
            ::= { xfEthernetBandwidthProfileEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10.1.1.6
        xfEthernetBandwidthProfileEir OBJECT-TYPE
            SYNTAX Integer32 (0..156250)
            UNITS "64Kbit/second"
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Excess Information Rate expressed as bits per second.
                
                Default value = 0 (same as 'drop none' profile)."
            ::= { xfEthernetBandwidthProfileEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.10.1.1.7
        xfEthernetBandwidthProfileEbs OBJECT-TYPE
            SYNTAX INTEGER
                {
