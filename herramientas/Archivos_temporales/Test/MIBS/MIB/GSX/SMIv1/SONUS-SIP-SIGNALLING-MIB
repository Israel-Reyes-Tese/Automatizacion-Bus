    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The next valid index to use when creating an entry in
             the sonusSctpProfileTable."
    ::= { sonusSctpProfile 1 }

sonusSctpProfileTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSctpProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "Table of SIP Signalling Port Sctp Profile data."
    ::= { sonusSctpProfile 2 }

sonusSctpProfileEntry OBJECT-TYPE
    SYNTAX          SonusSctpProfileEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "SIP Signalling Port SCTP configuration."
    INDEX           { sonusSctpProfileIndex }
    ::= { sonusSctpProfileTable 1 }

SonusSctpProfileEntry ::=
    SEQUENCE {
        sonusSctpProfileIndex           Gauge,
        sonusSctpProfileName            SonusName,
        sonusSctpProfileOutboundStreams Gauge,
        sonusSctpProfileInboundStreams  Gauge,
        sonusSctpProfilePeerTransportAddresses      Gauge,
        sonusSctpProfileRetransmissionsPerPath      Gauge,
        sonusSctpProfileHeartbeatInterval Gauge,
        sonusSctpProfileRowStatus       RowStatus,
        sonusSctpProfileOrderedDelivery SonusAdminState,
        sonusSctpProfileSackDelay       SonusAdminState,
        sonusSctpProfileRetransmissionsPerAssociation             Gauge,
        sonusSctpProfileRtoInitial      Gauge,
        sonusSctpProfileRtoMin          Gauge,
        sonusSctpProfileRtoMax          Gauge,
        sonusSctpProfileInitRetry       Gauge,
        sonusSctpProfilePathMtu         Gauge,
        sonusSctpProfileIdleTimeout     Gauge
    }

