    ::= { ptDSCDSCCandidateOutgoingADNTable 1 }

PtDSCDSCCandidateOutgoingADNEntry ::=
    SEQUENCE {
        ptDSCDSCCandidateOutgoingADNOrder Integer32,
        ptDSCDSCCandidateOutgoingADNCandidateADNName            DisplayString,
        ptDSCDSCCandidateOutgoingADNCandidateStatus           INTEGER,
        ptDSCDSCCandidateOutgoingADNDSCIndex    Integer32,
        ptDSCDSCCandidateOutgoingADNDSCRoutingTablesIndex                 Integer32,
        ptDSCDSCCandidateOutgoingADNDSCRoutingSearchUtilityIndex                        Integer32,
        ptDSCDSCCandidateOutgoingADNParentOID     OBJECT IDENTIFIER,
        ptDSCDSCCandidateOutgoingADNRowStatus     RowStatus,
        ptDSCDSCCandidateOutgoingADNIndex Integer32
    }

ptDSCDSCCandidateOutgoingADNOrder OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "the candidate ADN order where the DSC will try to send out the message. The smaller the order number, the early the DSC will try to send the message on that ADN."
    ::= { ptDSCDSCCandidateOutgoingADNEntry 1 }

ptDSCDSCCandidateOutgoingADNCandidateADNName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "the candidate outgoing ADN Name where the test message will be routed to."
    ::= { ptDSCDSCCandidateOutgoingADNEntry 2 }

