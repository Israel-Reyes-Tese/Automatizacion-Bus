            DESCRIPTION
               "An indication of the Target MAC Address Field to be
                transmitted:
                The Maintenance association End Point Identifier of
                another MEP in the same Maintenance Association.
                This address will be used if the value of the column
                xfServiceOamPmSessionDestIsMepId is 'true'.
               "
            ::= { xfServiceOamPmSessionCfgEntry 4 }

        xfServiceOamPmSessionDestMacAddress OBJECT-TYPE
            SYNTAX      MacAddress 
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
               "The Target MAC Address Field to be transmitted: A unicast
                destination MAC address.
                This address will be used if the value of the column
                xfServiceOamPmSessionDestIsMepId is 'false'.
               "
            ::= { xfServiceOamPmSessionCfgEntry 5 }

        xfServiceOamPmSessionCosPriority OBJECT-TYPE
            SYNTAX      Unsigned32 (0..7)
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
               "Specifies the class of service measurement priority.
                The value can be set between 0 (highest) and 7 (lowest), 
                where the default value is 0.
               "
            ::= { xfServiceOamPmSessionCfgEntry 6 }

        xfServiceOamPmSessionSummaryInterval OBJECT-TYPE
            SYNTAX      INTEGER
                {
                interval15min(15)
                }
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
               "Specifies the summary interval of the Service Oam Pm Session. 
                Only 15min is supported.
               "
            ::= { xfServiceOamPmSessionCfgEntry 7 }

        xfServiceOamPmSessionConfidenceInterval OBJECT-TYPE
            SYNTAX      Unsigned32 (1..100)
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
               "A DMM sent during LOC condition is declared as a NOT valid. 
                If the percentage of valid sent DMMs within the interval is not within the Confidence Interval 
                or no DMRs was received, the time interval shall be declared as NOT Valid.
                The Confidence Interval is a percentage value that represents the number of frames that 
                have to be processed into a valid interval. Default value for the Confidence Interval is 90%.
               "
            DEFVAL { 90 }
            ::= { xfServiceOamPmSessionCfgEntry 8 }

        xfServiceOamPmSessionFileCommand OBJECT-TYPE
            SYNTAX      INTEGER
                {
                pmFileRetrieve(1),
                pmFileAbort(2)
                }
            MAX-ACCESS  read-write
            STATUS      deprecated
            DESCRIPTION
               "Setting this object to pmFileRetrieve(1) will first create an XML file with selected
                contents according to xfServiceOamPmSessionFileCategory and then upload the file to
                the FTP server indicated by xfSoamPmActiveFTP. 
                The file will be placed in the FTP Server folder <drive:>\<ftp_root_directory>\tn_pm_data.

                A new upload cannot be started if xfPMFileStatus is in state transferActive(1).

                Value list:
                1: pmFileRetrieve(1)
                   - Upload the file.
                2: pmFileAbort(2)
                   - Cancel the file transfer.
               "
            ::= { xfServiceOamPmSessionCfgEntry 9 }

        xfServiceOamPmSessionFileStatus OBJECT-TYPE
            SYNTAX      INTEGER
                {
                transferActive(1),
                transferComplete(2),
                transferFailed(3),
                transferAborted(4),
                transferErrorBadFilename(5),
                transferErrorBadCommand(6),
                transferErrorNoData(7),
                errorInternalError(50),
                errorFileStorage(51),
                ftpPingFailed(52),
                ftpNoAccess(53),
                ftpConnectionDetailsMissing(54),
                ftpConnectionDetailsInvalid(55),
                ftpConnectionTimeout(56),
                ftpNoSuchRemoteFile(57),
                ftpNoSuchRemoteDir(58),
                ftpServiceNotAvailable(421),
                ftpUnableToOpenDataConnection(425),
                ftpConnectionClosed(426),
                ftpFileBusy(450),
                ftpLocalError(451),
                ftpInsufficientStorageSpace(452),
                ftpSyntaxError(501),
                ftpCommandNotImplemented(502),
                ftpBadSequenceCommands(503),
                ftpParameterNotImplemented(504),
                ftpNotLoggedIn(530),
                ftpNeedAccount(532),
                ftpFileUnavailable(550),
                ftpExceededStorageAllocation(552),
                ftpFileNameNotAllowed(553)
                }
            MAX-ACCESS  read-only
            STATUS      deprecated
            DESCRIPTION
               "Indicates the status of the file transfer to the FTP Server.

                Value list:
                1: transferActive(1)
                    - Set whilst the file is created and the transfer is active.
                2: transferComplete(2)
                     - Set if transfer is succeded.
                3: transferFailed(3) 
                    - May be set to this value if the transfer failed and more detailed error codes are not available.
                4: transferAborted(4) 
                    - Set if transfer aborted on command from Craft.
                5: transferErrorBadFilename(5)
                     - The filename in xfPMFileName is badly formed.
                6: transferErrorBadCommand(6)
                     - The value of xfPMFileCommand is out of range
                7: transferErrorNoData(7)
                     - There is no PM file to send.
                8: errorInternalError(50)
                     - An interal error detected before FTP operation.
                9: errorFileStorage(51)
                     - Error when erasing flash to store the downloaded file at the node.
                10: ftpPingFailed(52)
                     - FTP ping failed.
                11: ftpNoAccess(53)
                       - Not able to send the FTP commands.
                12: ftpConnectionDetailsMissing(54)
                       - FTP connection details are missing.
                13: ftpConnectionDetailsInvalid(55)
                       - FTP connection details are invalid.
                14: ftpConnectionTimeout(56)
                       - FTP connection timeout is detected.
                15: ftpNoSuchRemoteFile(57)
                       - No such remote file exists.
                16: ftpNoSuchRemoteDir(58)
                       - No such remote directory exists.
                17: ftpServiceNotAvailable(421)
                       - Service not available, closing control connection. 
                18: ftpUnableToOpenDataConnection(425)
                       - Can't open data connection.
                19: ftpConnectionClosed(426)
                       - Connection closed, transfer aborted. 
                20: ftpFileBusy(450)
                       - Requested file action not taken. File unavailable (e.g., file busy).
                21: ftpLocalError(451)
                       - Requested action aborted, local error in processing.
                22: ftpInsufficientStorageSpace(452)
                       - Requested action not taken. Insufficient storage space in system.
                23: ftpSyntaxError(501)
                       - Syntax error in parameters or arguments.
                24: ftpCommandNotImplemented(502)
                       - Command not implemented.
                25: ftpBadSequenceCommands(503)
                       - Bad sequence of commands.
                26: ftpParameterNotImplemented(504)
                       - Command not implemented for that parameter.
                27: ftpNotLoggedIn(530)
                       - User not logged in.
                28: ftpNeedAccount(532)
                       - Need account for storing files.
                29: ftpFileUnavailable(550)
                       - Requested action not taken. File unavailable (e.g., file not found, no access).
                30: ftpExceededStorageAllocation(552)
                       - Requested file action aborted, storage allocation exceeded
                31: ftpFileNameNotAllowed(553)
                       - Requested action not taken. Illegal file name.
               "
            ::= { xfServiceOamPmSessionCfgEntry 10 }

        xfServiceOamPmSessionFileName15Min OBJECT-TYPE
            SYNTAX      SnmpAdminString (SIZE(1..255))
            MAX-ACCESS  read-write
            STATUS      deprecated
            DESCRIPTION
               "Name of the XML file for performance data for 15min interval (96 intervals) that
                is beening uploaded to the FTP Server. 
                If no transfer is started, then this is the name of the file that was last uploaded.
                If no file name is specified, then NE will use the file name with the following
                default format defined in the standard 3GPP TS 32.432:
                <Type><Startdate>.<Starttime>-[<Enddate>.]<Endtime>[_-<jobId>][_<UniqueId>][_-_<RC>]
                The extension to the file name shall be .xml and .tgz
               "
            ::= { xfServiceOamPmSessionCfgEntry 11 }

        xfServiceOamPmSessionFileName24h OBJECT-TYPE
            SYNTAX      SnmpAdminString (SIZE(1..255))
            MAX-ACCESS  read-write
            STATUS      deprecated
            DESCRIPTION
               "Name of the XML file for performance data for previous 24 hours that is beening 
                uploaded to the FTP Server. 
                If no transfer is started, then this is the name of the file that was last uploaded.
                If no file name is specified, then NE will use the file name with the following
                default format defined in the standard 3GPP TS 32.432:
                <Type><Startdate>.<Starttime>-[<Enddate>.]<Endtime>[_-<jobId>][_<UniqueId>][_-_<RC>]
                The extension to the file name shall be .xml and .tgz
               "
            ::= { xfServiceOamPmSessionCfgEntry 12 }

