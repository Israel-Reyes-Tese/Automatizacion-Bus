ptDSCTransportServer OBJECT IDENTIFIER
    ::= { ptDSCTransportServers 1809 }

ptDSCTransportServerTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtDSCTransportServerEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptDSCTransportServer 1 }

ptDSCTransportServerEntry OBJECT-TYPE
    SYNTAX          PtDSCTransportServerEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of DSCTransportServer attributes."
    INDEX           { ptConnGroupIndex, ptDSCTransportServerDSCIndex, 
                      ptDSCTransportServerDSCNodesIndex, 
                      ptDSCTransportServerDSCNodeIndex, 
                      ptDSCTransportServerTransportServersIndex, 
                      ptDSCTransportServerIndex }
    ::= { ptDSCTransportServerTable 1 }

PtDSCTransportServerEntry ::=
    SEQUENCE {
        ptDSCTransportServerSlot        INTEGER,
        ptDSCTransportServerStatus      INTEGER,
        ptDSCTransportServerInternalID  INTEGER,
        ptDSCTransportServerTransportType INTEGER,
        ptDSCTransportServerPort        INTEGER,
        ptDSCTransportServerHost        DisplayString,
        ptDSCTransportServerADNProtocol INTEGER,
        ptDSCTransportServerSecurityParameterSet        DisplayString,
        ptDSCTransportServerRemoteIPValidation      INTEGER,
        ptDSCTransportServerDSCIndex    INTEGER,
        ptDSCTransportServerDSCNodesIndex INTEGER,
        ptDSCTransportServerDSCNodeIndex INTEGER,
        ptDSCTransportServerTransportServersIndex         INTEGER,
        ptDSCTransportServerActions     INTEGER,
        ptDSCTransportServerParentOID   OBJECT IDENTIFIER,
        ptDSCTransportServerRowStatus   RowStatus,
        ptDSCTransportServerIndex       INTEGER
    }

ptDSCTransportServerSlot OBJECT-TYPE
    SYNTAX          INTEGER { all(0) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Transport server associated DSC slot. \nValid values: Available routing CPU slot number and ALL (0). \nWhen set to ALL, the transport server is created on all the available routing CPUs."
    ::= { ptDSCTransportServerEntry 1 }

ptDSCTransportServerStatus OBJECT-TYPE
    SYNTAX          INTEGER { down(0), up(1), inactive(2), 
                        partiallyUp(3) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Status of the Transport Server (INACTIVE/DOWN/UP/PARTIALLY UP). \nWhen the Slot attribute is set to ALL (0), the PARTIALLY UP status means that one or more slots have port conflicts with existing servers and established ADN Connections and cannot bring this server in service. Please check the server unavailable major alarm (1807) for the list of slots that cannot bring the Transport Server in service locally."
    ::= { ptDSCTransportServerEntry 2 }

ptDSCTransportServerInternalID OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Transport Server unique internal ID. \nReserved for internal use."
    ::= { ptDSCTransportServerEntry 3 }

ptDSCTransportServerTransportType OBJECT-TYPE
    SYNTAX          INTEGER { sctp(0), tcp(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Transport Type (TCP/SCTP). \nThe type of transport used for incoming connections. \nTo establish a connection, each ADN connecting to this server requires an associated ADN Connection with matching or SERVER ONLY type. \nDefault value: SCTP."
    ::= { ptDSCTransportServerEntry 4 }

ptDSCTransportServerPort OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Port used for this server (1 to 65535). \nRFC6733 recommended port values are: \n 3868 for non-encrypted or CE-R/CE-A enabled TLS \n 5868 for full-time TLS/TCP or DTLS/SCTP (as per IANA reserved value)"
    ::= { ptDSCTransportServerEntry 5 }

ptDSCTransportServerHost OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Hostname or IP address of this server. \nIf a hostname is provided, then it may resolve to multiple IP addresses. In this case, a TCP server uses the first IP address, whereas the SCTP server uses all IP addresses. \nThe IP addresses must be provisioned on the card hosting the slot for the server to activate. \nValid values: Hostname or IP address."
    ::= { ptDSCTransportServerEntry 6 }

ptDSCTransportServerADNProtocol OBJECT-TYPE
    SYNTAX          INTEGER { diameter(0), xml(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Protocol used by the ADN Connections associated with the Transport Server (DIAMETER/XML). \nModifiable when the server is inactive. \nA DIAMETER server accepts connections from DIAMETER and UA ADNs. \nA XML server only accepts connections from UA XML ADNs. \nDefault value: DIAMETER."
    ::= { ptDSCTransportServerEntry 7 }

ptDSCTransportServerSecurityParameterSet OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Security Parameter Set associated with this Transport Server. \nSet to default for RFC3588 TLS. \nValid values: Existing Security Parameter Set. \nDefault value: default."
    ::= { ptDSCTransportServerEntry 8 }

ptDSCTransportServerRemoteIPValidation OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Validate remote IP addresses of incoming connections (DISABLED/ENABLED). \nIf enabled, the resolved values of the Allowed IPs list must match the \nsource address(es) of received TCP or SCTP packets. If the validation \nfails the connection is closed. This value cannot be set to ENABLED if \nno Allowed IP exist. \nDefault value: DISABLED."
    ::= { ptDSCTransportServerEntry 9 }

ptDSCTransportServerDSCIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
