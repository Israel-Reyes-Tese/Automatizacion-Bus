ptLBLoadBalancer OBJECT IDENTIFIER
    ::= { ptConnGroup 5001 }

ptLBLoadBalancerTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtLBLoadBalancerEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptLBLoadBalancer 1 }

ptLBLoadBalancerEntry OBJECT-TYPE
    SYNTAX          PtLBLoadBalancerEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of LBLoadBalancer attributes."
    INDEX           { ptConnGroupIndex, ptLBLoadBalancerIndex }
    ::= { ptLBLoadBalancerTable 1 }

PtLBLoadBalancerEntry ::=
    SEQUENCE {
        ptLBLoadBalancerID              Integer32,
        ptLBLoadBalancerName            DisplayString,
        ptLBLoadBalancerVersion         DisplayString,
        ptLBLoadBalancerErrors          INTEGER,
        ptLBLoadBalancerSetupManagement INTEGER,
        ptLBLoadBalancerDistributionManagement      INTEGER,
        ptLBLoadBalancerParentOID       OBJECT IDENTIFIER,
        ptLBLoadBalancerRowStatus       RowStatus,
        ptLBLoadBalancerIndex           Integer32
    }

ptLBLoadBalancerID OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Load Balancer Identifier (1 or 2). \nIf more than one Load Balancer is defined, the value is overwritten by the associated command line argument on startup."
    ::= { ptLBLoadBalancerEntry 1 }

ptLBLoadBalancerName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Name used to logically identify the Load Balancer Instance. \nIf more than one Load Balancer Instance is defined, it is recommended that the name be updated to a short and unique value to facilitate logs, alarms, and statistics correlation. \nValid values: alphanumeric characters, dashes, and underscores (maximum size of 64). \nDefault value: LB_X, where X is the Load Balancer ID."
    ::= { ptLBLoadBalancerEntry 2 }

ptLBLoadBalancerVersion OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Current Load Balancer application version. \nThis is a hardcoded value as per the Load Balancer application."
    ::= { ptLBLoadBalancerEntry 3 }

ptLBLoadBalancerErrors OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable or disable the logging of Load Balancer error messages to the debuglog (NOT LOGGED/LOGGED). \nThe logging of Load Balancer errors should be set to LOGGED for all production systems. \nDefault value: LOGGED."
    ::= { ptLBLoadBalancerEntry 6 }

ptLBLoadBalancerSetupManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable or disable the logging of startup and setup information to the debuglog (NOT LOGGED/LOGGED). \nThese logs are not related to traffic processing and have no impact on the performance of a running system. \nThe logging of Load Balancer startup and setup information should be set to LOGGED for most production systems. \nDefault value: LOGGED."
    ::= { ptLBLoadBalancerEntry 7 }

ptLBLoadBalancerDistributionManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable or disable the logging of inter-Load Balancer process communication to the debuglog (NOT LOGGED/LOGGED). \nThe resultant log messages are intended for Sonus Networks engineering staff and are not intended for customer use. \nThe logging of LB inter-process information should be set to NOT_LOGGED on all production systems so that the CPU is not unduly taxed. Default value: NOT LOGGED."
    ::= { ptLBLoadBalancerEntry 8 }

ptLBLoadBalancerParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptLBLoadBalancerEntry 253 }

