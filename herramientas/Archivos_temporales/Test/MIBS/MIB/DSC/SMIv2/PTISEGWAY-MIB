PtGWSTGWSSearchUtilityEntry ::=
    SEQUENCE {
        ptGWSTGWSSearchUtilityNA        Integer32,
        ptGWSTGWSSearchUtilityAdjacentPointCode       Integer32,
        ptGWSTGWSSearchUtilityApplicationID   DisplayString,
        ptGWSTGWSSearchUtilityOPC       Integer32,
        ptGWSTGWSSearchUtilityDPC       Integer32,
        ptGWSTGWSSearchUtilitySIO       Integer32,
        ptGWSTGWSSearchUtilityMessageType Integer32,
        ptGWSTGWSSearchUtilityAffectedPC Integer32,
        ptGWSTGWSSearchUtilitySCCPClass INTEGER,
        ptGWSTGWSSearchUtilitySCCPCLGPC Integer32,
        ptGWSTGWSSearchUtilitySCCPCLGSSN INTEGER,
        ptGWSTGWSSearchUtilitySCCPCLGDigits   DisplayString,
        ptGWSTGWSSearchUtilitySCCPCLDRI INTEGER,
        ptGWSTGWSSearchUtilitySCCPCLDTT INTEGER,
        ptGWSTGWSSearchUtilitySCCPCLDSSN INTEGER,
        ptGWSTGWSSearchUtilitySCCPCLDDigits   DisplayString,
        ptGWSTGWSSearchUtilityTCAPMessageType     INTEGER,
        ptGWSTGWSSearchUtilityTCAPApplicationContext            DisplayString,
        ptGWSTGWSSearchUtilityTCAPComponentType       INTEGER,
        ptGWSTGWSSearchUtilityTCAPOpCodeFamily      INTEGER,
        ptGWSTGWSSearchUtilityTCAPOpCode INTEGER,
        ptGWSTGWSSearchUtilityDiagnosticMessage       DisplayString,
        ptGWSTGWSSearchUtilityResult    DisplayString,
        ptGWSTGWSSearchUtilityResultScreeningTestMode             INTEGER,
        ptGWSTGWSSearchUtilityResultMSUTracing      INTEGER,
        ptGWSTGWSSearchUtilityMAPIMSI   DisplayString,
        ptGWSTGWSSearchUtilityGWSTTablesIndex     Integer32,
        ptGWSTGWSSearchUtilityActions   INTEGER,
        ptGWSTGWSSearchUtilityParentOID OBJECT IDENTIFIER,
        ptGWSTGWSSearchUtilityRowStatus RowStatus,
        ptGWSTGWSSearchUtilityIndex     Integer32
    }

ptGWSTGWSSearchUtilityNA OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Network Appearance for the Incoming Linkset or AppID."
    ::= { ptGWSTGWSSearchUtilityEntry 1 }

