        amaInSecTestFiles               Counter32,
        amaInProgramFiles               Counter32,
        amaInPriorFilesFour             Counter32,
        amaInPriorFilesThree            Counter32,
        amaInPriorFilesTwo              Counter32,
        amaInPriorFilesOne              Counter32,
        amaInLastFileTime               DateAndTime,
        amaInPriFilesMiss               Counter32,
        amaInFilesDupl                  Counter32,
        amaInPriAmaRec                  Counter32,
        amaInSecAmaRec                  Counter32,
        amaInPriErrRecRcv               Counter32,
        amaInSecErrRecRcv               Counter32,
        amaInErrRecDet                  Counter32,
        amaInEntryStatus                INTEGER,
        amaInCriticalLocFileSeqRestart  Counter32,
        amaInMinorLocFileSeqRestart     Counter32,
        amaInRemFileSeqRestart          Counter32
    }

