        ptDSCAVPDefinitionRowStatus     RowStatus,
        ptDSCAVPDefinitionIndex         INTEGER
    }

ptDSCAVPDefinitionVendorID OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Vendor ID Definition name responsible for the AVP. \nThe combination of Vendor ID and AVP Code uniquely identifies an AVP. \nValid values: name of existing Vendor ID Definition."
    ::= { ptDSCAVPDefinitionEntry 1 }

