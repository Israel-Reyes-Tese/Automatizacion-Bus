sonusSipSubCountTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSipSubCountEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This table presents SIP relayed subscription count statistics for the node."
    ::= { sonusSipSignallingMIBObjects 32 }

sonusSipSubCountEntry OBJECT-TYPE
    SYNTAX          SonusSipSubCountEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            ""
    INDEX           { sonusSipSubCountShelfIndex, 
                      sonusSipSubCountSlotIndex }
    ::= { sonusSipSubCountTable 1 }

SonusSipSubCountEntry ::=
    SEQUENCE {
        sonusSipSubCountShelfIndex      SonusShelfIndex,
        sonusSipSubCountSlotIndex       SonusSlotIndex,
        sonusSipSubCountReset           INTEGER,
        sonusSipSubCountAttempts        Counter,
        sonusSipSubCountCumCompletions  Counter,
        sonusSipSubCountStable          Counter,
        sonusSipSubCountTotal           Counter
    }

sonusSipSubCountShelfIndex OBJECT-TYPE
    SYNTAX          SonusShelfIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the target shelf to obtain the SIP subscription counters."
    ::= { sonusSipSubCountEntry 1 }

sonusSipSubCountSlotIndex OBJECT-TYPE
    SYNTAX          SonusSlotIndex
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Identifies the target slot."
    ::= { sonusSipSubCountEntry 2 }

sonusSipSubCountReset OBJECT-TYPE
    SYNTAX          INTEGER { unknown(1), reset(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Provides a means to reset a subset of the SIP subscription
             counters. Any set operation on this object will result in the
             		 attempt and cummulative completion counters being cleared."
    ::= { sonusSipSubCountEntry 3 }

sonusSipSubCountAttempts OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Indicates the number of SIP relayed subscription attempts on this card."
    ::= { sonusSipSubCountEntry 4 }

sonusSipSubCountCumCompletions OBJECT-TYPE
    SYNTAX          Counter
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Indicates the accumulative number of completed SIP subscription attempts on this card."
    ::= { sonusSipSubCountEntry 5 }

