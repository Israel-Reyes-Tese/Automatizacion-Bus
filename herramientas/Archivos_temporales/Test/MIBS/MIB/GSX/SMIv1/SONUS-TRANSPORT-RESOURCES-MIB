    SEQUENCE {
        sonusDBLEndpointStatIfIndex     Gauge,
        sonusDBLEndpointStatSrcIpAddress IpAddress,
        sonusDBLEndpointStatTriggerRule Gauge,
        sonusDBLEndpointStatApplicationType   Gauge,
        sonusDBLEndpointStatApplicationIndex    Gauge,
        sonusDBLEndpointStatSlot        SonusSlotIndex,
        sonusDBLEndpointStatCurrentDBLState   INTEGER,
        sonusDBLEndpointStatRemainEffectivePeriod         Gauge,
        sonusDBLEndpointStatDiscardPacketCnt    Counter,
        sonusDBLEndpointStatAction      INTEGER,
        sonusDBLEndpointStatTriggerEvent INTEGER,
        sonusDBLEndpointStatSrcIp6Address Ipv6Address,
        sonusDBLEndpointStatSrcIpVer    INTEGER
    }

sonusDBLEndpointStatIfIndex OBJECT-TYPE
    SYNTAX          Gauge (1..65535)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The LIF ifIndex of the ingress NIF or SIF where the DBL is created"
    ::= { sonusDBLEndpointStatEntry 1 }

