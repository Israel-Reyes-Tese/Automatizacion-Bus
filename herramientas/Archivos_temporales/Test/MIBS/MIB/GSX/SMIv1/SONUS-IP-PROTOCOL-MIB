sonusSctpAssocTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSctpAssocEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A table containing SCTP connection-specific information."
    ::= { sonusSctp 2 }

sonusSctpAssocEntry OBJECT-TYPE
    SYNTAX          SonusSctpAssocEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A conceptual row of the sctpAssocTable containing information
             about a particular current SCTP association.  Each row of this
             table is transient, in that it ceases to exist when (or soon
             after) the association makes the transition to the CLOSED
             state."
    INDEX           { sonusSctpAssocIpVersion, sonusSctpAssocShelf, 
                      sonusSctpAssocSlot, sonusSctpAssocInstance, 
                      sonusSctpAssocLocalAddress, 
                      sonusSctpAssocLocalPort, sonusSctpAssocRemAddress, 
                      sonusSctpAssocRemPort, sonusSctpAssocLocalAddress6, 
                      sonusSctpAssocRemAddress6 }
    ::= { sonusSctpAssocTable 1 }

SonusSctpAssocEntry ::=
    SEQUENCE {
        sonusSctpAssocState             INTEGER,
        sonusSctpAssocLocalAddrCount    INTEGER,
        sonusSctpAssocLocalAddress      IpAddress,
        sonusSctpAssocLocalPort         INTEGER,
        sonusSctpAssocRemAddrCount      INTEGER,
        sonusSctpAssocRemAddress        IpAddress,
        sonusSctpAssocRemPort           INTEGER,
        sonusSctpAssocShelf             SonusShelfIndex,
        sonusSctpAssocSlot              SonusSlotIndex,
        sonusSctpAssocInstance          INTEGER,
        sonusSctpAssocLocalAddress6     Ipv6Address,
        sonusSctpAssocRemAddress6       Ipv6Address,
        sonusSctpAssocIpVersion         INTEGER
    }

sonusSctpAssocState OBJECT-TYPE
    SYNTAX          INTEGER { closed(0), listen(1), cookieWait(2), 
                        cookieEchoed(3), established(4), 
                        shutdownPending(5), shutdownSent(6), 
                        shutdownReceived(7), shutdownRecvWait(8), 
                        shutdownAckSent(9), unreachable(10) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The state of this SCTP connection."
    ::= { sonusSctpAssocEntry 1 }

sonusSctpAssocLocalAddrCount OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number of local addresses SCTP association is bound to"
    ::= { sonusSctpAssocEntry 2 }

sonusSctpAssocLocalAddress OBJECT-TYPE
    SYNTAX          IpAddress
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The local IP address for this SCTP connection.  In the case
             of a connection in the listen state which is willing to
             accept connections for any IP interface associated with the
             node, the value 0.0.0.0 is used."
    ::= { sonusSctpAssocEntry 3 }

sonusSctpAssocLocalPort OBJECT-TYPE
    SYNTAX          INTEGER (0..65535)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The local port number for this SCTP connection."
    ::= { sonusSctpAssocEntry 4 }

