sonusDsx1FarEndCurrentTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusDsx1FarEndCurrentEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "The DS1 current table contains various statistics
             being collected for the far end current 15 minute
             interval."
    ::= { sonusDsx1MIBObjects 6 }

sonusDsx1FarEndCurrentEntry OBJECT-TYPE
    SYNTAX          SonusDsx1FarEndCurrentEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "An entry in the DS1 Current table."
    INDEX           { sonusDsx1FarEndCurrentShelfIndex, 
                      sonusDsx1FarEndCurrentSlotIndex, 
                      sonusDsx1FarEndCurrentPortIndex, 
                      sonusDsx1FarEndCurrentDs1Index }
    ::= { sonusDsx1FarEndCurrentTable 1 }

SonusDsx1FarEndCurrentEntry ::=
    SEQUENCE {
        sonusDsx1FarEndCurrentShelfIndex SonusShelfIndex,
        sonusDsx1FarEndCurrentSlotIndex SonusSlotIndex,
        sonusDsx1FarEndCurrentPortIndex INTEGER,
        sonusDsx1FarEndCurrentDs1Index  INTEGER,
        sonusDsx1FarEndCurrentElapsedTime INTEGER,
        sonusDsx1FarEndCurrentLESs      PerfCurrentCount,
        sonusDsx1FarEndCurrentPCVs      PerfCurrentCount,
        sonusDsx1FarEndCurrentPESs      PerfCurrentCount,
        sonusDsx1FarEndCurrentPSESs     PerfCurrentCount,
        sonusDsx1FarEndCurrentSEFSs     PerfCurrentCount,
        sonusDsx1FarEndCurrentCSSs      PerfCurrentCount,
        sonusDsx1FarEndCurrentUASs      PerfCurrentCount,
        sonusDsx1FarEndCurrentFC        INTEGER,
        sonusDsx1FarEndCurrentTcaLES    TruthValue,
        sonusDsx1FarEndCurrentTcaPCV    TruthValue,
        sonusDsx1FarEndCurrentTcaPES    TruthValue,
        sonusDsx1FarEndCurrentTcaPSES   TruthValue,
        sonusDsx1FarEndCurrentTcaSEFS   TruthValue,
        sonusDsx1FarEndCurrentTcaCSS    TruthValue,
        sonusDsx1FarEndCurrentTcaUAS    TruthValue,
        sonusDsx1FarEndCurrentMinorTcaLES TruthValue,
        sonusDsx1FarEndCurrentMinorTcaPCV TruthValue,
        sonusDsx1FarEndCurrentMinorTcaPES TruthValue,
        sonusDsx1FarEndCurrentMinorTcaPSES  TruthValue,
        sonusDsx1FarEndCurrentMinorTcaSEFS  TruthValue,
        sonusDsx1FarEndCurrentMinorTcaCSS TruthValue,
        sonusDsx1FarEndCurrentMinorTcaUAS TruthValue,
        sonusDsx1FarEndCurrentMajorTcaLES TruthValue,
        sonusDsx1FarEndCurrentMajorTcaPCV TruthValue,
        sonusDsx1FarEndCurrentMajorTcaPES TruthValue,
        sonusDsx1FarEndCurrentMajorTcaPSES  TruthValue,
        sonusDsx1FarEndCurrentMajorTcaSEFS  TruthValue,
        sonusDsx1FarEndCurrentMajorTcaCSS TruthValue,
        sonusDsx1FarEndCurrentMajorTcaUAS TruthValue,
        sonusDsx1FarEndCurrentCriticalTcaLES    TruthValue,
        sonusDsx1FarEndCurrentCriticalTcaPCV    TruthValue,
        sonusDsx1FarEndCurrentCriticalTcaPES    TruthValue,
        sonusDsx1FarEndCurrentCriticalTcaPSES     TruthValue,
        sonusDsx1FarEndCurrentCriticalTcaSEFS     TruthValue,
        sonusDsx1FarEndCurrentCriticalTcaCSS    TruthValue,
        sonusDsx1FarEndCurrentCriticalTcaUAS    TruthValue
    }

