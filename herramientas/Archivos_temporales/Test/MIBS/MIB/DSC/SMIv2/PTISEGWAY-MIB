    ::= { ptDSCADNConnectionEntry 13 }

ptDSCADNConnectionUserActivation OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "User activation (ENABLED/DISABLED). \nThe ADN Connection can be activated while the ADN itself is inactive. In this scenario, the connection remains INACTIVE but the User Activation attribute is set to ENABLED. When the ADN is activated, the user activated ADN Connections will also be activated. \nDefault value: DISABLED."
    ::= { ptDSCADNConnectionEntry 14 }

ptDSCADNConnectionLocalPort OBJECT-TYPE
    SYNTAX          INTEGER { ephemeral(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Local port for the Adjacent Diameter Node connection (EPHEMERAL, 1 to 65535). \nOnly applicable to Transport Type SCTP. For all other types, the value is set to EPHEMERAL and cannot be modified. \nOnly applicable to outgoing connections, and has no relation to the server used to accept incoming connections. \nDefault value: EPHEMERAL."
    ::= { ptDSCADNConnectionEntry 15 }

