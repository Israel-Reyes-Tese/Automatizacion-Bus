--
-- Node definitions
--      
	xfTMUObjects OBJECT IDENTIFIER ::= { xfTMUMIB 1 }
	xfTMUConformance OBJECT IDENTIFIER ::= { xfTMUMIB 2 }

------------------------------------------------------------------------------
-- xfTMUTable
------------------------------------------------------------------------------
	xfTMUTable OBJECT-TYPE
		SYNTAX SEQUENCE OF XfTMUEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"
			Table for MMU2 managed objects. The tabled is indexed with 
			entPhysicalIndex. 
			Note that objects might not be accessible if xfMCRBusy 
			in XF-MCR-MIB is busy(1).
			"
		::= { xfTMUObjects 1 }
		
	xfTMUEntry OBJECT-TYPE
		SYNTAX XfTMUEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"An entry in the xfTMUTable"
		INDEX { entPhysicalIndex}
		::= { xfTMUTable 1 }
	
	XfTMUEntry ::=
		SEQUENCE 
		{
			xfTMUTerminalBoards XfMCRTerminalBoards,
			xfTMUProtectionMode	XfMCRProtectionMode,
			xfTMUCapacity		XfMCRCapacity,
			xfTMUOMMode			XfMCROMMode,
			xfTMUOMTimeout		XfMCROMTimeout,
			xfTMUUartBaudrate	XfMCRUartBaudrate,
			xfTMUSeverityTrapEnable XfMCRSeverityTrapEnable,
			xfTMUSeverityLevel		XfMCRSeverityLevel     
		} 
			
--
-- The xfTMUTable MOs
--
				
	xfTMUTerminalBoards OBJECT-TYPE
		SYNTAX XfMCRTerminalBoards
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"
			This object sets the number of MCR boards in a MCR terminal.
			In release one only valid values are oneBoard and threeBoards.
			In a 1+0 configuration the value has to be set by EM.
			In a 1+1 configuration the value is set from the MO xfPCUTerminalBoards
			in XF-PCU-MIB.  	
			"
		DEFVAL { oneBoard }	
       ::= { xfTMUEntry 1 }

	xfTMUProtectionMode OBJECT-TYPE
		SYNTAX XfMCRProtectionMode        
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object shows the Protection mode.
			"
		::= { xfTMUEntry 2 }
	
	xfTMUCapacity OBJECT-TYPE
		SYNTAX XfMCRCapacity  
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object shows the Capacity of the MCR Link.
			"
		::= { xfTMUEntry 3 }

	xfTMUOMMode OBJECT-TYPE
		SYNTAX XfMCROMMode
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object controls the O&M-switch on the MMU2-board.
			"
		::= { xfTMUEntry 5 }     
		
	xfTMUOMTimeout OBJECT-TYPE
		SYNTAX XfMCROMTimeout         
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object is the timeout for OM_REQUEST.
			"
		::= { xfTMUEntry 6 }

	xfTMUUartBaudrate OBJECT-TYPE
		SYNTAX XfMCRUartBaudrate       
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object control the baudrate of the PCI Uart.
			"			
		::= { xfTMUEntry 7 }	
		
	xfTMUSeverityTrapEnable OBJECT-TYPE
		SYNTAX XfMCRSeverityTrapEnable       
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"
			This object control whether to send a trap or not if the xfTMUSeverityLevel changes
			"
		DEFVAL { enable }			
		::= { xfTMUEntry 8 }	
			
	xfTMUSeverityLevel OBJECT-TYPE
		SYNTAX XfMCRSeverityLevel       
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"
			This object tells the severity level of the CSS alarm. 
			It's the Alarm Status Indicator for the CSS alarm.
			"			
		::= { xfTMUEntry 9 }			
		
			                               
--
-- Conformance
--
	xfTMUCompliances OBJECT IDENTIFIER ::= { xfTMUConformance 1 }

	xfTMUGroups OBJECT IDENTIFIER ::= { xfTMUConformance 2 }

   	xfTMUFullCompliance MODULE-COMPLIANCE
       		STATUS      current
       		DESCRIPTION
           		"The compliance statement for SNMP entities which
            		implement everything."
       		MODULE -- This Module
           	MANDATORY-GROUPS { xfTMUCompleteGroup }
       		::= { xfTMUCompliances 1 }
       	
       		
       		
	xfTMUCompleteGroup OBJECT-GROUP
       	OBJECTS 
       	{
			xfTMUTerminalBoards,
			xfTMUProtectionMode,
			xfTMUCapacity,
			xfTMUOMMode,
			xfTMUOMTimeout,
			xfTMUUartBaudrate,
			xfTMUSeverityTrapEnable,
			xfTMUSeverityLevel	
		}
		STATUS      current
       		DESCRIPTION
           		"A collection of all current objects in this MIB
			 module."
		::= { xfTMUGroups 1 } 
		
	END
