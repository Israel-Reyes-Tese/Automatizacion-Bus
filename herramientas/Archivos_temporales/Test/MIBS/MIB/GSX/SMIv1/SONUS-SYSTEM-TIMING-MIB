    SYNTAX          SonusEventString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "  "
    ::= { sonusSystemTimingMIBNotificationsObjects 2 }

sonusTimingDownReason OBJECT-TYPE
    SYNTAX          SonusEventString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "  "
    ::= { sonusSystemTimingMIBNotificationsObjects 3 }

sonusSystemTimingActiveFailedNotification TRAP-TYPE
    ENTERPRISE      sonusSystemTimingMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusTimingSource, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          deprecated
    DESCRIPTION     
            "This trap has been replaced by sonusSystemTimingActiveFailedNotification2."
    ::= 1

sonusSystemTimingActiveSwitchoverNotification TRAP-TYPE
    ENTERPRISE      sonusSystemTimingMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, sonusTimingSource, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that the active System Timing 
             clock source has been switched to another source."
    ::= 2

sonusSystemTimingOscillatorActiveNotification TRAP-TYPE
    ENTERPRISE      sonusSystemTimingMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that all external sources of
             System Timing have failed and that the current 
             source of timing is the MTA's internal oscillator."
    ::= 3

sonusSystemTimingMTAFailedNotification TRAP-TYPE
    ENTERPRISE      sonusSystemTimingMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that the MTA indicated 
             has failed or been removed from the shelf."
    ::= 4

sonusSystemTimingMTAOperationalNotification TRAP-TYPE
    ENTERPRISE      sonusSystemTimingMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that the specified MTA
             is now operational."
    ::= 6

sonusSystemTimingMTATotalFailureNotification TRAP-TYPE
    ENTERPRISE      sonusSystemTimingMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel, sonusSequenceId, 
                      sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that no MTA's are available for 
             use to provide System Timing. They have either failed, 
             been removed from service or been removed from the shelf. 
             The shelf is no longer capable of being properly timed.
             
             Downstream Shelves or other equipment connected to this shelf's
             MITDC(Marlin Inter-chassis Timing Daisy Chain) may be affected."
    ::= 7

sonusSystemTimingActiveFailedNotification2 TRAP-TYPE
    ENTERPRISE      sonusSystemTimingMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, 
                      sonusTimingSource2, sonusTimingDownReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that the currently active System Timing clock source has failed 
             and adds a string interpretation of the source type."
    ::= 8

sonusSystemTimingNonActiveFailedNotification TRAP-TYPE
    ENTERPRISE      sonusSystemTimingMIBNotifications
    VARIABLES       { sonusShelfIndex, sonusSlotIndex, 
                      sonusTimingSource2, sonusTimingDownReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusSequenceId, sonusEventTime }
--     STATUS          mandatory
    DESCRIPTION     
            "This trap indicates that a non-active System Timing clock source has failed 
             and adds a string interpretation of the source type."
    ::= 9

END -- end of module SONUS-SYSTEM-TIMING-MIB.
