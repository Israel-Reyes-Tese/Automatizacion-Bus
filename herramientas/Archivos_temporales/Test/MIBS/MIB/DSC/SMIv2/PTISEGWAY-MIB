ptDSCServerPool OBJECT IDENTIFIER
    ::= { ptDSCServerPools 1842 }

ptDSCServerPoolTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtDSCServerPoolEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptDSCServerPool 1 }

ptDSCServerPoolEntry OBJECT-TYPE
    SYNTAX          PtDSCServerPoolEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of DSCServerPool attributes."
    INDEX           { ptConnGroupIndex, ptDSCServerPoolDSCIndex, 
                      ptDSCServerPoolDSCNodesIndex, 
                      ptDSCServerPoolDSCNodeIndex, 
                      ptDSCServerPoolServerPoolsIndex, 
                      ptDSCServerPoolIndex }
    ::= { ptDSCServerPoolTable 1 }

PtDSCServerPoolEntry ::=
    SEQUENCE {
        ptDSCServerPoolName             DisplayString,
        ptDSCServerPoolSessionBinding   INTEGER,
        ptDSCServerPoolRoutingTableName DisplayString,
        ptDSCServerPoolStatus           INTEGER,
        ptDSCServerPoolLoadBalancing    INTEGER,
        ptDSCServerPoolInternalID       Integer32,
        ptDSCServerPoolAVPAlias         DisplayString,
        ptDSCServerPoolDSCIndex         Integer32,
        ptDSCServerPoolDSCNodesIndex    Integer32,
        ptDSCServerPoolDSCNodeIndex     Integer32,
        ptDSCServerPoolServerPoolsIndex Integer32,
        ptDSCServerPoolActions          INTEGER,
        ptDSCServerPoolParentOID        OBJECT IDENTIFIER,
        ptDSCServerPoolRowStatus        RowStatus,
        ptDSCServerPoolIndex            Integer32
    }

ptDSCServerPoolName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Name to uniquely identify the Server Pool. \nThe name must be unique within the DSC Node. \nIt is recommended that the name be set to a short and unique value to facilitate logs and alarms correlation. \nValid values: alphanumeric characters, dashes, and underscores (maximum size of 32)."
    ::= { ptDSCServerPoolEntry 1 }

ptDSCServerPoolSessionBinding OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), session(1), pcrf(2) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Session Binding (DISABLED/SESSION/PCRF). \nIf set to DISABLED, load balancing the chosen load balancing algorithm is applied. \nIf set to SESSION, bind is performed to the Diameter Session-Id AVP. \nIf set to PCRF, bind is performed to the EPC IP-CAN session. \nDefault value: DISABLED."
    ::= { ptDSCServerPoolEntry 2 }

ptDSCServerPoolRoutingTableName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Routing Table Name for post-lookup entry point. \nValid values: dsc_default or an existing Routing Table Name. \nDefault value: dsc_default."
    ::= { ptDSCServerPoolEntry 3 }

ptDSCServerPoolStatus OBJECT-TYPE
    SYNTAX          INTEGER { active(0), inactive(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Server Pool State (INACTIVE/ACTIVE). \nActivate the Server Pool to enable this feature."
    ::= { ptDSCServerPoolEntry 4 }

ptDSCServerPoolLoadBalancing OBJECT-TYPE
    SYNTAX          INTEGER { roundRobinWeight(0), leastLoaded(1), 
                        roundRobinCost(2), hash(3) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Load Balancing \nWhen Round Robin Cost is selected then the lowest cost record is chosen as the routing destination. \nIf more than one record has the same cost then messages are \nload shared in a round robin manner across the available destinations. \n Valid costs are values 1 to 99. \nWhen Round Robin Weight is selected, the weights represent the ratio of messages delivered to each destination. \nFor example: Candidate A has weight = 10, B = 20 and C = 5. \nIn a group of messages, the first 10 messages are sent to A, then the next 20 to B and the next 5 to C. The process is then restarted at A for the next messages. \n With Hash based the routing candidate is selected by hashing a defined AVP value in the message. \nWhen Least Loaded is selected, the weights indicate the relative message capacity of each server (in messages per second). \nDefault Value: ROUND ROBIN WEIGHT."
    ::= { ptDSCServerPoolEntry 5 }

ptDSCServerPoolInternalID OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Server Pool internal ID. \nReserved for internal use."
    ::= { ptDSCServerPoolEntry 6 }

ptDSCServerPoolAVPAlias OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The name of an AVP Alias. This AVP is used by the hashing algorithm to perform traffic load balancing. \nValid values: An AVP Alias which has been created in the DSC AVP Alias Definitions tables. If the AVP is not present in the message, then the hop-by-hop header value is used. \nDefault value: session-id AVP."
    ::= { ptDSCServerPoolEntry 7 }

ptDSCServerPoolDSCIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSC."
    ::= { ptDSCServerPoolEntry 242 }

ptDSCServerPoolDSCNodesIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSCNodes."
    ::= { ptDSCServerPoolEntry 243 }

