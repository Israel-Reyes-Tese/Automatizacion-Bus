		-- 1.3.6.1.4.1.193.81.7.1.1.5.1.1.3
		xfCNSfpReceivedPowerThreshold OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Threshold for SFP received power alarm."
			::= { xfCNSfpEntry 3 }
					
		-- 1.3.6.1.4.1.193.81.7.1.1.5.1.1.4
		xfCNSfpTransmittedPowerThreshold OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Threshold for SFP transmitted power alarm."
			::= { xfCNSfpEntry 4 }

		-- 1.3.6.1.4.1.193.81.7.1.1.5.1.1.5
		xfCNSfpTemperature OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"SFP temperature in degrees Celsius, typical values range from -40 to +85."
			::= { xfCNSfpEntry 5 } 
			
		-- 1.3.6.1.4.1.193.81.7.1.1.5.1.1.6
		xfCNSfpBiasCurrent OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"SFP bias current in mA, typical value is 300 mA."
			::= { xfCNSfpEntry 6 }
			
		-- 1.3.6.1.4.1.193.81.7.1.1.5.1.1.7
		xfCNSfpSupplyVoltage OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"SFP supply voltage in tenths of volt, typical value is 3,6."
			::= { xfCNSfpEntry 7 }
			
		-- 1.3.6.1.4.1.193.81.7.1.1.5.1.1.8
		xfCNSfpAlarmStatus OBJECT-TYPE
            SYNTAX BITS
                {
                sfpReceivedPower0(0),
                sfpReceivedPower1(1),
                sfpReceivedPower2(2),
                sfpTransmittedPower1(3),
                sfpTransmittedPower2(4),
                sfpTransmittedPower3(5)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Indicates active alarm status of the PT2010 SFP functionality:
                
                SfpReceivedPower      

                  major    SFP received power is lower that the configured threshold.

                
                SfpTransmittedPower  
                  major    SFP transmitted power is lower that the configured threshold.
                
                
                Severity is indicated by 3 bits per status:
                bits 012
                     000   clear
                     001   undetermined
                     010   warning
                     011   minor
                     100   major
                     101   critical
                "
            ::= { xfCNSfpEntry 8 }


			
