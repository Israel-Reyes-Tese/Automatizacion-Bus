			::= { xfImaGroupXEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.2.1.2
		xfImaGroupRestartCommand OBJECT-TYPE
			SYNTAX INTEGER
				{
				none(0),
				restart(1)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object is used to force Ima Group restart.If the group needs to be restarted set the value to restart(1), else use none(0) as default"
			::= { xfImaGroupXEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.3
		xfImaGroupStatusTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfImaGroupStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains the information about the
				severity of the IMA group alarms. These alarms are:
				- Far-End Startup,
				- Abort Configuration,
				- Abort Far-End Configuration,
				- Insufficent Links,
				- Insufficent Far-End Links,
				- Far-End Blocked,
				- Timing Mismatch.
				The information stored in this table complete
				what is reported in the following objects of
				the imaGroupTable:
				- imaGroupFailureStatus,
				- imaGroupNeState,
				- imaGroupFeState.
				
				This table is an extension of the imaGroupTable,
				and it exploits the same indexing (imaGroupIndex)."
			REFERENCE
				"ATM Forum IMA v1.1, Section 12.2.3 on page 79.
				ATM Aggregation Unit: Requirement Specification (IMA)
				(1056-cnh160 2539/1 Uen)."
			::= { xfImaLayerHandling 3 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.3.1
		xfImaGroupStatusEntry OBJECT-TYPE
			SYNTAX XfImaGroupStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"List of entries in the table.
				The number of entries is equal to the number of
				entries in the imaGroupTable, i.e. to the number
				of IMA groups currently set up."
			AUGMENTS { imaGroupEntry }
			::= { xfImaGroupStatusTable 1 }

		
		XfImaGroupStatusEntry ::=
			SEQUENCE { 
				xfImaGroupStatus
					BITS
			 }

		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.3.1.1
		xfImaGroupStatus OBJECT-TYPE
			SYNTAX BITS
				{
				startupFe0(0),
				startupFe1(1),
				startupFe2(2),
				cfgAbort0(3),
				cfgAbort1(4),
				cfgAbort2(5),
				cfgAbortFe0(6),
				cfgAbortFe1(7),
				cfgAbortFe2(8),
				insuffLinks0(9),
				insuffLinks1(10),
				insuffLinks2(11),
				insuffLinksFe0(12),
				insuffLinksFe1(13),
				insuffLinksFe2(14),
				blockedFe0(15),
				blockedFe1(16),
				blockedFe2(17),
				timingSynch0(18),
				timingSynch1(19),
				timingSynch2(20)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object contains the severity of supported
				alarms per each IMA group.
				Severity is indicated by three bits per alarm:
				bits 012
				     000   clear
				     001   undetermined
				     010   warning
				     011   minor
				     100   major
				     101   critical"
			::= { xfImaGroupStatusEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.4
		xfImaLinkStatusTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfImaLinkStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains the information about the
				severity of the IMA link alarms. These alarms are:
				- Loss of IMA Frame,
				- Loss of Delay Syncronization,
				- Remote Failure Indication,
				- Tx Link Misconnected,
				- Rx Link Misconnected,
				- Far-End Tx Link Unusable,
				- Far-End Rx Link Unusable.
				The information stored in this table complete what
				is reported in the following objects of the imaLinkTable:
				- imaLinkNeRxFailureStatus,
				- imaLinkFeRxFailureStatus,
				- imaLinkNeTxState,
				- imaLinkNeRxState,
				- imaLinkFeTxState,
				- imaLinkFeRxState.
				This table is an extension of the imaLinkTable,
				and it exploits the same indexing (imaLinkIfIndex)."
			REFERENCE
				"ATM Forum IMA v1.1, Section 12.2.3 on page 79.
				ATM Aggregation Unit: Requirement Specification (IMA)
				(1056-cnh160 2539/1 Uen)."
			::= { xfImaLayerHandling 4 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.2.4.1
		xfImaLinkStatusEntry OBJECT-TYPE
			SYNTAX XfImaLinkStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"List of entries of the Table.
				The number of entries is equal to the number of
				entries in the imaLinkTable, i.e. to the number
				of IMA links currently set up."
			AUGMENTS { imaLinkEntry }
			::= { xfImaLinkStatusTable 1 }

		
