ptSGSUA OBJECT IDENTIFIER
    ::= { ptConnGroup 200 }

ptSGSUATable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtSGSUAEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptSGSUA 1 }

ptSGSUAEntry OBJECT-TYPE
    SYNTAX          PtSGSUAEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of SGSUA attributes."
    INDEX           { ptConnGroupIndex, ptSGSUAIndex }
    ::= { ptSGSUATable 1 }

PtSGSUAEntry ::=
    SEQUENCE {
        ptSGSUAPendingTimer             INTEGER,
        ptSGSUAMonitoring               INTEGER,
        ptSGSUAAssociationCapacity      INTEGER,
        ptSGSUAHighUsageOnset           INTEGER,
        ptSGSUAHighUsageAbatement       INTEGER,
        ptSGSUAUsageIntervalSeconds     INTEGER,
        ptSGSUAConnectionEstablished    INTEGER,
        ptSGSUAInactivitySend           INTEGER,
        ptSGSUAInactivityReceive        INTEGER,
        ptSGSUARelease                  INTEGER,
        ptSGSUARepeatRelease            INTEGER,
        ptSGSUASRNFreeze                INTEGER,
        ptSGSUAIMFSupport               INTEGER,
        ptSGSUASCTPProtocolIDValidation INTEGER,
        ptSGSUAActions                  INTEGER,
        ptSGSUAParentOID                OBJECT IDENTIFIER,
        ptSGSUARowStatus                RowStatus,
        ptSGSUAIndex                    INTEGER
    }

ptSGSUAPendingTimer OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Duration of PENDING state in msecs for an SUA AS. Pending timer is started when an AS transitions out of ACTIVE state. \nIf an ASP sends an ASPAC before the timer expires, the AS will immediately transition back to the ACTIVE status. Otherwise, when the timer expires the AS will either transition to INACTIVE or DOWN state. \nMessages will be queued while in PENDING state. \nValid range of this timer is 1 to 2000. \nTo disable the timer, set to 0. \nDefault value is 2000 msecs."
    ::= { ptSGSUAEntry 2 }

ptSGSUAMonitoring OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Monitoring feature (ENABLED/DISABLED)."
    ::= { ptSGSUAEntry 3 }

ptSGSUAAssociationCapacity OBJECT-TYPE
    SYNTAX          INTEGER (0..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Maximum number of bytes per second sent on a AS association."
    ::= { ptSGSUAEntry 4 }

ptSGSUAHighUsageOnset OBJECT-TYPE
    SYNTAX          INTEGER { default(-1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Traffic level at which high usage alarms are triggered. (% of association capacity).\n This setting is used for both the transmit and receive high usage alarms for SCTP associations used by SUA ASPs.\n The alarm is automatically cleared when traffic levels fall below the High Usage Abatement setting."
    ::= { ptSGSUAEntry 5 }

ptSGSUAHighUsageAbatement OBJECT-TYPE
    SYNTAX          INTEGER { default(-1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Traffic level at which high usage alarms are cleared. (% of association capacity).\n This setting is used for both the transmit and receive high usage alarms for SCTP associations used by SUA ASPs."
    ::= { ptSGSUAEntry 6 }

