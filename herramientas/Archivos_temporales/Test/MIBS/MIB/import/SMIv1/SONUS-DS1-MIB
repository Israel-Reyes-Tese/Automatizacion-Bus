        sonusDs1ProfileFarDayESThreshold INTEGER,
        sonusDs1ProfileFarDaySESThreshold INTEGER,
        sonusDs1ProfilePCVThreshold     INTEGER,
        sonusDs1ProfileLCVThreshold     INTEGER,
        sonusDs1ProfileUASThreshold     INTEGER,
        sonusDs1ProfileFarPCVThreshold  INTEGER,
        sonusDs1ProfileFarLCVThreshold  INTEGER,
        sonusDs1ProfileFarUASThreshold  INTEGER,
        sonusDs1ProfileDayPCVThreshold  INTEGER,
        sonusDs1ProfileDayLCVThreshold  INTEGER,
        sonusDs1ProfileDayUASThreshold  INTEGER,
        sonusDs1ProfileFarDayPCVThreshold INTEGER,
        sonusDs1ProfileFarDayLCVThreshold INTEGER,
        sonusDs1ProfileFarDayUASThreshold INTEGER,
        sonusDs1ProfileESThresholdState SonusAdminState,
