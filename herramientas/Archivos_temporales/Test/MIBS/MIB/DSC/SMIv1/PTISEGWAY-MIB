    SYNTAX          SEQUENCE OF PtiwfIWFConfigurationEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptiwfIWFConfiguration 1 }

ptiwfIWFConfigurationEntry OBJECT-TYPE
    SYNTAX          PtiwfIWFConfigurationEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of iwfIWFConfiguration attributes."
    INDEX           { ptConnGroupIndex, ptiwfIWFConfigurationIndex }
    ::= { ptiwfIWFConfigurationTable 1 }

PtiwfIWFConfigurationEntry ::=
    SEQUENCE {
        ptiwfIWFConfigurationIWFLicense INTEGER,
        ptiwfIWFConfigurationIWFHSSLicense  INTEGER,
        ptiwfIWFConfigurationIWFEIRLicense  INTEGER,
        ptiwfIWFConfigurationIWFSMSLicense  INTEGER,
        ptiwfIWFConfigurationTransactionTimeout       INTEGER,
        ptiwfIWFConfigurationMaxTracingPerInterval          INTEGER,
        ptiwfIWFConfigurationTracingInterval    INTEGER,
        ptiwfIWFConfigurationErrors     INTEGER,
        ptiwfIWFConfigurationSetupManagement    INTEGER,
        ptiwfIWFConfigurationExecutionTrace   INTEGER,
        ptiwfIWFConfigurationDistributionManagement           INTEGER,
        ptiwfIWFConfigurationActions    INTEGER,
        ptiwfIWFConfigurationParentOID  OBJECT IDENTIFIER,
        ptiwfIWFConfigurationRowStatus  RowStatus,
        ptiwfIWFConfigurationIndex      INTEGER
    }

ptiwfIWFConfigurationIWFLicense OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IWF feature license (UNLICENSED/LICENSED). \nIf the IWF is unlicensed, the IWF (SS7 and Diameter) traffic cannot be activated."
    ::= { ptiwfIWFConfigurationEntry 1 }

ptiwfIWFConfigurationIWFHSSLicense OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IWF HSS mapping license (UNLICENSED/LICENSED). \nIf the IWF HSS is unlicensed, related HSS traffic scenarios trigger appropriate error handling. A Diameter UNABLE_TO_DELIVER is returned for unsupported Diameter requests while a TCAP Abort due to procedure failure is returned for unsupported TCAP operations."
    ::= { ptiwfIWFConfigurationEntry 2 }

ptiwfIWFConfigurationIWFEIRLicense OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IWF EIR mapping license (UNLICENSED/LICENSED). \nIf the IWF EIR is unlicensed, the EIR traffic scenario triggers appropriate error handling. A Diameter UNABLE_TO_DELIVER is returned for unsupported Diameter requests while a TCAP Abort due to procedure failure is returned for unsupported TCAP operations."
    ::= { ptiwfIWFConfigurationEntry 3 }

ptiwfIWFConfigurationIWFSMSLicense OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IWF SMS mapping license (UNLICENSED/LICENSED). \nIf the IWF SMS is unlicensed, related SMS traffic scenarios trigger appropriate error handling. A Diameter UNABLE_TO_DELIVER is returned for unsupported Diameter requests while a TCAP Abort due to procedure failure is returned for unsupported TCAP operations."
    ::= { ptiwfIWFConfigurationEntry 4 }

ptiwfIWFConfigurationTransactionTimeout OBJECT-TYPE
    SYNTAX          INTEGER (10..1800)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "IWF Transaction Timeout (10 to 1800 seconds). \nPending transaction timeout for both the SS7 and Diameter networks in seconds. \nDefault value: 120 seconds."
    ::= { ptiwfIWFConfigurationEntry 5 }

ptiwfIWFConfigurationMaxTracingPerInterval OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Maximum number of traced messages logged in the given interval (0 to 99). \nMessage tracing, if enabled, assigns a unique identifier to incoming messages matching a Node Mapping record. The message can then be traced throughout the SS7 and Diameter applications for debugging purposes. \nIf a Diameter or SS7 message is already traced, IWF uses the provided message ID. \nSet to 0 to disable message tracing. \nDefault value: 10."
    ::= { ptiwfIWFConfigurationEntry 6 }

ptiwfIWFConfigurationTracingInterval OBJECT-TYPE
    SYNTAX          INTEGER { five(300000), ten(600000), fifteen(900000), 
                        twenty(1200000), thirty(1800000) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Message Tracing logging interval (5, 10, 15, 20, and 30 minutes). \nDefault value: FIVE (5) minutes."
    ::= { ptiwfIWFConfigurationEntry 7 }

ptiwfIWFConfigurationErrors OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable the logging of IWF error messages to the debuglog (NOT LOGGED/LOGGED). \nThe logging of IWF errors should be set to LOGGED for all production systems. \nDefault value: LOGGED."
    ::= { ptiwfIWFConfigurationEntry 8 }

ptiwfIWFConfigurationSetupManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable the logging of startup and setup information to the debuglog (NOT LOGGED/LOGGED). \nThese logs are not related to traffic processing and have no impact on the performance of a running system. \nThe logging of IWF startup and setup information should be set to LOGGED for most production systems. \nDefault value: LOGGED."
    ::= { ptiwfIWFConfigurationEntry 9 }

ptiwfIWFConfigurationExecutionTrace OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable the logging of the internal execution of the IWF traffic to the debuglog (NOT LOGGED/LOGGED). \nThe resultant messages are intended for Sonus Networks engineering staff and are not intended for customer use. \nThe logging of execution trace must be set to NOT LOGGED on all production systems so that the CPU is not unduly taxed. \nDefault value: NOT LOGGED."
    ::= { ptiwfIWFConfigurationEntry 10 }

ptiwfIWFConfigurationDistributionManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable the logging of inter-IWF process communication to the debuglog (NOT LOGGED/LOGGED). \nThe resultant messages are intended for Sonus Networks engineering staff and are not intended for customer use. \nThe logging of inter-process communication must be set to NOT LOGGED on all production systems so that the CPU is not unduly taxed. \nDefault value: NOT LOGGED."
    ::= { ptiwfIWFConfigurationEntry 11 }

ptiwfIWFConfigurationActions OBJECT-TYPE
    SYNTAX          INTEGER { activateTraffic(1), deactivateTraffic(2), 
                        resetTracing(3), restoreDebugDefaults(4) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The OID of all possible actions for this instance"
    ::= { ptiwfIWFConfigurationEntry 252 }

