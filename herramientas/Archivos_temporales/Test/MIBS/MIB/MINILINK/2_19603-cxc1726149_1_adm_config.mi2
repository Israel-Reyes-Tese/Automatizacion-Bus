-- xfSdhNe configuration of interface - SpiOptical

xfSpiOpticalConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF XfSpiOpticalConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table lists all configurations defined for SpiOptical."
    ::= { xfSdhNeConfig 1 }



xfSpiOpticalConfigEntry OBJECT-TYPE
    SYNTAX      XfSpiOpticalConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry will be automatically created only when the particular
        equipment, related to this interface has been equipped.
        An entry will be automatically deleted when the particular
        equipment, related to this interface has been de-equipped."
    INDEX       { xfSpiOpticalConfigIfIndex }
    ::= { xfSpiOpticalConfigTable 1 }


XfSpiOpticalConfigEntry ::=
    SEQUENCE  {
        xfSpiOpticalConfigIfIndex              InterfaceIndex,
        xfSpiOpticalTransmitter                TruthValue,
        xfSpiOpticalAls                        TruthValue,
        xfSpiOpticalAutomaticRestart           TruthValue,
        xfSpiOpticalLaserManualCommands        INTEGER,
        xfSpiOpticalConfigLoop                 TruthValue,
        xfSpiOpticalConfigILoop                TruthValue,
        xfSpiOpticalConfigLoopAis              TruthValue }




xfSpiOpticalConfigIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The interface index for interface SPI Optical."

    ::= { xfSpiOpticalConfigEntry 1 }


xfSpiOpticalTransmitter OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Configuration to switch the transmitter unconditionally on (true) or off (false).
         If it's going to be switched off, no other mechanism, such as ALS will work.
         "
    DEFVAL { true }
    ::= { xfSpiOpticalConfigEntry 2 }

xfSpiOpticalAls OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Configuration whether the ALS functionality is enabled (true) or disabled (false) at all.
         If disabled, no ALS operation will work at all.
         "
    DEFVAL { true }
    ::= { xfSpiOpticalConfigEntry 3 }

xfSpiOpticalAutomaticRestart OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Configuration whether the LASER automatic restart mechanism is turned on (true) or off (false).
         The restart mechanism will only work, if ALS is enabled (true) as such.
        "
    DEFVAL { true }
    ::= { xfSpiOpticalConfigEntry 4 }

xfSpiOpticalLaserManualCommands OBJECT-TYPE
    SYNTAX INTEGER
           {
              noCommand(0),
              alsManualRestart(1),
              alsManualRestartForTest(2),
              alsManualRestartUserDefined(3)
            }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The als commands are trigger commands. If activated, they are performed once
         and the status after that will switch to noCommand. The next state of the ALS
         state machine will be automatic restart on or off, dependent on the configuration
         of xfSDHsonetMediumAutomaticRestart.
         The commands:
         alsManualRestart:            Transmitter on for 2 seconds
         alsManualRestartForTest:     Transmitter on for 90 seconds
         alsManualRestartUserDefined: Transmitter on for user defined seconds,
                                      fixed to 12 seconds at the moment
         "
    DEFVAL { noCommand }
    ::= { xfSpiOpticalConfigEntry 5 }


xfSpiOpticalConfigLoop OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Configuration of the external loop."
    DEFVAL { false }
    ::= { xfSpiOpticalConfigEntry 6 }

xfSpiOpticalConfigILoop OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
