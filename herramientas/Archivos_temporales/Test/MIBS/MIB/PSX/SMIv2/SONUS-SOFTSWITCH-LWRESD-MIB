sonusSoftSwitchLwresdZoneHpcCounters OBJECT IDENTIFIER
    ::= { sonusSoftSwitchLwresdStatistics 4 }

sonusSoftSwitchLwresdHpcCountersZoneTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSoftSwitchLwresdHpcCountersZoneEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Zone table to hold ENUM/DNS Zone Hpc counters."
    ::= { sonusSoftSwitchLwresdZoneHpcCounters 1 }

sonusSoftSwitchLwresdHpcCountersZoneEntry OBJECT-TYPE
    SYNTAX          SonusSoftSwitchLwresdHpcCountersZoneEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the ENUM/DNS Zone table."
    INDEX           { sonusSoftSwitchLwresdHpcCountersZoneNameLength, 
                      sonusSoftSwitchLwresdHpcCountersZoneName, 
                      sonusSoftSwitchLwresdHpcCountersZoneTransportType }
    ::= { sonusSoftSwitchLwresdHpcCountersZoneTable 1 }

SonusSoftSwitchLwresdHpcCountersZoneEntry ::=
    SEQUENCE {
        sonusSoftSwitchLwresdHpcCountersZoneNameLength              SonusSoftSwitchDomainNameLength,
        sonusSoftSwitchLwresdHpcCountersZoneName        SonusSoftSwitchDomainName,
        sonusSoftSwitchLwresdHpcCountersZoneTransportType                 Integer32,
        sonusSoftSwitchLwresdHpcCountersZoneHpcReq          Counter32,
        sonusSoftSwitchLwresdHpcCountersZoneHpcRsp          Counter32,
        sonusSoftSwitchLwresdHpcCountersZoneHpcNoAnsRsp               Counter32,
        sonusSoftSwitchLwresdHpcCountersZoneHpcTimeout              Counter32,
        sonusSoftSwitchLwresdHpcCountersZoneHpcDestUnreachable                      Counter32,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcReq             Counter32,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcRsp             Counter32,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcNoAnsRsp                  Counter32,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcTimeout                 Counter32,
        sonusSoftSwitchLwresdHpcCountersZoneNonHpcDestUnreachable                         Counter32
    }

sonusSoftSwitchLwresdHpcCountersZoneNameLength OBJECT-TYPE
    SYNTAX          SonusSoftSwitchDomainNameLength
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Length of the Zone Name."
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 1 }

sonusSoftSwitchLwresdHpcCountersZoneName OBJECT-TYPE
    SYNTAX          SonusSoftSwitchDomainName
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Zone Name for which counters are applicable."
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 2 }

sonusSoftSwitchLwresdHpcCountersZoneTransportType OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The transport type used towards the Zone.1-tcp, 2-udp, 3-tcp fallback"
    ::= { sonusSoftSwitchLwresdHpcCountersZoneEntry 3 }

