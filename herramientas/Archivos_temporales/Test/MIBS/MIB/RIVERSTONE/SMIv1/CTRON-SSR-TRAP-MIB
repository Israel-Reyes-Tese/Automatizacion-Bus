envPowerSupplyFailed TRAP-TYPE
    ENTERPRISE      envTrapGroup
    VARIABLES       { sysHwPowerSupply }
--     STATUS          mandatory
    DESCRIPTION     
            "A power supply on the sending device has failed. The 
             sysHwPowerSupply object identifies the failed supply."
    ::= 1

