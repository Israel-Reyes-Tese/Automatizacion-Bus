        sonusIpSecSpdIpSecProfileNamePrefOne    DisplayString,
        sonusIpSecSpdIpSecProfileNamePrefTwo    DisplayString,
        sonusIpSecSpdIpSecProfileNamePrefThree      DisplayString,
        sonusIpSecSpdTrafficSelectorLocalIp6Addr        Ipv6Address,
        sonusIpSecSpdTrafficSelectorLocalIpv6PrefixLength                 INTEGER,
        sonusIpSecSpdTrafficSelectorRemoteIp6Addr         Ipv6Address,
        sonusIpSecSpdTrafficSelectorRemoteIpV6PrefixLength                  INTEGER,
        sonusIpSecSpdTrafficSelectorIpAddrVersion         INTEGER
