    ::= { ptL4CvtrL4CvtrConfigurationEntry 11 }

ptL4CvtrL4CvtrConfigurationMessageContent OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable debug logging of the message content; \nmust not be logged for production systems. \nSystem logs to debug the raw hexadecimal dump of the SS7 messages."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 12 }

ptL4CvtrL4CvtrConfigurationMessageParsing OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable debug logging of the message parsing; \nmust not be logged for production systems. \nSystem logs to debug the detailed decoded parsed output of the messages."
    ::= { ptL4CvtrL4CvtrConfigurationEntry 13 }

