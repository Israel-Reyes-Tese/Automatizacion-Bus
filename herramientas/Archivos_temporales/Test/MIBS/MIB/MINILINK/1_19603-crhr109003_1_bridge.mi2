			DESCRIPTION
				"Shows whether NPU/Node warm restart will result in Ethernet traffic
				disturbance.
				
				true(1)     : NPU/Node warm restart is hitless, 
				              i.e. there will be no traffic disturbance 
				              as a result of the warm restart.
				
				false(2)    : NPU/Node warm restart is not hitless, 
				              i.e. there will be traffic disturance
				              as a result of the warm restart."
			::= { xfNPU3ConfigEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.13
		xfBridgeRLIMEGroupTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfBridgeRLIMEGroupEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table represent the media spesific MO on RL-IME group level.
				
				Note that the RL-IME group also support speed (in the ifTable) as the SUM of the active links. 
				The RL-IME group will also support IfUpDownTrapEenable and ifOperstatus (up, Down and LowerLayerDown).
				
				If no connection is done towards the lower layer shall:
				
				ifSpeed = xfBridgeRLIMEGroupMaxSpeed = xfBridgeRLIMEGroupMinSpeed = 0;
				xfBridgeRLIMEGroupAlarmStatus shall be set to 0 (no alarm);
				xfBridgeRLIMEGroupSegmentationStatus = unknown.
				xfBridgeRLIMEGroupUsedLink = 0;
				ifOperStatus = lowerLayerDown;
				
				"
			::= { xfBridgeObjects 13 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.13.1
		xfBridgeRLIMEGroupEntry OBJECT-TYPE
			SYNTAX XfBridgeRLIMEGroupEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table"
			INDEX { ifIndex }
			::= { xfBridgeRLIMEGroupTable 1 }

		
		XfBridgeRLIMEGroupEntry ::=
			SEQUENCE { 
				xfBridgeRLIMEGroupAlarmEnable
					BITS,
				xfBridgeRLIMEGroupAlarmStatus
					BITS,
				xfBridgeRLIMEGroupSegmentationEnable
					INTEGER,
				xfBridgeRLIMEGroupSegmentationStatus
					INTEGER,
				xfBridgeRLIMEGroupMaxSpeed
					Gauge32,
				xfBridgeRLIMEGroupMinSpeed
					Gauge32,
				xfBridgeRLIMEGroupSupportedLinks
					Integer32,
				xfBridgeRLIMEGroupUsedLinks
					Integer32,
				xfBridgeRLIMEGroupFreeCap
					Integer32,
				xfBridgeRLIMEGroupMaxCap
					Integer32
			 }

		-- 1.3.6.1.4.1.193.81.4.1.1.1.13.1.1
		xfBridgeRLIMEGroupAlarmEnable OBJECT-TYPE
			SYNTAX BITS
				{
				degradedService(0),
				noTraffic(1),
				reassemblyFailure(2),
				oversubscription(3),
				llUnableToProtect(4)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This MO is used to enable one or more alarms on RL-IME Group level.
				
				Traps can be enabled for No Traffic alarm, Reassembly Failure Alarm, 
				Degraded Service Alarm, Oversubscription Alarm and Lower Layer Unable To Protect Alarm.
				
				Default No Traffic Alarm, Reassembly Failure Alarm, Oversubscription Alarm and Lower
				Layer Unable To Protect Alarm are enabled.
				
				Note that the if the ifLinkUpDownTrapEnable MO is set to disabled, all alarms will be turned off.
				"
			::= { xfBridgeRLIMEGroupEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.13.1.2
		xfBridgeRLIMEGroupAlarmStatus OBJECT-TYPE
			SYNTAX BITS
				{
				rlimeDegradedService0(0),
				rlimeDegradedService1(1),
				rlimeDegradedService2(2),
				rlimeNoTraffic0(3),
				rlimeNoTraffic1(4),
				rlimeNoTraffic2(5),
				rlimeReassemblyFailure0(6),
				rlimeReassemblyFailure1(7),
				rlimeReassemblyFailure2(8),
				rlimeOversubscription0(9),
				rlimeOversubscription1(10),
				rlimeOversubscription2(11),
				rlimeLLUnableToProtect0(12),
				rlimeLLUnableToProtect1(13),
				rlimeLLUnableToProtect2(14)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the alarm status of the RL-IME Group interface.
				
				Severity is indicated by three bits per alarm:
				
				bits 012
					 000   clear
					 001   undetermined
					 010   warning
					 011   minor
					 100   major
					 101   critical
				
				
				
				"
			::= { xfBridgeRLIMEGroupEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.13.1.3
		xfBridgeRLIMEGroupSegmentationEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				enabled(1),
				disabled(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This MO enables segmentation if the group is configured with only one link. 
				The MO can be configured independently of the number of links configured on the group,  but it is only applicable when only one link is configured.
				
				It will not be possible to set this MO if the xfBridgeRLIMESegmentationStatus is in the state unKnown or notSupported.
				
				Default this MO is set to value disabled
				"
			::= { xfBridgeRLIMEGroupEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.13.1.4
		xfBridgeRLIMEGroupSegmentationStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				active(1),
				inactive(2),
				unknown(3),
				notSupported(4)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This MO indicates the current status of segementation.
				
				"
			::= { xfBridgeRLIMEGroupEntry 4 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.13.1.5
		xfBridgeRLIMEGroupMaxSpeed OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
