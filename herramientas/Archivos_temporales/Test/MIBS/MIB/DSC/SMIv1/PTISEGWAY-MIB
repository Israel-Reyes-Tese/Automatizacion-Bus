    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Associated slot of the Application or Routing CPU of the IWF for this SCCP connection. \nOnly one SCCP Connection per slot. \nValid values: integers between 1 and 65535 inclusive."
    ::= { ptiwfSCCPConnectionEntry 1 }

ptiwfSCCPConnectionConnectivityStatus OBJECT-TYPE
    SYNTAX          INTEGER { inactive(0), notConnected(1), connected(2) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Connectivity Status (INACTIVE, NOT CONNECTED, CONNECTED). \nAssociated to alarm 1909 (IWF SCCP Connection Down)."
    ::= { ptiwfSCCPConnectionEntry 2 }

ptiwfSCCPConnectionHostname OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Associated hostname of the Routing CPU for this SCCP connection. \nBy default, set to localhost when the IWF and the SCCP run on the same CPU \nIf not set to localhost, the host must be unique across all slots. \nValid values: alphanumeric characters, dashes, underscores, and periods. Length must be between 1 and 32 inclusive. Must start with alphabetic, and end with alphanumeric character."
    ::= { ptiwfSCCPConnectionEntry 3 }

