        ptsnamiSNAMIConfigurationErrors INTEGER,
        ptsnamiSNAMIConfigurationSetupManagement        INTEGER,
        ptsnamiSNAMIConfigurationExecutionTrace       INTEGER,
        ptsnamiSNAMIConfigurationAuditLog INTEGER,
        ptsnamiSNAMIConfigurationActions INTEGER,
        ptsnamiSNAMIConfigurationParentOID  OBJECT IDENTIFIER,
        ptsnamiSNAMIConfigurationRowStatus  RowStatus,
        ptsnamiSNAMIConfigurationIndex  INTEGER
    }

