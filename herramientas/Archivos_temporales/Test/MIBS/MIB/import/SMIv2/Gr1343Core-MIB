amaAudit OBJECT IDENTIFIER
    ::= { ama 5 }

amaAuditTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF AmaAuditEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The (conceptual) table of auditing information, containing counts of incoming
             and outgoing primary AMA records, counts of changes to the number of
             primary AMA records in storage, and counts of primary AMA records upon
             which specialized processing has been performed. Each row contains
             accumulated counts of records processed over a fifteen-minute interval of the
             day. Row 0 represents midnight-12:15 AM, row 1 represents 12:15-12:30 AM, and
             so forth, until row 95, which represents 11:45 PM- midnight."
    ::= { amaAudit 1 }

amaAuditEntry OBJECT-TYPE
    SYNTAX          AmaAuditEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The (conceptual) entry of counts of changes to the number of records in
             storage, and counts of records upon which special processing has been
             performed, for a single fifteen-minute time interval."
    INDEX           { amaAuditTimeIndex }
    ::= { amaAuditTable 1 }

AmaAuditEntry ::=
    SEQUENCE {
        amaAuditTimeIndex               Integer32,
        amaAuditAMARecsIn               Counter32,
        amaAuditAMARecsOut              Counter32,
        amaAuditAMARecsConsolidated     Counter32,
        amaAuditAMARecsIntroduced       Counter32,
        amaAuditChangeInAMARecsStored   Integer32,
        amaAuditAMACalcRecsLost         Counter32,
        amaAuditAMACalcRecsDupl         Counter32,
        amaAuditAMADetectRecsLost       Counter32,
        amaAuditAMADetectRecsDupl       Counter32
    }

amaAuditTimeIndex OBJECT-TYPE
    SYNTAX          Integer32 (0..95)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The index to the rows of the AmaAuditSPMTable, and one of the indices to the
             rows of the amaAuditInOutTable. Each integer represents a fifteen-minute
             interval of the day, with the index '0' corresponding to midnight-12:15 AM, and
             the index '95' corresponding to 11:45 PM-midnight."
    ::= { amaAuditEntry 1 }

amaAuditAMARecsIn OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The total number of primary AMA records which arrive over any interface
             (network interface or removable media interface) in the time interval specified
             by the amaAuditTimeIndex."
    ::= { amaAuditEntry 2 }

amaAuditAMARecsOut OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The total number of primary AMA records which depart (i.e., become non-primary)
             over any interface (network interface or removable media interface)
             in the time interval specified by the amaAuditTimeIndex."
    ::= { amaAuditEntry 3 }

amaAuditAMARecsConsolidated OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of primary AMA records removed by the aggregation operation in
             the time interval specified by the amaAuditTimeIndex. For example, if 10
             primary AMA records are aggregated and 1 composite record is produced, this
             counter should be incremented by 9."
    ::= { amaAuditEntry 4 }

amaAuditAMARecsIntroduced OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of primary AMA records created by the distribution or
             decomposition operations in the time interval specified by the
             amaAuditTimeIndex. For example, if 1 primary AMA record is distributed to 10
             Application Systems, this counter should be incremented by 9."
    ::= { amaAuditEntry 5 }

amaAuditChangeInAMARecsStored OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The change in the number of primary AMA records residing in mass storage in
             the time interval specified by the amaAuditTimeIndex."
    ::= { amaAuditEntry 6 }

amaAuditAMACalcRecsLost OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of primary AMA records calculated to have been lost in the time
             interval specified by the amaAuditTimeIndex. A trap is generated when this
             counter is incremented."
    ::= { amaAuditEntry 7 }

amaAuditAMACalcRecsDupl OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of primary AMA records calculated to have been unintentionally
             duplicated in the time interval specified by the amaAuditTimeIndex. A trap is
             generated when this counter is incremented."
    ::= { amaAuditEntry 8 }

amaAuditAMADetectRecsLost OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of primary AMA records detected to have been lost in the time
             interval specified by the amaAuditTimeIndex. A trap is generated when this
             counter is incremented."
    ::= { amaAuditEntry 9 }

amaAuditAMADetectRecsDupl OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of primary AMA records detected to have been unintentionally
             duplicated in the time interval specified by the amaAuditTimeIndex. A trap is
             generated when this counter is incremented."
    ::= { amaAuditEntry 10 }

access OBJECT IDENTIFIER
    ::= { gr1343Core 3 }

accessLogin OBJECT IDENTIFIER
    ::= { access 1 }

accessLoginTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF AccessLoginEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The (conceptual) table of users' identification, authentication and access
             control information. A user could be a human or machine."
    ::= { accessLogin 1 }

accessLoginEntry OBJECT-TYPE
    SYNTAX          AccessLoginEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The (conceptual) entry for a user's access information."
    INDEX           { accessLoginIndex }
    ::= { accessLoginTable 1 }

AccessLoginEntry ::=
    SEQUENCE {
        accessLoginIndex                Integer32,
        accessLoginLoginId              DisplayString,
        accessLoginPassword             DisplayString,
        accessLoginCommunityName        DisplayString,
        accessLoginNumLogins            Counter32,
        accessLoginLastLogin            DateAndTime,
        accessLoginAuthorFtp            INTEGER,
        accessLoginAuthorRda            INTEGER,
        accessLoginAuthorTelnet         INTEGER,
        accessLoginAuthorTftp           INTEGER,
        accessLoginEntryStatus          INTEGER
    }

accessLoginIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..2147483647)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A unique value identifying the user."
    ::= { accessLoginEntry 1 }

accessLoginLoginId OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(6..20))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The user's login ID. This string is used for local identification and for remote
             identification via FTP, Telnet and (if supported) RDA."
    ::= { accessLoginEntry 2 }

accessLoginPassword OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(6..20))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The user's plaintext password. This string is used for local authentication and/
             or for remote identification via FTP, Telnet and (if supported) RDA."
    ::= { accessLoginEntry 3 }

accessLoginCommunityName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(6..20))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The user's community name. This string is sent in SNMPv1 PDUs."
    ::= { accessLoginEntry 4 }

accessLoginNumLogins OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The total number of logins to the local host by the user via local login, FTP,
             Telnet and (if supported) RDA."
    ::= { accessLoginEntry 5 }

accessLoginLastLogin OBJECT-TYPE
    SYNTAX          DateAndTime
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The date and time of the last successful login to the local host by the user."
    ::= { accessLoginEntry 6 }

accessLoginAuthorFtp OBJECT-TYPE
    SYNTAX          INTEGER { yes(1), no(2), unknown(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates if the user is authorized to set up an FTP session to/from the local
             host."
    ::= { accessLoginEntry 7 }

accessLoginAuthorRda OBJECT-TYPE
    SYNTAX          INTEGER { yes(1), no(2), unknown(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates if the user is authorized to set up an RDA session to/from the local
             host."
    ::= { accessLoginEntry 8 }

accessLoginAuthorTelnet OBJECT-TYPE
    SYNTAX          INTEGER { yes(1), no(2), unknown(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates if the user is authorized to set up a Telnet session to/from the local
             host."
    ::= { accessLoginEntry 9 }

accessLoginAuthorTftp OBJECT-TYPE
    SYNTAX          INTEGER { yes(1), no(2), unknown(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates if the user is authorized to set up a TFTP session to/from the local
             host."
    ::= { accessLoginEntry 10 }

accessLoginEntryStatus OBJECT-TYPE
    SYNTAX          INTEGER { valid(1), invalid(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This object indicates if a table entry is valid or invalid. The value of this object
             is set to 1 when the entry is active and is set to 2 when the entry is inactive."
    ::= { accessLoginEntry 11 }

accessAssociation OBJECT IDENTIFIER
    ::= { access 2 }

accessAssociationTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF AccessAssociationEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The (conceptual) table of IP security association information."
    ::= { accessAssociation 1 }

accessAssociationEntry OBJECT-TYPE
    SYNTAX          AccessAssociationEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The (conceptual) entry for one destination component's security association
             information."
    INDEX           { sessionCompIndex }
    ::= { accessAssociationTable 1 }

AccessAssociationEntry ::=
    SEQUENCE {
        accessAssociationAHAlg          INTEGER,
        accessAssociationAHKey          OCTET STRING,
        accessAssociationAHKeySize      Integer32,
        accessAssociationESPAlg         INTEGER,
        accessAssociationESPKey         OCTET STRING,
        accessAssociationESPKeySize     Integer32,
        accessAssociationIVPresence     INTEGER,
        accessAssociationIVSize         Integer32
    }

accessAssociationAHAlg OBJECT-TYPE
    SYNTAX          INTEGER { noah(1), keyedmd5(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates the authentication algorithm and algorithm mode being used with the
             IP Authentication Header (AH) mechanism."
    ::= { accessAssociationEntry 1 }

accessAssociationAHKey OBJECT-TYPE
    SYNTAX          OCTET STRING (SIZE(8..128))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The key used with the AH authentication algorithm. The key is a bit string
             which contains a minimum of 64 bits and a maximum of 1024 bits. The security
             key has no translation into an integer or characters."
    ::= { accessAssociationEntry 2 }

accessAssociationAHKeySize OBJECT-TYPE
    SYNTAX          Integer32 (64..1024)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates the number of low-order bits for the key used with the AH
             authentication algorithm."
    ::= { accessAssociationEntry 3 }

accessAssociationESPAlg OBJECT-TYPE
    SYNTAX          INTEGER { noesp(1), descbc(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates the encryption algorithm and algorithm mode being used with the IP
             Encapsulating Security Payload (ESP) mechanism."
    ::= { accessAssociationEntry 4 }

accessAssociationESPKey OBJECT-TYPE
    SYNTAX          OCTET STRING (SIZE(8..128))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The key used with the ESP encryption algorithm. The key is a bit string which
             contains a minimum of 64 bits and a maximum of 1024 bits. The security key has
             no translation into an integer or characters."
    ::= { accessAssociationEntry 5 }

accessAssociationESPKeySize OBJECT-TYPE
    SYNTAX          Integer32 (64..1024)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates the number of low-order bits for the key used with the ESP
             encryption algorithm."
    ::= { accessAssociationEntry 6 }

accessAssociationIVPresence OBJECT-TYPE
    SYNTAX          INTEGER { present(1), absent(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates the presence/absence of the Initialization Vector used with the ESP
             algorithm. The IV enables the encryption algorithm to decrypt datagrams
             without having to receive the remaining segments of the ESP payload."
    ::= { accessAssociationEntry 7 }

accessAssociationIVSize OBJECT-TYPE
    SYNTAX          Integer32 (1..256)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Indicates the number of bits for the Initialization Vector used with the ESP
             encryption algorithm."
    ::= { accessAssociationEntry 8 }

END -- end of module Gr1343Core-MIB.
