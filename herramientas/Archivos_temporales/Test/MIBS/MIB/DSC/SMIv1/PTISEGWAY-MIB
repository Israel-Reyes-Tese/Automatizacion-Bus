    SYNTAX          PtSWMSwitchMonitorEntry
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "A sequence of SWMSwitchMonitor attributes."
    INDEX           { ptConnGroupIndex, ptSWMSwitchMonitorIndex }
    ::= { ptSWMSwitchMonitorTable 1 }

PtSWMSwitchMonitorEntry ::=
    SEQUENCE {
        ptSWMSwitchMonitorErrors        INTEGER,
        ptSWMSwitchMonitorSetupManagement INTEGER,
        ptSWMSwitchMonitorExecutionTrace INTEGER,
        ptSWMSwitchMonitorPeerManagement INTEGER,
        ptSWMSwitchMonitorActions       INTEGER,
        ptSWMSwitchMonitorParentOID     OBJECT IDENTIFIER,
        ptSWMSwitchMonitorRowStatus     RowStatus,
        ptSWMSwitchMonitorIndex         INTEGER
    }

ptSWMSwitchMonitorErrors OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable debug logging of the Switch Monitor errors; \nshould be logged for production systems."
    ::= { ptSWMSwitchMonitorEntry 22 }

ptSWMSwitchMonitorSetupManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable debug logging of setup management; \nshould be logged for production systems."
    ::= { ptSWMSwitchMonitorEntry 23 }

ptSWMSwitchMonitorExecutionTrace OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable debug logging of the execution trace; \nmust not be logged for production systems."
    ::= { ptSWMSwitchMonitorEntry 24 }

ptSWMSwitchMonitorPeerManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Enable or disable debug logging of peer management; \nmust not be logged for production systems."
    ::= { ptSWMSwitchMonitorEntry 25 }

ptSWMSwitchMonitorActions OBJECT-TYPE
    SYNTAX          INTEGER { restoreDebugDefaults(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The OID of all possible actions for this instance"
    ::= { ptSWMSwitchMonitorEntry 252 }

