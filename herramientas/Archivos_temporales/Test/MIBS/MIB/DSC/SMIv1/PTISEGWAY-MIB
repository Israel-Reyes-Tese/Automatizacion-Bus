    SEQUENCE {
        ptSGM3UAOPCCICRangeCICMinimum   INTEGER,
        ptSGM3UAOPCCICRangeCICMaximum   INTEGER,
        ptSGM3UAOPCCICRangeOPC          INTEGER,
        ptSGM3UAOPCCICRangeM3UAIndex    INTEGER,
        ptSGM3UAOPCCICRangeM3UAAppServersIndex      INTEGER,
        ptSGM3UAOPCCICRangeM3UAAppServerIndex     INTEGER,
        ptSGM3UAOPCCICRangeParentOID    OBJECT IDENTIFIER,
        ptSGM3UAOPCCICRangeRowStatus    RowStatus,
        ptSGM3UAOPCCICRangeIndex        INTEGER
    }

ptSGM3UAOPCCICRangeCICMinimum OBJECT-TYPE
    SYNTAX          INTEGER { undefined(-1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The Minimum CIC value is used in conjunction with the Maximum CIC value in order to augment the M3UA Routing Key to which the CIC range belongs. The range allows the ASP to restrict the traffic that it will receive to only that traffic for those circuits included in the configured CIC ranges "
    ::= { ptSGM3UAOPCCICRangeEntry 1 }

ptSGM3UAOPCCICRangeCICMaximum OBJECT-TYPE
    SYNTAX          INTEGER { undefined(-1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The Maximum CIC value is used in conjunction with the Minimum CIC value in order to augment the M3UA Routing Key to which the CIC range belongs. The range allows the ASP to restrict the traffic that it will receive to only that for those circuits included in the configured CIC ranges "
    ::= { ptSGM3UAOPCCICRangeEntry 2 }

ptSGM3UAOPCCICRangeOPC OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "The OPC is an optional parameter which is used to augment M3UA Routing Keys. The addition of an OPC or a list of OPCs enables a Routing Key to be created where by an ASP can restrict the traffic it receives to only come from those originators listed. Please Note that if an OPC is configured then the ASP will not receive traffic from any other originating point codes"
    ::= { ptSGM3UAOPCCICRangeEntry 3 }

ptSGM3UAOPCCICRangeM3UAIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is M3UA."
    ::= { ptSGM3UAOPCCICRangeEntry 242 }

ptSGM3UAOPCCICRangeM3UAAppServersIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is M3UAAppServers."
    ::= { ptSGM3UAOPCCICRangeEntry 243 }

ptSGM3UAOPCCICRangeM3UAAppServerIndex OBJECT-TYPE
