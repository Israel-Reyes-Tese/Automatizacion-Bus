    SEQUENCE {
        sonusDBLAppRuleStatAppType      Gauge,
        sonusDBLAppRuleStatAppIndex     Gauge,
        sonusDBLAppRuleStatRuleIndex    Gauge,
--         sonusDBLAppRuleStatCumulativeEnforcements         Counter64,
        sonusDBLAppRuleStatCurrentPolicerEnforcements             Counter,
        sonusDBLAppRuleStatPreemptedPolicerEnforcements               Counter
    }

