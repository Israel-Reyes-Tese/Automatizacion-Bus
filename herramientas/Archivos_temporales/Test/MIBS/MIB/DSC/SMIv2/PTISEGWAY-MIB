        ptMTPAnnexALinkMTP3NAIndex      Integer32,
        ptMTPAnnexALinkLinksetsIndex    Integer32,
        ptMTPAnnexALinkLinksetIndex     Integer32,
        ptMTPAnnexALinkActions          INTEGER,
        ptMTPAnnexALinkParentOID        OBJECT IDENTIFIER,
        ptMTPAnnexALinkRowStatus        RowStatus,
        ptMTPAnnexALinkIndex            Integer32
    }

