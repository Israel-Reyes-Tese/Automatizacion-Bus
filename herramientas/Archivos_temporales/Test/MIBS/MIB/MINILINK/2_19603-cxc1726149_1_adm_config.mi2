-- xfSdhNe configuration of interface - MSTTP

xfMsTtpConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF XfMsTtpConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table lists all configurations defined for the Multiplex Section."
    ::= { xfSdhNeConfig 4 }



xfMsTtpConfigEntry OBJECT-TYPE
    SYNTAX      XfMsTtpConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry will be automatically created only when the
        particular equipment, related to this interface
        has been equipped.
        An entry will be automatically deleted when the
        particular equipment, related to this interface
        has been de-equipped."
    INDEX       { xfMsTtpIndex }
    ::= { xfMsTtpConfigTable 1 }


XfMsTtpConfigEntry ::=
    SEQUENCE  {
        xfMsTtpIndex                InterfaceIndex,
        xfMsSdMode                  XfSdMode,
        xfMsTtpExcThreshold         XfExcThreshold,
        xfMsTtpSdThreshold          XfSdThreshold,
        xfMsDEGM                    XfDEGM,
        xfMsDEGTHR                  XfDEGTHR,
        xfEgressSSM                 XfOverwriteQualityLevel,
        xfSdhStmClockSquelch        TruthValue,
        xfMsTtpLoops                INTEGER 
    }



xfMsTtpIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The interface index for interface Multiplex Section."

    ::= { xfMsTtpConfigEntry 1 }



xfMsSdMode OBJECT-TYPE
            SYNTAX XfSdMode
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                " Configuration whether poisson distribution of errors or bursty distribution of errors is assumed
                  for signal degradation calculation. If bursty distribution is assumed, the DEGTHTR and DEGM values become valid,
                  otherwise, in case of poisson distribution assumed, the SdThreshold becomes valid.
                "
            DEFVAL { burstyDistribution }
            ::= { xfMsTtpConfigEntry 2 }


xfMsTtpExcThreshold OBJECT-TYPE
    SYNTAX      XfExcThreshold
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "In case of poisson distribution of errors assumed, this is the configuration
        of the particular EXC Threshold."
    DEFVAL { -3 }
    ::= { xfMsTtpConfigEntry 3 }


xfMsTtpSdThreshold OBJECT-TYPE
    SYNTAX      XfSdThreshold
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "In case of poisson distribution of errors assumed, this is the configuration
        of the particular SD (DEG) Threshold."
    DEFVAL { -6 }
    ::= { xfMsTtpConfigEntry 4 }

xfMsDEGM OBJECT-TYPE
    SYNTAX      XfDEGM
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The degraded signal defect (dDEG) shall be declared if DEGM consecutive bad intervals
        (interval is the 1-second period used for performance monitoring) are detected.
        An interval is declared bad if the percentage of detected errored blocks in that interval,
        or the number of errored blocks in that interval >= Degraded Threshold (DEGTHR).
        For the case of dDEG in the MSn layer, the errored block is equal to a BIP violation.
        The degraded signal defect shall be cleared if DEGM consecutive good intervals are detected.
        An interval shall be declared good if the percentage of detected errored blocks in that interval,
        or the number of errored blocks in that interval < DEGTHR.
        The parameter DEGM shall be provisionable in the range 2 to 10.
        "
    DEFVAL { 7 }
    ::= { xfMsTtpConfigEntry 5 }

 xfMsDEGTHR OBJECT-TYPE
    SYNTAX      XfDEGTHR
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The parameter DEGTHR shall be provisioned as a percentage of errored blocks. It shall be in the range 0 <  DEGTHR  <= 100%.
        For higher rate interfaces, one per cent is equal to a large number of blocks. For example, in an STM-16 interface,
        1% is equal to a step of 30720 blocks in the interval for the multiplex section.
         "
    DEFVAL { 15 }
    ::= { xfMsTtpConfigEntry 6 }


xfEgressSSM OBJECT-TYPE
    SYNTAX      XfOverwriteQualityLevel
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of the outgoing quality level. This is normally set automatically.
        However, some operators want to overwrite it anyway.
        "
   DEFVAL { -1 }
   ::= { xfMsTtpConfigEntry 7 }

xfSdhStmClockSquelch OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " This object is used to enable/disable Squelch
             false -> Disabled
             true -> Enabled
        "
   DEFVAL { false }
   ::= { xfMsTtpConfigEntry 8 }  
   
xfMsTtpLoops OBJECT-TYPE
    SYNTAX      INTEGER
           {
              noLoop(0),
              lineLoop(1),
              localLoop(2),
              lineAndLocalLoop(3)
            }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " This object is used to enable the different loop types."
   DEFVAL { noLoop }
   ::= { xfMsTtpConfigEntry 9 }



