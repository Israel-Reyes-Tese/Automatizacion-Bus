    SEQUENCE {
        ptiwfIWFNodeMappingName         DisplayString,
        ptiwfIWFNodeMappingDiameterID   DisplayString,
        ptiwfIWFNodeMappingDiameterRealm DisplayString,
        ptiwfIWFNodeMappingSS7Number    DisplayString,
        ptiwfIWFNodeMappingSCCPGTI      INTEGER,
        ptiwfIWFNodeMappingSCCPRTI      INTEGER,
        ptiwfIWFNodeMappingSCCPPAPC     INTEGER,
        ptiwfIWFNodeMappingSCCPSSN      INTEGER,
        ptiwfIWFNodeMappingSCCPTT       INTEGER,
        ptiwfIWFNodeMappingSCCPNOA      INTEGER,
        ptiwfIWFNodeMappingSCCPNP       INTEGER,
        ptiwfIWFNodeMappingMessageTrace INTEGER,
        ptiwfIWFNodeMappingDescription  DisplayString,
        ptiwfIWFNodeMappingRecordID     INTEGER,
        ptiwfIWFNodeMappingIWFNodeMappingsIndex       INTEGER,
        ptiwfIWFNodeMappingParentOID    OBJECT IDENTIFIER,
        ptiwfIWFNodeMappingRowStatus    RowStatus,
        ptiwfIWFNodeMappingIndex        INTEGER
    }

ptiwfIWFNodeMappingName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "IWF Node Mapping record unique name. \nValid values: alphanumeric characters, dashes, and underscores (maximum size of 16)."
    ::= { ptiwfIWFNodeMappingEntry 1 }

ptiwfIWFNodeMappingDiameterID OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Diameter Identifier representing the Diameter Node. \nMust be unique across all Node Mappings. \nValid values: FQDN format (as per the DSC ADN Diameter ID validation)."
    ::= { ptiwfIWFNodeMappingEntry 2 }

ptiwfIWFNodeMappingDiameterRealm OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Diameter Realm of the Diameter Node. \nValid values: FQDN format (as per the DSC Realm Definition validation)."
    ::= { ptiwfIWFNodeMappingEntry 3 }

ptiwfIWFNodeMappingSS7Number OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Digit representation of the SS7 Node. \nTypically the E.164 number of the node. \nMust be unique across all Node Mappings. \nValid values: hexadecimal digit string (maximum size of 32)."
    ::= { ptiwfIWFNodeMappingEntry 4 }

ptiwfIWFNodeMappingSCCPGTI OBJECT-TYPE
    SYNTAX          INTEGER { gti0000(0), gti0001(1), gti0010(2), 
                        gti0011(3), gti0100(4) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Party Address Global Title Indicator (GTI 0000, GTI 0001, GTI 0010, GTI 0011, GTI 0100). \nCannot be set if the IWF SS7 SCCP PA Encoding attribute is not set. \nNote that GTI 0011 and GTI 0100 are only valid in ITU. \nDefault: GTI 0001 (ANSI), GTI 0100 (ITU)."
    ::= { ptiwfIWFNodeMappingEntry 5 }

ptiwfIWFNodeMappingSCCPRTI OBJECT-TYPE
    SYNTAX          INTEGER { routeOnPcssn(1), routeOnGt(2) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Party Address Routing Indicator (ROUTE ON PCSSN, ROUTE ON GT). \nIf set to ROUTE ON PCSSN, the SCCP SSN must be defined. \nDefault value: ROUTE OF GT."
    ::= { ptiwfIWFNodeMappingEntry 6 }

ptiwfIWFNodeMappingSCCPPAPC OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Party Address Point Code. \nSet to -1 (UNSET) to exclude the PC from the SCCP PA. \nCannot be set if the IWF SS7 PC format attributes are not set. \nValid values: point code format. \nDefault value: UNSET."
    ::= { ptiwfIWFNodeMappingEntry 7 }

ptiwfIWFNodeMappingSCCPSSN OBJECT-TYPE
    SYNTAX          INTEGER { unset(-1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Party Address Subsystem Number (-1 (UNSET), 0 to 255). \nSet to -1 (UNSET) to exclude the SSN from the SCCP PA. \nDefault value: UNSET."
    ::= { ptiwfIWFNodeMappingEntry 8 }

ptiwfIWFNodeMappingSCCPTT OBJECT-TYPE
    SYNTAX          INTEGER (0..255)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Party Address Translation Type (0 to 255). \nIncluded in the SCCP PA based on the GTI attribute (ANSI 0001/0010, ITU 0010/0011/0100). \nDefault value: 0."
    ::= { ptiwfIWFNodeMappingEntry 9 }

ptiwfIWFNodeMappingSCCPNOA OBJECT-TYPE
    SYNTAX          INTEGER (0..137)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Party Address Nature of Address (0 to 127). \nIncluded in the SCCP PA based on the GTI attribute (ITU 0001/0100). \nDefault value: 0."
    ::= { ptiwfIWFNodeMappingEntry 10 }

ptiwfIWFNodeMappingSCCPNP OBJECT-TYPE
    SYNTAX          INTEGER (0..15)
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "SCCP Party Address Numbering Plan (0 to 15). \nIncluded in the SCCP PA based on the GTI attribute (ANSI 0001, ITU 0011/0100). \nDefault value: 1."
    ::= { ptiwfIWFNodeMappingEntry 11 }

ptiwfIWFNodeMappingMessageTrace OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Message tracing support for this Node Mapping (DISABLED/ENABLED). \nAll messages associated to this Node Mapping will be traced (originating and destination). \nDefault value: DISABLED."
    ::= { ptiwfIWFNodeMappingEntry 12 }

ptiwfIWFNodeMappingDescription OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "Node Mapping Description. Leave empty if not required. Valid values: free format string (maximum size of 64)."
    ::= { ptiwfIWFNodeMappingEntry 13 }

