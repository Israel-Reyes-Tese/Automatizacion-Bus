        xfStpMstiPortEntry OBJECT-TYPE
            SYNTAX XfStpMstiPortEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of MSTI port."
            INDEX { xfStpMstiId, dot1dStpPort }
            ::= { xfStpMstiPortTable 1 }

        XfStpMstiPortEntry ::=
            SEQUENCE {
                xfStpMstiPortEnable
                    EnabledStatus,
                xfStpMstiPortForwardTransitions
                    Integer32,
                xfStpMstiPortDesignatedBridge
                    BridgeId,
                xfStpMstiPortDesignatedCost
                    Integer32,
                xfStpMstiPortDesignatedPort
                    OCTET STRING,
                xfStpMstiPortDesignatedRoot
                    BridgeId,
                xfStpMstiPortDisputed
