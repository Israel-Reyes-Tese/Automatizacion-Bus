        sonusNodeSrvrStatCpuUtilization Integer32,
        sonusNodeSrvrStatHwTypeRev      Integer32,
        sonusNodeSrvrStatSwVersionCode  SonusSoftwareVersion,
        sonusNodeSrvrStatEpldRev        Integer32,
        sonusNodeSrvrStatHostName       DisplayString,
        sonusNodeSrvrStatUserName       DisplayString,
        sonusNodeSrvrStatViewName       DisplayString,
        sonusNodeSrvrStatTotalMem       Integer32,
        sonusNodeSrvrStatFreeMem        Integer32,
        sonusNodeSrvrStatTotalSharedMem Integer32,
        sonusNodeSrvrStatFreeSharedMem  Integer32,
        sonusNodeSrvrStatUpTime         Integer32,
        sonusNodeSrvrAvgCpuUsageOneMinute Integer32,
        sonusNodeSrvrAvgCpuUsageFiveMinutes   Integer32,
        sonusNodeSrvrAvgMemUsageOneMinute Integer32,
        sonusNodeSrvrAvgMemUsageFiveMinutes   Integer32
    }

