ptPCEPCE OBJECT IDENTIFIER
    ::= { ptConnGroup 451 }

ptPCEPCETable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtPCEPCEEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptPCEPCE 1 }

ptPCEPCEEntry OBJECT-TYPE
    SYNTAX          PtPCEPCEEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of PCEPCE attributes."
    INDEX           { ptConnGroupIndex, ptPCEPCEIndex }
    ::= { ptPCEPCETable 1 }

PtPCEPCEEntry ::=
    SEQUENCE {
        ptPCEPCEParentOID               OBJECT IDENTIFIER,
        ptPCEPCERowStatus               RowStatus,
        ptPCEPCEIndex                   Integer32
    }

ptPCEPCEParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptPCEPCEEntry 253 }

ptPCEPCERowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The row status for the PCEPCE table"
    ::= { ptPCEPCEEntry 254 }

ptPCEPCEIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The row number for the PCEPCE table"
    ::= { ptPCEPCEEntry 255 }

ptPCEPCEConfiguration OBJECT IDENTIFIER
    ::= { ptConnGroup 452 }

ptPCEPCEConfigurationTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtPCEPCEConfigurationEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "No description supplied in the proxy config file."
    ::= { ptPCEPCEConfiguration 1 }

ptPCEPCEConfigurationEntry OBJECT-TYPE
    SYNTAX          PtPCEPCEConfigurationEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of PCEPCEConfiguration attributes."
    INDEX           { ptConnGroupIndex, ptPCEPCEConfigurationIndex }
    ::= { ptPCEPCEConfigurationTable 1 }

PtPCEPCEConfigurationEntry ::=
    SEQUENCE {
        ptPCEPCEConfigurationName       DisplayString,
        ptPCEPCEConfigurationSS7Variant INTEGER,
        ptPCEPCEConfigurationSupportedTraffic     INTEGER,
        ptPCEPCEConfigurationMonoMode   INTEGER,
        ptPCEPCEConfigurationConfigurationType      INTEGER,
        ptPCEPCEConfigurationPCETraffic INTEGER,
        ptPCEPCEConfigurationUnavailablePeerIDs       DisplayString,
        ptPCEPCEConfigurationISUPManagement   INTEGER,
        ptPCEPCEConfigurationCGBGeneration  INTEGER,
        ptPCEPCEConfigurationUCICSupport INTEGER,
        ptPCEPCEConfigurationPrivatePCAuditing      INTEGER,
        ptPCEPCEConfigurationPublicPCAuditing     INTEGER,
        ptPCEPCEConfigurationTransactionTimeout       INTEGER,
        ptPCEPCEConfigurationVariant    INTEGER,
        ptPCEPCEConfigurationDefaultPrivateOPC      Integer32,
        ptPCEPCEConfigurationBroadcastToPrivate       INTEGER,
        ptPCEPCEConfigurationISUPLearning INTEGER,
        ptPCEPCEConfigurationISUPLearningDigits       INTEGER,
        ptPCEPCEConfigurationISUPLearningQueryTimeout             INTEGER,
        ptPCEPCEConfigurationErrors     INTEGER,
        ptPCEPCEConfigurationSetupManagement    INTEGER,
        ptPCEPCEConfigurationExecutionTrace   INTEGER,
        ptPCEPCEConfigurationLayerCommunication       INTEGER,
        ptPCEPCEConfigurationPeerManagement   INTEGER,
        ptPCEPCEConfigurationSS7Messages INTEGER,
        ptPCEPCEConfigurationMaxPublicNodes   Integer32,
        ptPCEPCEConfigurationMaxPrivateNodes    Integer32,
        ptPCEPCEConfigurationMaxPublicGateways      Integer32,
        ptPCEPCEConfigurationMaxGTTTunnelGateways         Integer32,
        ptPCEPCEConfigurationMaxTrunkMappings     Integer32,
        ptPCEPCEConfigurationMaxISUPLearningExceptions              Integer32,
        ptPCEPCEConfigurationTransactionIDMapping         INTEGER,
        ptPCEPCEConfigurationIS41BillingIDRouting         INTEGER,
        ptPCEPCEConfigurationMaxIS41BillingIDMappings             Integer32,
        ptPCEPCEConfigurationIS41Conversion   INTEGER,
        ptPCEPCEConfigurationLDLDebugLevel  Integer32,
        ptPCEPCEConfigurationLDLSCTPDebugLevel      Integer32,
        ptPCEPCEConfigurationMaxReportingPerInterval            Integer32,
        ptPCEPCEConfigurationReportingInterval      INTEGER,
        ptPCEPCEConfigurationForwardedPublicQueries           INTEGER,
        ptPCEPCEConfigurationFileManagement   INTEGER,
        ptPCEPCEConfigurationDatabaseManagement       INTEGER,
        ptPCEPCEConfigurationSCCPTCAPMapping    INTEGER,
        ptPCEPCEConfigurationNonMemberRoutesetSupport             INTEGER,
        ptPCEPCEConfigurationActions    INTEGER,
        ptPCEPCEConfigurationParentOID  OBJECT IDENTIFIER,
        ptPCEPCEConfigurationRowStatus  RowStatus,
        ptPCEPCEConfigurationIndex      Integer32
    }

ptPCEPCEConfigurationName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Name of the CPU currently 'Active', or in control of the User provisioned data. \nThe active peer is identified by the LDL peer ID as found in the PCE LDL file (default pce_ldl.conf). \nAll CPUs will be handling PCE traffic in a load sharing manner. However the Active CPU will be in control of the User Provisioned data. The CPU designated as active can change transparently to the user. This attribute maybe consulted during software upgrades."
    ::= { ptPCEPCEConfigurationEntry 1 }

ptPCEPCEConfigurationSS7Variant OBJECT-TYPE
    SYNTAX          INTEGER { undefined(255), ansi(1), itu(2) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "This attribute sets the SS7 variant (ANSI/ITU). The variant listed here must be set to agree with the variant set at the MTP3. Applies to all PCE defined NAs. \nThis attribute does NOT indicate the TCAP variant, which will be determined on a message by message basis, so that ANSI networks can use ANSI and ITU TCAP messages at the same time as per their IN, AIN and GSM requirements."
    ::= { ptPCEPCEConfigurationEntry 2 }

ptPCEPCEConfigurationSupportedTraffic OBJECT-TYPE
    SYNTAX          INTEGER { isup(0), tcap(1), tcapIsup(2) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "This attribute indicates the levels of traffic to be supported by this system. The valid options are: (ISUP/TCAP/TCAP_ISUP)."
    ::= { ptPCEPCEConfigurationEntry 4 }

ptPCEPCEConfigurationMonoMode OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Mono Mode is enabled during certain maintenance activities, usually as directed by Sonus Networks Customer Support. The Mono Mode feature indicates to the PCE software that the other PCE CPU(s) are still functioning, but are not in communication with this CPU. In such a situation, we do not want this PCE to send out Circuit Group Block (CGB) ISUP messages when a private switch is unavailable, because that switch may be available to the other PCE CPUs. \nThe default value for this attribute is DISABLED."
    ::= { ptPCEPCEConfigurationEntry 5 }

ptPCEPCEConfigurationConfigurationType OBJECT-TYPE
    SYNTAX          INTEGER { basic(0), enhanced(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "PCE Configuration Type (BASIC/ENHANCED) is based on your licensed capabilities. BASIC PCE is the most common form of PCE and divides the SS7 network into 1 Public and 1 Private network. The ENHANCED PCE is used to migrate trunks from one switch to another, where both switches have the same Point Code. ENHANCED PCE divides the SS7 network into 1 Public and 2 Private networks."
    ::= { ptPCEPCEConfigurationEntry 6 }

ptPCEPCEConfigurationPCETraffic OBJECT-TYPE
    SYNTAX          INTEGER { inactive(0), active(1), pceUnlicensed(2), 
                        enhancedUnlicensed(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "PCE Traffic (INACTIVE/ACTIVE/PCE_UNLICENSED); This attribute will indicate the state of the PCE traffic. The state can be changed between INACTIVE and ACTIVE, by using the action buttons on this screen. \nPCE requires you to intentionally deactivate traffic before changing some traffic impacting settings such as Variant. This is done as an extra check to confirm the change."
    ::= { ptPCEPCEConfigurationEntry 7 }

ptPCEPCEConfigurationUnavailablePeerIDs OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Should one or more CPUs lose communication with this CPU, then those CPUs will be listed here. The most common cause of communication problems are IP network related."
    ::= { ptPCEPCEConfigurationEntry 8 }

ptPCEPCEConfigurationISUPManagement OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable of disable the ISUP Management feature. \nIf ISUP Management is disabled, public network messages for unreachable private network destinations are discarded. \nIf ISUP Management is enabled, public network messages for unreachable private network destinations are processed (see PCE User Guide)."
    ::= { ptPCEPCEConfigurationEntry 9 }

ptPCEPCEConfigurationCGBGeneration OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable or disable the CGB Generation feature. \nThe CGB Generation feature only takes effect if the ISUP Management feature is also enabled."
    ::= { ptPCEPCEConfigurationEntry 10 }

ptPCEPCEConfigurationUCICSupport OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable or disable the Public Network UCIC Support."
    ::= { ptPCEPCEConfigurationEntry 11 }

ptPCEPCEConfigurationPrivatePCAuditing OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Interval at which the Private PC Audits are sent (in seconds); \nThis must be enabled if a private network MTP3 has cluster, network, or default Routesets. \nA point code audit will be automatically cleared on the receipt of a corresponding MTP-RESUME (MTP3 TFA) or MTP-LOCAL-STATUS Unavailable (all linksets unavailable). \nA value of zero seconds will turn off the feature. \nThe default value is 30 seconds."
    ::= { ptPCEPCEConfigurationEntry 12 }

ptPCEPCEConfigurationPublicPCAuditing OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Interval at which the Public PC Audits are sent (in seconds); \nThis must be enabled if the public network MTP3 has cluster, network, or default Routesets. \nA point code audit will be automatically cleared on the receipt of a corresponding MTP-RESUME (MTP3 TFA) or MTP-LOCAL-STATUS Unavailable (all linksets unavailable). \nA value of zero seconds will turn off the feature. \nThe default value is 30 seconds."
    ::= { ptPCEPCEConfigurationEntry 13 }

ptPCEPCEConfigurationTransactionTimeout OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Interval at which the TCAP mapping records are cleared (in seconds). \nThe completion of a transaction will clear the corresponding record automatically but an unfinished transaction must be cleared after their lifespan expires."
    ::= { ptPCEPCEConfigurationEntry 15 }

ptPCEPCEConfigurationVariant OBJECT-TYPE
    SYNTAX          INTEGER { autoDetect(0), ansi(1), itu(2) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "TCAP Variant (AUTO DETECT/ANSI/ITU). \nThe TCAP Variant can be determined on a per message basis based on the TCAP message type. If the TCAP Variant is not set to AUTO DETECT and a message is not supported in the given TCAP Variant, the message is discarded."
    ::= { ptPCEPCEConfigurationEntry 16 }

ptPCEPCEConfigurationDefaultPrivateOPC OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "TCAP Default Private OPC. \nFor public TCAP traffic to a private network where a Public Node is not defined, set the message OPC and SCCP CLG, if present, to the TCAP Default Private OPC. \nTo remove this feature set the value to '-1', which will remove the PC value from the attribute."
    ::= { ptPCEPCEConfigurationEntry 17 }

ptPCEPCEConfigurationBroadcastToPrivate OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), random(1), all(2) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Broadcast public initiated TCAP query to one or all Private Nodes. \nIf set to ALL, the public initiated TCAP query in forwarded to all Private Nodes. If set to RANDOM, the public initiated TCAP query will be sent to a randomly chosen Private Node. \nIf disabled and GTT routing has failed, the message will be discarded"
    ::= { ptPCEPCEConfigurationEntry 18 }

ptPCEPCEConfigurationISUPLearning OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Interval at which the TCAP ISUP Learning records are cleared (in seconds). \nSet to 0 (DISABLED) to disable the TCAP ISUP Learning feature."
    ::= { ptPCEPCEConfigurationEntry 19 }

ptPCEPCEConfigurationISUPLearningDigits OBJECT-TYPE
    SYNTAX          INTEGER { called(0), originalElseCalled(1), 
                        originalAndCalled(2), calledAndCalling(3), 
                        originalElseCalledAndCalling(4), all(5) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "ISUP IAM digit parameters which are used for TCAP ISUP Learning. \nSet to CALLED for Called Party Number parameter. \nSet to ORIGINAL ELSE CALLED for Original Called Number if present else Called Party Number parameter. \nSet to ORIGINAL AND CALLED for the Original Called Number if present and the Called Party Number parameter(s). \nSet to CALLED AND CALLING for Called and Calling Party Number parameters. \nSet to ORIGINAL ELSE CALLED AND CALLING for the Original Called Number if present else the Called Party Number, and the Calling Party Number. \nSet to ALL for Called, Calling and Original Called (if present) Party Number parameters."
    ::= { ptPCEPCEConfigurationEntry 20 }

ptPCEPCEConfigurationISUPLearningQueryTimeout OBJECT-TYPE
    SYNTAX          INTEGER { noneBroadcast(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "TCAP ISUP Learning query timeout for a Public TCAP query (in milliseconds). \nBy default, the timeout is set to NONE (BROADCAST) where each IAM immediately broadcasts its learned information to all peers. For heavy ISUP traffic with minimal TCAP queries, the query-and-response implementation is recommended with a 1000 milliseconds delay."
    ::= { ptPCEPCEConfigurationEntry 21 }

ptPCEPCEConfigurationErrors OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "If this flag is set to LOGGED, then error situation that require attention will be noted in the Debug Log file. The Syslog file may also contain error situation information that the operator should be aware off. \nIt is strongly recommended that the Error Debug Flag be set to LOGGED for all systems."
    ::= { ptPCEPCEConfigurationEntry 22 }

ptPCEPCEConfigurationSetupManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "If this flag is set to LOGGED, then information concerning the internal procedures that occur during process startup will be presented in the Debug Log file. \nIt is recommended that the Setup Management Debug Flag be set to LOGGED for all systems. The amount of data produced by this flag is not large, because it is referenced only at startup and in some recovery situations."
    ::= { ptPCEPCEConfigurationEntry 23 }

ptPCEPCEConfigurationExecutionTrace OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "If the Execution Trace debug flag is set to LOGGED, then information concerning the internal processing of the PCE process will be sent to the Debug Log file. This information is intended for Sonus Networks Customer Support staff and may not be legible by most customers. \n The recommended setting of this flag is NOT_LOGGED, so that the system resources are not unduly taken up in the production of log messages."
    ::= { ptPCEPCEConfigurationEntry 24 }

ptPCEPCEConfigurationLayerCommunication OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "If the Layer Communication debug flag is set to LOGGED, then information concerning the internal connections of the PCE process to the MTP3, SCCP and GTT will be sent to the Debug Log file. This information is intended for Sonus Networks Customer Support staff and may not be legible by many customers. \n The recommended setting of this flag is NOT_LOGGED, so that the system resources are not unduly taken up in the production of log messages."
    ::= { ptPCEPCEConfigurationEntry 25 }

ptPCEPCEConfigurationPeerManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "If the Peer Management debug flag is set to LOGGED, then information concerning the internal connections of the PCE process its peers on other CPUs will be sent to the Debug Log file. This information is intended for Sonus Networks Customer Support staff and may not be legible by many customers. \n The recommended setting of this flag is NOT_LOGGED, so that the system resources are not unduly taken up in the production of log messages."
    ::= { ptPCEPCEConfigurationEntry 26 }

ptPCEPCEConfigurationSS7Messages OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "If the SS7 Messages debug flag is set to LOGGED, then the contents of the SS7 messages will be presented in the Debug Log file. This information is very useful when first learning how the PCE works and how to provision it. However it can produce very large volumes of data and tax a busy system. \n The recommended setting of this flag is NOT_LOGGED, so that the system resources are not unduly taken up in the production of log messages."
    ::= { ptPCEPCEConfigurationEntry 27 }

