ptMTPATMLink OBJECT IDENTIFIER
    ::= { ptMTPLinkset 72 }

ptMTPATMLinkTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF PtMTPATMLinkEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of Link attributes."
    ::= { ptMTPATMLink 1 }

ptMTPATMLinkEntry OBJECT-TYPE
    SYNTAX          PtMTPATMLinkEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A sequence of MTPATMLink attributes."
    INDEX           { ptConnGroupIndex, ptMTPATMLinkMTP3NAManagerIndex, 
                      ptMTPATMLinkMTP3NAIndex, ptMTPATMLinkLinksetsIndex, 
                      ptMTPATMLinkLinksetIndex, ptMTPATMLinkIndex }
    ::= { ptMTPATMLinkTable 1 }

PtMTPATMLinkEntry ::=
    SEQUENCE {
        ptMTPATMLinkContext             DisplayString,
        ptMTPATMLinkAPC                 Integer32,
        ptMTPATMLinkSLC                 Integer32,
        ptMTPATMLinkDREID               Integer32,
        ptMTPATMLinkCage                Integer32,
        ptMTPATMLinkChannel             Integer32,
        ptMTPATMLinkL2State             INTEGER,
        ptMTPATMLinkL3State             INTEGER,
        ptMTPATMLinkCongestionLevel     INTEGER,
        ptMTPATMLinkDiagnosticMessage   DisplayString,
        ptMTPATMLinkL2DebugLevel        Integer32,
        ptMTPATMLinkUserActivated       INTEGER,
        ptMTPATMLinkLinkName            DisplayString,
        ptMTPATMLinkAlarms              INTEGER,
        ptMTPATMLinkRxMSUCount          Integer32,
        ptMTPATMLinkTxMSUCount          Integer32,
        ptMTPATMLinkMTP3NAManagerIndex  Integer32,
        ptMTPATMLinkMTP3NAIndex         Integer32,
        ptMTPATMLinkLinksetsIndex       Integer32,
        ptMTPATMLinkLinksetIndex        Integer32,
        ptMTPATMLinkActions             INTEGER,
        ptMTPATMLinkParentOID           OBJECT IDENTIFIER,
        ptMTPATMLinkRowStatus           RowStatus,
        ptMTPATMLinkIndex               Integer32
    }

ptMTPATMLinkContext OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "A condensed textual representation of the link object."
    ::= { ptMTPATMLinkEntry 1 }

ptMTPATMLinkAPC OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Adjacent SS7 point code of this link."
    ::= { ptMTPATMLinkEntry 2 }

ptMTPATMLinkSLC OBJECT-TYPE
    SYNTAX          Integer32 (0..15)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Signaling Link Code for this link. Value of 0-15"
    ::= { ptMTPATMLinkEntry 3 }

ptMTPATMLinkDREID OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "DRE to own this link."
    ::= { ptMTPATMLinkEntry 4 }

ptMTPATMLinkCage OBJECT-TYPE
    SYNTAX          Integer32 (-1..64)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Cage used by this link"
    ::= { ptMTPATMLinkEntry 5 }

ptMTPATMLinkChannel OBJECT-TYPE
    SYNTAX          Integer32 (-1 | 1..8)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Channel used by this link, valid values are 1 to 8"
    ::= { ptMTPATMLinkEntry 7 }

ptMTPATMLinkL2State OBJECT-TYPE
    SYNTAX          INTEGER { deactivated(0), proving(1), inService(2), 
                        outOfService(3), failed(4), localProcOutage(5), 
                        remProcOutage(6), boProcOutage(7), 
                        localLoopback(8), notEnabled(9), 
                        notInitialized(10), userShutdown(11) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of MTP Level 2 for this link. Deactivated, Proving, In-Service, Out of Service, Failed, Local Processor Outage, Remote Processor Outage, Both Processor Outage, Local Loopback mode, Not Enabled, Not Initialized"
    ::= { ptMTPATMLinkEntry 8 }

ptMTPATMLinkL3State OBJECT-TYPE
    SYNTAX          INTEGER { deactivated(0), proving(1), inService(2), 
                        outOfService(3), locallyInhibited(4), 
                        remInhibited(5), bothInhibited(6), blocked(7), 
                        blockedInhibited(8), localProcOutage(9) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Status of MTP Level 3 for this link. Deactivated, Proving, In-Service, Out of Service, Locally Inhibited, Remotely Inhibited, Both Inhibited, Blocked, Blocked and Uninhibited, Local Processor Outage"
    ::= { ptMTPATMLinkEntry 9 }

ptMTPATMLinkCongestionLevel OBJECT-TYPE
    SYNTAX          INTEGER { false(0), level1(1), level2(2), level3(3), 
                        true(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Congestion level of this link. \n For ANSI networks the values range from 1 to 3 or FALSE if not congested. \n For ITU networks the value is either TRUE or FALSE."
    ::= { ptMTPATMLinkEntry 10 }

