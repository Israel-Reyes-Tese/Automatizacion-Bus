-- Node definitions
--
		-- 1.3.6.1.4.1.193.81.4.1.1.1
		xfBridgeObjects OBJECT IDENTIFIER ::= { xfBridgeMib 1 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.1
		xfBridgeHDLCTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfBridgeHDLCEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table holds interface specific information for the HDLC 
				interface. I.e. the table is indexed by the ifIndex of the 
				HDLC interface."
			::= { xfBridgeObjects 1 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.1.1
		xfBridgeHDLCEntry OBJECT-TYPE
			SYNTAX XfBridgeHDLCEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table."
			INDEX { ifIndex }
			::= { xfBridgeHDLCTable 1 }

		
		XfBridgeHDLCEntry ::=
			SEQUENCE { 
				xfBridgeHDLCStatus
					BITS,
				xfBridgeHDLCIMFarEndMAC
					MacAddress
			 }

		-- 1.3.6.1.4.1.193.81.4.1.1.1.1.1.1
		xfBridgeHDLCStatus OBJECT-TYPE
			SYNTAX BITS
				{
				hdlcDegradingService0(0),
				hdlcDegradingService1(1),
				hdlcDegradingService2(2),
				hdclNoTrafficPossible0(3),
				hdclNoTrafficPossible1(4),
				hdclNoTrafficPossible2(5)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the status of the HDLC interface.
				Severity is indicated by three bits per alarm:
				bits 012
				     000   clear
				     001   undetermined
				     010   warning
				     011   minor
				     100   major
				     101   critical"
			::= { xfBridgeHDLCEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.1.1.2
		xfBridgeHDLCIMFarEndMAC OBJECT-TYPE
			SYNTAX MacAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the far end MAC address."
			::= { xfBridgeHDLCEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.2
		xfBridgeIMTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfBridgeIMEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table holds interface specific information for the IM 
				interface. I.e. the table is indexed by the ifIndex of the 
				IM interface."
			::= { xfBridgeObjects 2 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.2.1
		xfBridgeIMEntry OBJECT-TYPE
			SYNTAX XfBridgeIMEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table."
			INDEX { ifIndex }
			::= { xfBridgeIMTable 1 }

		
		XfBridgeIMEntry ::=
			SEQUENCE { 
				xfBridgeIMNearStatus
					BITS,
				xfBridgeIMFarStatus
					BITS
			 }

		-- 1.3.6.1.4.1.193.81.4.1.1.1.2.1.1
		xfBridgeIMNearStatus OBJECT-TYPE
			SYNTAX BITS
				{
				noMultiFrame(0),
				wrongMacAddress(1),
				unsupportedMuxScheme(2),
				linkDegraded(3),
				delayTooHigh(4),
				delayTooLow(5),
				unsupportedProtocol(6),
				noControlFrameReceived(7),
				duplicatedLink(8),
				remoteAccDataNotReceived(9),
				rxDataNotReceived(10)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This objects indicates the status of the IM near-end interface."
			::= { xfBridgeIMEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.2.1.2
		xfBridgeIMFarStatus OBJECT-TYPE
			SYNTAX BITS
				{
				noMultiFrame(0),
				wrongMacAddress(1),
				unsupportedMuxScheme(2),
				linkDegraded(3),
				delayTooHigh(4),
				delayTooLow(5),
				unSupportedProtocol(6),
				noControlFrameReceived(7)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This objects indicates the status of the IM far-end interface."
			::= { xfBridgeIMEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.3
		xfBridgeConnectionTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfBridgeConnectionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains all the connections between Ethernet interface(s)
				and the PDH interfaces."
			::= { xfBridgeObjects 3 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.3.1
		xfBridgeConnectionEntry OBJECT-TYPE
			SYNTAX XfBridgeConnectionEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table. The index ifIndex points to the 
				Ethernet interface."
			INDEX { ifIndex, xfBridgeConnectionServerInterface }
			::= { xfBridgeConnectionTable 1 }

		
		XfBridgeConnectionEntry ::=
			SEQUENCE { 
				xfBridgeConnectionServerInterface
					InterfaceIndex,
				xfBridgeConnectionRowStatus
					RowStatus
			 }


		-- 1.3.6.1.4.1.193.81.4.1.1.1.3.1.1
		xfBridgeConnectionServerInterface OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates the interface index of the serving PDH
				interface."
			::= { xfBridgeConnectionEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.3.1.2
		xfBridgeConnectionRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Standard row status used for setting-up and releasing 
				bridge connections bewteen Ethernet and the serving PDH interfaces.
				active(1)      activates the connection. This will only be possible
				               for a valid Ethernet and server ifIndex.A valid server
				               interface is free and available to the TDM bus.
				               Upon activation the corresponding IM interface will 
				               be created in the ifTable, ifStackTable etc. If this 
				               is the first connection for the Ethernet interface 
				               then a HDLC interface will be created in the ifTable, 
				               ifStackTable etc. The server interface will dissappear
				               from the tables in the tables in the XF-TRAFFIC-ROUTING-MIB.
				
				notInService(2)after a succesfull createAndWait the row will have 
				               this status. A row cannot be set into 'notInService' 
				               (inconsistent value) it has to be destroyed
				               and created again. 
				
				notReady(3)    A row will get this status after 'createAndWait' 
				               and an unsuccesfull 'active' and not all 
				               objects are properly set.
				
				createAndGo(4) used to set-up a connection and 
				               activate it. Used in multiple variable 
				               bindings since all objects in the row
				               must have a valid value before the row 
				               is activated.
				
				createAndWait(5) the row for the connection is 
				                 created but not actived. Used when 
				                 multiple variable binding are not 
				                 available.A row will be created with 
				                 default values.
				
				destroy(6)    removes a connection. This will also remove
				              the corresponding IM interface and if it is the 
				              last connection the HDLC interface."
			::= { xfBridgeConnectionEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4
		xfBridgeIMGroupTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfBridgeIMGroupEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table holds interface specific information for the IM group 
				interface. I.e. the table is indexed by the ifIndex of the 
				IM group interface."
			::= { xfBridgeObjects 4 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1
		xfBridgeIMGroupEntry OBJECT-TYPE
			SYNTAX XfBridgeIMGroupEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry in the table"
			INDEX { ifIndex }
			::= { xfBridgeIMGroupTable 1 }

		
		XfBridgeIMGroupEntry ::=
			SEQUENCE { 
				xfBridgeIMGroupStatus
					BITS,
				xfBridgeIMGroupFarEndMAC
					MacAddress,
				xfBridgeIMGroupAlarmEnable
					BITS,
				xfBridgeIMGroupIfInErrorFilterTime
					Integer32,
				xfBridgeIMGroupIfInErrorThresholdHigh
					Integer32,
				xfBridgeIMGroupIfInErrorThresholdLow
					Integer32,
				xfBridgeIMGroupFreeIMLinks
					Integer32,
				xfBridgeIMGroupUsage
					INTEGER,
				xfBridgeIMGroupMaxSpeed
					Gauge32,
				xfBridgeIMGroupSpeedLimit100Enable
					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.1
		xfBridgeIMGroupStatus OBJECT-TYPE
			SYNTAX BITS
				{
				imgDegradingService0(0),
				imgDegradingService1(1),
				imgDegradingService2(2),
				imgNoTrafficPossible0(3),
				imgNoTrafficPossible1(4),
				imgNoTrafficPossible2(5),
				imgQualityOfService0(6),
				imgQualityOfService1(7),
				imgQualityOfService2(8),
				imgKeepAlive0(9),
				imgKeepAlive1(10),
				imgKeepAlive2(11)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the status of the IM Group interface.
				Severity is indicated by three bits per alarm:
				bits 012
				     000   clear
				     001   undetermined
				     010   warning
				     011   minor
				     100   major
				     101   critical"
			::= { xfBridgeIMGroupEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.2
		xfBridgeIMGroupFarEndMAC OBJECT-TYPE
			SYNTAX MacAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the far end MAC address."
			::= { xfBridgeIMGroupEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.3
		xfBridgeIMGroupAlarmEnable OBJECT-TYPE
			SYNTAX BITS
				{
				imGroupDown(0),
				imGroupDegradedService(1),
				imGroupThreshold(2),
				imGroupKeepAlive(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This object indicates the trap setting for the IM Group interface.
				Traps are defined for IM group down, IM group degraded service, 
				IM group threshold and IM group keep alive "
			::= { xfBridgeIMGroupEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.4
		xfBridgeIMGroupIfInErrorFilterTime OBJECT-TYPE
			SYNTAX Integer32 (3..900)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used to specify the filter time. Defines the period the quality of service is measured 
				on the IM Group"
			DEFVAL { 10 }
			::= { xfBridgeIMGroupEntry 4 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.5
		xfBridgeIMGroupIfInErrorThresholdHigh OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used to specify the threshold that indicates bad quality of service on the IM Group"
			::= { xfBridgeIMGroupEntry 5 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.6
		xfBridgeIMGroupIfInErrorThresholdLow OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used to specify the threshold that indicates acceptable quality of service on the IM Group"
			::= { xfBridgeIMGroupEntry 6 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.7
		xfBridgeIMGroupFreeIMLinks OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of free IM links for this IM group."
			::= { xfBridgeIMGroupEntry 7 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.8
		xfBridgeIMGroupUsage OBJECT-TYPE
			SYNTAX INTEGER
				{
				unused(1),
				wan(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Specifies what the IM Group is used for.
				
				unused(1)         : not in use.
				wan(2)            : used as a WAN interface (create an ethernet interface
				                   over the IM group."
			::= { xfBridgeIMGroupEntry 8 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.9
		xfBridgeIMGroupMaxSpeed OBJECT-TYPE
			SYNTAX Gauge32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Specifies the maximum configured speed of the IM group. This 
				speed is derived from the number of IM links that are contained in
				 the group, regardless IM link the status.
				
				ifSpeed shows the current actual speed whereas this object
				shows the maximum speed that is configured (i.e. the
				number of IM links).
				"
			::= { xfBridgeIMGroupEntry 9 }

		
		-- 1.3.6.1.4.1.193.81.4.1.1.1.4.1.10
		xfBridgeIMGroupSpeedLimit100Enable OBJECT-TYPE
			SYNTAX INTEGER
				{
				enabled(1),
				disabled(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Used to specify if the IM group speed shall be limited to maximum 100 Mbits.
				This attribute should be set if the IM group in the ETU board is interfacing ETU2.  
				"
			DEFVAL { 2 }
			::= { xfBridgeIMGroupEntry 10 }

		
