    OBJECTS         { sonusEventDescription, sonusEventClass, 
                      sonusEventLevel, sonusEventTime }
    STATUS          current
    DESCRIPTION     
            "DNS Server is Black listed"
    ::= { sonusSoftSwitchLwresdMIBNotificationsPrefix 1 }

