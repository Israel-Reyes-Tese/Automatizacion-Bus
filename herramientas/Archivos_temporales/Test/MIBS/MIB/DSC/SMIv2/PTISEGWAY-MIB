PtsnamiSNAMIConfigurationEntry ::=
    SEQUENCE {
        ptsnamiSNAMIConfigurationSNAMILicense     INTEGER,
        ptsnamiSNAMIConfigurationSNAMIStatus    INTEGER,
        ptsnamiSNAMIConfigurationSR5129ThresholdTimeout               Integer32,
        ptsnamiSNAMIConfigurationSR5129LocalNodeName            DisplayString,
        ptsnamiSNAMIConfigurationErrors INTEGER,
        ptsnamiSNAMIConfigurationSetupManagement        INTEGER,
        ptsnamiSNAMIConfigurationExecutionTrace       INTEGER,
        ptsnamiSNAMIConfigurationAuditLog INTEGER,
        ptsnamiSNAMIConfigurationActions INTEGER,
        ptsnamiSNAMIConfigurationParentOID  OBJECT IDENTIFIER,
        ptsnamiSNAMIConfigurationRowStatus  RowStatus,
        ptsnamiSNAMIConfigurationIndex  Integer32
    }

ptsnamiSNAMIConfigurationSNAMILicense OBJECT-TYPE
    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "SNAMI feature license (UNLICENSED/LICENSED). \nIf the SNAMI is unlicensed, the SNAMI traffic cannot be activated."
    ::= { ptsnamiSNAMIConfigurationEntry 1 }

ptsnamiSNAMIConfigurationSNAMIStatus OBJECT-TYPE
    SYNTAX          INTEGER { inactive(0), notReady(1), ready(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "SNAMI status (INACTIVE/NOT READY/READY). \nThe SNAMI service is READY when SR-5129 connections can be made or a command is being processed."
    ::= { ptsnamiSNAMIConfigurationEntry 2 }

ptsnamiSNAMIConfigurationSR5129ThresholdTimeout OBJECT-TYPE
    SYNTAX          Integer32 (0..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "SR-5129 Threshold Timeout (in seconds). \nIf no message is processed on the connection during this interval, the connection will be dropped. \nSet to 0 to disable the timeout. \nDefault value: 30 seconds. \nValid values: 0 to +."
    ::= { ptsnamiSNAMIConfigurationEntry 4 }

ptsnamiSNAMIConfigurationSR5129LocalNodeName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "SR-5129 Local Node Name. \nSNAMI server will include this node name the SR-5129 packets. Incoming packets in the Destination Node Name field should have the same node name, or be empty. \nValid values: alphanumeric (fixed size of 17)."
    ::= { ptsnamiSNAMIConfigurationEntry 5 }

ptsnamiSNAMIConfigurationErrors OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable or disable the logging of SNAMI error messages to the debuglog (NOT LOGGED/LOGGED). \nNote: GR-310 command failures due to a precondition check are not logged to the debuglog. See the GR-310 audit log for those related issues. \nThe logging of SNAMI errors should be set to LOGGED for all production systems. \nDefault value: LOGGED."
    ::= { ptsnamiSNAMIConfigurationEntry 7 }

ptsnamiSNAMIConfigurationSetupManagement OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable or disable the logging of startup and setup information to the debuglog (NOT LOGGED/LOGGED). \nThese logs are not related to traffic processing and have no impact on the performance of a running system. \nThe logging of SNAMI startup and setup information should be set to LOGGED for most production systems. \nDefault value: LOGGED."
    ::= { ptsnamiSNAMIConfigurationEntry 8 }

ptsnamiSNAMIConfigurationExecutionTrace OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable or disable the logging of the execution trace to the debuglog (NOT LOGGED/LOGGED). \nThese logs are related to traffic and GR310 server processing and have impact on the performance of a running system. \nThe logging of SNAMI execution trace should be set to NOT LOGGED for most production systems. \nDefault value: NOT LOGGED."
    ::= { ptsnamiSNAMIConfigurationEntry 9 }

ptsnamiSNAMIConfigurationAuditLog OBJECT-TYPE
    SYNTAX          INTEGER { notLogged(0), logged(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Enable or disable the logging of the GR-310 command processing to the GR-310 audit log (NOT LOGGED/LOGGED). \nThe logging of audit log is recommended to be set to LOGGED on all production systems for provisioning accountability. \nDefault value: NOT LOGGED."
    ::= { ptsnamiSNAMIConfigurationEntry 10 }

ptsnamiSNAMIConfigurationActions OBJECT-TYPE
    SYNTAX          INTEGER { activate(1), deactivate(2) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "The OID of all possible actions for this instance"
    ::= { ptsnamiSNAMIConfigurationEntry 252 }

