    INDEX           { rsiSubsystemID, rsiThresholdID }
    ::= { rsiThresholdCounterTable 1 }

RsiThresholdCounterEntry ::=
    SEQUENCE {
        rsiThresholdID                  DisplayString,
        rsiThresholdOid                 DisplayString,
        rsiThresholdType                INTEGER,
        rsiThresholdMode                INTEGER,
        rsiThresholdLeakValue           INTEGER,
        rsiThresholdLowValue            INTEGER,
        rsiThresholdValue               INTEGER,
        rsiThresholdAssociatedId        DisplayString,
        rsiThresholdAction              INTEGER,
        rsiThresholdTimeDifference      INTEGER,
        rsiThresholdScanInterval        INTEGER,
        rsiThresholdModeInterval        INTEGER,
        rsiThresholdEntityType          DisplayString,
        rsiThresholdEntityId            DisplayString,
        rsiThresholdDeferredProcessing  INTEGER,
        rsiThresholdEnable              INTEGER
    }

rsiThresholdID OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is threshold id"
    ::= { rsiThresholdCounterEntry 1 }

rsiThresholdOid OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This is threshold oid"
    ::= { rsiThresholdCounterEntry 2 }

rsiThresholdType OBJECT-TYPE
    SYNTAX          INTEGER { absolute(0), timediff(1), percent(2), 
                        timediffpercent(3) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This identifies the type of a particular threshold.The 
             possible values are absolute, timediff, percent and timediffpercent"
    ::= { rsiThresholdCounterEntry 3 }

rsiThresholdMode OBJECT-TYPE
    SYNTAX          INTEGER { reset(0), leak(1), none(2) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This identifies the threshold mode .The 
             possible values are reset, leak and none."
    ::= { rsiThresholdCounterEntry 4 }

rsiThresholdLeakValue OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This identifies the threshold leak value.It will always be greater than zero."
    ::= { rsiThresholdCounterEntry 5 }

rsiThresholdLowValue OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This identifies the threshold's low value.It will always less than 
             rsiThresholdValue.If it is equal to rsiThresholdValue and rsiThresholdDeferredProcessing 
             is true,then alarms can be sent at regular intervals"
    ::= { rsiThresholdCounterEntry 6 }

rsiThresholdValue OBJECT-TYPE
    SYNTAX          INTEGER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "This identifies the threshold's value.It will always be greater than zero."
    ::= { rsiThresholdCounterEntry 7 }

rsiThresholdAssociatedId OBJECT-TYPE
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "This identifies the threshold's associated id.This is used for 'percent'
             and 'timediffpercent' resiThresholdType only."
    ::= { rsiThresholdCounterEntry 8 }

rsiThresholdAction OBJECT-TYPE
    SYNTAX          INTEGER { alarmAndLog(0), logOnly(1) }
    ACCESS          read-write
    STATUS          mandatory
