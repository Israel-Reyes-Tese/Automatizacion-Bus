        ptGTTPCRegistrationParentOID    OBJECT IDENTIFIER,
        ptGTTPCRegistrationRowStatus    RowStatus,
        ptGTTPCRegistrationIndex        INTEGER
    }

ptGTTPCRegistrationPointCode OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Point code used to register GTT with SCCP."
    ::= { ptGTTPCRegistrationEntry 1 }

ptGTTPCRegistrationStatus OBJECT-TYPE
    SYNTAX          INTEGER { notRegistered(0), partiallyRegistered(1), 
                        registered(2), blocked(3) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "PC Registration status (NOT REGISTERED, PARTIALLY REGISTERED, REGISTERED, BLOCKED). \nOn a properly provisioned system this attribute should read 'REGISTERED.' IF there are no GTT processes on any CPUs connected to any SCCP, then this attribute will read 'NOT REGISTERED.' If there are some connections, but not on all CPUs then the attribute will display 'PARTIALLY REGISTERED.' If 'LPC Registration' is disabled on the GTT NA Configuration, then the attribute will display 'BLOCKED'. \nIf the status is anything but REGISTERED, check the SYSLOG to determine the cause of the registration problems. A likely cause of registration problems is attempting to have the GTT process register with a Virtual Point Code, that has not been created as a VNODE at the MTP level."
    ::= { ptGTTPCRegistrationEntry 2 }

ptGTTPCRegistrationDBDefName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "GTT Database Definition Name. \nThis attribute specifies which translation database to use for incoming messages from this PC registration. \nBy default, the string is unset (empty string) which translates to using the parent NA value. \nIf the PC Registration Point Code attribute is set to the LPC of the NA, the DB Def Name cannot be modified and remains unset (empty string) to always apply the corresponding NA DB Def."
    ::= { ptGTTPCRegistrationEntry 3 }

ptGTTPCRegistrationGTTNAManagerIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is GTTNAManager."
    ::= { ptGTTPCRegistrationEntry 242 }

ptGTTPCRegistrationGTTNAIndex OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          not-accessible
    STATUS          mandatory
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is GTTNA."
    ::= { ptGTTPCRegistrationEntry 243 }

ptGTTPCRegistrationParentOID OBJECT-TYPE
    SYNTAX          OBJECT IDENTIFIER
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The OID of the parent object for this instance"
    ::= { ptGTTPCRegistrationEntry 253 }

ptGTTPCRegistrationRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    ACCESS          read-write
    STATUS          mandatory
    DESCRIPTION     
            "The row status for the GTTPCRegistration table"
    ::= { ptGTTPCRegistrationEntry 254 }

