        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.5
        xfEthernetEplControlProtocolTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfEthernetEplControlProtocolEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table is used to configure the Layer 2 Control Protocol disposition.
                
                If this table is not available then xfLayer2ControlProtocolTable is applied for Layer1 EPL ports."
            ::= { xfBridgeConnection 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.5.1
        xfEthernetEplControlProtocolEntry OBJECT-TYPE
            SYNTAX XfEthernetEplControlProtocolEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Description."
            INDEX { xfEthernetEplInterface1, xfEthernetEplInterface2, xfEthernetEplControlProtocol }
            ::= { xfEthernetEplControlProtocolTable 1 }

        XfEthernetEplControlProtocolEntry ::=
            SEQUENCE {
                xfEthernetEplControlProtocol
                    INTEGER,
                xfEthernetEplControlProtocolCapability
                    BITS,
                xfEthernetEplControlProtocolDisposition
                    INTEGER
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.5.1.1
        xfEthernetEplControlProtocol OBJECT-TYPE
            SYNTAX INTEGER
                {
                bpdu(1),
                macPause(2),
                lacp(3),
                lamp(4),
                linkOam(5),
                futureStdSlow(6),
                portAuth802Dot1X(7),
                futureStdBridge(8),
                allBridgeAddr(9),
                gmrp(10),
                gvrp(11),
                futureStdGarp(12),
                providerBpdu(13),
                providerGvrp(14),
                lldp(15),
                esmc(16)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Specifies the Layer2 Control Protocol:
                
                Bpdu(1): Customer Bridge Protocol Data Unit (BPDU) (MAC address 01-80-C2-00-00-00)
                MacPause(2): MAC Pause (MAC address 01-80-C2-00-00-01)
                Lacp(3): Link Aggregation Control Protocol (LACP) (MAC address 01-80-C2-00-00-02 subtype 0x01)
                Lamp(4): Link Aggregation Marker Protocol (LAMP) (MAC address 01-80-C2-00-00-02 subtype 0x02)
                LinkOam(5): Link OAM (MAC address 01-80-C2-00-00-02 subtype 0x03)
                Esmc(16): Ethernet Synchronization Messaging Channel (ESMC) (MAC address 01-80-C2-00-00-02 subtype 0x0A)
                FutureStdSlow(6): Reserved for future standard slow protocols (MAC address 01-80-C2-00-00-02 subtype 0x04 - 0x09)
                PortAuth_802_1X(7): 802.1X Port Authentication (MAC address 01-80-C2-00-00-03)
                FutureStdBridge(8): Reserved for future standard bridge protocols (MAC address 01-80-C2-00-00-04, 01-80-C2-00-00-05, 01-80-C2-00-00-06, 01-80-C2-00-00-07, 01-80-C2-00-00-09, 01-80-C2-00-00-0A, 01-80-C2-00-00-0B, 01-80-C2-00-00-0C)
                AllBridgeAddr(9): All Bridges address (MAC address 01-80-C2-00-00-10)
                Gmrp(10): GMRP (MAC address 01-80-C2-00-00-20)
                Gvrp(11): GVRP (MAC address 01-80-C2-00-00-21)
                FutureStdGarp(12): Reserved for future standard GARP protocols (MAC address 01-80-C2-00-00-22..01-80-C2-00-00-2F)
                ProviderBpdu(13): Provider Bridge Protocol Data Unit (BPDU) (MAC address 01-80-C2-00-00-08)
                ProviderGvrp(14): Provider GVRP (MAC address 01-80-C2-00-00-0D)
                Lldp(15): Link Layer Discovery Protocol (MAC address 01-80-C2-00-00-0E)
                Esmc(16): Ethernet Synchronization Messaging Channel (ESMC) (MAC address 01-80-C2-00-00-02 subtype 0x0A)"
            ::= { xfEthernetEplControlProtocolEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.5.1.2
        xfEthernetEplControlProtocolCapability OBJECT-TYPE
            SYNTAX BITS
                {
                peer(0),
                forward(1),
                peerForward(2)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "This bitmask indicates the allowed dispositions for the Layer 2 Control Protocol.
                
                000: only discard is supported
                001: discard and peer are supported
                010: discard and forward unchanged are supported
                011: discard, peer and forward unchanged are supported
                100..111: reserved for future use"
            ::= { xfEthernetEplControlProtocolEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.4.5.1.3
        xfEthernetEplControlProtocolDisposition OBJECT-TYPE
            SYNTAX INTEGER
                {
                discard(1),
                peer(2),
                forward(3)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Indicates the actual disposition for the Layer 2 Control Protocol.
                
                The disposition in the xfEthernetEplControlProtocolTable shall override 
                the disposition required by any protocol software running in the node 
                (for example, if the disposition for the LinkOAM protocol is discard, 
                LinkOAM PDUs shall be discarded even though LinkOAM is enabled
                on the port).
                
                For cases in which more than one protocol uses the same MAC DA 
                (e.g. LACP and LinkOAM), the required action related to forwarding is 
                the same: if the disposition for one of these protocols is set to forward, 
                the disposition for all other protocols with the same MAC DA is also 
                set to forward.
                
                It shall not be possible to set the disposition to forward if the protocol 
                software is running in the node.
                It shall not be possible to enable the protocol software in the node if 
                the disposition is set to forward.
                "
            ::= { xfEthernetEplControlProtocolEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5
        xfBridgeProtection OBJECT IDENTIFIER ::= { xfBridgeGeneral 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1
        xfBridgeProtectionTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfBridgeProtectionEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This protection table is a based on the equipment protection MIB in TN, 
                but with some adjustments to cater for the specific needs of bridge 
                redundancy."
            ::= { xfBridgeProtection 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1
        xfBridgeProtectionEntry OBJECT-TYPE
            SYNTAX XfBridgeProtectionEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of bridge protection."
            INDEX { entPhysicalIndex }
            ::= { xfBridgeProtectionTable 1 }

        XfBridgeProtectionEntry ::=
            SEQUENCE {
                xfBridgeProtectionUnit2
                    PhysicalIndex,
                xfBridgeProtectionEnable
                    INTEGER,
                xfBridgeProtectionMode
                    INTEGER,
                xfBridgeProtectionActiveUnit
                    PhysicalIndex,
                xfBridgeProtectionRestore
                    INTEGER,
                xfBridgeProtectionWaitToRestoreTime
                    Integer32,
                xfBridgeProtectionNumberOfProtectionSwitchesPerformed
                    Integer32,
                xfBridgeProtectionLastTimeSwitchPerformed
                    TimeTicks,
                xfBridgeProtectionLastSwitchCause
                    INTEGER,
                xfBridgeProtectionLastStatisticsReset
                    TimeTicks,
                xfBridgeProtectionPacketLossHighThreshold
                    Integer32,
                xfBridgeProtectionPacketLossLowThreshold
                    Integer32
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.1
        xfBridgeProtectionUnit2 OBJECT-TYPE
            SYNTAX PhysicalIndex
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Indicates the NPU in secondary slot"
            ::= { xfBridgeProtectionEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.2
        xfBridgeProtectionEnable OBJECT-TYPE
            SYNTAX INTEGER
                {
                disable(1),
                enable(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This parameter sets the possibility to select the active switch. Note, that setting
                 this parameter might have traffic impact.

                      disable(1): The active switch is on the NPU in the primary slot.

                      enable(2):  The active switch can change depending on the xfBridgeProtectionMode,
                      the xfBridgeProtectionActiveUnit and the xfBridgeProtectionRestore parameters.

                  NB: The definition of the protected state is that
                  - Both NPUs are present in the TN,
                  - no faults on the NPUs,
                  - bridge protection is enabled,
                  - the active switch is on the NPU in the secondary slot,
                  - protection mode is automatic.

                  The revertive switch is that the system returns to the protected state after the
                  recovery of the NPU in the secondary slot.

                  Default is disable(1)."
            DEFVAL { disable }
            ::= { xfBridgeProtectionEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.3
        xfBridgeProtectionMode OBJECT-TYPE
            SYNTAX INTEGER
                {
                automatic(1),
                manual(2),
                forced(3)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to select the protection mode.
    
                       automatic(1):    In protected state, the switching functionality is protected
                                        from at most one of the following failures
                                        - BR button pressed on one of the NPUs
                                        - power failure on one of the NPUs
                                        - hardware or software failure on one of the NPUs

                        manual(2)   :   The switching functionality is not protected. Operator selects
                                        active switch with xfBridgeProtectionActiveUnit. Note that switch
                                        is not performed if the desired active unit has fault.

                        forced(3)   :    The switching functionality is not protected. Operator selects
                                         active switch with xfBridgeProtectionActiveUnit. Note that switch
                                         is performed in any case.

    
                  Default value is automatic(1).

                  NB: If xfBridgeProtectionMode is changed to manual or forced, xfBridgeProtectionRestore
                  is also changed to manual."
            DEFVAL { automatic }
            ::= { xfBridgeProtectionEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.4
        xfBridgeProtectionActiveUnit OBJECT-TYPE
            SYNTAX PhysicalIndex
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Indicates the current active switch.
    
                 This parameter can be set (allowed) and cannot be set (x) in the following scenarios.
                 N/A means that this scenario does not exist. Note that, xfBridgeProtectionEnable must
                 be enabled.

                 
                                      | The NPU in secondary |  The NPU in primary
                                      | slot has the         |  slot has the
                                      | active switch        |  active switch
                                      |                      |
                           Protection | auto       manual    |  auto      manual
                           mode       |                      |
                  --------------------+----------------------+-----------------------
                  Restore  auto       |  x          N/A      |   x         N/A
                  mode                |                      |
                                      |                      |
                           manual     |  x          allowed  |  allowed   allowed

                "
            ::= { xfBridgeProtectionEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.5
        xfBridgeProtectionRestore OBJECT-TYPE
            SYNTAX INTEGER
                {
                automatic(1),
                manual(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Indicates whether a revertive switch must automatically be made.
    
                      automatic (1)  : After a recovery from failure of NPU in secondary slot, the 
                                           protected setup is automatically restored.

                      manual (2)     : After a recovery from failure of NPU in secondary slot, the
                                           protected setup is not automatically restored.
        
                 NB: xfBridgeProtectionRestore cannot be automatic if xfBridgeProtectionMode is manual or
                 forced.

                 Default is automatic(1)"
            DEFVAL { automatic }
            ::= { xfBridgeProtectionEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.6
        xfBridgeProtectionWaitToRestoreTime OBJECT-TYPE
            SYNTAX Integer32 (0..86400)
            UNITS "1 second"
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Time to wait with revertive switch in case revertive switch is automatic.
                
                The unit is second. It can be set with 1 second granularity.
                
                Default is 0.
                "
            DEFVAL { 0 }
            ::= { xfBridgeProtectionEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.7
        xfBridgeProtectionNumberOfProtectionSwitchesPerformed OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Gives statistics on how often a protective switch has been executed:
                 Number of times an automatic switch was performed due to a failure since 
                 xfBridgeProtectionLastStatisticsReset."
            ::= { xfBridgeProtectionEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.8
        xfBridgeProtectionLastTimeSwitchPerformed OBJECT-TYPE
            SYNTAX TimeTicks
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Indicates the last time (the value of sysUpTime) when automatic switch
                 was performed due to a failure."
            ::= { xfBridgeProtectionEntry 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.9
        xfBridgeProtectionLastSwitchCause OBJECT-TYPE
            SYNTAX INTEGER
                {
                removalOfNPUinSecondarySlot(1),
                faultOfNPUinSecondarySlot(2),
                activeBridgeFault(3),
                none(4)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Indicates the failure reason why an automatic switch was performed."
            ::= { xfBridgeProtectionEntry 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1.5.1.1.10
        xfBridgeProtectionLastStatisticsReset OBJECT-TYPE
            SYNTAX TimeTicks
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "This parameter either shows the last time the system started up or when bridge protection 
                was enabled last time. sysUpTime value is used."
            ::= { xfBridgeProtectionEntry 10 }

        xfBridgeProtectionPacketLossHighThreshold OBJECT-TYPE
            SYNTAX Integer32 (1..10000)
            UNITS "packet number"
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "An alarm is raised if the number of packets lost between the two NPUs in 1 seconds
                 reaches or exceeds this number.
                "
            DEFVAL { 1 }
            ::= { xfBridgeProtectionEntry 11 }

        xfBridgeProtectionPacketLossLowThreshold OBJECT-TYPE
            SYNTAX Integer32 (1..10000)
            UNITS "packet number"
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The alarm is cleared if the number of packets lost between the two NPUs in 1 seconds
                 drops below this number.
                "
            DEFVAL { 1 }
            ::= { xfBridgeProtectionEntry 12 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2
        xfBridgePort OBJECT IDENTIFIER ::= { xfEthernetBridgeObjects 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5
        xfBridgePortTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfBridgePortEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An extension to dot1dBasePortTable."
            ::= { xfBridgePort 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1
        xfBridgePortEntry OBJECT-TYPE
            SYNTAX XfBridgePortEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of a bridge port."
            INDEX { dot1dBasePort }
            ::= { xfBridgePortTable 1 }

        XfBridgePortEntry ::=
            SEQUENCE {
                xfBridgePortRole
                    INTEGER,
                xfBridgePortSrcMacWhiteList
                    Integer32,
                xfBridgePortDscpMap
                    Integer32,
                xfBridgePortTrusted
                    INTEGER,
                xfBridgePortMaxLearnedAddresses
                    Integer32,
                xfBridgePortLearnedAddresses
                    Counter32,
                xfBridgePortLearnedAddressesExceedDiscards
                    Counter32,
                xfBridgePortForbiddenEgressMask
                    PortList,
                xfBridgePortWhiteListDiscards
                    Counter32,
                xfBridgePortFrameTypeDiscards
                    Counter32,
                xfBridgePortAcceptableFrameTypes
                    BITS,
                xfBridgePortUsageCapabilities
                    BITS,
                xfBridgePortUsedBy
                    BITS,
                xfBridgePortMaxFrameSize
                    Integer32,
                xfBridgePortBandwidthProfileTarget
                    INTEGER,
                xfBridgePortDefaultCos
                    Integer32,
                xfBridgePortBandwidthProfile
                    Integer32,
                xfBridgePortPbCepPvid
                    VlanIndex,
                xfBridgePortRoleTest
                    INTEGER,
                xfBridgePortClearLearnedAddresses
                    Integer32,
                xfBridgePortTpid
                    INTEGER,
                xfBridgePortMacLearningEnable
                    TruthValue
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.1
        xfBridgePortRole OBJECT-TYPE
            SYNTAX INTEGER
                {
                undefined(0),
                uni(1),
                inni(2),
                mirror(3),
                cep(4),
                cnp(5),
                layer1(6)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Shall specify what the port is used for:
                
                undefined(0): the port is not assigned a role
                uni(1)      : the port is used as UNI in customer mode (xfBridgeMode = qBridge(2))
                inni(2)     : the port is used as I-NNI in customer mode and as Provider Network Port in provider mode
                mirror(3)   : the port is used as a mirror port for test purpose.
                cep(4)      : the port is used as Customer Edge Port (CEP) in provider mode (xfBridgeMode = adBridge(3))
                cnp(5)      : the port is used as Customer Network Port (CNP) in provider mode (xfBridgeMode = adBridge(3))
                layer1(6)   : the port is used for layer1 service. In this case Bridge mode is not relevant.
                
                If xfBridgeMode is qBridge(2):
                Default is undefined(0) if the bridge port is not connected to an interface.
                Default is uni(1) if the port is connected to a LAN interface.
                Default is inni(2) if the port is connected to a WAN interface.
                
                If xfBridgeMode is adBridge(3):
                Default is inni(2).
                
                If xfBridgeMode is qBridge(2), cep(4) and cnp(5) are not allowed.
                If xfBridgeMode is adBridge(3), uni(1) is not allowed."
            ::= { xfBridgePortEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.2
        xfBridgePortSrcMacWhiteList OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to set which MAC-address list, i.e. xfMacListIndex,
                to use as a white-list for this port. This applies
                only for an UNI port (i.e. xfBridgePortRole='uni').
                
                The value 0 indicates that there is no Mac-address 
                white-list access control for the port. 
                
                Default value is 0.
                
                For INNI, layer1 and mirror ports, this object is set to 0 and can not be modified."
            ::= { xfBridgePortEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.3
        xfBridgePortDscpMap OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS deprecated
            DESCRIPTION
                "Used to set which DSCP mapping set in the xfDscpMapTable to use.
                The DSCP mapping set is used only if 
                xfBridgePortTrusted is set to onlyDscpTrusted'.
                
                Default is 0 which means no DSCP mapping is used."
            ::= { xfBridgePortEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.4
        xfBridgePortTrusted OBJECT-TYPE
            SYNTAX INTEGER
                {
                untrusted(1),
                dscpIPv4Trusted(2),
                ctagPcpTrusted(3),
                dscpIPv6Trusted(4),
                expTrusted(5),
                stagPcpTrusted(6),
                dscpIPv4IPv6Trusted(7),
                expDscpTrusted(8)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies which priority field in the incoming packet on the UNI port is trusted.
                
                untrusted(1)           : No priority field on the received packet is trusted.
                                      Default priority is used.
                
                dscpIPv4Trusted(2)     : DSCP field on IPv4 is trusted. If the packet is not IPv4, 
                                      default priority is used.
                
                ctagPcpTrusted(3)      : PCP field on the C-tag is trusted. If the packet is untagged,
                                      default priority is used.
                
                dscpIPv6Trusted(4)     : DSCP field on IPv6 is trusted. If the packet is not IPv6, 
                                      default priority is used.
                
                expTrusted(5)          :  EXP field on MPLS is trusted. If the packet is not MPLS, 
                                       default priority is used.
                
                stagPcpTrusted(6)      : PCP field on the S-tag is trusted. If the packet is untagged,
                                       default priority is used.
                
                dscpIPv4IPv6Trusted(7) : DSCP field on either IPv4 or IPv6 is trusted. If the packet is
                                       neither IPv4 nor IPv6 then default priority is used.
                
                expDscpTrusted(8)      : EXP field on MPLS is trusted. If the packet is not MPLS then DSCP field on either IPv4 or IPv6 is trusted.
                                       If the packet is not MPLS, IPv4 nor IPv6 then default priority is used.
                
                default ctagPcpTrusted(3).
                
                ctagPcpTrusted(3) shall be allowed for UNI in customer mode and CEP in provider mode,
                and CNP in provider mode only if xfBridgePortAcceptableFrameTypes=110
                (110 : Admit Only Untagged and Priority Tagged Frames).
                
                stagPcpTrusted(6) shall be allowed for CNP in provider mode, and shall NOT be allowed 
                for UNI in customer mode and CEP in provider mode.
                
                For INNI and mirror ports, this object is set to ctagPcpTrusted(3) and can not be modified.
                
                This parameter is used also for layer1 EPL ports if xfEthernetEplServiceConfigTrusted is not available."
            ::= { xfBridgePortEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.5
        xfBridgePortMaxLearnedAddresses OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This defines the max number of MAC addresses to be 
                allowed learned on a port. 
                
                When the limit is reached packets will be discarded. 
                
                0 means that the function is turned off, i.e. limited by 
                bridge capacity. 
                
                For UNI (i.e. xfBridgePortRole = 'uni')
                 - Default is 20
                
                For I-NNI (i.e. xfBridgePortRole = 'inni') and MTP  (i.e. xfBridgePortRole = 'mirror'
                  - It is set to 0 and is read-only.
                
                Max value < 16 k addresses."
            ::= { xfBridgePortEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.6
        xfBridgePortLearnedAddresses OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The number of MAC SA in the learning database refering 
                to this port."
            ::= { xfBridgePortEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.7
        xfBridgePortLearnedAddressesExceedDiscards OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The number of packets droped due to violating the limiting of source MAC address per prot."
            ::= { xfBridgePortEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.8
        xfBridgePortForbiddenEgressMask OBJECT-TYPE
            SYNTAX PortList
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Traffic in on this port is not allowed to be sent out on the ports in the 
                ForbiddenEgressPorts.
                
                This is in addtiton to limitations given by the V-LAN.
                
                This applies only to UNI and INNI ports in customer mode (xfBridgeMode=qBridge).
                
                Default is all bits set to zero."
            ::= { xfBridgePortEntry 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.9
        xfBridgePortWhiteListDiscards OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Number of packets discarded due to mismatch with
                MAC white list entries."
            ::= { xfBridgePortEntry 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.10
        xfBridgePortFrameTypeDiscards OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Number of packets discarded due to frame type control."
            ::= { xfBridgePortEntry 10 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.11
        xfBridgePortAcceptableFrameTypes OBJECT-TYPE
            SYNTAX BITS
                {
                admitUntagged(0),
                admitPriorityTagged(1),
                admitVlanTagged(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies frame types to accept on an UNI (xfBridgePortRole =uni(1)) port 
                and on a CNP (xfBridgePortRole =cnp(5)) port.
                
                This object substitutes dot1qPortAcceptableFrameTypes. 
                dot1qPortAcceptableFrameTypes will be read-only object with value 
                admitAll(1).
                
                admitUntagged(0)            admit untagged frames.
                admitPriorityTagged(1)      admit priority tagged frames.
                admitVlanTagged(2)          admit vlan tagged frames.
                
                Allowed values are:
                     Bit     012
                            - 111 : Admit All Frames(admitUntagged | admitPriorityTagged | admitVlanTagged)
                            - 110 : Admit Only Untagged and Priority Tagged Frames (admitUntagged | admitPriorityTagged)
                            - 001 : Admit Only VLAN Tagged Frames (admitVlanTagged)
                
                Default value for UNI and CNP is 111.
                For CEP and Layer1 EPL ports this is set to 111 and can not be modified.
                For INNI and mirror ports, this is set to 001 (admitVlanTagged) and can not be modified."
            ::= { xfBridgePortEntry 11 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.12
        xfBridgePortUsageCapabilities OBJECT-TYPE
            SYNTAX BITS
                {
                layer1Connection(0),
                lagMembership(1),
                vlanMembership(2),
                ptpClockPortCap(3),
                hrlbMembership(4)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "A bitmap that shows what the bridge port can be used for.
                
                layer1Connection(0)         : The bridge port can be connected to a layer1 interface, e.g LAN.
                lagMembership(1)            : The bridge port can be a LAG member.
                vlanMembership(2)           : The bridge port can be a VLAN member other
                                              than the default vlan(1).
                ptpClockPortCap(3)          : The bridge port can be assigned to a PTP clock port.
                hrlbMembership(4)           : The bridge port can be assigned to a hRLB group.
                "
            ::= { xfBridgePortEntry 12 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.13
        xfBridgePortUsedBy OBJECT-TYPE
            SYNTAX BITS
                {
                layer1Connect(0),
                lagMemberMaster(1),
                lagMemberSlave(2),
                vlanMember(3),
                ptpClockPort(4),
                hrlbGroup(5),
                hrlbGroupMember(6)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "A bitmap that shows what the bridge port is used for.
                
                layer1Connect(0)               : The bridge port is connected to a layer1 interface, e.g LAN
                lagMemberMaster(1)             : The bridge port is a member of a LAG and is a master port
                lagMemberSlave(2)              : The bridge port is a member of a LAG and is a slave port
                vlanMember(3)                  : The bridge port is a member of a VLAN other than the default
                                                 vlan(1).
                ptpClockPort(4)                : The bridge port is used by a PTP clock port.
                hrlbGroup(5)                   : The bridge port is the group port of a hRLB group.
                hrlbGroupMember(6)             : The bridge port is member of a hRLB group.
                "
            ::= { xfBridgePortEntry 13 }


        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.14
        xfBridgePortMaxFrameSize OBJECT-TYPE
            SYNTAX Integer32 (1518..9216)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to limit the maximum frame size for this port. It can be set only 
                if the port is UNI (uni,cep,cnp - see xfBridgePortRole).
                
                Default value is 2000 for UNI ports, 9216 for other ports.
                "
            ::= { xfBridgePortEntry 14 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.15
        xfBridgePortBandwidthProfileTarget OBJECT-TYPE
            SYNTAX INTEGER
                {
                noPolice(1),
                policePort(2),
                policePortAndCos(3),
                policePortAndVlan(4)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies the level of policing. This applies only to customer-facing ports 
                (UNI, CEP, CNP). INNI (and other) ports will not have traffic policing.
                
                  noPolice(1)          : no traffic policing at this port.
                  policePort(2)        : police traffic per port.
                  policePortAndCos(3)  : police traffic per port and cos. This is 
                                         not possible if the bridge port is 'untrusted'
                  policePortAndVlan(4) : police traffic per port and VLAN.
                 
                Can not be set to policePortAndCos(3) if the port is untrusted (see xfBridgePortTrust).
                
                Default is noPolice(1).
                "
            ::= { xfBridgePortEntry 15 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.16
        xfBridgePortDefaultCos OBJECT-TYPE
            SYNTAX Integer32 (0..63)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies the default cos to use in case the incoming packet 
                lacks priority fields as specifed by xfBridgePortTrusted. This 
                is used when xfBridgePortBandwidthProfileTarget is 
                'policePortAndCos'. It applies only to customer-facing ports (UNI, 
                CEP, CNP). INNI ports will not police traffic.
                
                For UNI, CEP, and CNP with xfBridgePortAcceptableFrameTypes=110 (Admit Only Untagged and Priority Tagged Frames):
                Values: 0 - 63  if xfBridgePortTrusted='dscpIPv4Trusted'/'dscpIPv6Trusted'/'dscpIPv4IPv6Trusted'/'expDscpTrusted'
                            0 - 7    if xfBridgePortTrusted='expTrusted'
                            0        if xfBridgePortTrusted='ctagPcpTrusted' and can not be modified.
                xfBridgePortTrusted
                For CNP:
                Values: 0 - 63  if xfBridgePortTrusted='dscpIPv4Trusted'/'dscpIPv6Trusted'/'dscpIPv4IPv6Trusted'/'expDscpTrusted'
                            0 - 7    if xfBridgePortTrusted='expTrusted'
                            0        if xfBridgePortTrusted='stagPcpTrusted' and can not be modified.
                
                Default is 0.
                
                This parameter is used also for Layer1 EPL ports if xfEthernetEplServiceConfigDefaultPriority is not available."
            ::= { xfBridgePortEntry 16 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.17
        xfBridgePortBandwidthProfile OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Points to an entry in xfEthernetBandwidthProfileTable.
                This applies only if xfBridgePortBandwidthProfileTarget is 'policePort'. 
                It applies only customer-facing ports (UNI, CEP, CNP). INNI and Layer1 EPL ports will not
                have traffic policing.
                
                Default is 1 (No rate limiting/drop none profile)"
            ::= { xfBridgePortEntry 17 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.18
        xfBridgePortPbCepPvid OBJECT-TYPE
            SYNTAX VlanIndex
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The PVID of a CEP, i.e. the VLAN ID assigned to untagged frames or 
                Priority-Tagged frames received on this port.
                
                This parameter is valid only for CEP when the bridge is in provider mode.
                This parameter is Read-Write.
                
                If this parameter is changed and had been registered in xfPbCvidRegistrationTable, 
                the related entries in xfPbCvidRegistrationTable and xfPbProviderEdgePortTable 
                are automatically adjusted accordingly."
            ::= { xfBridgePortEntry 18 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.19
        xfBridgePortRoleTest OBJECT-TYPE
            SYNTAX INTEGER
                {
                undefined(0),
                uni(1),
                inni(2),
                mirror(3),
                cep(4),
                cnp(5),
                layer1(6)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object behaves as xfBridgePortRole but ONLY TEST PHASE 
                is executed when this object is set (i.e. xfBridgePortRole remains unchanged)."
            ::= { xfBridgePortEntry 19 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.20
        xfBridgePortClearLearnedAddresses OBJECT-TYPE
            SYNTAX Integer32 (0..4094)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object is used to clear all learned MAC Addresses on the switch port for the given VLAN.

                Setting the value to 0 will clear learned addresses for all VLAN where the switch port is a member.

                Setting the value to 1-4094 will clear the learned addresses on the VLAN ID matching the value.
                "
            ::= { xfBridgePortEntry 20 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.21
        xfBridgePortTpid OBJECT-TYPE
            SYNTAX INTEGER
                {
                default(0),
                tpid8100(1),
                tpid88a8(2)
                }

            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object is used to set the outer TPID on I-NNI ports to allow Q-in-Q frames into 802.1ad networks.
                 default(0)  : Restores the default TPID.
                 tpid8100(1) : The outer TPID for the port is 0x8100. Used on a 802.1ad node will replace the outer TPID
                               of the frames, allowing 802.1q and Q-in-Q frames, turning them into 802.1ad, but discarding
                               802.1ad frames.
                 tpid88a8(2) : The outer TPID for the port is 0x88a8. Used on a 802.1q node will replace the outer TPID of
                               the frames, allowing 802.1ad frames, turning them into Q-in-Q, but discarding 802.1q and Q-in-Q
                               frames."
            ::= { xfBridgePortEntry 21 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.5.1.22
        xfBridgePortMacLearningEnable OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This object is used to enable or disable the MAC learning functionality on a port.

                 By default its value is true(1). If you set it false(2) MAC learning on this port will be disabled.
                "
            ::= { xfBridgePortEntry 22 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.7
        xfBridgePortMirrorTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfBridgePortMirrorEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table of ports used as mirrors for test purpose
                (i.e. xfBridgePortRole = 'mirror')."
            ::= { xfBridgePort 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.7.1
        xfBridgePortMirrorEntry OBJECT-TYPE
            SYNTAX XfBridgePortMirrorEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of a port mirror."
            INDEX { xfBridgePortMirrorMTP }
            ::= { xfBridgePortMirrorTable 1 }

        XfBridgePortMirrorEntry ::=
            SEQUENCE {
                xfBridgePortMirrorMTP
                    INTEGER,
                xfBridgePortMirroredPort
                    INTEGER,
                xfBridgePortMirrorrEgressCvid
                    Integer32,
                xfBridgePortMirrorMode
                    INTEGER
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.7.1.1
        xfBridgePortMirrorMTP OBJECT-TYPE
            SYNTAX INTEGER (1..65535)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A bridge port index that identifies Mirror-To-Port."
            ::= { xfBridgePortMirrorEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.7.1.2
        xfBridgePortMirroredPort OBJECT-TYPE
            SYNTAX INTEGER (1..65535)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The port id (i.e.dot1dBasePort) of a port that is mirrored.
                
                Default is zero which means that no port is yet selected."
            ::= { xfBridgePortMirrorEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.7.1.3
        xfBridgePortMirrorrEgressCvid OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to set the C-VID tag used tag the mirrored egress traffic
                on the mirrored-to-port.
                
                Default is zero which means that CVID is not set yet."
            ::= { xfBridgePortMirrorEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.7.1.4
        xfBridgePortMirrorMode OBJECT-TYPE
            SYNTAX INTEGER
                {
                ingress(1),
                egress(2),
                ingressAndEgress(3)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Specifies what to mirror:
                
                ingress(1)          : mirror only ingress traffic.
                egress(2)           : mirror only egress traffic.
                ingressAndEgress(3) : mirror both ingress and egress traffic.
                
                Default is ingressAndEgress(3)."
            ::= { xfBridgePortMirrorEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8
        xfBridgePortStormCtrlTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfBridgePortStormCtrlEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table used to specifiy storm control at bridgeport (i.e.
                xfBridgePortRole is set to 'uni').
                
                This is not applicable for layer1 EPL ports."
            ::= { xfBridgePort 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1
        xfBridgePortStormCtrlEntry OBJECT-TYPE
            SYNTAX XfBridgePortStormCtrlEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Port entry."
            INDEX { dot1dBasePort }
            ::= { xfBridgePortStormCtrlTable 1 }

        XfBridgePortStormCtrlEntry ::=
            SEQUENCE {
                xfBridgePortStormCtrlDlfEnable
                    INTEGER,
                xfBridgePortStormCtrlDlfMaxBw
                    Integer32,
                xfBridgePortStormCtrlDlfDiscards
                    Counter32,
                xfBridgePortStormCtrlMcEnable
                    INTEGER,
                xfBridgePortStormCtrlMcMaxBw
                    Integer32,
                xfBridgePortStormCtrlMcDiscards
                    Counter32,
                xfBridgePortStormCtrlBcEnable
                    INTEGER,
                xfBridgePortStormCtrlBcMaxBw
                    Integer32,
                xfBridgePortStormCtrlBcDiscards
                    Counter32
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1.1
        xfBridgePortStormCtrlDlfEnable OBJECT-TYPE
            SYNTAX INTEGER
                {
                enable(1),
                disable(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to enable/disable storm control for destination
                look-up failures.
                
                enable(1)  : storm control for destination look-up failures
                                   enabled.
                disable(2) : storm control for destination look-up failures
                                   disabled.
                
                Default is disable(2).
                
                For layer1 EPL ports, this object can not be modified.
                "
            ::= { xfBridgePortStormCtrlEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1.2
        xfBridgePortStormCtrlDlfMaxBw OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to specify the rate limit (in units of frames/second)
                imposed on destination look-up failure flooding
                (if xfBridgePortStormCtrlDlfEnable='enable').
                
                Default value is 100.
                
                For layer1 EPL ports, this is set to 0 and it can not be modified."
            ::= { xfBridgePortStormCtrlEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1.3
        xfBridgePortStormCtrlDlfDiscards OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS deprecated
            DESCRIPTION
                "Number of discarded packets due to destination look-up
                failures strom protection."
            ::= { xfBridgePortStormCtrlEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1.4
        xfBridgePortStormCtrlMcEnable OBJECT-TYPE
            SYNTAX INTEGER
                {
                enable(1),
                disable(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to enable/disable storm control for multicast frames.
                
                enable(1)  : storm control for multicast frames enabled.
                disable(2) : storm control for multicast frames disabled.
                
                Default is disable(2).
                
                For layer1 EPL ports, this object can not be modified."
            ::= { xfBridgePortStormCtrlEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1.5
        xfBridgePortStormCtrlMcMaxBw OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to specify the rate limit (in units of frames/second)
                imposed on transmission of multicast frames
                (if xfBridgePortStormCtrlMcEnable = 'enable').
                
                Default value is 100.
                
                For layer1 EPL ports, this is set to 0 and it can not be modified."
            ::= { xfBridgePortStormCtrlEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1.6
        xfBridgePortStormCtrlMcDiscards OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS deprecated
            DESCRIPTION
                "Number of discarded packets due to multicast storm
                protection."
            ::= { xfBridgePortStormCtrlEntry 6 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1.7
        xfBridgePortStormCtrlBcEnable OBJECT-TYPE
            SYNTAX INTEGER
                {
                enable(1),
                disable(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                " Used to enable/disable storm control for broadcast frames.
                
                enable(1)  : storm control for multicast frames enabled.
                disable(2) : storm control for multicast frames disabled.
                
                Default is disable2).
                
                For layer1 EPL port, this object can not be modified."
            ::= { xfBridgePortStormCtrlEntry 7 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1.8
        xfBridgePortStormCtrlBcMaxBw OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Used to specify the rate limit (in units of frames/second)
                imposed on transmission of broadcast frames
                (if broadcast storm protection control is enabled).
                
                Default value is 100.
                
                For layer1 EPL ports, this is set to 0 and it can not be modified."
            ::= { xfBridgePortStormCtrlEntry 8 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.8.1.9
        xfBridgePortStormCtrlBcDiscards OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS deprecated
            DESCRIPTION
                "Number of discarded packets due to broadcast strom
                protection."
            ::= { xfBridgePortStormCtrlEntry 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.9
        xfBridgePortUserPriorityMappingTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfBridgePortUserPriorityMappingEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table is used to map user priority to network priority.
                The user priority is derived from the received frame
                and xfBridgePortTrusted specifies which L2/L3 priority
                fields on the received frame to use as user priority.
                
                The size of this table varies according to xfBridgePortTrusted.
                
                when xfBridgePortTrusted is set to 'ctagPcpTrusted', this table serves
                as a priority regeneration table.
                
                This table is used for layer1 EPL ports if xfEthernetEplUserPriorityMappingTable table is not available.
                "
            ::= { xfBridgePort 9 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.9.1
        xfBridgePortUserPriorityMappingEntry OBJECT-TYPE
            SYNTAX XfBridgePortUserPriorityMappingEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A mapping of user priority to network priority."
            INDEX { dot1dBasePort, xfBridgePortUserPriority }
            ::= { xfBridgePortUserPriorityMappingTable 1 }

        XfBridgePortUserPriorityMappingEntry ::=
            SEQUENCE {
                xfBridgePortUserPriority
                    Integer32,
                xfBridgePortNetworkPriority
                    Integer32
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.9.1.1
        xfBridgePortUserPriority OBJECT-TYPE
            SYNTAX Integer32 (0..2147483647)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "The value of the received user priority."
            ::= { xfBridgePortUserPriorityMappingEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.9.1.2
        xfBridgePortNetworkPriority OBJECT-TYPE
            SYNTAX Integer32 (0..7)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Priority value to which the user priority is mapped to."
            ::= { xfBridgePortUserPriorityMappingEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.10
        xfBridgePortCosGroupMappingTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfBridgePortCosGroupMappingEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table is used to specify the police of each cos.
                It maps the incoming cos (as specified by the xfBridgePortTrusted)
                to a cosGroup and each cosGroup has a seperate police. This table
                applies only to customer-facing ports (UNI, CEP, CNP) and when
                xfBridgePortBandwidthProfileTarget is set to 'policePortAndCos(3)'.
                INNI ports do not police traffic.
                
                The number of entries in this table depends on the value of
                xfBridgePortTrusted as follows:
                
                xfBridgePortTrusted = untrusted(1)       : This table is not applicable/available (empty).
                xfBridgePortTrusted = dscpIPv4Trusted(2) : This table has 64 entries for each dscp value
                xfBridgePortTrusted = dscpIPv6Trusted(4) : This table has 64 entries for each dscp value
                xfBridgePortTrusted = ctagPcpTrusted(3)  : only for UNI, CEP, and CNP with xfBridgePortAcceptableFrameTypes=110
                                                           (110:Admit Only Untagged and Priority Tagged Frames) - This table has
                                                           8 entries for each pcp value
                xfBridgePortTrusted = stagPcpTrusted(6)  : only for CNP - This table has 8 entries for each pcp value
                xfBridgePortTrusted = expTrusted(5)      : This table has 8 entries for each exp value
                xfBridgePortTrusted = dscpIPv4IPv6Trusted(7) : This table has 64 entries for each dscp value
                xfBridgePortTrusted = expDscpTrusted(8) : This table has 64 entries for each dscp value
                
                Changing xfBridgePortTrusted resets this table to default values.
                
                Reference: This table behaves in the same manner as xfBridgePortUserPriorityMappingTable."
            ::= { xfBridgePort 10 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.10.1
        xfBridgePortCosGroupMappingEntry OBJECT-TYPE
            SYNTAX XfBridgePortCosGroupMappingEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A mapping of a cos to a cos group."
            INDEX { dot1dBasePort, xfBridgePortCos }
            ::= { xfBridgePortCosGroupMappingTable 1 }

        XfBridgePortCosGroupMappingEntry ::=
            SEQUENCE {
                xfBridgePortCos
                    Integer32,
                xfBridgePortCosGroup
                    Integer32
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.10.1.1
        xfBridgePortCos OBJECT-TYPE
            SYNTAX Integer32 (0..63)
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The incoming cos value as specified by xfBridgePortTrusted."
            ::= { xfBridgePortCosGroupMappingEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.10.1.2
        xfBridgePortCosGroup OBJECT-TYPE
            SYNTAX Integer32 (1..8)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The cosGroup to which the incoming cos is mapped to.
                
                Default is 1 for all cos'es (i.e all cos'es are mapped
                to cos group 1)."
            ::= { xfBridgePortCosGroupMappingEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.11
        xfBridgePortCosGroupTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfBridgePortCosGroupEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "This table has 8 entries, one for each cos group. It specifies the
                bandwidth profile parameters of a cos group. 
                
                This table applies only to customer-facing ports (UNI, CEP, CNP) and when
                xfBridgePortBandwidthProfileTarget is set to 'policePortAndCos(3)'.
                INNI ports (and other ports) do not support traffic policing.
                "
            ::= { xfBridgePort 11 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.11.1
        xfBridgePortCosGroupEntry OBJECT-TYPE
            SYNTAX XfBridgePortCosGroupEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A cos group."
            INDEX { dot1dBasePort, xfBridgePortCosGroup }
            ::= { xfBridgePortCosGroupTable 1 }

        XfBridgePortCosGroupEntry ::=
            SEQUENCE {
                xfBridgePortCosGroupBandwidthProfile
                    Integer32
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.11.1.1
        xfBridgePortCosGroupBandwidthProfile OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Points to an entry in xfEthernetBandwidthProfileTable.
                This applies only if xfBridgePortBandwidthProfileTarget is 'policePortAndCos'.
                It applies only to customer-facing ports (UNI, CEP, CNP). INNI ports (and other)
                will not have traffic policing.
                
                Default is 1 (No rate limiting/drop none profile)
                "
            ::= { xfBridgePortCosGroupEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.12
        xfPbCvidRegistrationTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfPbCvidRegistrationEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A table used in a CEP to create the mapping between a C-VID and a
                 service represented by an S-VID."
            ::= { xfBridgePort 12 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.12.1
        xfPbCvidRegistrationEntry OBJECT-TYPE
            SYNTAX XfPbCvidRegistrationEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of a cvid registration."
            INDEX { dot1dBasePort, xfPbCvidRegistrationCvid }
            ::= { xfPbCvidRegistrationTable 1 }

        XfPbCvidRegistrationEntry ::=
            SEQUENCE {
                xfPbCvidRegistrationCvid
                    VlanIndex,
                xfPbCvidRegistrationSvid
                    VlanIndex,
                xfPbCvidRegistrationUntaggedPep
                    TruthValue,
                xfPbCvidRegistrationUntaggedCep
                    TruthValue,
                xfPbCvidRegistrationRowStatus
                    RowStatus
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.12.1.1
        xfPbCvidRegistrationCvid OBJECT-TYPE
            SYNTAX VlanIndex (0..4094)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "C-VID of this C-VID registration entry."
            ::= { xfPbCvidRegistrationEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.12.1.2
        xfPbCvidRegistrationSvid OBJECT-TYPE
            SYNTAX VlanIndex
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "S-VID for this C-VID registration entry."
            ::= { xfPbCvidRegistrationEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.12.1.3
        xfPbCvidRegistrationUntaggedPep OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "A flag indicating if this C-VID should be carried untagged at the PEP.
                A value of true(1) means untagged.
                
                if xfCVidRegistrationCVid equals xfPbCepPvid in xfBridgePortTable
                when the port is CEP, then TRUE; else FALSE."
            ::= { xfPbCvidRegistrationEntry 3 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.12.1.4
        xfPbCvidRegistrationUntaggedCep OBJECT-TYPE
            SYNTAX TruthValue
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "A flag indicating if this C-VID should be carried untagged at the CEP.
                A value of true(1) means untagged.
                
                if xfCVidRegistrationCVid equals xfPbCepPvid in xfBridgePortTable when
                the port is CEP, then TRUE; else FALSE."
            ::= { xfPbCvidRegistrationEntry 4 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.12.1.5
        xfPbCvidRegistrationRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "This is used to create/destroy an entry in this table."
            ::= { xfPbCvidRegistrationEntry 5 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.13
        xfPbEdgePortTable OBJECT-TYPE
            SYNTAX SEQUENCE OF XfPbEdgePortEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "A Provider Edge Port (PEP) table which indicates the subset of parameters
                needed for each PEP.
                
                This table depends on the C-VID Registration table created on the related CEP.
                More specifically:
                - when the first C-VID is registered to a particular S-VID in the C-VID registration table,
                  an entry shall be added to the Provider Edge Port table to show the PEP associated
                  to that S-VID. (If other C-VIDs are registered to the same S-VID, the entry in the Provider
                  Edge Port table is not modified.)
                - when the last C-VID is unregistered from that particular S-VID in the C-VID registration
                  table, the corresponding entry in the Provider Edge Port table shall be deleted.
                
                This table is read-only and has at most 512 entries."
            ::= { xfBridgePort 13 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.13.1
        xfPbEdgePortEntry OBJECT-TYPE
            SYNTAX XfPbEdgePortEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry of Provided Edge port."
            INDEX { dot1dBasePort, xfPbEdgePortSvid }
            ::= { xfPbEdgePortTable 1 }

        XfPbEdgePortEntry ::=
            SEQUENCE {
                xfPbEdgePortSvid
                    VlanIndex,
                xfPbEdgePortPvid
                    VlanIndex,
                xfPbEdgePortEnableIngressFiltering
                    TruthValue,
                xfPbEdgePortAcceptableFrameTypes
                    INTEGER,
                xfPbEdgePortDefaultUserPriority
                    Integer32
             }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.13.1.1
        xfPbEdgePortSvid OBJECT-TYPE
            SYNTAX VlanIndex (1..4094)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "The 12 bit S-VID associated with the PEP."
            ::= { xfPbEdgePortEntry 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.13.1.2
        xfPbEdgePortPvid OBJECT-TYPE
            SYNTAX VlanIndex
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "A 12-bit C-VID to be used for untagged frames received at the Provider Edge Port.
                
                This MO is read-only.
                If the PVID of a CEP is registered to this PEP, the PEP PVID is equal to the PVID
                of the CEP.Otherwise, it is set to 4095.
                
                This means that the PEP to which the PVID of the CEP is mapped allows untagged
                and priority tagged frames leaving the S-component through the PEP and towards
                the CEP. The other PEPs do not."
            ::= { xfPbEdgePortEntry 2 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.2.13.1.3
