--
-- RPS Configuration
--
-- This subtree supports the creation/deletion of an N+1 group.
--

-- xfRpsConfig OBJECT IDENTIFIER ::= { xfRpsMIB 1 }


xfRpsAvailableSlots OBJECT-TYPE
    SYNTAX      RpsConfigSlots
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "The slots containing MMUs that can be included in the N+1 protection group, according to the Terminals inserted in the AMM"               
    ::= { xfRpsConfig 1 }
 
                                            
xfRpsSelectedSlots OBJECT-TYPE
    SYNTAX      RpsConfigSlots
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
       "The slots that belong to the N+1 protection group. Only slots with the bits set to 1 in xfRpsAvailableSlots can be set to 1 in 
       this object, while anyone of the slots can be set to 0. In order to create a N+1 group, least two MMU units shall be selected (an OP and the PR). 
       PR removal is only allowed together with all MMUs belonging to the group."     
    ::= { xfRpsConfig 2 }
       
     
xfRpsOperatingMode OBJECT-TYPE
   SYNTAX INTEGER {
      operative(1),
      commissioning(2)
      }
    MAX-ACCESS  read-write
    STATUS      current                                                        
    DESCRIPTION "
    - Operative: normal way of operation
    - Commissioning: a LOS on the SFP does not prevent that channel to be protected"
   ::= { xfRpsConfig 3 }

       
--
-- RPS Channels Configuration
--
-- This subtree contains the configurations of the channels belonging to an N+1 group.
--

xfRpsChannelsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF XfRpsChannelsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains one row for each channel that has been configured to belong to N+1 group. 
        The table is automatically created when setting the object xfRpsConfigType"
    ::= { xfRpsConfig 4 } -- TBD check this. It was { xfRpsChannels 2 }


xfRpsChannelsEntry OBJECT-TYPE
    SYNTAX      XfRpsChannelsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A conceptual row in the xfRpsChannelsTable."
    INDEX { entPhysicalIndex }
    ::= { xfRpsChannelsTable  1 }   
    
    
XfRpsChannelsEntry ::= SEQUENCE {
	    xfRpsChannelsName                SnmpAdminString,
	    xfRpsChannelsPriority            INTEGER,
	    xfRpsChannelsOperatingMode       INTEGER,
	    xfRpsChannelsRxSide              INTEGER,
	    xfRpsChannelsTxSide              INTEGER,
	    xfRpsChannelsCurrentRequest      INTEGER,
	    xfRpsChannelsOpPreferred         INTEGER
    }

xfRpsChannelsName OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE (1..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The name of the member in the N+1 Group (OP1,OP2...,PR)."
    ::= { xfRpsChannelsEntry 1 }


xfRpsChannelsPriority OBJECT-TYPE
    SYNTAX INTEGER {  
      other(0),
      low(1),
      high(2)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "The priority of the channel: for OP channels only low(1) and high(2) are accepted; for PR channel only the value other(0) is accepted"
    ::= { xfRpsChannelsEntry 2 }

xfRpsChannelsOperatingMode OBJECT-TYPE
    SYNTAX INTEGER {
      automatic(1),
      locked(2),
      forcedSwitched(3),
      manualSwitched(4),
      noOccasional(5),
      looseOccasionalSTM1(6),
      fixedOccasionalSTM1(7),
      looseOccasionalEthernet(8),
      fixedOccasionalEthernet(9)
    }
    MAX-ACCESS  read-write
    STATUS      current                                                        
    DESCRIPTION "The command to perform an operation for a particular channel. 
    For rows pointing to OP channels only the values automatic, locked, forcedSwitched, manualSwitched are applicable. 
    The forcedSwitched value can only be set one row at a time and if the protection row is not set to fixedOccasionalSTM1, fixedOccasionalEthernet or locked.    
    The manualSwitched value can only be set one row at a time, if no other row contains forcedSwitched and if the protection row 
    is not set to fixedOccasionalSTM1, fixedOccasionalEthernet or locked.    
    For rows pointing to PR channel, only the values locked, noOccasional, looseOccasionalSTM1, fixedOccasionalSTM1, 
    looseOccasionalEthernet and fixedOccasionalEthernet are applicable.
    For PR, fixedOccasionalSTM1 and fixedOccasionalEthernet can be set only if no OP is set to forcedSwitched or manualSwitched 
    "
    ::= { xfRpsChannelsEntry 3 }

xfRpsChannelsRxSide OBJECT-TYPE
    SYNTAX INTEGER { 
            other(0),
			opSink(1),
			prSink(2)
		}
    MAX-ACCESS  read-only
    STATUS current
    DESCRIPTION "The Radio RST Sink from which the OP channel is fetching data. Returns 'other' for PR"
    ::= { xfRpsChannelsEntry 4 }

xfRpsChannelsTxSide OBJECT-TYPE
    SYNTAX INTEGER {
            other(0),
			opSource(1),
			prAndOpSource(2)
		}
    MAX-ACCESS  read-only
    STATUS current
    DESCRIPTION "The Radio RST Source to which the channel is transmitted. Returns 'other' for PR"
    ::= { xfRpsChannelsEntry 5 }

xfRpsChannelsCurrentRequest OBJECT-TYPE
    SYNTAX INTEGER {                            
      locked(1),        -- OP (15), PR (1)
      forced(2),        -- OP (3) 
      outOfService(3),  -- OP (14), PR (2)
      signalFail(4),    -- OP (4) , PR (4)
      highBer(5),	    -- OP (5) , PR (5)
      lowBer(6),        -- OP (6) , PR (6)
      earlyWarning(7),  -- OP (7) , PR (7)
      manual(8),        -- OP (8)
      looseOT(9),       -- PR (9)
      none(10)		    -- OP (other), PR (other)
      
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The current request active for the channel the row points to"
    ::= { xfRpsChannelsEntry 6 }

xfRpsChannelsOpPreferred OBJECT-TYPE
    SYNTAX INTEGER {  
      other(0),
      preferred(1),
      notPreferred(2)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This object allows selecting one of the n OP channel for a fast switch on the PR channel, which is permanently bridged in TX side on 
    PR path while the PR channel is available. This allows to reduce the total switching time for the selected OP from 10ms to <1ms. 
    When one of the other n OPs channels need to use the PR channel, the OP preferred is temporarily removed from PR path.  
	The OP preferred  value can only be set one row at a time.
	The default value is notPreferred for any OP.
	The object is readonly for PR (other).
	"
    ::= { xfRpsChannelsEntry 7 }


-- end of xfRpsChannelsTable


