    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Number that is used in communication with Level 3"
    ::= { ptIOHWHSLATMChannelEntry 1 }

