    ::= { ptGWSTGWSTConfigurationEntry 31 }

ptGWSTGWSTConfigurationMaxRedirectsTotal OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "Maximum number of redirect records across all tables."
    ::= { ptGWSTGWSTConfigurationEntry 32 }

ptGWSTGWSTConfigurationMaxRedirectsPerEPR OBJECT-TYPE
    SYNTAX          INTEGER (-2147483648..2147483647)
