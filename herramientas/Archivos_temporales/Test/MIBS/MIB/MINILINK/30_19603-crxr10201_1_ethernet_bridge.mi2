        -- 1.3.6.1.4.1.193.81.4.1.2.1
        xfEthernetBridgeObjects OBJECT IDENTIFIER ::= { xfEthernetBridgeMIB 1 }

        -- 1.3.6.1.4.1.193.81.4.1.2.1.1
