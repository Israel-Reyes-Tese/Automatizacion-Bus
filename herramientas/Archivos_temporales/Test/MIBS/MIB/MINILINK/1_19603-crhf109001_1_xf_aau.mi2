		xfAtmLayerHandling OBJECT IDENTIFIER ::= { xfAtmAggregationUnitMibObjects 3 }

		
--  Node definitions regarding the whole system.
-- The objects here handled are concerning the
-- unit parameters.
-- 
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.1
		xfAtmNeConfiguration OBJECT IDENTIFIER ::= { xfAtmLayerHandling 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.1.1
		xfAtmNeMaxVcc OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Maximum number of VC cross-connections allowed
				on this board.
				Default value: '1500'."
			::= { xfAtmNeConfiguration 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.1.2
		xfAtmNeMaxVpc OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Maximum number of VP connections allowed on
				board.
				Default value: '100'."
			::= { xfAtmNeConfiguration 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.1.3
		xfAtmNormalIfNum OBJECT-TYPE
			SYNTAX Integer32 (0..24)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A Normal ATM interface is an ATM interface
				typically (but not exclusively) positioned
				towards a NodeB.
				Their number is limited to 9 and it is
				decreased at each new creation of an ATM
				interface belonging to this type. '0' means
				that no new Normal interfaces are admitted.
				Default value: 9."
			::= { xfAtmNeConfiguration 3 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.1.4
		xfAtmExtendedIfNum OBJECT-TYPE
			SYNTAX Integer32 (0..7)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"An Extended ATM interface is an ATM interface
				typically (but not exclusively) positioned
				towards a RNC.
				Their number is limited to 7 and it is
				decreased at each new creation of an ATM
				interface belonging to this type. '0' means
				that no new Extended interfaces are admitted.
				Default value: 7."
			::= { xfAtmNeConfiguration 4 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.1.5
		xfAtmTotalIfNum OBJECT-TYPE
			SYNTAX Integer32 (0..31)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Number of ATM interfaces currently configured in the
				AAU system. This value is calculated taking into account
				both the interfaces created but not yet configured,
				and the interfaces created and configured.
				This MO is increased at each new ATM interface addition.
				0 means that no ATM interface is present in the system;
				16 means that no further interfaces can be created.
				Default value: 0."
			::= { xfAtmNeConfiguration 5 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.1.6
		xfAtmNNIIfNum OBJECT-TYPE
			SYNTAX Integer32 (0..30)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The value for this MO ranges from 0 to 30. 
				30 means that no ATM IF is configured as NNI; 
				0 means that no further ATM IF can be configured as NNI. 
				Every time an ATM IF is configured as NNI, 
				the counter is decreased by one; 
				When an ATM IF yet configured as NNI is removed or 
				reconfigured as UNI, the counter is increased.
				This counter do not affect the availability of ATM if
				(i.e. do not affect the xfAtmTotalIfNum). 
				Default value: 30."
			::= { xfAtmNeConfiguration 6 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.2
		xfAtmIfConfigurationTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfAtmIfConfigurationEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table adds ATM interface configuration parameters
				to the standard ones defined in atmInterfaceConfTable,
				(of which xfAtmIfConfigurationTable is an extension)."
			::= { xfAtmLayerHandling 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.2.1
		xfAtmIfConfigurationEntry OBJECT-TYPE
			SYNTAX XfAtmIfConfigurationEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"List of entries of the table."
			AUGMENTS { atmInterfaceConfEntry }
			::= { xfAtmIfConfigurationTable 1 }

		
		XfAtmIfConfigurationEntry ::=
			SEQUENCE { 
				xfAtmNetBw
					Integer32,
				xfAtmRxAvailableBw
					Integer32,
				xfAtmTxAvailableBw
					Integer32,
				xfAtmVpiPerVccUsed
					OCTET STRING,
				xfAtmIfType
					INTEGER
			 }

		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.2.1.1
		xfAtmNetBw OBJECT-TYPE
			SYNTAX Integer32
			UNITS "Cells/sec"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Net bandwidth of the ATM interface.
				It is the real bandwidth used by data traffic.
				The dafault value (4528 cells/sec) corresponds
				to the case of an ATM interface configured
				over one E1 link."
			::= { xfAtmIfConfigurationEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.2.1.2
		xfAtmRxAvailableBw OBJECT-TYPE
			SYNTAX Integer32
			UNITS "Cells/sec"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Available bandwidth for the guaranteed traffic
				classes on the ATM interface in the ingress
				direction. The dafault value (4528 cells/sec)
				corresponds to the case of an ATM interface
				configured over one E1 link."
			::= { xfAtmIfConfigurationEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.2.1.3
		xfAtmTxAvailableBw OBJECT-TYPE
			SYNTAX Integer32
			UNITS "Cells/sec"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Available bandwidth for the guaranteed traffic
				classes on the ATM interface in the egress
				direction. The dafault value (4528 cells/sec)
				corresponds to the case of an ATM interface
				configured over one E1 link."
			::= { xfAtmIfConfigurationEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.2.1.4
		xfAtmVpiPerVccUsed OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (5..239))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"String of active VPI values in use at the interface
				for VC switching.
				The maximum number of usable VPI values depends on
				the configuration of the following MIB objects:
				
				- xfAtmIfType
				- atmInterfaceMaxActiveVciBits
				 
				The string is a dotted representation of the used VPI values like:
				
				    X1.X2.X3. ...  .Xn
				
				where n is the maximum number of usable VPI values
				and Xi is either a numeric value for a used VPI or
				the '#' character, representing an empty VPI slot.
				
				As an example if at an ATM interface a maximum number
				of 6 VPI values can be used for VC switching, a string
				containing:
				              '248.#.75.#.#.10'
				
				means that VPI values 10,75 and 248 are in use for
				VC switching but there are three more usable VPI
				values for VC switching."
			::= { xfAtmIfConfigurationEntry 4 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.2.1.5
		xfAtmIfType OBJECT-TYPE
			SYNTAX INTEGER
				{
				undefined(0),
				normal(1),
				extended(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"According to interface type different VCI ranges are supported
				and configurable in the MO atmInterfaceMaxActiveVciBits.
				
				Due to limitation of the AAU ATM switch device (APC) when
				the corresponding VCI range is configured only a limited
				number of VPIs values can be used for VC switching at an ATM interface
				(see possible configurations below).
				
				Supported values for xfAtmIfType are:
				
				undefined(0): interface type not configured. This value can not be written.
				              It only shows the default, not configured state of a just created
				              ATM interface.
				
				normal(1):    VCI range / VPIs for VC switching
				               32.. 127 / 32
				               32.. 255 / 16
				               32.. 511 /  8
				               32..1023 /  4
				
				extended(2):  VCI range / VPIs for VC switching
				               32.. 255 / 48
				               32.. 511 / 24
				               32..1023 / 12
				               32..2047 /  6
				               32..4095 /  3
				
				NOTE: writing a valid value in the object (normal or extended) causes
				atmInterfaceMaxActiveVciBits to be reset, for consistency.
				Configuration of VCI range shall then be done setting a valid value
				in atmInterfaceMaxActiveVciBits.
				
				The usable/used VPIs for VC switching at the interface
				are specified by MO xfAtmVpiPerVccUsed.
				"
			::= { xfAtmIfConfigurationEntry 5 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.3
		xfAtmTCAlarmStatusTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfAtmTCAlarmStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains the information about the
				ATM Transmission Convergence sub-layer alarms
				their severity. There is only one alarm managed
				from this syb-layer:
				- Loss of Cell Delination.
				
				The indexing used is based on the E1 ifIndex
				which the upper layer (ATM G.804 or ATM IMA)
				is cross-connected to."
			::= { xfAtmLayerHandling 3 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.3.1
		xfAtmTCAlarmStatusEntry OBJECT-TYPE
			SYNTAX XfAtmTCAlarmStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"List of entries of the table."
			INDEX { ifIndex }
			::= { xfAtmTCAlarmStatusTable 1 }

		
		XfAtmTCAlarmStatusEntry ::=
			SEQUENCE { 
				xfAtmTCAlarmStatus
					BITS
			 }

		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.3.1.1
		xfAtmTCAlarmStatus OBJECT-TYPE
			SYNTAX BITS
				{
				lcd0(0),
				lcd1(1),
				lcd2(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object contains the severity of supported
				alarms per each E1 link cross-connected and
				configured as either G.804 or IMA link.
				Severity is indicated by three bits per alarm:
				bits 012
				     000   clear
				     001   undetermined
				     010   warning
				     011   minor
				     100   major
				     101   critical"
			::= { xfAtmTCAlarmStatusEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.4
		xfAtmVpTpAlarmStatusTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfAtmVpTpAlarmStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains the information about the
				severity of the ATM VC cross-connections alarms
				(at the VP connections termination points).
				The alarms supported are:
				- VP RDI,
				- VP AIS. 
				This table is indexed by the interface index
				(ifIndex), and by the atmVpi identifier of the
				VC cross-connection.
				"
			::= { xfAtmLayerHandling 4 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.4.1
		xfAtmVpTpAlarmStatusEntry OBJECT-TYPE
			SYNTAX XfAtmVpTpAlarmStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"List of entries of the table."
			INDEX { ifIndex, atmVclVpi }
			::= { xfAtmVpTpAlarmStatusTable 1 }

		
		XfAtmVpTpAlarmStatusEntry ::=
			SEQUENCE { 
				xfAtmVpTpAlarmStatus
					BITS
			 }

		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.4.1.1
		xfAtmVpTpAlarmStatus OBJECT-TYPE
			SYNTAX BITS
				{
				vpAis0(0),
				vpAis1(1),
				vpAis2(2),
				vpRdi0(3),
				vpRdi1(4),
				vpRdi2(5)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object contains the severity of supported
				alarms per each ATM VC cross-connection.
				Severity is indicated by three bits per alarm:
				bits 012
				     000   clear
				     001   undetermined
				     010   warning
				     011   minor
				     100   major
				     101   critical"
			::= { xfAtmVpTpAlarmStatusEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.5
		xfAtmTrafficDescrParamXTable OBJECT-TYPE
			SYNTAX SEQUENCE OF XfAtmTrafficDescrParamXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains a set of parameters which
				allow configuring a specific traffic profile.
				This table is an extension of the
				atmTrafficDescrParamTable. At the moment of the
				traffic descriptor creation (by means of
				atmTrafficDescrRowStatus), all the mandatory
				parameters and the extended ones must be provided."
			::= { xfAtmLayerHandling 5 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.5.1
		xfAtmTrafficDescrParamXEntry OBJECT-TYPE
			SYNTAX XfAtmTrafficDescrParamXEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"List of entries of the table."
			AUGMENTS { atmTrafficDescrParamEntry }
			::= { xfAtmTrafficDescrParamXTable 1 }

		
		XfAtmTrafficDescrParamXEntry ::=
			SEQUENCE { 
				xfAtmTrafficPolicingEnable
					INTEGER,
				xfAtmTrafficDescrName
					OCTET STRING,
				xfAtmTrafficDescrNumConn
					Counter32
			 }

		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.5.1.1
		xfAtmTrafficPolicingEnable OBJECT-TYPE
			SYNTAX INTEGER
				{
				disabled(0),
				enabled(1)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object enables/disables the policer relatively
				to the connection using this traffic descriptor."
			DEFVAL { enabled }
			::= { xfAtmTrafficDescrParamXEntry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.5.1.2
		xfAtmTrafficDescrName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (3..31))
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object enables the Element Manager to choose
				a name for each traffic descriptor. This selection
				must be done at the moment of the creation.
				The name is a string which must be unique per ATM unit."
			::= { xfAtmTrafficDescrParamXEntry 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.5.1.3
		xfAtmTrafficDescrNumConn OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object is a counter reporting the number of
				ATM connections currently using this traffic descriptor.
				If no ATM connections is using this traffic descriptor,
				the counter is set to '0' (default value);
				otherwise, a value greater than '0' is reported."
			::= { xfAtmTrafficDescrParamXEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.6
		xfAtmG804Table OBJECT-TYPE
			SYNTAX SEQUENCE OF XfAtmG804Entry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table is used to handle ATM interfaces directly
				stacked on the PDH layer (ATM over E1) by means of
				G.804 adaptation. This handling foresees:
				- ATM interface creation,
				- ATM interface removal,
				- Mapping of the ATM interface onto a physical device.
				This table is indexed using the interface index
				(ifIndex) of the E1 interface."
			::= { xfAtmLayerHandling 6 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.6.1
		xfAtmG804Entry OBJECT-TYPE
			SYNTAX XfAtmG804Entry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"List of entries of the table."
			INDEX { ifIndex }
			::= { xfAtmG804Table 1 }

		
		XfAtmG804Entry ::=
			SEQUENCE { 
				xfAtmG804DeviceIndex
					INTEGER,
				xfAtmG804RowStatus
					RowStatus,
				xfAtmG804IfIndex
					InterfaceIndex
			 }

		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.6.1.1
		xfAtmG804DeviceIndex OBJECT-TYPE
			SYNTAX INTEGER
				{
				unknown(0),
				imaDevice1(1),
				imaDevice2(2),
				imaDevice3(3)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Index of the device supporting the ATM interface."
			DEFVAL { unknown }
			::= { xfAtmG804Entry 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.6.1.2
		xfAtmG804RowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This object allows create, change, and delete
				operations on ATM interfaces directly stacked
				over the PDH layer (one E1 link) by means of
				the G.804 adaptation.
				
				To create a new row in the ifTable regarding
				the ATM interface, this object must be set to
				createAndGo. Only the following values are supported:
				
				active(1)         activates the ATM interface.
				                  This state will be possible
				                  only if the device index
				                  where to allocate the
				                  interface is provided.
				createAndGo(4)    used to set-up a connection and
				                  activate it. Used in multiple
				                  variable bindings since all objects
				                  in the row must have a valid value
				                  before the row is activated.
				destroy(6)        removes a connection. This will also
				                  remove all the ATM connections
				                  configured over this ATM interface."
			::= { xfAtmG804Entry 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.1.3.6.1.3
		xfAtmG804IfIndex OBJECT-TYPE
			SYNTAX InterfaceIndex
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object stores the ifIndex of the ATM interface
				identifying the corresponding entry in the Interfaces MIB."
			::= { xfAtmG804Entry 3 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2
		xfAtmAggregationUnitMibConformance OBJECT IDENTIFIER ::= { xfAtmAggregationUnitMib 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1
		xfAtmAggregationUnitMibGroups OBJECT IDENTIFIER ::= { xfAtmAggregationUnitMibConformance 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.1
		xfImaDevicesGroup OBJECT-GROUP
			OBJECTS { xfImaDevicesNumber, xfImaDevicesMaxLinksPerGroup, xfImaDevicesMaxAtmIf, xfImaDevicesPorts, xfImaDevicesFreePorts, 
				xfImaDevicesAllocatedGroups }
			STATUS current
			DESCRIPTION 
				"Set of objects providing the status and the configuration
				of the IMA devices."
			::= { xfAtmAggregationUnitMibGroups 1 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.2
		xfImaGroupXGroup OBJECT-GROUP
			OBJECTS { xfImaGroupIndexNext, xfImaGroupDeviceIndex, xfImaGroupRestartCommand }
			STATUS current
			DESCRIPTION 
				"Set of objects completing an IMA group configuration
				with the association between the new IMA group and
				the IMA device where it is created."
			::= { xfAtmAggregationUnitMibGroups 2 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.3
		xfImaGroupStatusGroup OBJECT-GROUP
			OBJECTS { xfImaGroupStatus }
			STATUS current
			DESCRIPTION 
				"Set of objects providing the status and the severity
				of IMA group alarms."
			::= { xfAtmAggregationUnitMibGroups 3 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.4
		xfImaLinkStatusGroup OBJECT-GROUP
			OBJECTS { xfImaLinkStatus }
			STATUS current
			DESCRIPTION 
				"Set of objects providing the status and the severity
				of IMA link alarms."
			::= { xfAtmAggregationUnitMibGroups 4 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.13
		xfAtmNeConfigurationGroup OBJECT-GROUP
			OBJECTS { xfAtmNeMaxVcc, xfAtmNeMaxVpc, xfAtmNormalIfNum, xfAtmExtendedIfNum, xfAtmTotalIfNum, 
				xfAtmNNIIfNum }
			STATUS current
			DESCRIPTION 
				"Set of objects providing the configuration parameters
				related to the ATM unit."
			::= { xfAtmAggregationUnitMibGroups 13 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.14
		xfAtmIfConfigurationGroup OBJECT-GROUP
			OBJECTS { xfAtmNetBw, xfAtmRxAvailableBw, xfAtmTxAvailableBw, xfAtmVpiPerVccUsed, xfAtmIfType
				 }
			STATUS current
			DESCRIPTION 
				"Set of objects providing the configuration parameters
				related to the ATM interfaces."
			::= { xfAtmAggregationUnitMibGroups 14 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.15
		xfAtmTCAlarmStatusGroup OBJECT-GROUP
			OBJECTS { xfAtmTCAlarmStatus }
			STATUS current
			DESCRIPTION 
				"Set of objects providing the status and the severity
				of the TC sub-layer alarms."
			::= { xfAtmAggregationUnitMibGroups 15 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.16
		xfAtmVpTpAlarmStatusGroup OBJECT-GROUP
			OBJECTS { xfAtmVpTpAlarmStatus }
			STATUS current
			DESCRIPTION 
				"Set of objects providing the status and the severity
				of the ATM VC cross-connection alarms."
			::= { xfAtmAggregationUnitMibGroups 16 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.17
		xfAtmTrafficDescrParamXGroup OBJECT-GROUP
			OBJECTS { xfAtmTrafficPolicingEnable, xfAtmTrafficDescrName, xfAtmTrafficDescrNumConn }
			STATUS current
			DESCRIPTION 
				"Set of objects providing configuration parameters 
				of ATM traffic descriptors, additional with the respect
				to atmTrafficDescrParamTable."
			::= { xfAtmAggregationUnitMibGroups 17 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.1.18
		xfAtmG804Group OBJECT-GROUP
			OBJECTS { xfAtmG804DeviceIndex, xfAtmG804RowStatus, xfAtmG804IfIndex }
			STATUS current
			DESCRIPTION 
				"Set of objects enabling the management to create,
				configure, and destroy new ATM interfaces stacked
				directly over the PDH layer."
			::= { xfAtmAggregationUnitMibGroups 18 }

		
		-- 1.3.6.1.4.1.193.81.4.2.1.2.2
		xfAtmAggregationUnitMibCompliances OBJECT IDENTIFIER ::= { xfAtmAggregationUnitMibConformance 2 }

		
--  this module
		-- 1.3.6.1.4.1.193.81.4.2.1.2.2.1
		xfAtmAggregationUnitMibCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"This module reports the compliance statement for
				network elements implementing ATM Aggregation Unit
				interfaces"
			MODULE -- this module
				MANDATORY-GROUPS { xfImaDevicesGroup, xfImaGroupXGroup, xfImaGroupStatusGroup, xfImaLinkStatusGroup, xfAtmG804Group, 
					xfAtmTrafficDescrParamXGroup, xfAtmVpTpAlarmStatusGroup, xfAtmIfConfigurationGroup, xfAtmNeConfigurationGroup, xfAtmTCAlarmStatusGroup
					 }
			::= { xfAtmAggregationUnitMibCompliances 1 }

		
	
	END

