    SYNTAX          INTEGER { unlicensed(0), licensed(1) }
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "INAP Gateway feature license (UNLICENSED/LICENSED). \nIf the INAP Gateway is unlicensed, the SS7 traffic cannot be activated."
    ::= { ptinapgwINAPGWConfigurationEntry 1 }

