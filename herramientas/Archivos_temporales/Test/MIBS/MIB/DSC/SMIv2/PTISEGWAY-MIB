ptDSCAVPDefinitionVendorID OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Vendor ID Definition name responsible for the AVP. \nThe combination of Vendor ID and AVP Code uniquely identifies an AVP. \nValid values: name of existing Vendor ID Definition."
    ::= { ptDSCAVPDefinitionEntry 1 }

ptDSCAVPDefinitionAVPCode OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Code of the AVP. \nThe combination of Vendor ID and AVP Code uniquely identifies an AVP. \nValid values: integers."
    ::= { ptDSCAVPDefinitionEntry 2 }

ptDSCAVPDefinitionName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "String representation of the AVP Definition. \nLeading and trailing whitespaces are trimmed. \nValid values: alphanumeric characters, dashes, underscores, spaces, and dots (maximum size of 64). Cannot start the name with reserved dsc_ or pt_ (case insensitive)."
    ::= { ptDSCAVPDefinitionEntry 3 }

ptDSCAVPDefinitionAVPType OBJECT-TYPE
    SYNTAX          INTEGER { utf8string(10), octetstring(20), 
                        unsigned32(30), unsigned64(40), integer32(50), 
                        integer64(51), time(60), grouped(70), 
                        enumerated(80), diameteridentity(90), 
                        diameteruri(100), ipfilterrule(110), 
                        address(120), unknown(130), float32(140), 
                        float64(150) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "AVP Type defines the validation and encoding format of the AVP values. \nImportant for Modification Record commands affecting the AVP values. \nValid values: UTF8Strings, OctetString, Unsigned32, Unsigned64, Integer32, Integer64, Time, Grouped, Enumerated, DiameterIdentity, DiameterURI, IPFilterRule, Address, Float32, Float64, and Unknown (as per RFC6733). \nDefault value: UTF8String (reference only)."
    ::= { ptDSCAVPDefinitionEntry 4 }

ptDSCAVPDefinitionComment OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "AVP Definition optional comment for supplementary information. \nOften includes the specification reference for the AVP definition. \nValid values: free-form string of maximum size 64 characters."
    ::= { ptDSCAVPDefinitionEntry 5 }

