    ::= { ptMTPM2UALinkEntry 14 }

ptMTPM2UALinkAlarms OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Alarms affect the alarms associated with the Link. DISABLED mode: Alarms will not be raised. ENABLED mode: Alarms will be raised. \nValid values: ENABLED and DISABLED. \nDefault value: ENABLED."
    ::= { ptMTPM2UALinkEntry 15 }

ptMTPM2UALinkLinkOscillationFilter OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Link Oscillation Filter. The timer can be enabled \n by setting this attribute to ENABLED. It can be disabled \n by setting this filed to DISABLED. The timer if enabled, \n prevents this M2UAlink from oscillating between \n aligned and non-aligned states repeatedly. It works \n in ANSI only using timer value from T32"
    ::= { ptMTPM2UALinkEntry 17 }

ptMTPM2UALinkDataLinkBandwidth OBJECT-TYPE
