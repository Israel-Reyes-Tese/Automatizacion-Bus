    STATUS          mandatory
    DESCRIPTION     
            "
             Average Bandwidth Usage during KPI bin interval.
             "
    ::= { sonusEthernetPortEthernetPktPortStatusIntervalStatisticsEntry 27 }

sonusEthernetPortEthernetPktPortStatusIntervalStatisticsPeakUsageBW OBJECT-TYPE
    SYNTAX          Gauge
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION     
            "
             Peak Bandwidth Usage during KPI bin interval.
             "
    ::= { sonusEthernetPortEthernetPktPortStatusIntervalStatisticsEntry 28 }

END -- end of module SONUS-PORT.
