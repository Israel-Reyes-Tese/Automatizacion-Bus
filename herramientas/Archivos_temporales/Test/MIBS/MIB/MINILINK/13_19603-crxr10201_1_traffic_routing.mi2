		xfBertResult OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates the result for the BERT at the moment. In the 
				form like '3.11143E-3'. Value  from last 
				test will be standing when BERT is stopped."
			::= { xfBertEntry 3 }

		
		-- 1.3.6.1.4.1.193.81.2.6.4.1.1.4
		xfBertResourceStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				available(1),
				unAvailable(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates whether resources are available to perform BERT."
