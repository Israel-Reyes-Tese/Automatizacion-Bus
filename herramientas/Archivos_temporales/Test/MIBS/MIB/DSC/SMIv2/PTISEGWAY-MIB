                      ptDSCSCTPParameterSetDSCParameterSetsIndex, 
                      ptDSCSCTPParameterSetIndex }
    ::= { ptDSCSCTPParameterSetTable 1 }

PtDSCSCTPParameterSetEntry ::=
    SEQUENCE {
        ptDSCSCTPParameterSetName       DisplayString,
        ptDSCSCTPParameterSetRTOInitial Integer32,
        ptDSCSCTPParameterSetRTOMinimum Integer32,
        ptDSCSCTPParameterSetRTOMaximum Integer32,
        ptDSCSCTPParameterSetMaximumPathRetrans       Integer32,
        ptDSCSCTPParameterSetMaximumAssocRetrans        Integer32,
        ptDSCSCTPParameterSetHeartbeat  INTEGER,
        ptDSCSCTPParameterSetBundling   INTEGER,
        ptDSCSCTPParameterSetHeartbeatInterval      Integer32,
        ptDSCSCTPParameterSetDelayedACKTO INTEGER,
        ptDSCSCTPParameterSetDSCP       Integer32,
        ptDSCSCTPParameterSetInternalID Integer32,
        ptDSCSCTPParameterSetDSCIndex   Integer32,
        ptDSCSCTPParameterSetDSCParameterSetsIndex          Integer32,
        ptDSCSCTPParameterSetParentOID  OBJECT IDENTIFIER,
        ptDSCSCTPParameterSetRowStatus  RowStatus,
        ptDSCSCTPParameterSetIndex      Integer32
    }

ptDSCSCTPParameterSetName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Name to uniquely identify the Parameter Set. \nLeading and trailing whitespaces are trimmed. \nValid values: alphanumeric characters, dashes, underscores, spaces, and dots (maximum size of 64)."
    ::= { ptDSCSCTPParameterSetEntry 1 }

ptDSCSCTPParameterSetRTOInitial OBJECT-TYPE
    SYNTAX          Integer32 (0..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Retransmission Timeout (RTO) Initial sets the initial expected time for packets to be sent and returned (0 to + ms). \nMust be greater than or equal to the RTO Minimum and less than or equal to the RTO Maximum. \nDefault value: 2000 ms."
    ::= { ptDSCSCTPParameterSetEntry 2 }

ptDSCSCTPParameterSetRTOMinimum OBJECT-TYPE
    SYNTAX          Integer32 (0..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Retransmission Timeout (RTO) Minimum is the smallest expected time for packets to be sent and returned (0 to + ms). \nDefault value: 60 ms."
    ::= { ptDSCSCTPParameterSetEntry 3 }

ptDSCSCTPParameterSetRTOMaximum OBJECT-TYPE
    SYNTAX          Integer32 (0..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Retransmission Timeout (RTO) Maximum is the largest expected time for packets to be sent and returned (0 to + ms). \nDefault value: 2000 ms."
    ::= { ptDSCSCTPParameterSetEntry 4 }

ptDSCSCTPParameterSetMaximumPathRetrans OBJECT-TYPE
    SYNTAX          Integer32 (0..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Maximum Path Retransmissions sets the number of retransmissions that will be attempted over the current transmission path without success before the path is deemed unavailable and a new transmission path is sought. \nValid values: 0 to +. \nDefault value: 4 (tailored for a single-homed association)."
    ::= { ptDSCSCTPParameterSetEntry 5 }

ptDSCSCTPParameterSetMaximumAssocRetrans OBJECT-TYPE
    SYNTAX          Integer32 (0..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Maximum Association Retransmissions sets the number of retransmissions to a remote device over all possible paths that will be attempted without success before deeming the end device as unavailable. \nValid values: 0 to +. \nDefault value: 4 (tailored for a single-homed association)."
    ::= { ptDSCSCTPParameterSetEntry 6 }

ptDSCSCTPParameterSetHeartbeat OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Heartbeat feature (DISABLED/ENABLED). \nEnabling the Heartbeat feature triggers the sending of cyclical test messages on the connection to determine its availability on the network. \nDefault value: ENABLED."
    ::= { ptDSCSCTPParameterSetEntry 8 }

ptDSCSCTPParameterSetBundling OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Bundling feature (DISABLED/ENABLED). \nEnabling Bundling allows the bundling of messages into larger packets for greater line efficiency. \nDefault value: ENABLED."
    ::= { ptDSCSCTPParameterSetEntry 9 }

ptDSCSCTPParameterSetHeartbeatInterval OBJECT-TYPE
    SYNTAX          Integer32 (0..65535)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Heartbeat Interval for the SCTP association (0 to 65535 ms). \nDefault value: 150 ms."
    ::= { ptDSCSCTPParameterSetEntry 10 }

ptDSCSCTPParameterSetDelayedACKTO OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Delayed Acknowledgement Time Out (ACK TO) sets the time that the system waits after receiving a packet before sending an acknowledgement (10 to 500 ms). \nTo disable the Delayed ACK feature, set the value to 0. \nDefault value: 20 ms."
    ::= { ptDSCSCTPParameterSetEntry 11 }

ptDSCSCTPParameterSetDSCP OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Differentiated Services Code Point is used to classify traffic within a network in order to determine the priority of the various classes of traffic (0 to 63). \nDefault value: 0."
    ::= { ptDSCSCTPParameterSetEntry 15 }

ptDSCSCTPParameterSetInternalID OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Parameter set unique internal ID. \nReserved for internal use."
    ::= { ptDSCSCTPParameterSetEntry 99 }

ptDSCSCTPParameterSetDSCIndex OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This is the index of the parent type for this object.
             This parent type is DSC."
    ::= { ptDSCSCTPParameterSetEntry 242 }

