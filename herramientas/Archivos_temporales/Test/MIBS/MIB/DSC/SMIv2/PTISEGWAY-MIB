        ptSGSUASCTPServerHeartbeatInterval  Integer32,
        ptSGSUASCTPServerRTOInitial     Integer32,
        ptSGSUASCTPServerRTOMinimum     Integer32,
        ptSGSUASCTPServerRTOMaximum     Integer32,
        ptSGSUASCTPServerMaximumInitRetrans   Integer32,
        ptSGSUASCTPServerMaximumAssocRetrans    Integer32,
        ptSGSUASCTPServerMaximumPathRetrans   Integer32,
        ptSGSUASCTPServerBundling       INTEGER,
        ptSGSUASCTPServerBundlingTimeout Integer32,
        ptSGSUASCTPServerDelayedAckTimeout  Integer32,
        ptSGSUASCTPServerChecksum       INTEGER,
        ptSGSUASCTPServerInStreams      Integer32,
        ptSGSUASCTPServerOutStreams     Integer32,
        ptSGSUASCTPServerDefaultSCTPValues  INTEGER,
        ptSGSUASCTPServerListenStatus   INTEGER,
        ptSGSUASCTPServerSUAIndex       Integer32,
        ptSGSUASCTPServerSUASCTPServersIndex    Integer32,
        ptSGSUASCTPServerActions        INTEGER,
        ptSGSUASCTPServerParentOID      OBJECT IDENTIFIER,
        ptSGSUASCTPServerRowStatus      RowStatus,
        ptSGSUASCTPServerIndex          Integer32
    }

