        ptSGSUAConfiguredASPSUAConfiguredASPsIndex          Integer32,
        ptSGSUAConfiguredASPActions     INTEGER,
        ptSGSUAConfiguredASPParentOID   OBJECT IDENTIFIER,
        ptSGSUAConfiguredASPRowStatus   RowStatus,
        ptSGSUAConfiguredASPIndex       Integer32
    }

ptSGSUAConfiguredASPID OBJECT-TYPE
    SYNTAX          Integer32 (1..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "SUA configured ASP unique identifier."
    ::= { ptSGSUAConfiguredASPEntry 1 }

ptSGSUAConfiguredASPPort OBJECT-TYPE
    SYNTAX          Integer32 (-2147483648..2147483647)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Remote port from which ASP will connect."
    ::= { ptSGSUAConfiguredASPEntry 2 }

ptSGSUAConfiguredASPIPAddress1 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "SUA Configured ASP primary address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned either in /etc/hosts or via a properly-configured DNS Server and should resolve to a subset of the remote IP Addresses configured for the host where the ASP terminates. Hostname resolution is attempted periodically. This attribute defines the primary IP address specifying the SCTP primary path to the remote SCTP endpoint. If the resolution returns more than one IP address, no guarantee is made as to non-ambiguous definition of the SCTP primary path."
    ::= { ptSGSUAConfiguredASPEntry 3 }

ptSGSUAConfiguredASPIPAddress2 OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "SUA Configured ASP secondary address. This may be a hostname or an IP Address. If a hostname is used it must be provisioned either in /etc/hosts or via a properly-configured DNS Server and should resolve to a subset of the remote IP Addresses configured for the host where the ASP terminates. Hostname resolution is attempted periodically."
    ::= { ptSGSUAConfiguredASPEntry 4 }

ptSGSUAConfiguredASPASPIDValidation OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "It is highly recommended that for increased security that this attribute remain enabled. When enabled the ASP using the configured endpoint MUST either NOT send the ASP ID parameter OR send the preconfigured ID. If an ASP ID parameter contains an identifier other than what is preconfigured, an ERROR (Invalid ASP Identifier) will be sent. When disabled, the ASP Identifier contained in the ASPUP is not validated. This is typically useful when changing the ID used by the ASP is especially troublesome"
    ::= { ptSGSUAConfiguredASPEntry 5 }

ptSGSUAConfiguredASPIncomingAppID OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "SUA Incoming AppID for Gateway Screening. On creation, the attribute is defaulted to SUA_DEFAULT."
    ::= { ptSGSUAConfiguredASPEntry 6 }

ptSGSUAConfiguredASPName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..255))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "SUA configured ASP name. \nAlphanumeric with dash and underscore for a maximum of 16 characters. \nAutomatically moved to upper-case. Must be unique in SUA. \nReserved value DYNAMIC for non-configured ASPs."
    ::= { ptSGSUAConfiguredASPEntry 7 }

ptSGSUAConfiguredASPExtendedMessageSupport OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "SUA support of extended message outgoing from this ASP. \nEnabled by default, the SUA data message Hop Counter parameter is forwarded as received into the SCCP UAP QoS parameter hop counter field. SCCP automatically converts the message to a XUDT(S) to include non-zero Hop Counter values. \nDisabling this feature ignores the SUA hop counter value and always send to SCCP the UAP QoS with a hop counter 0. SCCP will only generate and XUDT(S) message in this scenario if segmentation is required (too large MSU)."
    ::= { ptSGSUAConfiguredASPEntry 8 }

ptSGSUAConfiguredASPUserActivated OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "M3UA ASP Association User State (ENABLED/DISABLED)."
    ::= { ptSGSUAConfiguredASPEntry 18 }

ptSGSUAConfiguredASPDefaultNetworkAppearance OBJECT-TYPE
    SYNTAX          INTEGER { unset(-1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Upon receipt of an SUA message which does not contain a Routing Context or Network Appearance parameter, the SG uses this value to determine the destination SS7 network. If this value is UNSET (-1), the Default Network Appearance specified in the SUA SCTP Servers is used to determine the destination SS7 network. The Network Appearance must also be configured as an SCCP NA. The default value is UNSET. \nChanging this attribute value can impact traffic."
    ::= { ptSGSUAConfiguredASPEntry 19 }

ptSGSUAConfiguredASPIMFSupport OBJECT-TYPE
    SYNTAX          INTEGER { disabled(0), enabled(1) }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION     
            "Integrated Monitoring Feed feature support for this ASP. \nIMF must be licensed to enable this attribute. IMF Support attributes can be cleared for all configured SUA ASPs simultaneously by using the Reset IMF action in the SUA Configured ASPs screen."
    ::= { ptSGSUAConfiguredASPEntry 20 }

