    
    
MIB-EMSPLUS-IPASOLINK-PTP-MIB DEFINITIONS ::= BEGIN
    
    IMPORTS
        InterfaceIndex, InterfaceIndexOrZero
            FROM IF-MIB
        InetAddress, InetAddressType
            FROM INET-ADDRESS-MIB
        OBJECT-TYPE
            FROM RFC-1212
        Gauge, IpAddress, Opaque
            FROM RFC1155-SMI
        enterprises
            FROM SNMPv2-SMI
        DateAndTime, DisplayString, TruthValue
            FROM SNMPv2-TC;
    
    EnableDisableValue ::=
        INTEGER { invalid(0), disable(1), enable(2) }
    
    
    IpeClockIdentity ::=
        OCTET STRING (SIZE(0..8))
    
    
    IpeClockIntervalBase2 ::=
        INTEGER (-128..127)
    
    
    IpeClockPortState ::=
        INTEGER { invalid(0), initializing(1), faulty(2), disabled(3),
                        listening(4), preMaster(5), master(6), passive(7),
                        uncalibrated(8), slave(9) }
    
    
    IpeClockState ::=
        INTEGER { invalid(0), freerun(1), holdover(2), acquiring(3),
                        frequencyLocked(4), phaseAligned(5), warmUp(6) }
    
    
    IpeClockTxMode ::=
        INTEGER { invalid(0), unicast(1), multicast(2), multicastmix(3),
                        unknown(99) }
    
    
    IpeQualityAccuracy ::=
        INTEGER { invalid(0), reserved00(1), nanoSecond25(32),
                        nanoSecond100(33), nanoSecond250(34), microSec1(35),
                        microSec2dot5(36), microSec10(37), microSec25(38),
                        microSec100(39), microSec250(40), milliSec1(41),
                        milliSec2dot5(42), milliSec10(43), milliSec25(44),
                        milliSec100(45), milliSec250(46), second1(47),
                        second10(48), secondGreater10(49), unknown(254),
                        reserved255(255) }
    
    
    IpeQualityClass ::=
        Gauge (0..255)
    
    
    IpeVlanIndex ::=
        INTEGER (1..4094)
    
    
    IpeVlanIndexOrZero ::=
        INTEGER (0..4094)
    
    
    SeverityValue ::=
        INTEGER { cleared(1), indetermine(2), critical(3), major(4),
                        minor(5), warning(6), notReport(7) }
    
    
    -- 1.3.6.1.4.1.119
    nec OBJECT IDENTIFIER
        ::= { enterprises 119 }
    
    -- 1.3.6.1.4.1.119.2
    nec-mib OBJECT IDENTIFIER
        ::= { nec 2 }
    
    -- 1.3.6.1.4.1.119.2.3
    necProductDepend OBJECT IDENTIFIER
        ::= { nec-mib 3 }
    
    -- 1.3.6.1.4.1.119.2.3.69
    radioEquipment OBJECT IDENTIFIER
        ::= { necProductDepend 69 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213
    commonPnmsPlus OBJECT IDENTIFIER
        ::= { radioEquipment 213 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502
    ipasolink OBJECT IDENTIFIER
        ::= { commonPnmsPlus 502 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501
    pasoNeoIpe-common OBJECT IDENTIFIER
        ::= { ipasolink 501 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3
    alarmStatusGroup OBJECT IDENTIFIER
        ::= { pasoNeoIpe-common 3 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47
    asPtpGroup OBJECT IDENTIFIER
        ::= { alarmStatusGroup 47 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1
    asPtpClockGroup OBJECT IDENTIFIER
        ::= { asPtpGroup 1 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1
    asPtpClockModeTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF AsPtpClockModeEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "This table contains the PTP Clock information."
        ::= { asPtpClockGroup 1 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1.1
    asPtpClockModeEntry OBJECT-TYPE
        SYNTAX      AsPtpClockModeEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            ""
        INDEX       { asPtpClockModeNEAddressAddrType, asPtpClockModeNEAddress, asPtpClockModeIndex }
        ::= { asPtpClockModeTable 1 }
    
    AsPtpClockModeEntry ::= SEQUENCE {
        asPtpClockModeIndex                 INTEGER,
        asPtpClockModeNEAddress             InetAddress,
        asPtpClockProductDescription        DisplayString,
        asPtpClockRevisionData              DisplayString,
        asPtpClockRunningState              IpeClockState,
        asPtpClockQlValue                   INTEGER,
        asPtpClockClockIdentity             IpeClockIdentity,
        asPtpClockModeNEAddressAddrType     InetAddressType
    }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1.1.1
    asPtpClockModeIndex OBJECT-TYPE
        SYNTAX      INTEGER
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "Index of this table. The table is created per PTP card.
             value range : slot number + 1 (iPASOLINK400)
                         : 1 (iPASOLINK VR series)"
        ::= { asPtpClockModeEntry 1 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1.1.2
    asPtpClockModeNEAddress OBJECT-TYPE
        SYNTAX      InetAddress
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "reserved."
        ::= { asPtpClockModeEntry 2 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1.1.3
    asPtpClockProductDescription OBJECT-TYPE
        SYNTAX      DisplayString (SIZE(0..64))
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the Product Description."
        ::= { asPtpClockModeEntry 3 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1.1.4
    asPtpClockRevisionData OBJECT-TYPE
        SYNTAX      DisplayString (SIZE(0..32))
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the revisions for node hardware (HW),
             firmware (FW), and software (SW)."
        ::= { asPtpClockModeEntry 4 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1.1.5
    asPtpClockRunningState OBJECT-TYPE
        SYNTAX      IpeClockState
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the Clock state returned by PTP engine"
        ::= { asPtpClockModeEntry 5 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1.1.6
    asPtpClockQlValue OBJECT-TYPE
        SYNTAX      INTEGER { invalid(0), qlPrc(1), qlSsuA(2), qlSsuB(3),
                        qlOriginal(4), qlSec(5), qlDnu(6), qlInvx(7),
                        qlFailed(8), notAvail(9), qlPrs(20), qlStu(21),
                        qlSt2(22), qlTnc(23), qlSt3e(24), qlSt3(25),
                        qlSmc(26), qlSt4(27), qlDus(29) }
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the PTP Clock Quality Level."
        ::= { asPtpClockModeEntry 6 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1.1.7
    asPtpClockClockIdentity OBJECT-TYPE
        SYNTAX      IpeClockIdentity
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the clock identity."
        ::= { asPtpClockModeEntry 7 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.1.1.1001
    asPtpClockModeNEAddressAddrType OBJECT-TYPE
        SYNTAX      InetAddressType
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "Represents the IP address type of NE address index.
             Only ipv4(1) and ipv6(2) are supported."
        ::= { asPtpClockModeEntry 1001 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4
    asPtpParentDSTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF AsPtpParentDSEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "Table of information about the PTP clock Parent Datasets"
        ::= { asPtpClockGroup 4 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1
    asPtpParentDSEntry OBJECT-TYPE
        SYNTAX      AsPtpParentDSEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            ""
        INDEX       { asPtpParentDSNEAddressAddrType, asPtpParentDSNEAddress, asPtpParentDSIndex }
        ::= { asPtpParentDSTable 1 }
    
    AsPtpParentDSEntry ::= SEQUENCE {
        asPtpParentDSIndex                      INTEGER,
        asPtpParentDSNEAddress                  InetAddress,
        asPtpParentDSPortIdentity               OCTET STRING,
        asPtpParentDSStatus                     TruthValue,
        asPtpParentDSOffset                     IpeClockIntervalBase2,
        asPtpParentDSClockPhChRate              INTEGER,
        asPtpParentDSGMClockIdentity            IpeClockIdentity,
        asPtpParentDSGMClockPriority1           INTEGER,
        asPtpParentDSGMClockPriority2           INTEGER,
        asPtpParentDSGMClockQualityClass        IpeQualityClass,
        asPtpParentDSGMClockQualityAccuracy     IpeQualityAccuracy,
        asPtpParentDSGMClockQualityOffset       Gauge,
        asPtpParentDSNEAddressAddrType          InetAddressType
    }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.1
    asPtpParentDSIndex OBJECT-TYPE
        SYNTAX      INTEGER
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "Index of this table. The table is created per PTP card.
             value range : slot number + 1 (iPASOLINK400)
                         : 1 (iPASOLINK VR series)"
        ::= { asPtpParentDSEntry 1 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.2
    asPtpParentDSNEAddress OBJECT-TYPE
        SYNTAX      InetAddress
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "reserved."
        ::= { asPtpParentDSEntry 2 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.3
    asPtpParentDSPortIdentity OBJECT-TYPE
        SYNTAX      OCTET STRING (SIZE(0..10))
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the value of portIdentity of the port on
             the master that issues the Sync messages used in synchronizing
             this clock."
        ::= { asPtpParentDSEntry 3 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.4
    asPtpParentDSStatus OBJECT-TYPE
        SYNTAX      TruthValue
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the Parent Dataset ParentStats value.
    
             This value indicates whether the values of ParentDSOffset and
             ParentDSClockPhChRate have been measured and are valid.
             A TRUE value shall indicate valid data."
        ::= { asPtpParentDSEntry 4 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.5
    asPtpParentDSOffset OBJECT-TYPE
        SYNTAX      IpeClockIntervalBase2
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the Parent Dataset
             ParentOffsetScaledLogVariance value.
    
             This value is the variance of the parent clocks phase as measured
             by the local clock."
        ::= { asPtpParentDSEntry 5 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.6
    asPtpParentDSClockPhChRate OBJECT-TYPE
        SYNTAX      INTEGER
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the clock's parent dataset
             ParentClockPhaseChangeRate value.
    
             This value is an estimate of the parent clocks phase change rate
             as measured by the slave clock."
        ::= { asPtpParentDSEntry 6 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.7
    asPtpParentDSGMClockIdentity OBJECT-TYPE
        SYNTAX      IpeClockIdentity
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the parent dataset Grandmaster clock
             identity."
        ::= { asPtpParentDSEntry 7 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.8
    asPtpParentDSGMClockPriority1 OBJECT-TYPE
        SYNTAX      INTEGER (0..255)
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the parent dataset Grandmaster clock
             priority1."
        ::= { asPtpParentDSEntry 8 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.9
    asPtpParentDSGMClockPriority2 OBJECT-TYPE
        SYNTAX      INTEGER (0..255)
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the parent dataset grandmaster clock
             priority2."
        ::= { asPtpParentDSEntry 9 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.10
    asPtpParentDSGMClockQualityClass OBJECT-TYPE
        SYNTAX      IpeQualityClass
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the parent dataset grandmaster clock
             quality class."
        ::= { asPtpParentDSEntry 10 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.11
    asPtpParentDSGMClockQualityAccuracy OBJECT-TYPE
        SYNTAX      IpeQualityAccuracy
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the parent dataset grandmaster clock
             quality accuracy."
        ::= { asPtpParentDSEntry 11 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.12
    asPtpParentDSGMClockQualityOffset OBJECT-TYPE
        SYNTAX      Gauge
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the parent dataset grandmaster clock
             quality offset."
        ::= { asPtpParentDSEntry 12 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.4.1.1001
    asPtpParentDSNEAddressAddrType OBJECT-TYPE
        SYNTAX      InetAddressType
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "Represents the IP address type of NE address index.
             Only ipv4(1) and ipv6(2) are supported."
        ::= { asPtpParentDSEntry 1001 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7
    asPtpPortCurrentTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF AsPtpPortCurrentEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "Table of information about the clock ports running dataset."
        ::= { asPtpClockGroup 7 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1
    asPtpPortCurrentEntry OBJECT-TYPE
        SYNTAX      AsPtpPortCurrentEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            ""
        INDEX       { asPtpPortCurrentNEAddressAddrType, asPtpPortCurrentNEAddress, asPtpPortCurrentIfIndex, asPtpPortCurrentIfNumber }
        ::= { asPtpPortCurrentTable 1 }
    
    AsPtpPortCurrentEntry ::= SEQUENCE {
        asPtpPortCurrentIfIndex             InterfaceIndex,
        asPtpPortCurrentIfNumber            INTEGER,
        asPtpPortCurrentNEAddress           InetAddress,
        asPtpPortCurrentRole                INTEGER,
        asPtpPortCurrentState               IpeClockPortState,
        asPtpPortCurrentSyncOneStep         TruthValue,
        asPtpPortCurrentIpVersion           INTEGER,
        asPtpPortCurrentEncapType           INTEGER,
        asPtpPortCurrentTxMode              IpeClockTxMode,
        asPtpPortCurrentRxMode              IpeClockTxMode,
        asPtpPortCurrentPeerAddressType     InetAddressType,
        asPtpPortCurrentPeerAddress         InetAddress,
        asPtpPortCurrentNumOfAssocPorts     Gauge,
        asPtpPortCurrentNEAddressAddrType   InetAddressType
    }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.1
    asPtpPortCurrentIfIndex OBJECT-TYPE
        SYNTAX      InterfaceIndex
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "InterfaceIndex of this table."
        ::= { asPtpPortCurrentEntry 1 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.2
    asPtpPortCurrentIfNumber OBJECT-TYPE
        SYNTAX      INTEGER (1..32)
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "Administrative Number of logical port."
        ::= { asPtpPortCurrentEntry 2 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.3
    asPtpPortCurrentNEAddress OBJECT-TYPE
        SYNTAX      InetAddress
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "reserved."
        ::= { asPtpPortCurrentEntry 3 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.4
    asPtpPortCurrentRole OBJECT-TYPE
        SYNTAX      INTEGER { invalid(0), master(1), slave(2) }
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the Clock Role."
        ::= { asPtpPortCurrentEntry 4 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.5
    asPtpPortCurrentState OBJECT-TYPE
        SYNTAX      IpeClockPortState
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            "This object specifies the port state returned by PTP engine."
        ::= { asPtpPortCurrentEntry 5 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.6
    asPtpPortCurrentSyncOneStep OBJECT-TYPE
        SYNTAX      TruthValue
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies that one-step clock operation between the
             PTP master and slave device is enabled."
        ::= { asPtpPortCurrentEntry 6 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.7
    asPtpPortCurrentIpVersion OBJECT-TYPE
        SYNTAX      INTEGER
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifirst the IP version being used for PTP
             communication (the mapping used)."
        ::= { asPtpPortCurrentEntry 7 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.8
    asPtpPortCurrentEncapType OBJECT-TYPE
        SYNTAX      INTEGER
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the type of encapsulation if the interface
             is adding extra layers (eg. VLAN, Pseudowire encapsulation...)
             for the PTP messages."
        ::= { asPtpPortCurrentEntry 8 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.9
    asPtpPortCurrentTxMode OBJECT-TYPE
        SYNTAX      IpeClockTxMode
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the clock transmission mode as
    
             unicast:       Using unicast commnuication channel.
             multicast:     Using Multicast communication channel.
             multicast-mix: Using multicast-unicast communication channel"
        ::= { asPtpPortCurrentEntry 9 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.10
    asPtpPortCurrentRxMode OBJECT-TYPE
        SYNTAX      IpeClockTxMode
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the clock transmission mode as
    
             unicast:       Using unicast commnuication channel.
             multicast:     Using Multicast communication channel.
             multicast-mix: Using multicast-unicast communication channel"
        ::= { asPtpPortCurrentEntry 10 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.11
    asPtpPortCurrentPeerAddressType OBJECT-TYPE
        SYNTAX      InetAddressType
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the current peer's network address used
             for PTP communication. Based on the scenario and the setup
             involved, the values might look like these -
             Scenario                   Value
             -------------------   ----------------
             Single Master          master port
             Multiple Masters       selected master port
             Single Slave           slave port
             Multiple Slaves        <empty>
    
             (In relevant setups, information on available slaves and
             available masters will be available through
             asPtpPortAssociateTable)"
        ::= { asPtpPortCurrentEntry 11 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.12
    asPtpPortCurrentPeerAddress OBJECT-TYPE
        SYNTAX      InetAddress
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies the current peer's network address used
             for PTP communication. Based on the scenario and the setup
             involved, the values might look like these -
             Scenario                   Value
             -------------------   ----------------
             Single Master          master port
             Multiple Masters       selected master port
             Single Slave           slave port
             Multiple Slaves        <empty>
    
             (In relevant setups, information on available slaves and
             available masters will be available through
             asPtpPortAssociateTable)"
        ::= { asPtpPortCurrentEntry 12 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.13
    asPtpPortCurrentNumOfAssocPorts OBJECT-TYPE
        SYNTAX      Gauge
        ACCESS      read-only
        STATUS      obsolete
        DESCRIPTION
            "This object specifies -
             For a master port - the number of PTP slave sessions (peers)
             associated with this PTP port.
             For a slave port - the number of masters available to this slave
             port (might or might not be peered)."
        ::= { asPtpPortCurrentEntry 13 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.7.1.1001
    asPtpPortCurrentNEAddressAddrType OBJECT-TYPE
        SYNTAX      InetAddressType
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "Represents the IP address type of NE address index.
             Only ipv4(1) and ipv6(2) are supported."
        ::= { asPtpPortCurrentEntry 1001 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.9
    asPtpUnicastMasterTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF AsPtpUnicastMasterEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "This table contains unicast master node information and are
             created when the unicast master is registered."
        ::= { asPtpClockGroup 9 }
    
    -- 1.3.6.1.4.1.119.2.3.69.213.502.501.3.47.1.9.1
    asPtpUnicastMasterEntry OBJECT-TYPE
        SYNTAX      AsPtpUnicastMasterEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            ""
        INDEX       { asPtpUnicastMasterNEAddressAddrType, asPtpUnicastMasterNEAddress, asPtpUnicastMasterCardIndex, asPtpUnicastMasterIndex }
        ::= { asPtpUnicastMasterTable 1 }
    
    AsPtpUnicastMasterEntry ::= SEQUENCE {
        asPtpUnicastMasterCardIndex             INTEGER,
        asPtpUnicastMasterIndex                 INTEGER,
        asPtpUnicastMasterNEAddress             InetAddress,
        asPtpUnicastMasterClockId               IpeClockIdentity,
        asPtpUnicastMasterInetAddressType       InetAddressType,
        asPtpUnicastMasterInetAddress           InetAddress,
        asPtpUnicastMasterClockClass            IpeQualityClass,
        asPtpUnicastMasterStatus                INTEGER,
        asPtpUnicastMasterLossAnnounceAlarm     SeverityValue,
        asPtpUnicastMasterLossSyncAlarm         SeverityValue,
        asPtpUnicastMasterUnusableAlarm         SeverityValue,
        asPtpUnicastMasterAssociatedIfIndex     InterfaceIndexOrZero,
        asPtpUnicastMasterAssociatedIfNum       INTEGER,
